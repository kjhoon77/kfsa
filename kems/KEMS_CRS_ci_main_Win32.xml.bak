/* 글로벌 이벤트 공통 함수 */
function gfn_OnLoadCompleted() {

}

function gfn_OnExit() {
    if (GetSession() == 1) {
        if (gs_IsLocal == "N") {
            if (!gfn_Confirm(MSGW00050)) { // 정말로 로그아웃 하시겠습니까?
                 return false;
            }
        }

        if (length(trim(gs_LoginMgno)) > 0) {
            tit_createActionInfo(); // X-Framework용
            tit_clearActionInfo();
            tit_addSingleActionInfo("com:updateLastLoginHistory", "", 0, "");
            // 서버 호출 
            tit_callService(
                ""
                ,""
                ,""	// inDs
                ,""	// outDs
                ,"GLOBALLOGINMGNO=" + quote(gs_LoginMgno) // args
                ,""
                ,true
                ,true
                ,true
            );
        }
    }

    return true;
}

function gfn_OnError(obj,strUrl,nErrorCode,strErrorMsg) {
	gfn_Alert("OnError : " + nErrorCode + "/" + strErrorMsg);
}

/*
 * 기능 : Http 통신중 Error 발생시 발생하는 Event.
 */
function gfn_OnHttpError(obj,url,errorcode,errormsg) {
	switch (errorcode) {
	    case -2085613056:
			gfn_Alert("Protocol Adaptor에서 에러가 발생");
			break;
		case -2085605317:
			gfn_Alert("Network 이상");
			break;
		case -2085601264:
			gfn_Alert("HTTP 404: Not found, 문서를 찾을 수 없음 ");
			break;
		case -2085601263:
			gfn_Alert("HTTP 405: Method not allowed, 소스를 허용안함 ");
			break;
		case -2085601262:
			gfn_Alert("HTTP 500: Internal server error, 내부서 오류(잘못된 스크립트 실행시)");
			break;
		case -2085601261:
			gfn_Alert("HTTP 503: Service unavailable, 외부 서비스가 없거나 현재 멈춤 상태");
			break;
	}
}

/* 글로벌 함수 */
/*
 * 기능 : 업무화면의 윈도우를 오픈한다.
 * parameter : sFormUrl - 프로그램ID(Form ID)
 *             sFormTitle - 프로그램명(Form명)
 *             sMenuId - Menu ID
 * Return : 없음
 */
/*
 * 기능 : 업무화면의 윈도우를 오픈한다.
 * parameter : sFormUrl - 프로그램ID(Form ID)
 *             sFormTitle - 프로그램명(Form명)
 *             sMenuId - Menu ID
 *             sAddArg - 폼에 넘길 추가 파라메터. 예 : sSysGubu="training" sId="test"
 * Return : 없음
 */
function gfn_OpenChildWindow(sFormUrl, sFormTitle, sMenuId, sAddArg) {
    if (trim(sFormUrl).length() < 1) {
        return; // 존재하지않는 url이면 종료한다.
    }

    var ln_NewTabOrder = global.mnuTop.tabTopMenu.TabIndex;
    var ls_NewSystemGubun = gs_SystemGubun;
    var ls_NewSystemMenuId = gs_SystemMenuId;

    var ls_FormId = split(sFormUrl, "::"); // 시스템구분(Prefix)
    var ms_Arg = "sFormUrl=" + quote(sFormUrl);
    ms_Arg += " sFormTitle=" + quote(sFormTitle);
    ms_Arg += " sMenuId=" + quote(sMenuId);
    if (trim(sAddArg).length() > 0) {
        ms_Arg += " " + sAddArg;
    }

    ls_FormId = split(ls_FormId[1], "."); // Form Id용

    // 이미 활성화된 화면인지 체크한다.
    for (var i = 0; i < gds_OpenedForm.GetRowCount(); i++) {
        var ms_OpenFormNm = gds_OpenedForm.GetColumn(i, "MURL");

        if (object(ms_OpenFormNm) == ls_FormId[0]) {
            if (object(ms_OpenFormNm).sMenuId == sMenuId) {
                object(ms_OpenFormNm).setFocus();
                gds_SelectedForm.setColumn(0, "SELECTEDFORM", ls_FormId[0]); // 현재 선택된 화면ID
                return;
            } else {
                if (gfn_Confirm("중복 화면ID가 존재합니다. \n\n새로운 화면을 여실려면 기존 화면을 닫아야 합니다. \n\n기존 화면을 닫으시겠습니까?")) {
                    object(ms_OpenFormNm).close();
                    gs_SystemGubun = ls_NewSystemGubun;
                    gs_SystemMenuId = ls_NewSystemMenuId;
                    global.mnuTop.tabTopMenu.TabIndex = ln_NewTabOrder;
                } else {
                    return;
                }
            }
        }
    }

    // 활성화 폼이 30개 이상은 제한한다.
    if (gds_OpenedForm.RowCount() >= 10) {
        gfn_Alert("열린 화면이 10개 제한을 초과하셨습니다.");
        return;
    }

    // 신규 화면을 Open 한다.
    //NewWindow(ls_FormId[0], "COM::frmCOM0090MFrameWorkForm.xml", ms_Arg, 810, 600, "Scroll=True AutoSize=True Status=True", -1, -1);
    NewWindow(ls_FormId[0], sFormUrl, ms_Arg, -1, -1, "Scroll=False AutoSize=True Status=True Title=" + sFormTitle, 0, 0);
    //trace(iif(object(ls_FormId[0]).IsExistVar("ls_SystemMenuId"), "Y", "N"));

    // 활성화 폼 List에 해당 정보를 넣는다.
    gds_OpenedForm.AddRow();
    gds_OpenedForm.SetColumn(gds_OpenedForm.Currow, "MMENUID", sMenuId); // 메뉴ID
    gds_OpenedForm.SetColumn(gds_OpenedForm.Currow, "MURL", ls_FormId[0]); // 화면MURL
    gds_OpenedForm.SetColumn(gds_OpenedForm.Currow, "MMENUNM", sFormTitle); // 화면Title

    gds_SelectedForm.SetColumn(0, "SELECTEDFORM", ls_FormId[0]); // 현재 선택된 화면ID

    gfn_SetOpenedFormCnt(); // 열린 화면 갯수를 표시한다.
}

/* 글로벌 함수 */
/*
 * 기능 : 업무화면의 윈도우를 오픈한다.
 * parameter : sFormUrl - 프로그램ID(Form ID)
 *             sFormTitle - 프로그램명(Form명)
 *             sMenuId - Menu ID
 * Return : 없음
 */
/*
 * 기능 : 업무화면의 윈도우를 오픈한다.
 * parameter : sFormUrl - 프로그램ID(Form ID)
 *             sFormTitle - 프로그램명(Form명)
 *             sMenuId - Menu ID
 *             sAddArg - 폼에 넘길 추가 파라메터. 예 : sSysGubu="training" sId="test"
 * Return : 없음
 */
function gfn_OpenLinkChildWindow(sFormUrl, sFormTitle, sMenuId, sAddArg) {
    if (trim(sFormUrl).length() < 1) {
        return; // 존재하지않는 url이면 종료한다.
    }

    var ln_NewTabOrder = global.mnuTop.tabTopMenu.TabIndex;
    var ls_NewSystemGubun = gs_SystemGubun;
    var ls_NewSystemMenuId = gs_SystemMenuId;

    var ls_FormId = split(sFormUrl, "::"); // 시스템구분(Prefix)
    var ms_Arg = "sFormUrl=" + quote(sFormUrl);
    ms_Arg += " sFormTitle=" + quote(sFormTitle);
    ms_Arg += " sMenuId=" + quote(sMenuId);
    if (trim(sAddArg).length() > 0) {
        ms_Arg += " " + sAddArg;
    }

    ls_FormId = split(ls_FormId[1], "."); // Form Id용

    // 이미 활성화된 화면인지 체크한다.
    for (var i = 0; i < gds_OpenedForm.GetRowCount(); i++) {
        var ms_OpenFormNm = gds_OpenedForm.GetColumn(i, "MURL");

        if (object(ms_OpenFormNm) == ls_FormId[0]) {

            if (object(ms_OpenFormNm).sMenuId == sMenuId) {
				object(ms_OpenFormNm).close();
				gs_SystemGubun = ls_NewSystemGubun;
				gs_SystemMenuId = ls_NewSystemMenuId;
				global.mnuTop.tabTopMenu.TabIndex = ln_NewTabOrder;
            }
        }
    }

    // 활성화 폼이 10개 이상은 제한한다.
    if (gds_OpenedForm.RowCount() >= 10) {
        gfn_Alert("열린 화면이 10개 제한을 초과하셨습니다.");
        return;
    }

    // 신규 화면을 Open 한다.
    //NewWindow(ls_FormId[0], "COM::frmCOM0090MFrameWorkForm.xml", ms_Arg, 810, 600, "Scroll=True AutoSize=True Status=True", -1, -1);
    NewWindow(ls_FormId[0], sFormUrl, ms_Arg, -1, -1, "Scroll=False AutoSize=True Status=True Title=" + sFormTitle, 0, 0);
    //trace(iif(object(ls_FormId[0]).IsExistVar("ls_SystemMenuId"), "Y", "N"));

    // 활성화 폼 List에 해당 정보를 넣는다.
    gds_OpenedForm.AddRow();
    gds_OpenedForm.SetColumn(gds_OpenedForm.Currow, "MMENUID", sMenuId); // 메뉴ID
    gds_OpenedForm.SetColumn(gds_OpenedForm.Currow, "MURL", ls_FormId[0]); // 화면MURL
    gds_OpenedForm.SetColumn(gds_OpenedForm.Currow, "MMENUNM", sFormTitle); // 화면Title

    gds_SelectedForm.SetColumn(0, "SELECTEDFORM", ls_FormId[0]); // 현재 선택된 화면ID

    gfn_SetOpenedFormCnt(); // 열린 화면 갯수를 표시한다.
}


/*
 * 기능 : Open 메뉴 리스트에서 해당 폼을 삭제한다.
 * parameter : sUrl - Form Url
 * Return : 없음
 */
function gfn_DeleteOpenedMenu(sUrl) {
    var nRow = gds_OpenedForm.FindRow("MURL", sUrl);
    if (nRow >= 0) {
        gds_OpenedForm.DeleteRow(nRow);
    }

    global.mnuLeft.cbxOpenForm.Index = 0;
    gfn_SetOpenedFormCnt(); // 열린 화면 갯수를 표시한다.
}

/*
 * 기능 : 열린 화면 갯수를 표시한다.
 * parameter : 없음
 * Return : 없음
 */
function gfn_SetOpenedFormCnt() {
    global.mnuLeft.lbOpenFormCnt.Value = gds_OpenedForm.RowCount() + " 개"; // Open Windows Count

    if (gds_OpenedForm.RowCount() < 2) {
        global.mnuLeft.cbxOpenForm.Enable = false;
    } else {
        global.mnuLeft.cbxOpenForm.Enable = true;
    }
}

/* 글로벌 데이터셋 이벤트 */
function gds_OpenedForm_OnRowPosChanged(obj,nOldRow,nRow)
{
    gfn_SetOpenedFormCnt(); // 열린 화면 갯수를 표시한다.
}

/* 세션 종료 */
function gfn_OnBeforeExit(obj)
{

    if (GetSession() == 1) {	// form세션이 살아있으면
        if (gs_IsLocal == "N") {
            if (!gfn_Confirm(MSGW00050)) { // 정말로 로그아웃 하시겠습니까?
                 return false;
            }
        }

        if (length(trim(gs_LoginMgno)) > 0) {
            tit_createActionInfo(); // X-Framework용
            tit_clearActionInfo();
            tit_addSingleActionInfo("com:updateLastLoginHistory", "", 0, "");
            // 서버 호출 
            tit_callService(
                ""
                ,""
                ,""	// inDs
                ,""	// outDs
                ,"GLOBALLOGINMGNO=" + quote(gs_LoginMgno) // args
                ,""
                ,true
                ,true
                ,true
            );
        }
    //return true;
    }   
     
	// 로그아웃
	tit_createActionInfo();
	tit_clearActionInfo();
	tit_callService(
		"AuthAction"
		,"logout"
		,""	// inDs
		,""	// outDs
		,"gs_PSABUN=" + quote(gds_oUserInfo.GetColumn(0, "PSABUN"))
		,""
		,false
		,true
		,true
	);


	//gfn_Alert("정상적으로 로그아웃 되었습니다.");
}

function gds_ioSciCheck_OnLoadCompleted(obj,nErrorCode,strErrorMsg,nReason)
{
	
	/*
	
		ds_ioScicheck
		
		KIND         - 내/외국인 구분
		STRSENDBUF   - 실명확인요청전문
		RTNTEXT      - 실명확인결과전문
		
		STRPORT      - 포트번호
		STRJUMINNO   - 주민등록번호
		STRNAME      - 이름
		STRRESULT    - 실명확인결과
		STRDUP       - 중복가입정보
		STRVER       - CI버전
		STRCI        - CI 연계정보 1
		STRCI2       - CI 연계정보 2
		
		STRRETUNCODE - 실명확인결과값
		
	*/
	//gds_ioSciCheck.AddColumn("STRCI2");
	// 정상
	if("000" == gds_ioSciCheck.GetColumn(0, "STRRETUNCODE") || gfn_isNull(gds_ioSciCheck.GetColumn(0, "STRRETUNCODE"))) {
	
		if("1" == gds_ioSciCheck.GetColumn(0, "STRRESULT")) {          // 정상
			gds_ioSciCheck.SetColumn(0, "STRCI2", "실명인증 완료");
		} else if("2" == gds_ioSciCheck.GetColumn(0, "STRRESULT")) {   // 성명불일치
			gds_ioSciCheck.SetColumn(0, "STRCI2", "실명인증 실패(성명불일치)");
		} else if("3" == gds_ioSciCheck.GetColumn(0, "STRRESULT")) {   // 성명없음
			gds_ioSciCheck.SetColumn(0, "STRCI2", "실명인증 실패(성명없음)");
		} else if("4" == gds_ioSciCheck.GetColumn(0, "STRRESULT")) {   // 주민번호 조합오류
			gds_ioSciCheck.SetColumn(0, "STRCI2", "실명인증 실패(주민번호 조합오류)");
		} else if("6" == gds_ioSciCheck.GetColumn(0, "STRRESULT")) {   // 전문 항목 오류
			gds_ioSciCheck.SetColumn(0, "STRCI2", "실명인증 실패(전문 항목 오류)");
		} else if("7" == gds_ioSciCheck.GetColumn(0, "STRRESULT")) {   // 명의도용 차단
			gds_ioSciCheck.SetColumn(0, "STRCI2", "실명인증 실패(명의도용 차단)");
		} else if("8" == gds_ioSciCheck.GetColumn(0, "STRRESULT")) {   // 성명불일치
			gds_ioSciCheck.SetColumn(0, "STRCI2", "실명인증 실패(성명불일치)");
		} else if("9" == gds_ioSciCheck.GetColumn(0, "STRRESULT")) {   // 실명확인 당일차단
			gds_ioSciCheck.SetColumn(0, "STRCI2", "실명인증 실패(실명확인 당일차단)");
			// 하루에 여러번 조회하니 코드값이 1 -> 9로 변경됨
		}
		
	} else if("001" == gds_ioSciCheck.GetColumn(0, "STRRETUNCODE")) {   // 사용자 ID 오류
		gds_ioSciCheck.SetColumn(0, "STRCI2", "실명인증 실패(사용자ID 오류)");
	} else if("002" == gds_ioSciCheck.GetColumn(0, "STRRETUNCODE")) {   // 비밀번호 오류
		gds_ioSciCheck.SetColumn(0, "STRCI2", "실명인증 실패(비밀번호 오류)");
	} else if("003" == gds_ioSciCheck.GetColumn(0, "STRRETUNCODE")) {   // 사용권한 없음
		gds_ioSciCheck.SetColumn(0, "STRCI2", "실명인증 실패(사용권한 없음)");
	} else if("111" == gds_ioSciCheck.GetColumn(0, "STRRETUNCODE")) {   // SCI평가정보 System 장애
		gds_ioSciCheck.SetColumn(0, "STRCI2", "실명인증 실패(SCI평가정보 System 장애)");
	} else if("112" == gds_ioSciCheck.GetColumn(0, "STRRETUNCODE")) {   // SCI평가정보 DataBase 장애
		gds_ioSciCheck.SetColumn(0, "STRCI2", "실명인증 실패(SCI평가정보 DataBase 장애)");
	} else if("299" == gds_ioSciCheck.GetColumn(0, "STRRETUNCODE")) {   // 전문 Format Type Error
		gds_ioSciCheck.SetColumn(0, "STRCI2", "실명인증 실패(주민번호 형식 오류)");
	} else if("301" == gds_ioSciCheck.GetColumn(0, "STRRETUNCODE")) {   // 전문 종별 코드 오류
		gds_ioSciCheck.SetColumn(0, "STRCI2", "실명인증 실패(전문 종별 코드 오류)");
	} else if("302" == gds_ioSciCheck.GetColumn(0, "STRRETUNCODE")) {   // 업무 구분 코드 오류
		gds_ioSciCheck.SetColumn(0, "STRCI2", "실명인증 실패(업무 구분 코드 오류)");
	} else if("303" == gds_ioSciCheck.GetColumn(0, "STRRETUNCODE")) {   // 응답 코드 오류
		gds_ioSciCheck.SetColumn(0, "STRCI2", "실명인증 실패(응답 코드 오류)");
	} else if("444" == gds_ioSciCheck.GetColumn(0, "STRRETUNCODE")) {   // 명의도용방지 설정
		gds_ioSciCheck.SetColumn(0, "STRCI2", "실명인증 실패(명의도용방지 설정)");
	} else {   // 기타오류
		gds_ioSciCheck.SetColumn(0, "STRCI2", "실명인증 실패");
	}
	
	//trace(gds_ioSciCheck.savexml());
	
}

