    calDateStart.Value = GetDate();
    calDateEnd.Value = GetDate();
}

function Form_OnUnloadCompleted(obj) {
	Close();
}

function Form_OnKeyDown(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam) {
    switch (nChar) {
        case 120 : // 출력(F9)
            lfn_Print();
            break;
        case 121 : // 초기화(F10)
                lfn_Cancel();
            break;
        case 123 : // 화면출력(F12)
            lfn_PrintScreen();
            break;
        case 27 : // 종료(Esc)
            lfn_End();
            break;
    }
}

function lfn_End(obj)
{
	Close();
}
