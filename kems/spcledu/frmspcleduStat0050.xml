 { //2급		For(var j=0;j<ds_LicenseList.rowcount;j++)	{			TAB_Training.tab2.XChart1.Value(0, j) = ds_LicenseList.GetColumn(j,"BANG2_1"); 			TAB_Training.tab2.XChart1.Value(1, j) = ds_LicenseList.GetColumn(j,"BANG2_2");  		}          } else if (strCode == "4") { //안전관리자		For(var j=0;j<ds_LicenseList.rowcount;j++)	{			TAB_Training.tab2.XChart1.Value(0, j) = ds_LicenseList.GetColumn(j,"DAN1_1"); 			TAB_Training.tab2.XChart1.Value(1, j) = ds_LicenseList.GetColumn(j,"DAN1_2");  		}     } else if (strCode == "5") { //운송자		For(var j=0;j<ds_LicenseList.rowcount;j++)	{			TAB_Training.tab2.XChart1.Value(0, j) = ds_LicenseList.GetColumn(j,"DAN2_1"); 			TAB_Training.tab2.XChart1.Value(1, j) = ds_LicenseList.GetColumn(j,"DAN2_2");  		} 		  		       } else {         alert("확인요망");     }     			TAB_Training.tab2.XChart1.DataEditor = True  ;	TAB_Training.tab2.XChart1.DataEditorObj.Docked = "Docked_Bottom"; 			TAB_Training.tab2.XChart1.SerLeg(0) = "교부"  ;	TAB_Training.tab2.XChart1.SerLeg(1) = "재교부" ;		Phy_Start(strCode);	Phy2_Start(TAB_Training.tab2.cbxJibu.Value);	}//----------------------------------★★★★★★★★//차트에서 콤보박스 변환 시(왼쪽)   //----------------------------------function TAB_Training_tab2_cbxBang_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex){	Chart_Start(strCode);}/*-----------------------------------------------------------------------------**                                                                              **           파이 형태 차트 보여주기(왼족)                                      **                                                                              **------------------------------------------------------------------------------*/function Phy_Start(strCode){    //-----------소숫점 자리수 표현-------------------------------------------------------     TAB_Training.tab2.XChart0.Axis[0].LabelsFormat.Decimals = 0;	  //소수점 자리수 표현    //------------------------------------------------------------------------------------	TAB_Training.tab2.XChart0.PointLabels = true;	    //TAB_Training.tab2.XChart0.Gallery = "Gallery_Pie";	TAB_Training.tab2.XChart0.Chart3D = true;		TAB_Training.tab2.XChart0.BackColor = "white";	TAB_Training.tab2.XChart0.BorderColor = "white";		TAB_Training.tab2.XChart0.OpenData("COD_Values", 1, ds_LicenseList.rowcount);		if (ds_LicenseList.rowcount == 0){		TAB_Training.tab2.XChart0.Visible = false;	}else{		TAB_Training.tab2.XChart0.Visible = true;	}		if (strCode == "0") { //전체		For(i=0;i<ds_LicenseList.rowcount;i++){			var Value0 = ToNumber(ds_LicenseList.GetColumn(i,"LSPAYMENT_1"));  			TAB_Training.tab2.XChart0.Value(0, i) = Value0;		}    } else if (strCode == "1") { //공공		For(i=0;i<ds_LicenseList.rowcount;i++){			var Value0 = ToNumber(ds_LicenseList.GetColumn(i,"BANG0_1"));  			TAB_Training.tab2.XChart0.Value(0, i) = Value0;		}         } else if (strCode == "2") { //1급		For(i=0;i<ds_LicenseList.rowcount;i++){			var Value0 = ToNumber(ds_LicenseList.GetColumn(i,"BANG1_1"));  			TAB_Training.tab2.XChart0.Value(0, i) = Value0;		}     } else if (strCode == "3") { //2		For(i=0;i<ds_LicenseList.rowcount;i++){			var Value0 = ToNumber(ds_LicenseList.GetColumn(i,"BANG2_1"));  			TAB_Training.tab2.XChart0.Value(0, i) = Value0;		}         } else if (strCode == "4") { //		For(i=0;i<ds_LicenseList.rowcount;i++){			var Value0 = ToNumber(ds_LicenseList.GetColumn(i,"DAN1_1"));  			TAB_Training.tab2.XChart0.Value(0, i) = Value0;		}  		     } else if (strCode == "5") { //		For(i=0;i<ds_LicenseList.rowcount;i++){			var Value0 = ToNumber(ds_LicenseList.GetColumn(i,"DAN2_1"));  			TAB_Training.tab2.XChart0.Value(0, i) = Value0;		}  		 		      } else {         alert("확인요망");    }			TAB_Training.tab2.XChart0.CloseData("COD_Values");	TAB_Training.tab2.XChart0.Point[2].SeparateSlice =35;		For(var j=0;j<ds_LicenseList.rowcount;j++)	{		TAB_Training.tab2.XChart0.Legend(j) = ds_LicenseList.GetColumn(j,"GTTEAMNM");   	}}//----------------------------------★★★★★★★★//차트에서 콤보박스 변환 시(오른쪽)   //----------------------------------function TAB_Training_tab2_cbxJibu_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex){    Phy2_Start(strCode);}   /*-----------------------------------------------------------------------------**                                                                              **           파이 형태 차트 보여주기(오른족)                                    **                                                                              **------------------------------------------------------------------------------*/function Phy2_Start(strCode){    //-----------소숫점 자리수 표현-------------------------------------------------------     TAB_Training.tab2.XChart2.Axis[0].LabelsFormat.Decimals = 0;	  //소수점 자리수 표현    //------------------------------------------------------------------------------------	TAB_Training.tab2.XChart2.PointLabels = true;	    //TAB_Training.tab2.XChart2.Gallery = "Gallery_Pie";	TAB_Training.tab2.XChart2.Chart3D = true;		TAB_Training.tab2.XChart2.BackColor = "white";	TAB_Training.tab2.XChart2.BorderColor = "white";		TAB_Training.tab2.XChart2.OpenData("COD_Values", 1, 5);	if (ds_LicenseList.rowcount == 0){		TAB_Training.tab2.XChart2.Visible = false;	}else{		TAB_Training.tab2.XChart2.Visible = true;	}    var strIdx =  ds_LicenseList.FindRow( "GTMGNO", strCode );//특정 지부의 index값을 자져온다.    var Value0 = 0;//공공	var Value1 = 0;//1급    var Value2 = 0;//2급	var Value3 = 0;//안전관리자	var Value4 = 0;//운송자	if (strIdx == "-1"){//콤보에서 전체 선택시 		if (strCode == ''){			For(i=0;i<ds_LicenseList.rowcount;i++){					var Val0 = ToNumber(ds_LicenseList.GetColumn(i,"BANG0_1")) ;  				var Val1 = ToNumber(ds_LicenseList.GetColumn(i,"BANG1_1")) ;				var Val2 = ToNumber(ds_LicenseList.GetColumn(i,"BANG2_1")) ;  				var Val3 = ToNumber(ds_LicenseList.GetColumn(i,"DAN1_1")) ;				var Val4 = ToNumber(ds_LicenseList.GetColumn(i,"DAN2_1")) ;							  				Value0 =  Value0 + Val0;				Value1 =  Value1 + Val1;				Value2 =  Value2 + Val2;				Value3 =  Value3 + Val3;				Value4 =  Value4 + Val4;			}		}else{			Value0 =  0;			Value1 =  0;			Value2 =  0;			Value3 =  0;			Value4 =  0;	     }	} else {//특정 지부 선택시	    Value0 = ToNumber(ds_LicenseList.GetColumn(strIdx,"BANG0_1")) ;  	    Value1 = ToNumber(ds_LicenseList.GetColumn(strIdx,"BANG1_1")) ;  	    Value2 = ToNumber(ds_LicenseList.GetColumn(strIdx,"BANG2_1")) ;  	    Value3 = ToNumber(ds_LicenseList.GetColumn(strIdx,"DAN1_1")) ;  	    Value4 = ToNumber(ds_LicenseList.GetColumn(strIdx,"DAN2_1")) ;  		}				TAB_Training.tab2.XChart2.Value(0, 0) = Value0;//	TAB_Training.tab2.XChart2.Value(0, 1) = Value1;// 	TAB_Training.tab2.XChart2.Value(0, 2) = Value2;// 	TAB_Training.tab2.XChart2.Value(0, 3) = Value3;// 	TAB_Training.tab2.XChart2.Value(0, 4) = Value4;// 		    TAB_Training.tab2.XChart2.OpenData("COD_Values", 1, 5);	TAB_Training.tab2.XChart2.Point[2].SeparateSlice =35;	TAB_Training.tab2.XChart2.Legend(0) = "공공";   	TAB_Training.tab2.XChart2.Legend(1) = "1급";   	TAB_Training.tab2.XChart2.Legend(2) = "2급";   	TAB_Training.tab2.XChart2.Legend(3) = "안전관리자";   	TAB_Training.tab2.XChart2.Legend(4) = "운송자";   }//======================================================================================================//엑샐인쇄 시작----------------------------------------------------------------------------------------function btnExcell_OnClick(obj){	var strText = "수첩발급 수수료 현황(강습수료자 제외)";	setExcelGridHeader(TAB_Training.tab1.gdList,strText);		}//엑셀 함수 입니다. (위에 제목을 넣기 위함)function setExcelGridHeader(grdObj,strText){	create("grid","grdExcel",'BindDataSet="' + grdObj.BindDataset +'"');	grdExcel.contents = grdObj.Contents;		var headCnt = getHeadCount(grdExcel);	var totCol  = grdExcel.GetColCount();	var irow;	for(var i = 0 ; i < headCnt ; i++ )	{		irow = toInteger(grdExcel.GetCellProp("head",i,"row"));		grdExcel.SetCellProp("head",i,"row",irow+1);	}	var str_temp = grdExcel.GetCurContents();	var str_Head = '<cell align="left" col="0" colspan="' + totCol + '" display="text" text="' + strText + '"/>';		str_temp = replace(str_temp, '</head>',str_Head +'</head>');	grdExcel.contents = str_temp;	grdExcel.ExportExcelEx("sheet1","A1",false,true,true);	Destroy(grdExcel);}//엑셀 함수 입니다. (위에 제목을 넣기 위함)function getHeadCount(grdObj){	var strConts = grdObj.Contents;		var iPos = pos(strConts,"<head>");	var strHead = Mid(strConts,iPos);	iPos = pos(strHead,"</head>");	strHead = Mid(strHead,0,iPos+7);	var spObj = split(strHead,chr(10),"webstyle");	return spObj.length()-2;}//엑셀인쇄 끝-----------------------------------------------------------------------------------//======================================================================================================//보고서 출력function btnPrint_OnClick(obj){	gfn_CallReportPopup("frmBizTraining0050R01LicenseIssueFee.rex"                      , "ds_LicenseList"                      , "datefr=" + calDateFr.Value + "^dateto=" + calDateTo.Value                       , true);}//차트인쇄function btnPrintChart_OnClick(obj){	TAB_Training.TabIndex = 1;	TAB_Training.tab2.XChart1.ToolBar = true;	TAB_Training.tab2.XChart1.MenuBar = true;	}//화면인쇄function btnPrintWarmun_OnClick(obj){	printscreen();}]]></Script></Window>