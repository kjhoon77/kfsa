 gs_SystemMenuId;    var ls_FormId = split(sFormUrl, "::");    ls_FormId = split(ls_FormId[1], ".");    obj.Title = sFormTitle + "(" + ls_FormId[0] + ")";    gfn_MakeRequiredInputMark(this); // 필수 입력란을 설정한다.    ms_FormAuth = gfn_GetFormAuth(this); // 권한을 조회한다.    if (ms_FormAuth == "W") { // 권한에 따른 버튼 설정        lfn_SetFormAuth(true); // 저장 권한    } else if (ms_FormAuth == "R") {        lfn_SetFormAuth(false); // 읽기 권한    } else {        gfn_Alert(MSGI00080);        close();    }    // 공통 코드 조회    gfn_SearchTrainingCourse("T", ds_oCourse, "Y", "전체"); // 직능 코드 조회    cbxCourse.Value = "60";    gfn_SearchJibu(cbxJibu, "Y", "전체"); // 지부 코드 조회    edExamYear.Text = left(GetDate(), 4);    edExamOrder.SetFocus();}function Form_OnKeyDown(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam) {    gfn_Form_OnKeyDown(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam); // 공통 키 이벤트 처리}function Form_OnSize(obj,nCx,nCy,nState){    if (gs_IsResizingForm == "Y") {        gdExamPass.Height = nCy - gdExamPass.Top - 4;    }}function lfn_Search() {    if (!gfn_FormValidate(this)) { // 입력 Validation Check        return false;    }    ds_ioJubsuList.ClearData(); // 데이타셋 초기화    tit_clearActionInfo();	tit_addSearchActionInfo("spcledu:searchJubsuList", false);	// 서버 호출 	tit_callService(		""		,""		,""	                                            //  inDs		,"ds_ioJubsuList=ds_oJubsuList"	                //  outDs		,"EOMGGTMGNO=" + quote(cbxJibu.Value)           //  Args            + " EOYEAR=" + quote(edExamYear.Text)       //  Args            + " EOTCCOURSECD=" + quote(cbxCourse.Value)	//  Args            + " EOEXAMORDER=" + quote(edExamOrder.Text)	//  Args            + " GUBUN=" + quote(gubun)	// args		,"lfn_AfterSearchExamPass"		,false	);}function lfn_AfterSearchExamPass(nErrorCode, strErrorMsg) {    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.        gfn_ErrorMsg(nErrorCode, strErrorMsg);        return;    }}function lfn_Save(obj) {    if ( ds_ioJubsuList.GetCount() < 1 ) {        gfn_Alert("조회를 먼저 하세요.");        return;    }    if (!gfn_Confirm(MSGW00040)) { // 정말로 수정 저장 하시겠습니까?        return false;    }    tit_clearActionInfo();        tit_addSaveActionInfo(        ""	    // insert        , "spcledu:insertLicense"	// update        , ""	                            // delete        , ""	                            // flag column 명        , ""	                            // key sql id        , 0	                                // key 증가 값        , ""         , 0         , "N"	                            // 실행타입    );    // 서버 호출     tit_callService(        ""        ,""        ,"ds_iLicense=ds_ioJubsuList:U"	// inDs        ,""	// outDs        ,""	// args         ,"lfn_AfterSaveJubsuList"        ,false    );}function lfn_AfterSaveJubsuList(nErrorCode, strErrorMsg) {    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.        gfn_ErrorMsg(nErrorCode, strErrorMsg);        return;    }     lfn_Search();    //gfn_Alert ("OMR 처리전 일괄 시험합격 처리가 완료되었습니다.");}/* * 기능 : 버튼 활성/비활성시킨다. * parameter : bStatus - 활성/비활성여부(true:활성, false:비활성) * return : 없음 */function lfn_SetFormAuth(bStatus) {    btnSave.Enable = bStatus; // 저장 버튼    gdExamPass.Editable = bStatus;}/* 사용자 이벤트 함수 */function btnMutilSort_OnClick(obj){	gfn_GridMultiSort(gdExamPass, 1); // 멀티 정렬을 실행한다.}function gdExamJubsu_OnHeadClick(obj,nCell,nX,nY,nPivotIndex){	var lobj_Dataset = object(obj.BindDataset);	if (nCell == 0) { // 선택        if (obj.GetCellProp("head", nCell, "text") == 1) { // 전체가 이미 선택된 경우            obj.SetCellProp("head", nCell, "text", 0);            for(i = 0; i < lobj_Dataset.RowCount(); i++) {                lobj_Dataset.SetColumn(i, "EJPASSYN", "0");            }        } else { // 전체가 이미 선택되지 않은 경우            obj.SetCellProp("head", nCell, "text", 1);            for(i = 0; i < lobj_Dataset.RowCount(); i++) {                lobj_Dataset.SetColumn(i, "EJPASSYN", "1");            }        }	} else { // 헤드 정렬        lfn_SortGrid_OnHeadClick(obj,nCell,nX,nY,nPivotIndex);	}}function ExamOrderMgno_OnKillFocus(obj){	gfn_ComponentOnKillFocus(obj);    obj.Text = iif(gfn_IsNull(obj.Text), "", lpad(obj.Text, "0", 3));    }function btnToExcel_OnClick(obj){	if ( ds_oExamPass.GetTotalRowCount() > 0 ) {        lfn_Excel();	} else {        gfn_Alert("합격자 조회를 먼저 하세요.");	}}]]></Script></Window>