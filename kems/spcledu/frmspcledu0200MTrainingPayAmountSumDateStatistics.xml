Char,bShift,bControl,bAlt,nLLParam,nHLParam) {    // to do    gfn_Form_OnKeyDown(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam); // 공통 키 이벤트 처리}function Form_OnSize(obj,nCx,nCy,nState){    if (gs_IsResizingForm == "Y") {        gdTrainingJubsu.Height = nCy - gdTrainingJubsu.Top - 4;        gdTrainingJubsu.Width = nCx - gdTrainingJubsu.Left - 4;    }}/* * 기능 : 조회 */var flag = "";function lfn_Search(obj) {    if (!gfn_FormValidate(this)) { // 조회 Validation Check        return false;    }        if (radGubun.Value == "M") {        flag = 1;    } else {        flag = 2;    }        medTotalCnt.Value = "";    ds_oTrainingPayAmountSumDate.ClearData(); // 데이타셋 초기화    tit_clearActionInfo();	tit_addCsvSearchActionInfo("spcledu:searchTrainingPayAmountSumDateStatistics", false);	// 서버 호출 	tit_callService(		""		,""		,""	// inDs		,"ds_oTrainingPayAmountSumDate=ds_oTrainingPayAmountSumDate"	// outDs		,"TOMGGTMGNO=" + quote(cbxJibu.Value)            + " TOTCCOURSECD=" + quote(cbxCourse.Value)            + " TOYEAR=" + quote(edTrainingYear.Text)            + " TSPROCGUBUN=" + quote(radGubun.Value)            + " TSPROCMONTHSTART=" + quote(left(calMonthStart.Value, 6))            + " TSPROCMONTHEND=" + quote(left(calMonthEnd.Value, 6))            + " TSPROCDATESTART=" + quote(left(calDateStart.Value, 8))            + " TSPROCDATEEND=" + quote(left(calDateEnd.Value, 8))             + " COURSEGUBUN=T"// args 		,"lfn_AfterSearchTrainingJubsu"		,false	);}/* * 기능 : 출력 */function lfn_Print(obj){    if (ds_oTrainingPayAmountSumDate.GetCount() < 1) {        gfn_Alert("출력할 데이터가 없습니다.");        return false;    }    // to do Validation Check    if (flag == 1) {        gfn_CallReportPopup("frmspcledu0200R02TrainingMonthFeeSumStatistics.rex"                          , "ds_oTrainingPayAmountSumDate"                          , "TSPROCMONTHSTART=" + left(calMonthStart.Value, 6)                            + "^TSPROCMONTHEND=" + left(calMonthEnd.Value, 6)                            + "^TCCOURSENM=" + cbxCourse.Text);    } else {        gfn_CallReportPopup("frmspcledu0200R01TrainingDayFeeSumStatistics.rex"                          , "ds_oTrainingPayAmountSumDate"                          , "TSPROCDATESTART=" + left(calDateStart.Value, 8)                            + "^TSPROCDATEEND=" + left(calDateEnd.Value, 8)                            + "^TCCOURSENM=" + cbxCourse.Text);    }    }/* 공통 필수 종료 *//* 사용자 함수 *//* * 기능 : 결강자 조회 후 처리해야 할 내용 * parameter : nErrorCode - 에러코드 *             strErrorMsg - 에러메시지 * Return : 없음 */function lfn_AfterSearchTrainingJubsu(nErrorCode, strErrorMsg) {    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.        gfn_ErrorMsg(nErrorCode, strErrorMsg);        return;    }        if (ds_oTrainingPayAmountSumDate.rowcount == 0) {        medTotalCnt.Value = ds_oTrainingPayAmountSumDate.rowcount;    } else {        medTotalCnt.Value = ds_oTrainingPayAmountSumDate.rowcount - 1;    }        gfn_SearchMessage(gdTrainingJubsu); // 조회 메세지를 처리하고 그리드의 정렬 헤드 마크를 제거한다.}/* * 기능 : Transaction 서비스 호출 후 처리해야 할 내용 * parameter : strSvcID - 서비스ID *             nErrorCode - 에러코드 *             strErrorMsg - 에러메시지 * Return : 없음 */function lfn_AfterSaveTrainingJubsu(nErrorCode, strErrorMsg) {    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.        gfn_ErrorMsg(nErrorCode, strErrorMsg);        return;    }    gfn_SaveMessage(); // 저장 메세지를 처리한다.    lfn_Search(); // 결강자 조회}/* * 기능 : 버튼 활성/비활성시킨다. * parameter : bStatus - 활성/비활성여부(true:활성, false:비활성) * return : 없음 */function lfn_SetFormAuth(bStatus) {}/* 사용자 이벤트 함수 */function btnMutilSort_OnClick(obj){	gfn_GridMultiSort(gdTrainingJubsu, 0); // 멀티 정렬을 실행한다.}/* 회차 포커스 아웃될때 자동 0 입력 */function edTrainingOrder_OnKillFocus(obj){	gfn_ComponentOnKillFocu