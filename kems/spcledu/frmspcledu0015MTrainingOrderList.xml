FormAuth == "W") { // 권한에 따른 버튼 설정        lfn_SetFormAuth(true); // 저장 권한    } else if (ms_FormAuth == "R") {        lfn_SetFormAuth(false); // 읽기 권한    } else {        gfn_Alert(MSGI00080);        close();    }    // 공통 코드 조회    gfn_SearchCommonCode(ds_oMunjeGubun, "MUNJEGUBUN", "Y", "");    gfn_SearchTrainingCourse("T", ds_oCourse, "Y", "전체"); // 직능 코드 조회    cbxCourse.Value = "60";    gfn_SearchJibu(cbxJibu, "Y", "전체"); // 지부 코드 조회    gfn_SearchCommonCode(ds_oCourseGubun, "COURSEGUBUN", "Y", "전체");    edTrainingYear.Text = left(GetDate(), 4);        edTrainingOrderStart.Text = "001";    edTrainingOrderEnd.Text = "999";    edTrainingOrderStart.SetFocus();}function Form_OnKeyDown(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam) {    // to do    gfn_Form_OnKeyDown(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam); // 공통 키 이벤트 처리}function Form_OnSize(obj,nCx,nCy,nState){    if (gs_IsResizingForm == "Y") {        gdTrainingOrder.Height = nCy - gdTrainingOrder.Top - 4;        gdTrainingOrder.Width = nCx - gdTrainingOrder.Left - 4;    }}/* * 기능 : 조회 */function lfn_Search(obj) {    if (edTrainingOrderEnd.Enable == true && edTrainingOrderEnd.Text == "") {        gfn_Alert("종료회차를 입력해주십시오.");        edTrainingOrderEnd.SetFocus();        return false;    }    if (edTrainingOrderStart.Text > edTrainingOrderEnd.Text) {        gfn_Alert("시작회차가 종료회차보다 늦습니다.");        edTrainingOrderStart.SetFocus();        return false;    }    if (calDateStart.Value > calDateEnd.Value) {        gfn_Alert("시작일자가 종료일자보다 늦습니다.");        edTrainingOrderStart.SetFocus();        return false;    }    if (!gfn_FormValidate(this)) { // 입력 Validation Check        return false;    }        medTotalCnt.Value = "";    ds_oTrainingOrder.ClearData(); // 데이타셋 초기화        // 조회이력을 저장한다.      ls_HistoryArg = "지부=" + quote(cbxJibu.Text)                + ",년도=" + quote(edTrainingYear.Text)                + ",직능=" + quote(cbxCourse.Text)                + ",회차시작=" + quote(edTrainingOrderStart.Text)                + ",회차종료=" + quote(edTrainingOrderEnd.Text)                + ",교육시작일=" + quote(left(calDateStart.Value, 8))                + ",교육종료일=" + quote(left(calDateEnd.Value, 8));    gfn_InsertSearchPrintHistory(        "S"        , this.ID        , global.mnuLeft.cbxOpenForm.Text        , ls_HistoryArg    );    tit_clearActionInfo();	tit_addCsvSearchActionInfo("spcledu:searchTrainingOrderStat", false);	// 서버 호출 	tit_callService(		""		,""		,""	// inDs		,"ds_oTrainingOrder=ds_oTrainingOrder"	// outDs		,"TOMGJIBUCD=" + quote(cbxJibu.Value)            + " TOYEAR=" + quote(edTrainingYear.Text)            + " TCCOURSECD=" + quote(cbxCourse.Value)            + " TOTRAININGORDERSTART=" + quote(edTrainingOrderStart.Text)            + " TOTRAININGORDEREND=" + quote(edTrainingOrderEnd.Text)            + " TOSTARTDATE=" + quote(left(calDateStart.Value, 8))            + " TOENDDATE=" + quote(left(calDateEnd.Value, 8)) // args 		,"lfn_AfterSearchTrainingOrder"		,false	);}/* * 기능 : 출력 */function lfn_Print(obj){    // 조회이력을 저장한다.      ls_HistoryArg = "지부=" + quote(cbxJibu.Text)                + ",년도=" + quote(edTrainingYear.Text)                + ",직능=" + quote(cbxCourse.Text)                + ",회차시작=" + quote(edTrainingOrderStart.Text)                + ",회차종료=" + quote(edTrainingOrderEnd.Text)                + ",교육시작일=" + quote(left(calDateStart.Value, 8))                + ",교육종료일=" + quote(left(calDateEnd.Value, 8));    gfn_InsertSearchPrintHistory(        "P"        , this.ID        , global.mnuLeft.cbxOpenForm.Text        , ls_HistoryArg    );    // to do Validation Check    //gfn_CallReportPopup("", "ds_oTrainingOrder", "");}/* 공통 필수 종료 *//* 사용자 함수 *//* * 기능 : 결강자 조회 후 처리해야 할 내용 * parameter : nErrorCode - 에러코드 *  