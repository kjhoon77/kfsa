ate(), 4) + "0101";        calDateEnd.Value = GetDate();        calDateStart.UserData = "I:시작일자";        calDateEnd.UserData = "I:종료일자";        edTrainingOrderStart.UserData = "";        edTrainingOrderEnd.UserData = "";        calDateStart.SetFocus();	}}/* 회차 포커스 아웃될때 자동 0 입력 */function edTrainingOrder_OnKillFocus(obj){	gfn_ComponentOnKillFocus(obj);    if (obj.Text != "") {        obj.Text = iif(gfn_IsNull(obj.Text), "", lpad(obj.Text, "0", 3));    }}function edTrainingOrderEnd_EnterSearch(obj,nChar,bShift,bCtrl, bAlt,LLParam,HLParam){	if (nChar == 13) { // Enter Key        edTrainingOrderEnd.Text = iif(gfn_IsNull(edTrainingOrderEnd.Text), "", lpad(edTrainingOrderEnd.Text, "0", 3));        lfn_Search();    }}function btnToExcel_OnClick(obj) {    if (ds_oTrainingOrder.GetRowCount() < 1) {        gfn_Alert("출력할 데이터가 존재하지 않습니다.");        return false;    }	lfn_Excel(obj);}]]></Script></Window>