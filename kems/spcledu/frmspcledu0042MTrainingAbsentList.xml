      ls_HistoryArg = "지부=" + quote(cbxJibu.Text)                + ",직능=" + quote(cbxCourse.Text)                + ",년도=" + quote(edTrainingYear.Text)                + ",회차=" + quote(edTrainingOrder.Text);    gfn_InsertSearchPrintHistory(        "S"        , this.ID        , global.mnuLeft.cbxOpenForm.Text        , ls_HistoryArg    );    tit_clearActionInfo();	tit_addSearchActionInfo("spcledu:searchTrainingAbsentStat", false);	// 서버 호출 	tit_callService(		""		,""		,""	// inDs		,"ds_oTrainingAbsent=ds_oTrainingAbsent"	// outDs		,"TOMGJIBUCD=" + quote(cbxJibu.Value)            + " TCCOURSECD=" + quote(cbxCourse.Value)            + " TOYEAR=" + quote(edTrainingYear.Text)            + " TOTRAININGORDER=" + quote(edTrainingOrder.Text)// args 		,"lfn_AfterSearchTrainingAbsent"		,false	);}/* * 기능 : 결강자목록출력 */function lfn_Print(obj){        if (ds_oTrainingAbsentTemp.RowCount() < 1) {        gfn_Alert("결강자가 존재하지 않습니다.");        return false;    }        // 출력이력을 저장한다.      ls_HistoryArg = "지부=" + quote(cbxJibu.Text)                + ",직능=" + quote(cbxCourse.Text)                + ",년도=" + quote(edTrainingYear.Text)                + ",회차=" + quote(edTrainingOrder.Text);    gfn_InsertSearchPrintHistory(        "P"        , this.ID        , "결강자목록출력"        , ls_HistoryArg    );        ds_oTrainingAbsentTemp.Sort("TOHJUBSUNO:A");    gfn_CallReportPopup("frmspcledu0042R01TrainingAbsentList.rex", "ds_oTrainingAbsentTemp", "");}/* * 기능 : 결강자명단출력 */function lfn_PrintJubsuList(obj){        if (ds_oTrainingAbsentTemp.RowCount() < 1) {        gfn_Alert("결강자가 존재하지 않습니다.");        return false;    }        // 출력이력을 저장한다.      ls_HistoryArg = "지부=" + quote(cbxJibu.Text)                + ",직능=" + quote(cbxCourse.Text)                + ",년도=" + quote(edTrainingYear.Text)                + ",회차=" + quote(edTrainingOrder.Text);    gfn_InsertSearchPrintHis