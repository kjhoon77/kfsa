/* 공통 필수 시작 */function Form_OnLoadCompleted(obj) {    gfn_Form_OnLoadCompleted(obj); // 공통 폼 로딩 이벤트 처리    // 폼 환경설정    ls_SystemGubun = gs_SystemGubun;    ls_SystemMenuId = gs_SystemMenuId;    var ls_FormId = split(sFormUrl, "::");    ls_FormId = split(ls_FormId[1], ".");    obj.Title = sFormTitle + "(" + ls_FormId[0] + ")";    gfn_MakeRequiredInputMark(this); // 필수 입력란을 설정한다.    ms_FormAuth = gfn_GetFormAuth(this); // 권한을 조회한다.    if (ms_FormAuth == "W") { // 권한에 따른 버튼 설정        lfn_SetFormAuth(true); // 저장 권한    } else if (ms_FormAuth == "R") {        lfn_SetFormAuth(false); // 읽기 권한    } else {        gfn_Alert(MSGI00080);        close();    }    // 공통 코드 조회    gfn_SearchCommonCode(ds_oSunapGubun, "SUNAPGUBUN", "Y", "");    gfn_SearchTrainingCourse("T", ds_oCourse, "Y", "전체"); // 직능 코드 조회    cbxCourse.Value = "60";    gfn_SearchJibu(cbxJibu, "Y", "전체"); // 지부 코드 조회      edTrainingYear.Text = left(GetDate(), 4);    calDateStart.Value = GetDate();    calDateEnd.Value = GetDate();        calDateStart.SetFocus();}function Form_OnKeyDown(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam) {    // to do    gfn_Form_OnKeyDown(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam); // 공통 키 이벤트 처리}function Form_OnSize(obj,nCx,nCy,nState){    if (gs_IsResizingForm == "Y") {        gdTrainingJubsu.Height = nCy - gdTrainingJubsu.Top - 4;        gdTrainingJubsu.Width = nCx - gdTrainingJubsu.Left - 4;    }}/* * 기능 : 조회 */function lfn_Search(obj) {    if (!gfn_FormValidate(this)) { // 조회 Validation Check        return false;    }    if (calDateStart.Value > calDateEnd.Value) {        gfn_Alert("시작일자가 종료일자보다 늦습니다.");        calDateStart.SetFocus();        return false;    }    ds_oTrainingRepay.ClearData(); // 데이타셋 초기화        // 조회이력을 저장한다.      ls_HistoryArg = "지부=" + quote(cbxJibu.Text)                + ",직능=" + quote(cbxCourse.Text)                + ",년도=" + quote(edTrainingYear.Text)                + ",처리일자시작=" + quote(left(calDateStart.Value, 8))                + ",처리일자종료=" + quote(left(calDateEnd.Value, 8))                + ",정렬방법=" + quote(radSort.Text);    gfn_InsertSearchPrintHistory(        "S"        , this.ID        , global.mnuLeft.cbxOpenForm.Text        , ls_HistoryArg    );        medTotalCnt.Value = "";    tit_clearActionInfo();	tit_addCsvSearchActionInfo("spcledu:searchTrainingRepayList", false);	// 서버 호출 	tit_callService(		""		,""		,""	// inDs		,"ds_oTrainingRepay=ds_oTrainingRepay"	// outDs		,"TOMGGTMGNO=" + quote(cbxJibu.Value)            + " TOTCCOURSECD=" + quote(cbxCourse.Value)            + " TOYEAR=" + quote(edTrainingYear.Text)            + " TSPROCDATESTART=" + quote(left(calDateStart.Value, 8))            + " TSPROCDATEEND=" + quote(left(calDateEnd.Value, 8))            + " SORT=" + quote(radSort.Value)            + " COURSEGUBUN=T"// args 		,"lfn_AfterSearchTrainingJubsu"		,false	);}/* * 기능 : 출력 */function lfn_Print(obj){    // to do Validation Check    if (ds_oTrainingRepay.RowCount() < 1) {        gfn_Alert("출력할 데이터가 존재하지 않습니다.");        return false;    }        // 출력이력을 저장한다.      ls_HistoryArg = "지부=" + quote(cbxJibu.Text)                + ",직능=" + quote(cbxCourse.Text)                + ",년도=" + quote(edTrainingYear.Text)                + ",처리일자시작=" + quote(left(calDateStart.Value, 8))                + ",처리일자종료=" + quote(left(calDateEnd.Value, 8))                + ",정렬방법=" + quote(radSort.Text);    gfn_InsertSearchPrintHistory(        "P"        , this.ID        , global.mnuLeft.cbxOpenForm.Text        , ls_HistoryArg    );        gfn_CallReportPopup("frmspcledu0210R01TrainingRepayList.rex"                      , "ds_oTrainingRepay"                      , "TSPROCDATESTART=" + quote(left(calDateStart.Value, 8))                        + "^TSPROCDATEEND=" + quote(left(calDateEnd.Value, 8))                        + "^TCCOURSENM=" + quote(cbxCourse.Text));}/* 공통 필수 종료 *//* 사용자 함수 *//* * 기능 : 결강자 조회 후 처리해야 할 내용 * parameter : nErrorCode - 에러코드 *             strErrorMsg - 에러메시지 * Return : 없음 */function lfn_AfterSearchTrainingJubsu(nErrorCode, strErrorMsg) {    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.        gfn_ErrorMsg(nErrorCode, strErrorMsg);        return;    }    if (ds_oTrainingRepay.GetRowCount() > 0) {        gdTrainingJubsu.Format = "default0";    } else {        gdTrainingJubsu.Format = "default";    }        medTotalCnt.Value = ds_oTrainingRepay.rowcount;    gfn_SearchMessage(gdTrainingJubsu); // 조회 메세지를 처리하고 그리드의 정렬 헤드 마크를 제거한다.}/* * 기능 : Transaction 서비스 호출 후 처리해야 할 내용 * parameter : strSvcID - 서비스ID *             nErrorCode - 에러코드 *             strErrorMsg - 에러메시지 * Return : 없음 */function lfn_AfterSaveTrainingJubsu(nErrorCode, strErrorMsg) {    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.        gfn_ErrorMsg(nErrorCode, strErrorMsg);        return;    }    gfn_SaveMessage(); // 저장 메세지를 처리한다.    lfn_Search(); // 결강자 조회}/* * 기능 : 버튼 활성/비활성시킨다. * parameter : bStatus - 활성/비활성여부(true:활성, false:비활성) * return : 없음 */function lfn_SetFormAuth() {}/* 사용자 이벤트 함수 */function btnMutilSort_OnClick(obj){	gfn_GridMultiSort(gdTrainingJubsu, 0); // 멀티 정렬을 실행한다.}function btnToExcel_OnClick(obj) {    if (ds_oTrainingRepay.GetRowCount() < 1) {        gfn_Alert("출력할 데이터가 존재하지 않습니다.");        return false;    }	lfn_Excel(obj);}]]></Script></Window>