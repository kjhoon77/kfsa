/*
 * 기능 : 조회
 */
function lfn_Search(obj)
{
    ds_oLicenseReport.ClearData(); // 데이타셋 초기화
    tit_clearActionInfo();
    tit_addCsvSearchActionInfo("training:searchLicenseReport", false);
    // 서버 호출
    tit_callService(
        ""
        ,""
        ,""	// inDs
        ,"ds_oLicenseReport=ds_oLicenseReport"	// outDs
        ,"LLCSNOSTART=" + quote(edYear + "-" + substr(cbxJibu.Value,1,2) + "-" + cbxCourse.Value + "-" + cbxPassGubun.Value + "-" + edLcsNoStart.Text)
            + " LLCSNOEND=" + quote(edYear + "-" + substr(cbxJibu.Value,1,2) + "-" + cbxCourse.Value + "-" + cbxPassGubun.Value + "-" + edLcsNoEnd.Text)
            
            + " PASSGUBUN=" + quote(cbxPassGubun.Value)	// 취득구분
            
            + " PRINTGUBUN=" + quote(radPrintGubun.Value)	// 출력구분
        ,""
        ,false
        ,""
        ,true
    );
    
    medSearchCnt.Value = ds_oLicenseReport.GetRowCount();
    
    medCheckCnt.Value = "";
    	
}




/*
 * 기능 : 출력
 */
function lfn_Print4700(obj) {
	var ls_PrintGubun = ""; // 프린터 구분(1:HP 4700DN, 2: HP 65050)
    
    if (!gfn_FormValidate(this)) { // 입력 Validation Check
        return false;
    }

	// 프린터구분 출력
	if (obj.ID == "btnPrint4700") {
	
		ls_PrintGubun = "1";
		
	} else if (obj.ID == "btnPrint65050") {
	
		ls_PrintGubun = "2";
		
	}
	
    if (ds_oLicenseReport.RowCount() < 1) {
        gfn_Alert("출력할 데이터가 없습니다.");
        return false;
    }
   
    if(!gfn_CheckChangedGrid(this))  //선택한것이 있는지 체크 메세지 보임
    { alert("출력할 인원을 선택해 주십시오.");      
        return false;
    }

    if (edLcsNoStart.Text == "000000"
        || edLcsNoEnd.Text == "000000"
        || length(edLcsNoStart.Text) < 1
        || length(edLcsNoEnd.Text) < 1) { // 입력 Validation Check
        gfn_Alert("출력할 수첩이 없습니다.");
        return false;
    }
	// HP 4700DN 용 출력	if (ls_PrintGubun == "1") {
		if (!gfn_Confirm("수첩을 일괄출력 하시겠습니까?\n\n"+
						 "  * HP 4700DN 프린터 출력전용\n"+
						 "  * 환경설정의 출력환경설정도 프린터를 HP 4700DN으로 설정 후 출력")) { // 입력 Validation Check	
			return false;	
		}
	}
    
    // HP E65050 용 출력
	if (ls_PrintGubun == "2") {
		if (!gfn_Confirm("수첩을 일괄출력 하시겠습니까?\n\n"+
						 "  * HP E65050 프린터 출력전용\n"+
						 "  * 환경설정의 출력환경설정도 프린터를 HP E65050으로 설정 후 출력")) { // 입력 Validation Check	
			return false;	
		}	}

    var ls_LicenseImageDir = "/PRTDATA/" + left(getDate(),4) + "/" + SubStr(getDate(),4,2);

    var ls_RexFile = "";
    switch (cbxCourse.Value) {
    	// 출력물보안 미적용 리포트
    case 10: case 20: case 21: case 22: case 23:		// 공공,특급,1급,2급,3급 소방안전관리자
		if (ls_PrintGubun == "1") {
			ls_RexFile = "frmtraining0250R01License4700.rex";		// HP 4700DN 용 출력
		} else if (ls_PrintGubun == "2") {
			ls_RexFile = "frmtraining0250R01License65050.rex";		// HP E65050 용 출력
		}
        break;
        
