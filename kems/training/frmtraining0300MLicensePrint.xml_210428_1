function cbxPassGubun_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex) {

    ds_oLicenseCnt.ClearData();
    
    tit_clearActionInfo();
    tit_addCsvSearchActionInfo("training:searchLicenseCnt", false);
    
    // 서버 호출 
    tit_callService(
        ""
        ,""
        ,""	// inDs
        ,"ds_oLicenseCnt=ds_oLicenseCnt"	// outDs
        ,"LLCSNO=" + quote(edYear + "-" + substr(cbxJibu.Value,1,2) + "-" + cbxCourse.Value + "-" + cbxPassGubun.Value)
            + " LISSUEDATE=" + quote(calPassDate.Value) // args 
        ,""
        ,false
        ,""
        ,true
    );
    
    edLcsNoStart.Text = ds_oLicenseCnt.GetColumn(0, "MINNO");
    edLcsNoEnd.Text = ds_oLicenseCnt.GetColumn(0, "MAXNO");
	ds_oLicenseReport.ClearData();
}

/**
 * 직능 변경
 */
function cbxCourse_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex) {
	if (cbxCourse.Value == "20") {
	
        ds_oPassGubun.Filter("CD=='1'");		// 수료
	
	} else if (cbxCourse.Value == "10") {		
	
        ds_oPassGubun.Filter("CD=='2'");		// 시험	
	
	} else if (cbxCourse.Value == "21") {
	
        ds_oPassGubun.Filter("CD=='2'");		// 시험	
	
	} else if (cbxCourse.Value == "22") {
	
        ds_oPassGubun.Filter("CD=='2'");		// 시험	
        
	} else if (cbxCourse.Value == "23") {
	
        ds_oPassGubun.Filter("CD=='2'");		// 시험	
	
	} else if (cbxCourse.Value == "24") {
	
        ds_oPassGubun.Filter("CD=='1'");		// 수료
	
	} else if (cbxCourse.Value == "28") {
        //ds_oPassGubun.Filter("CD=='1'||CD=='6'");
        ds_oPassGubun.Filter("CD=='1'");		// 수료
	} else if (cbxCourse.Value == "50") {
        ds_oPassGubun.Filter("CD=='2'");		// 시험	
	} else if (cbxCourse.Value == "51") {
        ds_oPassGubun.Filter("CD=='2'");		// 시험	
	} else if (cbxCourse.Value == "52") {
	
        ds_oPassGubun.Filter("CD=='1'");		// 수료

	} else {
        ds_oPassGubun.UnFilter();
	}
	cbxPassGubun_OnChanged();
}

