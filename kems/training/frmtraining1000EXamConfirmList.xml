	<Script><![CDATA[/*

 * 프로그램명 : frmtraining1000EXamConfirmList

 * 설명 : 응시자격심사 현황

 * 작성일 : 2023.05.03

 * 작성자 : 김동한

 * 

 */
 
 #include "LIB::training.js"; // 공통업무 관련
 
 /* 공통 필수 시작 */    

function Form_OnLoadCompleted(obj) {

	gfn_Form_OnLoadCompleted(obj); // 공통 폼 로딩 이벤트 처리
	
	// 폼 환경설정

    ls_SystemGubun = gs_SystemGubun;

    ls_SystemMenuId = gs_SystemMenuId;

    var ls_FormId = split(sFormUrl, "::");

    ls_FormId = split(ls_FormId[1], ".");

    obj.Title = sFormTitle + "(" + ls_FormId[0] + ")";
    
    // 공통 코드 조회
	// 시험직능(E) / 수첩직능(L) / 강습직능(T)
	gfn_SearchJibu(cbxJibu, "Y", "전체"); // 지부 코드 조회
    gfn_SearchKCode(ds_oJubsuGubun, "M", "전체");
    
    gfn_SearchKCode(ds_KemsOnline, "P", "전체");
    gfn_SearchKCode(ds_status, "Q", "전체");
	gfn_SearchKCode(ds_oJubsuGubunCheck, "R", "전체");
	
    gfn_MakeRequiredInputMark(this); // 필수 입력란을 설정한다.
    
    ms_FormAuth = gfn_GetFormAuth(this); // 권한을 조회한다.

	cbxCourse.Value = "";
	cbxJubsuGubun.Value = "";
	
	edExamYear.Text = left(GetDate(), 4);
	
	cldfromdt.Value = left(toString(AddMonth(GetDate(), -1)), 6) + "01"; // 시작일을 전달 1일로 설정

    cldtodt.Value = AddDate(GetDate());    
    
    cbxStatus.Index = 0;
    cbxKemsOnline.Index = 0;
    cbxJubsugubunCheck.Index = 0;
}

function Form_OnKeyDown(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam) 
{
    // to do
    gfn_Form_OnKeyDown(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam); // 공통 키 이벤트 처리
}


/*

 * 기능 : 조회

 */

function lfn_Search(obj) {

    ds_oProfessor.ClearData(); // 데이타셋 초기화

    medJubsuCnt.Value = "";

    tit_clearActionInfo();

	tit_addCsvSearchActionInfo("training:searchExamConfirmList", false);

	// 서버 호출 

	tit_callService(

		""

		,""

		,""	// inDs

		,"ds_oProfessor=ds_oProfessor"	// outDs

		,"EAMGGTMGNO=" + quote(cbxJibu.Value)

            + " EATCCOURSECD=" + quote(cbxCourse.Value)

            + " EAYEAR=" + quote(edExamYear.Text)

            + " EAFROMDT=" + quote(cldFromdt.Value)

            + " EATODT=" + quote(cldTodt.Value)

            + " EASTATUS=" + quote(cbxStatus.Value)

            + " EAJUBSUGUBUN2=" + quote(cbxJubsugubunCheck.Value)
            	
            + " EAJUBSUGUBUN=" + quote(cbxJubsuGubun.Value)	// args 
            
            + " SITEGUBUN="+ quote(cbxKemsOnline.Value)
            
         

		,"lfn_AfterSearchExamConfirmList"

		,false
	);
	
	   trace(cbxKemsOnline.Value);
}

function lfn_AfterSearchExamConfirmList(nErrorCode, strErrorMsg) {

    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.

        gfn_ErrorMsg(nErrorCode, strErrorMsg);

        return;

    }

    medJubsuCnt.Value = ds_oProfessor.RowCount();

    gfn_SearchMessage(gdProfessor); // 조회 메세지를 처리하고 그리드의 정렬 헤드 마크를 제거한다.

}

/* 사용자 이벤트 함수 */

function btnMutilSort_OnClick(obj) {

	gfn_GridMultiSort(gdProfessor, 0); // 멀티 정렬을 실행한다.

}

function Form_OnSize(obj,nCx,nCy,nState) {

    if (gs_IsResizingForm == "Y") {

        gdProfessor.Height = nCy - gdProfessor.Top - 4;

        gdProfessor.Width = nCx - gdProfessor.Left - 4;

    }

}



function btnToExcel_OnClick(obj)
{
   if (ds_oProfessor.GetRowCount() < 1) {

        gfn_Alert("출력할 데이터가 존재하지 않습니다.");

        return false;

    }

	// 개인정보 다운로드 사유 등록
	var parValue = "sCode=1201";	// COMMON.PERSONALDOWNLOADH
		parValue += " sRowCnt=" + ds_oProfessor.GetRowCount();
		parValue += " sTitle=" + quote(gdProfessor.UserData);
	ls_IsRemark = Dialog("COM::frmCOM2100PDownloadRemark.xml", parValue, "", "", "", -1, -1);	

	if (ls_IsRemark == "" || ls_IsRemark == null || ls_IsRemark == "FAIL") {
		return false;
	}
	lfn_Excel(obj);	
}

function cbxCourse_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{
	// 직능에 따라 응시자격구분을 셋팅한다.
	if(nOldIndex <> nNewIndex)
	{
		ds_oJubsugubun.Filter("DATA2='"+cbxCourse.Value+"'");
		cbxJubsuGubun.Value = "";
		cbxJubsugubunCheck.Value = "";
	}	
}
