function radWorkGubun_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{
	ds_ioSmsStat.ClearData();
	ds_ioSmsStatExam.ClearData();
	ds_ioSmsStatSpeaker.ClearData();
	ds_ioSmsStatSupervisor.ClearData();

	if (strCode == "T") { // 교육접수
		divWork1.Visible = true;	// 강습시험 조회조건
		divWork3.Visible = false;	// 초빙강사 조회조건
		divWork4.Visible = false;	// 시험감독관 조회조건

		gdSmsStat.Format = "workgubun1";
        gdSmsStat.BindDataset = "ds_ioSmsStat";
		btnPrintOrder.Enable = true;
		btnPrintDate.Enable = true;
		btnPrintJibu.Enable = true;

        GubunCode = "T";
	} else if (strCode == "E") { // 시험접수

		divWork1.Visible = true;	// 강습시험 조회조건
		divWork3.Visible = false;	// 초빙강사 조회조건
		divWork4.Visible = false;	// 시험감독관 조회조건

		gdSmsStat.Format = "workgubun1";
        gdSmsStat.BindDataset = "ds_ioSmsStatExam";
		btnPrintOrder.Enable = true;
		btnPrintDate.Enable = true;
		btnPrintJibu.Enable = true;
		
        GubunCode = "E";
        
	} else if (strCode == "P") { // 초빙강사
	
		divWork1.Visible = false;	// 강습시험 조회조건
		divWork3.Visible = true;	// 초빙강사 조회조건
		divWork4.Visible = false;	// 시험감독관 조회조건
	
		gdSmsStat.Format = "workgubun3";
        gdSmsStat.BindDataset = "ds_ioSmsStatSpeaker";
		btnPrintOrder.Enable = false;
		btnPrintDate.Enable = false;
		btnPrintJibu.Enable = false;
		
        GubunCode = "P";
        
	} else if (strCode == "S") { // 시험감독관
		divWork1.Visible = false;	// 강습시험 조회조건
		divWork3.Visible = false;	// 초빙강사 조회조건
		divWork4.Visible = true;	// 시험감독관 조회조건
		
		gdSmsStat.Format = "workgubun3";

        gdSmsStat.BindDataset = "ds_ioSmsStatSupervisor";
		btnPrintOrder.Enable = false;
		btnPrintDate.Enable = false;
		btnPrintJibu.Enable = false;
		
        GubunCode = "S";
	}
	
	medTotalCnt.Text = "0";
	medSmsCnt.Text = "0";
	medMmsCnt.Text = "0";
}
