#include "LIB::training.js"; // 공통업무 관련

var gubun = "N";
/* 공통 필수 시작 */
function Form_OnLoadCompleted(obj) {

    gfn_Form_OnLoadCompleted(obj); // 공통 폼 로딩 이벤트 처리

    // 폼 환경설정
    ls_SystemGubun = gs_SystemGubun;
    ls_SystemMenuId = gs_SystemMenuId;
    var ls_FormId = split(sFormUrl, "::");
    ls_FormId = split(ls_FormId[1], ".");
    obj.Title = sFormTitle + "(" + ls_FormId[0] + ")";

    gfn_MakeRequiredInputMark(this); // 필수 입력란을 설정한다.

    //ms_FormAuth = gfn_GetFormAuth(this); // 권한을 조회한다.
    ms_FormAuth = "W";
    if (ms_FormAuth == "W") { // 권한에 따른 버튼 설정
        lfn_SetFormAuth(true); // 저장 권한
    } else if (ms_FormAuth == "R") {
        lfn_SetFormAuth(false); // 읽기 권한
    } else {
        gfn_Alert(MSGI00080);
        close();
    }

    // 공통 코드 조회
//    gfn_SearchTrainingCourse("T", ds_oCourse, "Y", "전체"); // 직능 코드 조회
    
    cbxCourse.Value = "22";
    gfn_SearchJibu(cbxJibu, "Y", "전체"); // 지부 코드 조회

    edOrderStart.SetFocus();
    edYear.Text = left(GetDate(), 4);
    
	if (substr(GetDate(),6,2) == "01") {								// 집계일자
		calDate.Value = toString(parseInt(substr(GetDate(),0,6))-1) + "30";
	} else {
		calDate.Value = parseInt(substr(GetDate(),0,8))-1;
	}
    
}

function Form_OnKeyDown(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam) {

    gfn_Form_OnKeyDown(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam); // 공통 키 이벤트 처리

}

function Form_OnSize(obj,nCx,nCy,nState)
{

    if (gs_IsResizingForm == "Y") {
        gdExamJubsu.Height = nCy - gdExamJubsu.Top - 4;
        gdExamJubsu.Width = nCx - gdExamJubsu.Left - 4;
    }

}

/*
 * 기능 : 조회
 */
function lfn_Search(obj) {
    if (!gfn_FormValidate(this)) { // 입력 Validation Check
        return false;
    }
    
    if (edYear.Text <= "2015") {
		calDate.Value = "";
		calDate.Enable = false;
    } else {	// 2016년 자료부터 시점관리
		calDate.Enable = true;
	}
 
    ///////////////////// 조회이력 남기기
    ls_HistoryArg = "지부=" + quote(cbxJibu.value)   //-- 지부
              +" 년도=" + quote(edYear.Text)  //-- 년도 
              +" 직능=" + quote(cbxCourse.value)   //-- 직능              
              +" 회차1=" + quote(edOrderStart.text) // -- 회차1
              +" 회차2=" + quote(edOrderEnd.text); // -- 회차1
          
        gfn_InsertSearchPrintHistory(
            "P"
            , this.ID
            , global.mnuLeft.cbxOpenForm.Text
            , ls_HistoryArg
        ); 
    /////////////////////  
    
        ds_ioLicenseAlreadySunabNotRepay.ClearData(); // 데이타셋 초기화
        gdExamJubsu.Format = ds_ioLicenseAlreadySunabNotRepay; 
        tit_clearActionInfo();
        tit_addSearchActionInfo("training:searchTraining0652LicenseAlreadySunabNotRepay", false);
        
        // 서버 호출 
        tit_callService(
            ""
            ,""
            ,""	// inDs
            ,"ds_ioLicenseAlreadySunabNotRepay = ds_oLicenseAlreadySunabNotRepay"	// outDs
            ,"TOMGGTMGNO=" + quote(cbxJibu.Value)
                + " TOYEAR=" + quote(edYear.Text)
                + " TOTCCOURSECD=" + quote(cbxCourse.Value)	// args
                + " TOEXAMORDER1=" + quote(edOrderStart.Text)	// args 
                + " TOEXAMORDER2=" + quote(edOrderEnd.Text)	// args 
                + " TOTDATE=" + quote(left(calDate.Value, 8))	// args 
                // args
            ,"lfn_AfterJubsuList"
            ,false
            ,""
            ,true
        );    
    
}

function lfn_AfterJubsuList(nErrorCode, strErrorMsg) {

    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.

        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;

    }
        
    medFailCnt.Value = ds_ioLicenseAlreadySunabNotRepay.RowCount();
    gfn_SearchMessage(gdExamJubsu); // 조회 메세지를 처리하고 그리드의 정렬 헤드 마크를 제거한다.

}

