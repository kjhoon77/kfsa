function radTrainingOrder_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{
	edOrderStart.Text = "";
	edOrderEnd.Text = "";
	if (radTrainingOrder.Value == "0") {
		edOrderStart.Enable = false;
		edOrderEnd.Enable = false;
	} else {
		edOrderStart.Enable = true;
		edOrderEnd.Enable = true;
		edOrderStart.SetFocus();
	}
}

function edYear_OnKillFocus(obj)
{
    if (edYear.Text <= "2015") {
		calDate.Value = "";
		calDate.Enable = false;
    } else {	// 2016년 자료부터 시점관리
    
		calDate.Enable = true;
	}
}

