function radWorkGubun_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{
	if (strCode == "1") { //  시험일자
        calExamDateStart.Enable = true;
        calExamDateEnd.Enable = true;
        calDateStart.Enable = false;
        calDateEnd.Enable = false;
               
	} else { // 수납일자
        calExamDateStart.Enable = false;
        calExamDateEnd.Enable = false;
        calDateStart.Enable = true;
        calDateEnd.Enable = true;
        
	}
}
