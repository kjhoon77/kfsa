              +" EOTCCOURSECD=" + quote(cbxCourse.value)   //-- 직능  
                          
              +" EJEXAMDATE=" + quote(calExamDateStart.value) // -- 시험일자, 
              +" EJEXAMDATE2=" + quote(calExamDateEnd.value) // -- 시험일자2
              
              +" radWorkGubun=" + quote(radWorkGubun.value) // -- 시험, 처리알자 구분
              
              +" ESPROCDATE=" + quote(calDateStart.value) // -- 처리일자
              +" ESPROCDATE2=" + quote(calDateEnd.value) // -- 처리일자2	// args 
