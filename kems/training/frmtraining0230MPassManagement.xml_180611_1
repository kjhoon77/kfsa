/*
 *  결강등의 이유로 수료 리스트에 나오지 않는 인원을 접수 테이블에 미수료로 만들어 준다.
 */ 
function lfn_Save1()
{
	if(ds_ioTrainingPassN.count() == 0) {  //선택한것이 있는지 체크 메세지 보임
    // alert("조회를 먼저 하여 주십시오. 이후에도 이 메시지가 나타난다면 결강 처리된 인원이 없습니다. ");      
    // 이것은 위 주석을 나타냅니다.
        return false;
    }       
    
	tit_clearActionInfo();
	tit_AddMultiActionInfo(
	
		"training:insertTrainingPassN"	// insert  		
		, ""	// update  
		, ""	// delete
		, ""	// flag column 명
		, ""	// key sql id
		, 0	// key 증가 값
		, "" 
		, 0 
		, "N"	// 실행타입
	);
	// 서버 호출 
	tit_callService(
		""
		,""
		,"ds_iTrainingPassN=ds_ioTrainingPassN"/// inDs
				
		,""	// outDs
		,"PSABUN=" + gds_oUserInfo.GetColumn(0, "PDEPTCD") // -- 사번 " 
       
		// args 
		,"lfn_AfterSaveTraining"
		,false
	);	
	
	
}



function ds_ioTrainingPassY_OnColumnChanged(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{
	var cnt = 0; // 결강자수
	var cnt2 = 0; // 결강자수
	for(i = 0; i < ds_ioTrainingPassY.RowCount(); i++) {
        if (ds_ioTrainingPassY.GetColumn(i, "SEL") == 1) {
            cnt += 1; // 시험접수인원 카운트
        } else {
            cnt2+= 1; // 미접수인원
        }
    }
    medPassCntYet.Value = cnt;
    medNoPassCntYet.Value = cnt2;
}

function lfn_localExcel(obj)
{  
    if (ds_ioTrainingPassY.GetRowCount() < 1) {
        gfn_Alert("출력할 데이터가 존재하지 않습니다.");
        return false;
    }
	lfn_Excel(obj);
}


