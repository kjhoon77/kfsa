 */
function lfn_save(obj) {
    if(!gfn_CheckChangedGrid(this))  //선택한것이 있는지 체크 메세지 보임
    { 
    
		gfn_Alert("수료 처리대상자를 선택해 주십시오.");      
        return false;
    }        
    if (!gfn_FormValidate(this)) { // 입력 Validation Check
		return false;
	}/*
	// 교보재관리와 연동되어 교재수불내역을 입력했는지 여부
	if (ds_ioTrainingPassY.GetColumn(0, "ITEMINPUTCNT") == '0') {
	
		gfn_Alert("교재수불내역이 입력되지 않았습니다. \n\n"
		         +"교보재관리 > 교재수불관리에서 ''강습교육일정 연계''를 하고\n\n"
		         +"교재 수불내역을 입력하신 후 수료처리를 진행하십시오!");
		
        return false;
	
	}
*/
	// 2018.06.05  교육종료일 교육종료시간-2 시간 이후 수료처리 가능
	var vEndHour = substr(ds_ioTrainingPassY.GetColumn(0, "TOENDTIME"),0,2);
	var vEndMinute = substr(ds_ioTrainingPassY.GetColumn(0, "TOENDTIME"),2,2);
	var vNowTime =  substr(GetDate(),8,4);
	var vProcPossibleTime = toString(toInteger(vEndHour)-2)+vEndMinute;
	alert(vEndHour+ "   " +vEndMinute+ "   " +vNowTime+ "   " +vProcPossibleTime);
	if ((ds_ioTrainingPassY.GetColumn(0, "TOENDDATE") > left(GetDate(),8)) ||
		(vProcPossibleTime > vNowTime)) {
		gfn_Alert("교육종료시간 2시간 전부터 수료처리 가능합니다.");   
		return false;
	}
	
	// 특급,1급,2급,3급소관, 생활안전강사, 심폐소생술과정  이면 
	if (cbxCourse.Value == '10' || cbxCourse.Value == '21' || cbxCourse.Value == '22' 
	    || cbxCourse.Value == '23' || cbxCourse.Value == '50' || cbxCourse.Value == '51') {
		if(!Confirm("선택한 인원에 대해 수료를 처리하시겠습니까?")){  //선택한것이 있는지 체크 메세지 보임	
			return false;	
		}
	} else {	//공공기관 소방안전관리자, 위험물안전관리자, 위험물운송자, 위험물통합과정
			
		if(!Confirm("선택한 인원에 대해 수료를 처리하시겠습니까?\n\n"
				  + "수료처리가 완료되면 더이상 수정(수첩폐기 등)이 불가합니다.\n\n"
				  + "수료자 명단을 다시한번 확인 바랍니다.")){  //선택한것이 있는지 체크 메세지 보임	
			return false;		}
    }
	
	if (cbxCourse.Value == '10' || cbxCourse.Value == '21' || cbxCourse.Value == '22'
	    || cbxCourse.Value == '23' || cbxCourse.Value == '50' || cbxCourse.Value == '51') {
        lfn_save_Pass();    // 수료까지    
	} else if (cbxCourse.Value == '20' || cbxCourse.Value == '24' || cbxCourse.Value == '28') {
        lfn_save_License(); // 수료 수첩까지
	} else if (cbxCourse.Value == '29') {
	   // lfn_save_License1(); 
	    lfn_save_License2_2();
	}
	lfn_SaveModifyHistory("U"); // 변경이력을 저장한다.
}	

