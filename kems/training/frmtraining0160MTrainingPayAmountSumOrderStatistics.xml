/* 사용자 이벤트 함수 */
function btnMutilSort_OnClick(obj)
{
	gfn_GridMultiSort(gdTrainingJubsu, 0); // 멀티 정렬을 실행한다.
}

/* 회차 포커스 아웃될때 자동 0 입력 */
function edTrainingOrder_OnKillFocus(obj)
{
	gfn_ComponentOnKillFocus(obj);

    if (obj.Text != "") {
        obj.Text = iif(gfn_IsNull(obj.Text), "", lpad(obj.Text, "0", 3));
    }
}

function edTrainingOrderEnd_EnterSearch(obj,nChar,bShift,bCtrl, bAlt,LLParam,HLParam)
{
	if (nChar == 13) { // Enter Key
        edTrainingOrderEnd.Text = iif(gfn_IsNull(edTrainingOrderEnd.Text), "", lpad(edTrainingOrderEnd.Text, "0", 3));
        lfn_Search();
    }
}

function btnToExcel_OnClick(obj) {
    if (ds_oTrainingJubsu.GetRowCount() < 1) {
        gfn_Alert("출력할 데이터가 존재하지 않습니다.");
        return false;
    }
	lfn_Excel(obj);
}

function radCourseGubun_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{
	if (radGubun.Value == "1") {
        calEduDateStart.Visible = true;
        calEduDateEnd.Visible = true;
        calEduDateStart.Value = "";
        calEduDateEnd.Value = "";
        lbEduDate.Text = "교육일자";
        calSunapDateStart.Value = "";
        calSunapDateEnd.Value = "";
        calSunapDateStart.Visible = false;
        calSunapDateEnd.Visible = false;
	} else {
        calEduDateStart.Visible = false;
        calEduDateEnd.Visible = false;
        calEduDateStart.Value = "";
        calEduDateEnd.Value = "";
        lbEduDate.Text = "수납일자";        
        calSunapDateStart.Visible = true;
        calSunapDateEnd.Visible = true;
        calSunapDateEnd.Value = GetDate();
        calSunapDateStart.Value = "";
	}
}]]></Script>
