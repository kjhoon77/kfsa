#include "LIB::training.js"; // 공통업무 관련

/* 공통 필수 시작 */
function Form_OnLoadCompleted(obj) {
    gfn_Form_OnLoadCompleted(obj); // 공통 폼 로딩 이벤트 처리

    // 폼 환경설정
    ls_SystemGubun = gs_SystemGubun;
    ls_SystemMenuId = gs_SystemMenuId;
    var ls_FormId = split(sFormUrl, "::");
    ls_FormId = split(ls_FormId[1], ".");
    obj.Title = sFormTitle + "(" + ls_FormId[0] + ")";

    gfn_MakeRequiredInputMark(this); // 필수 입력란을 설정한다.

    ms_FormAuth = gfn_GetFormAuth(this); // 권한을 조회한다.
    if (ms_FormAuth == "W") { // 권한에 따른 버튼 설정
        lfn_SetFormAuth(true); // 저장 권한
    } else if (ms_FormAuth == "R") {
        lfn_SetFormAuth(false); // 읽기 권한
    } else {
        gfn_Alert(MSGI00080);
        close();
    }

    // 공통 코드 조회
    gfn_SearchTrainingCourse("T", ds_oCourse, "Y", "전체"); // 직능 코드 조회
    cbxCourse.Value = "22";
    gfn_SearchJibu(cbxJibu, "Y", "전체"); // 지부 코드 조회  
    edTrainingYear.Text = left(GetDate(), 4);
    edTrainingOrderStart.Text = "001";
    edTrainingOrderEnd.Text = "001";
    edTrainingOrderStart.SetFocus();
    
    calEduDateStart.Visible = true;
    calEduDateEnd.Visible = true;
    lbEduDate.Text = "교육일자";
    calSunapDateStart.Visible = false;
    calSunapDateEnd.Visible = false;
}

function Form_OnKeyDown(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam) {
    // to do

    gfn_Form_OnKeyDown(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam); // 공통 키 이벤트 처리
}

function Form_OnSize(obj,nCx,nCy,nState)
{
    if (gs_IsResizingForm == "Y") {
        gdTrainingJubsu.Height = nCy - gdTrainingJubsu.Top - 4;
        gdTrainingJubsu.Width = nCx - gdTrainingJubsu.Left - 4;
    }
}

/*
 * 기능 : 조회
 */
function lfn_Search(obj) {
    if (edTrainingOrderStart.Text > edTrainingOrderEnd.Text) {
        gfn_Alert("종료회차가 시작회차보다 늦습니다.");
        edTrainingOrderStart.SetFocus();
        return false;
    }
    if (radGubun.Value == "1") {
        if (calEduDateStart.Value > calEduDateEnd.Value) {
            gfn_Alert("시작일자가 종료일자보다 늦습니다.");
            calEduDateStart.SetFocus();
            return false;
        }
    } else if (radGubun.Value == "2") {
        if (calSunapDateStart.Value > calSunapDateEnd.Value) {
            gfn_Alert("시작일자가 종료일자보다 늦습니다.");
            calSunapDateStart.SetFocus();
            return false;
        }
    }    
    
    medTotalCnt.Value = "";
    
    ds_oTrainingJubsu.ClearData(); // 데이타셋 초기화

    tit_clearActionInfo();
	tit_addCsvSearchActionInfo("training:searchTrainingPayAmountSumOrderStatistics", false);
	// 서버 호출 
	tit_callService(
		""
		,""
		,""	// inDs
		,"ds_oTrainingJubsu=ds_oTrainingJubsu"	// outDs
		,"TOMGGTMGNO=" + quote(cbxJibu.Value)
            + " TOTCCOURSECD=" + quote(cbxCourse.Value)
            + " TOYEAR=" + quote(edTrainingYear.Text)
            + " THORDERSEQSTART=" + quote(edTrainingOrderStart.Text)
            + " THORDERSEQEND=" + quote(edTrainingOrderEnd.Text)
            + " TOCOURSEGUBUN=" + quote(radCourseGubun.Value)
            + " TOSTARTDATE=" + quote(left(calEduDateStart.Value, 8))
            + " TOENDDATE=" + quote(left(calEduDateEnd.Value, 8))
            + " TSPROCDATESTART=" + quote(left(calSunapDateStart.Value, 8))
            + " TSPROCDATEEND=" + quote(left(calSunapDateEnd.Value, 8))
            + " MUNJE=" + quote(cbxCourse.Value)
            + " COURSEGUBUN=T"// args 
		,"lfn_AfterSearchTrainingJubsu"
		,false
	);
}

/*
 * 기능 : 출력
 */
function lfn_Print(obj)
{
    if (ds_oTrainingJubsu.RowCount() < 1) {
        gfn_Alert("출력할 데이터가 없습니다.");
        return false;
    }
    // to do Validation Check
    gfn_CallReportPopup("frmtraining0160R01TrainingPayAmountSumOrderStatistics.rex", "ds_oTrainingJubsu"
                      , "TSPROCDATESTART=" + quote(calSunapDateStart.Value)
                        + "^TSPROCDATEEND=" + quote(calSunapDateEnd.Value)
                        + "^GTDEPTNM=" + quote(cbxJibu.Text)
                        + "^TCCOURSENM=" + quote(cbxCourse.Text));
}
/* 공통 필수 종료 */

/* 사용자 함수 */
/*
 * 기능 : 결강자 조회 후 처리해야 할 내용
 * parameter : nErrorCode - 에러코드
 *             strErrorMsg - 에러메시지
 * Return : 없음
 */
function lfn_AfterSearchTrainingJubsu(nErrorCode, strErrorMsg) {
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }
    if (ds_oTrainingJubsu.GetRowCount() > 0) {
        gdTrainingJubsu.Format = "default0";
    } else {
        gdTrainingJubsu.Format = "default";
    }
    
    medTotalCnt.Value = ds_oTrainingJubsu.rowcount;
    gfn_SearchMessage(gdTrainingJubsu); // 조회 메세지를 처리하고 그리드의 정렬 헤드 마크를 제거한다.
}

/*
 * 기능 : Transaction 서비스 호출 후 처리해야 할 내용
 * parameter : strSvcID - 서비스ID
 *             nErrorCode - 에러코드
 *             strErrorMsg - 에러메시지
 * Return : 없음
 */
function lfn_AfterSaveTrainingJubsu(nErrorCode, strErrorMsg) {
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }

    gfn_SaveMessage(); // 저장 메세지를 처리한다.
    lfn_Search(); // 결강자 조회
}

