/*
 * 기능 : 저장
 */
function lfn_Save(obj) {
    if (!gfn_FormValidate(this)) { // 입력 Validation Check
        return false;
    }

    if (mb_IsNewData || ds_ioLicense.GetCount() == 0) { // 신규 저장
        // 키 중복 체크
    
        if (!gfn_Confirm(MSGW00020)) { // 정말로 추가 저장 하시겠습니까?
            return false;
        }

        // 입력된 값을 그리드로 복사한다.
        ds_ioLicense.InsertRow(0);
        ds_ioLicense.SetColumn(0, "USE_YN", chkUseYn.Value);
        ds_ioLicense.SetColumn(0, "CCGROUPCD", edGroupCode.Text);
        ds_ioLicense.SetColumn(0, "CCCD", edLicense.Text);
        ds_ioLicense.SetColumn(0, "CCCDNM", edLicenseName.Text);
        ds_ioLicense.SetColumn(0, "CCORDERSEQ", medOrderSeq.Value);
        ds_ioLicense.SetColumn(0, "CCREMARK", edRemark.Text);
    } else { // 수정 저장
        if (!gfn_Confirm(MSGW00040)) { // 정말로 수정 저장 하시겠습니까?
            return false;
        }

        // 입력된 값을 그리드로 복사한다.
        ds_ioLicense.SetColumn(ds_ioLicense.currow, "USE_YN", chkUseYn.Value);
        ds_ioLicense.SetColumn(ds_ioLicense.currow, "CCCDNM", edLicenseName.Text);
        ds_ioLicense.SetColumn(ds_ioLicense.currow, "CCORDERSEQ", medOrderSeq.Value);
        ds_ioLicense.SetColumn(ds_ioLicense.currow, "CCREMARK", edRemark.Text);
    }

    gfn_FormInit(this); // 입력란 초기화

    tit_clearActionInfo();
    tit_addSaveActionInfo(
        "training:insertLicense"	// insert
        , "training:updateLicense"	// update
        , ""	// delete
        , ""	// flag column 명
        , ""	// key sql id
        , 0	// key 증가 값
        , "" 
        , 0 
        , "N"	// 실행타입
    );
    // 서버 호출 
    tit_callService(
        ""
        ,""
        ,"ds_iLicense=ds_ioLicense:U"	// inDs
        ,""	// outDs
        ,""	// args 
        ,"lfn_AfterSaveLicense"
        ,false
    );
}
