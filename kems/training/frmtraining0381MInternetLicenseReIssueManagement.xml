/*
 * 기능 : 조회
 */
function lfn_Search(obj) {
    ds_ioLicense.ClearData(); // 데이타셋 초기화

    tit_clearActionInfo();
	tit_addSearchActionInfo("training:searchLicense", false);
	// 서버 호출 
	tit_callService(
		""
		,""
		,""	// inDs
		,"ds_ioLicense=ds_oLicense"	// outDs
		,"CCGROUPCD=" + quote(edGroupCode.Text)
            + " CCCD=" + quote(edLicense.Text)
            + " CCCDNM=" + quote(edLicenseName.Text)	// args 
		,"lfn_AfterSearchLicense"
		,false
	);
}

