// 추가
function lfn_Input(obj)
{
	
	if (!gfn_Confirm("신규 유형을 등록 하시겠습니까?\n입력완료 후 저장 버튼을 누르세요.")) {
        return false;
	}
	edInputMode.Text = "신 규";
    lfn_Cancel(); // 값 초기화
    // 기본값 설정    
    radWorkGubun.Value = "2";
    radSendGubun.Value = "0";
    taMessage.SetFocus();
	
	
}


function radSendGubun_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{
	if (strCode == "0") {
	
		lbTotalLength.Text = "/ 80 Byte";
			
	} else if (strCode == "1") {
			
		lbTotalLength.Text = "/ 4000 Byte";
	
	} else {
	
		lbTotalLength.Text = "/ 80 Byte";
		
	}
}

function ds_ioSmsPattern_OnRowPosChanged(obj,nOldRow,nRow)
{
    // 그리드의 값을 입력란에 복사한다. 추가상태인 경우 복사 후 저장을 하기 위해 제외한다.
    if (obj.GetRowType(nRow) <> "insert" && nRow <> -1) {
        cbxJibu.Value = obj.GetColumn(nRow, "SPGTMGNO");
        radWorkGubun.Value = obj.GetColumn(nRow, "SPBIZGUBUN");
        radSendGubun.Value = obj.GetColumn(nRow, "SPTYPE");
        taMessage.Text = obj.GetColumn(nRow, "SPPATTERN");
        gfn_MakeKeyMark(this); // 키값 수정 금지 설정
	}	
}


function taMessage_OnCharChanged(obj,strPreText,strPostText)
{
	lbLength.Text = lfn_CheckByte(obj.value);
	
	if (radSendGubun.Value == "0") {
		if (lfn_CheckLength(obj, "단문메시지는 ", 80) == false) {	
			obj.text = strPreText;	
		}
		
	} else {

		if (lfn_CheckLength(obj, "장문(MMS)메시지는 ", 4000) == false) {	
			obj.text = strPreText;	
		}
	
	}
	lbLength.Text = lfn_CheckByte(obj.value);
}

/*
 * 기능 : 문자열 길이를 계산한다.
 * parameter : obj - 컴포넌트 오브젝트
 *             sMsg - 메세지 앞부분
 *             nLength - 최대문자열수
 * return : 없음
 */
function lfn_CheckLength(obj, sMsg, nLength)
{
    obj.value = ltrim(obj.value);
    var ln_ObjLength = lfn_CheckByte(obj.value);
    if (ln_ObjLength > nLength) {
        gfn_Alert(sMsg + nLength + '자를(영문기준) 초과할 수 없습니다. 현재 ' + ln_ObjLength + '글자입니다.' + ' 80자까지만 내용이 발송됩니다.');
        return false;
    }
    return true;
}

/*
 * 기능 : 바이트수 계산한다.
 * parameter : sString - 계산할 문자열
 * return : 문자열 바이트수
 */
function lfn_CheckByte(sString)
{
    var ln_StringByte = 0;
    var ln_Len = sString.length;
    for (var i = 0; i < ln_Len; i++) {
        if (sString.CharAt(i) >= ' ' && sString.CharAt(i) <= '~' ) {
            ln_StringByte++;
        } else {
            ln_StringByte += 2;
        }
    }
    return ln_StringByte;
}

