/*
 * 기능 : 저장
 */
function lfn_Save(obj)
{
        taMessage.UserData ="S:전달 메세지";             // 전달 메세지와 회신 연락처 확인 위함
    if (!gfn_FormValidate(this)) { // 입력 Validation Check
		return false;
	}
	
	if (gfn_IsNull(radWorkGubun.Value)) {
	
		gfn_Alert('업무구분은 전체를 선택할 수 없습니다.');
	
		return false;
	}
	
	if (gfn_IsNull(radSendGubun.Value)) {
	
		gfn_Alert('문자구분은 전체를 선택할 수 없습니다.');
	
		return false;
	}	
	
	if (edInputMode.Text == "신 규") {
	
        // 키 중복 체크
        if (!gfn_Confirm(MSGW00020)) { // 정말로 추가 저장 하시겠습니까?
            return false;
        }
        
        // 입력된 값을 그리드로 복사한다.
        ds_ioSmsPattern.InsertRow(0);
        ds_ioSmsPattern.SetColumn(0, "SPGTMGNO", cbxJibu.Value);
        ds_ioSmsPattern.SetColumn(0, "SPTYPE", radSendGubun.Value);
        ds_ioSmsPattern.SetColumn(0, "SPBIZGUBUN", radWorkGubun.Value);
        ds_ioSmsPattern.SetColumn(0, "SPPATTERN", taMessage.Text);
        
    } else {
    
        if (!gfn_Confirm(MSGW00040)) { // 정말로 수정 저장 하시겠습니까?
            return false;
        }
        
        ds_ioSmsPattern.SetColumn(ds_ioSmsPattern.currow, "SPGTMGNO", cbxJibu.Value);
        ds_ioSmsPattern.SetColumn(ds_ioSmsPattern.currow, "SPTYPE", radSendGubun.Value);
        ds_ioSmsPattern.SetColumn(ds_ioSmsPattern.currow, "SPBIZGUBUN", radWorkGubun.Value);
        ds_ioSmsPattern.SetColumn(ds_ioSmsPattern.currow, "SPPATTERN", taMessage.Text);
        
    }
        
    gfn_FormInit(this); // 입력란 초기화
    tit_clearActionInfo();
    tit_addSaveActionInfo(
        "training:insertTraining0692SmsPattern"	// insert
        , "training:updateTraining0692SmsPattern"	// update
        , ""	// delete
        , ""	// flag column 명
        , ""	// key sql id
