/*
 * 기능 : 조회
 */
function lfn_Search(obj) {
    if (!gfn_FormValidate(this)) { // 입력 Validation Check
        return false;
    }
    ds_ioSmsPattern.ClearData(); // 데이타셋 초기화

/////////////////////   

	tit_clearActionInfo();
	tit_addCsvSearchActionInfo("training:searchTraining0692SmsPattern", false);
	// 서버 호출 
	tit_callService(
		""
		,""
		,""	// inDs
		,"ds_ioSmsPattern=ds_ioSmsPattern"	// outDs
		,"SPGTMGNO=" + quote(cbxJibu.value)     //-- 지부
			+" SPBIZGUBUN=" + quote(radWorkGubun.value) //-- 업무구분
		   			+" SPTYPE=" + quote(radSendGubun.value)//  -- 발송구분
			
			// args 
		,"lfn_AfterSearchSmsPattern"
		,false
	);
}

/* 공통 필수 종료 */

/* 사용자 함수 */
/*
 * 기능 : 유형 조회 후 처리해야 할 내용
 * parameter : nErrorCode - 에러코드
 *             strErrorMsg - 에러메시지
 * Return : 없음
 */
function lfn_AfterSearchSmsPattern(nErrorCode, strErrorMsg) {
   if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }
    gfn_SearchMessage(gdSmsPattern); // 조회 메세지를 처리하고 그리드의 정렬 헤드 마크를 제거한다.
    
    edInputMode.Text = "수 정";
    
    gdSmsPattern_OnCellClick();
    
    taMessage_OnCharChanged(taMessage,taMessage.Text, "");
}

/*
 * 기능 : 버튼 활성/비활성시킨다.
 * parameter : bStatus - 활성/비활성여부(true:활성, false:비활성)
 * return : 없음
