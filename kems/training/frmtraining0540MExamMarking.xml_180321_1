//	    gds_oUserInfo.GetColumn(0, "PDEPTCD") == "1011") {

		// 특급시험이 아니면
//		if (ds_oExamOrder.GetColumn(0, "TCCOURSECD") <> "10") {
		
			lfn_GetErrorCount();
			
			if (!gfn_isNull(ds_oErrorCount.GetColumn(0, "ECERRCNT")) && ds_oErrorCount.GetColumn(0, "ECERRCNT") > 0) {
			 
				gfn_alert("답안 리딩 후 처리하지 않은 에러내역이 " + 
						   ds_oErrorCount.GetColumn(0, "ECERRCNT") + "건 남아있습니다. 처리 후 채점을 진행하세요!");
			
			}
