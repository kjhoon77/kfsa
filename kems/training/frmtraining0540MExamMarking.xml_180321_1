    ds_oErrorCount.ClearData(); // 데이타셋 초기화

    ldb.Sync = true;
    transaction ("selectErrorCount"
        , "SvcLdb::searchErrorCount.xml" // BSB Service
        , "" // inDS
        , "ds_oErrorCount=output" // outDS
        , ""
        , "");
    ldb.Sync = false;
    
}]]></Script>
