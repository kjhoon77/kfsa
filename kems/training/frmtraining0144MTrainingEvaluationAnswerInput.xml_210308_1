/**
 * 답안 열단위 정렬
 */
function lfn_SplitAnswerPaper() {

    var seq = 1;
    ds_iAnswerPaper.ClearData();

    for (var i=0; i<ds_oAnswerPaper.GetRowCount(); i++) {

        var userAns;
        
        if (ds_oAnswerPaper.GetColumn(i, "APTCCOURSECD") == "20"
            || ds_oAnswerPaper.GetColumn(i, "APTCCOURSECD") == "21"
            || ds_oAnswerPaper.GetColumn(i, "APTCCOURSECD") == "22"
            
            || ds_oAnswerPaper.GetColumn(i, "APTCCOURSECD") == "23"
            
            || ds_oAnswerPaper.GetColumn(i, "APTCCOURSECD") == "50") {	// 공공,1급,2급,3급, 생활안전강사
            userAns = 30; //문항 마지막번호 고정
            
        } else if (ds_oAnswerPaper.GetColumn(i, "APTCCOURSECD") == "10") {	// 특급
            userAns = 100; //문항 마지막번호 고정
            
        } else if (ds_oAnswerPaper.GetColumn(i, "APTCCOURSECD") == "51") {	// 심폐소생술과정
            userAns = 20; //문항 마지막번호 고정
                        
        } else if (ds_oAnswerPaper.GetColumn(i, "APTCCOURSECD") == "24") {	// 위관
        
            userAns = 50;
            
        } else if (ds_oAnswerPaper.GetColumn(i, "APTCCOURSECD") == "28") {	// 운송자
        
            userAns = 50;
        
        } else if (ds_oAnswerPaper.GetColumn(i, "APTCCOURSECD") == "29") {	// 통합자
        
            userAns = 50;
            
        } else {		// 
            userAns = 30;
            
        }
        for (var j=1; j<userAns; j++) {
        
			// 문항 값이 있으면
			if (!gfn_isNull(ds_oAnswerPaper.GetColumn(i, "EAPITEMNO"+j))) {
				var nCurrow = ds_iAnswerPaper.AddRow();	
				ds_iAnswerPaper.SetColumn(nCurrow, "EAPTOMGNO" , ds_oAnswerPaper.GetColumn(i, "EAPTOMGNO"));
				
				ds_iAnswerPaper.SetColumn(nCurrow, "EAPTCCOURSECD" , ds_oAnswerPaper.GetColumn(i, "EAPTCCOURSECD"));
				
				ds_iAnswerPaper.SetColumn(nCurrow, "EAPJUBSUNONAME" , ds_oAnswerPaper.GetColumn(i, "EAPJUBSUNONAME"));	
				ds_iAnswerPaper.SetColumn(nCurrow, "EAPSUBJECTCD" , ds_oAnswerPaper.GetColumn(i, "EAPSUBJECTCD"));
				
				ds_iAnswerPaper.SetColumn(nCurrow, "EAPEVALGUBUN" , radEvalGubun.Value);
				ds_iAnswerPaper.SetColumn(nCurrow, "EAPITEMNO" , replace(replace(replace(ds_oAnswerPaper.GetColumn(i, "EAPITEMNO"+j), ds_oAnswerPaper.GetColumn(i, "ESCSUBJECTNM"), "")," ",""),"번", ""));	
				ds_iAnswerPaper.SetColumn(nCurrow, "EAPANSWER" , replace(ds_oAnswerPaper.GetColumn(i, "EAPANSWER"+j),"번", ""));	
				ds_iAnswerPaper.SetColumn(nCurrow, "EAPANSWERYN" , "N");	
				ds_iAnswerPaper.SetColumn(nCurrow, "EAPITEMSCORE" , "0");	
            }

        }
    }
//TRACE(ds_oAnswerPaper.SAVEXML());
//TRACE(ds_iAnswerPaper.SAVEXML());
}

// 채점 
function lfn_MarkingAnswerPaper(){
    var ls_AnswerCheck = "N"; // 정답여부
    var li_CorrectAnswerCnt = 0; // 정답여부
    var li_CorrectRow = 0; // 정답지 row
    var ls_AnswerPaperMark = "0"; // 답지 답
    var ls_CorrectPaperMark = "0"; // 정답
    
    SetWaitCursor(true);
    SetCapture();

    progMarkingCheck.Pos = 0;
    progMarkingCheck.Max = ds_iAnswerPaper.GetRowCount();
    
    for (var i=0; i<ds_iAnswerPaper.GetRowCount(); i++) {
    
        progMarkingCheck.StepIt();
    
        li_CorrectRow = ds_oCorrectAnswer.searchrow("ECPTCCOURSECD=" + quote(ds_iAnswerPaper.GetColumn(i, "EAPTCCOURSECD"))
                + " && ECPSUBJECTCD=" + quote(ds_iAnswerPaper.GetColumn(i, "EAPSUBJECTCD"))
                + " && ECPITEMNO=" + quote(ds_iAnswerPaper.GetColumn(i, "EAPITEMNO"))
        );
        ls_AnswerPaperMark = ds_iAnswerPaper.GetColumn(i, "EAPANSWER");
        ls_CorrectPaperMark = ds_oCorrectAnswer.GetColumn(li_CorrectRow, "ECPCORRECTANSWER");
        li_CorrectAnswerCnt = toNumber(ds_oCorrectAnswer.GetColumn(li_CorrectRow, "ECPCORRECTCNT"));
//trace(ds_iAnswerPaper.GetColumn(i, "EAPSUBJECTCD") + " : " +ds_iAnswerPaper.GetColumn(i, "EAPITEMNO") + " : " + ls_AnswerPaperMark + "     " + ls_CorrectPaperMark);
        if (ls_AnswerPaperMark = ls_CorrectPaperMark) { // 정답 비교

            ls_AnswerCheck = "Y";

        } else if (length(ls_CorrectPaperMark) > 1) { // 정답이 2개 이상일 때
        
            li_CorrectAnswerCnt = 0;
            
            for (var j=0; j<length(ls_AnswerPaperMark); j++) {
                for (var k=0; k<length(ls_CorrectPaperMark); k++) {
                    if (substr(ls_AnswerPaperMark, j, 1) = substr(ls_CorrectPaperMark, k, 1)) {
                        ls_AnswerCheck = "Y";
                        li_CorrectAnswerCnt++;
                        break;
                    } else {
                        ls_AnswerCheck = "N";
                    }
                }
                
                if (ls_AnswerCheck == "N") { // 틀린 답이 하나라도 있을 경우
                    li_CorrectAnswerCnt = 0;
                    break;
                }
            }
            
        } else {

            ls_AnswerCheck = "N";

        }
        
        if (ls_AnswerCheck == "Y" && li_CorrectAnswerCnt >= toNumber(ds_oCorrectAnswer.GetColumn(li_CorrectRow, "ECPCORRECTCNT"))) { // 답이 틀린게 없고, 정답 개수가 맞을 때
            ds_iAnswerPaper.SetColumn(i, "EAPANSWERYN", ls_AnswerCheck);
            ds_iAnswerPaper.SetColumn(i, "EAPITEMSCORE", ds_oCorrectAnswer.GetColumn(li_CorrectRow, "ECPITEMSCORE"));
        }
    }
    
    SetWaitCursor(false);

}


