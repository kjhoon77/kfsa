/* 공통 필수 종료 */

/* 사용자 함수 */
/*
 * 기능 : 결시자 조회 후 처리해야 할 내용
 * parameter : nErrorCode - 에러코드
 *             strErrorMsg - 에러메시지
 * Return : 없음
 */
function lfn_AfterSearchExamAbsent(nErrorCode, strErrorMsg) {
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }
	// 특급소방안전관리자의 경우 그리드 포맷 변경
	if (cbxCourse.Value == "10") {
		
		gdExamAbsent.Format = "SPECIALGRADE";
		
	} else {
	
		gdExamAbsent.Format = "NORMAL";
	
	}

    medTotalCnt.Value = ds_oExamAbsent.GetCount();
    gfn_SearchMessage(gdExamAbsent); // 조회 메세지를 처리하고 그리드의 정렬 헤드 마크를 제거한다.
            var cnt  = 0; // 총 결시자수
    var cnt1 = 0; // 1차 결시자수
    var cnt2 = 0; // 2차 결시자수
	for(i = 0; i < ds_oExamAbsent.RowCount(); i++) {
        if (ds_oExamAbsent.GetColumn(i, "EAGUBUN") == 'N') {
            cnt += 1; //  총 결시자수 카운트

        }
        
        if (ds_oExamAbsent.GetColumn(i, "EA1CHASU") == 'N') {
            cnt1 += 1; // 1차 결시자수 카운트

        }
        
        if (ds_oExamAbsent.GetColumn(i, "EA2CHASU") == 'N') {
            cnt2 += 1; // 2차 결시자수 카운트

        }
    }
	medAbsenCnt.Value = cnt;   // 총 결시자수 입력

	medAbsenCnt1.Value = cnt1; // 1차 결시자수 입력

    medAbsenCnt2.Value = cnt2; // 2차 결시자수 입력
    
    // 결시자만 뽑아서 임시 데이터셋에 저장
    ds_oExamAbsentTemp.Clear();
    ds_oExamAbsentTemp.Copy(ds_oExamAbsent);    
    
    for (i=ds_oExamAbsent.GetRowCount();i>=0;i--) {
        if (ds_oExamAbsentTemp.GetColumn(i, "EAGUBUN") <> "N") {
            ds_oExamAbsentTemp.DeleteRow(i);
        }
    }

}

