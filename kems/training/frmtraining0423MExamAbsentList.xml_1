        return false;
    }
    edExamOrderStart.Text = iif(gfn_IsNull(edExamOrderStart.Text), "", lpad(edExamOrderStart.Text, "0", 3));
    
    edExamOrderEnd.Text = iif(gfn_IsNull(edExamOrderEnd.Text), "", lpad(edExamOrderEnd.Text, "0", 3));
    
	// 특급소방안전관리자의 경우 그리드 포맷 변경
