	<Script><![CDATA[/*

 * 프로그램명 : frmtraining0020PTrainingCyberInfo.xml

 * 설명 : 사이버교육 상세정보

 * 작성일 : 2011.05.24

 * 작성자 : 정현호

 */



#include "LIB::COMMON.js"; // 공통 관련

/*
 sTomggtmgno	: 지부코드
 sCoursecd		: 직능코드
 sToyear		: 년도
 sToorder		: 회차
 sTomgno		: 회차관리번호
 sTohjubsuno	: 접수번호
*/


/* 공통 필수 시작 */

function Form_OnLoadCompleted(obj) {

    gfn_Form_OnLoadCompleted(obj); // 공통 폼 로딩 이벤트 처리



    if (IsExistVar("sTohjubsuno")) {

        lfn_Search();
        
		var sArg = "&COTOMGNO=" + sTomgno;	// args
		
		sArg += "&CJJUBSUNO=" + sTohjubsuno;
			
		var url = "http://cyber.kfsi.or.kr/kfsa/InfoAction.do?method=progress" + sArg;
		
		//trace(url);
        
      	var rtn = AxMsie.Navigate2(url, ,"_self", "", "");
      	
    }



}



function Form_OnUnloadCompleted(obj) {

    Close();

}



function Form_OnKeyDown(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam) {

    switch (nChar) {

        case 27 : // 종료(Esc)

            lfn_End();

            break;

        case 113 : // 조회(F2)

            lfn_Search();

            break;

    }

}



/*

 * 기능 : 조회

 */

function lfn_Search(obj) {

    ds_oCyberLearning.ClearData(); // 데이타셋 초기화



    tit_clearActionInfo();

    tit_addSearchActionInfo("training:searchTraining0020CyberLearninginfo", false);

    // 서버 호출

    tit_callService(

        ""

        , ""

        , "" // inDs

        , "ds_oCyberLearning=ds_oCyberLearning" // outDs

        , "CCCOURSECD=" + quote(sCoursecd) // args

            + " CCEDUGUBUN='T'"
            
            + " CJYEAR=" + quote(sToyear) // args

            + " CJGTMGNO=" + quote(sTomggtmgno)
            
            + " CJTRAINNINGORDER=" + quote(sToorder) // args

            + " CJJUBSUNO=" + quote(sTohjubsuno)

        , "lfn_AfterSearchTrainingCyberLearninginfo"

        ,false

        ,false

        ,true

    );

}


/*

 * 기능 : 종료

 */

function lfn_End(obj) {

    Close();

}

/* 공통 필수 종료 */



/* 사용자 함수 */

/*

 * 기능 : 조회 후 처리해야 할 내용

 * parameter : nErrorCode - 에러코드

 *             strErrorMsg - 에러메시지

 * Return : 없음

 */

function lfn_AfterSearchTrainingCyberLearninginfo(nErrorCode, strErrorMsg) {

    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.

        gfn_ErrorMsg(nErrorCode, strErrorMsg);

        return;

    }

    if (ds_oCyberLearning.rowcount == 0) {
		
		gfn_Alert("조회된 정보가 없습니다.");
		
		lfn_End();
		
    }

}


