/*
 * 기능 : 출력
 */
function lfn_Print(obj)
{
    if (!gfn_FormValidate(this)) { // 입력 Validation Check
        return false;
    }
    
    switch (radPrintGubun.Value) {
        case "00" : // 성적일람표(게시용)
            flag = 0;
            lfn_SearchScoreNo();
            break;

        case "10" : // 성적일람표(응시번호순)
            flag = 1;
            lfn_SearchScoreNo();
            break;
        case "20" : // 성적일람표(석차순)
            flag = 2;
            lfn_SearchScoreNo();
            break;
        case "21" : // 성적일람표(응시자제공용)
            flag = 21;
            lfn_SearchScoreNo();
            break;
        case "30" : // 합격자명단
            flag = 3;
            lfn_SearchPassName();
            break;
        case "50" : // 합격자명단(게시판용)
            flag = 5;
            lfn_SearchPassName();
            break;
        case "40" : // 불합격자명단
            flag = 4;
            lfn_SearchNoPassName();
            break;
        case "60" : // 불합격자명단(게시판용)
            flag = 6;
            lfn_SearchNoPassName();
            break;
        case "70" : // 회차별 성적 통계 현황        
            flag = 7;
            lfn_SearchOrderScoreStat();
            break;
        case "80" : // 점수대별 인원분포      
            flag = 8;
            lfn_SearchJumsuPersonDistribution();
            break;
        case "90" : // 문항분석        
            if (cbxCourse.Value != "10" && cbxCourse.Value != "21" && cbxCourse.Value != "22"
             
                && cbxCourse.Value != "23" && cbxCourse.Value != "50" && cbxCourse.Value != "51") {
                if (cbxExamRepeat.Value == "0") {
                    gfn_Alert("문항분석은 최종차수 조회가 불가능 합니다.\n평가 차수별로 조회하십시오.");
                    return false;
                }
            }            
            flag = 9;
            lfn_SearchQuestionItemAnalysis();
            break;
        case "91" : // 문항별 정답현황
            flag = 91;
            lfn_SearchQuestionItemCorrectAnswer();
            break;
            
	}
}
