    // 기본값 설정
}







function lfn_save(obj) {
	// 공공소관, 위관, 운송자, 통합자
	if (cbxCourse.Value == "20" || cbxCourse.Value == "24" || cbxCourse.Value == "28" || cbxCourse.Value == "29") {

		// 교육운영과 와 시스템 관리자만 폐기 권한 부여 (2015.11.09 교육운영과 고은희 주임 요청)
		if (!((gds_oUserInfo.GetColumn(0, "PDEPTCD") == "0251") || (gds_oUserInfo.GetColumn(0, "ADMINGUBUN") == "SM"))) {/*
			gfn_alert("[공공기관소방안전관리자, 위험물안전관리자, 위험물운송자,\n\n위험물통합과정]에 대한 " +
			          "수료 취소는 [교육운영과]에서만 가능합니다. \n\n" +
					  "취소가 필요한 경우 교육운영과로 요청하세요! \n\n" +
					  "[특급,1급,2급소관,생활안전강사는 지부에서 수료취소 가능]");
*/
			gfn_alert("수료취소는 교육운영과로 문서(공문)를 통해\n\n" +
			          "요청하시기 바랍니다.\n\n" +
					  "[특급,1급,2급소관,생활안전강사,심폐소생술과정은 지부에서 수료취소 가능]");

			return false;
			
		}

	}

    edCancelRemark.UserData ="S:취소사유";
    if(!gfn_CheckChangedGrid(this))  //선택한것이 있는지 체크 메세지 보임
    { alert("수료 취소대상자를 선택해 주십시오.");      
        return false;
    }        
  
    if (!gfn_FormValidate(this)) { // 입력 Validation Check
		return false;
	}
	 if(!Confirm("선택한 인원에 대해 수료 취소를 하시겠습니까?")){  //선택한것이 있는지 체크 메세지 보임
        return false;
    }
    	// 특급,1급,2급,3급소관, 생활안전강사, 심폐소생술과정
	if(cbxCourse.Value == "10" || cbxCourse.Value == "21" || cbxCourse.Value == "22"
	   || cbxCourse.Value == "23" || cbxCourse.Value == "50" || cbxCourse.Value == "51") {
        lfn_save1();    // 수료만
	// 공공소관, 위관, 운송자, 교관양성과정
	} else if (cbxCourse.Value == "20" || cbxCourse.Value == "24" || cbxCourse.Value == "28" || cbxCourse.Value == "52") {
	
		lfn_save_License(); // 수료 수첩까지
			
	// 위험물통합과정
	} else if (cbxCourse.Value == "29") {
		lfn_save_LicenseBoth(); 
	}
	lfn_SaveModifyHistory("U"); // 변경이력을 저장한다.
}	

