/*
 * 기능 : 조회
 */
function lfn_Search(obj) {
    edCancelRemark.UserData ="S:";
    if (!gfn_FormValidate(this)) { // 입력 Validation Check
		return false;
	}
    ds_ioTrainingPassCancel.ClearData(); // 데이타셋 초기화
    ds_ioTrainingPassCancel_Group.ClearData(); // 데이타셋 초기화

    ls_HistoryArg = "지부=" + quote(cbxJibu.Text)
            + ",직능=" + quote(cbxCourse.Text)
            + ",년도=" + quote(edTrainingYear.Text)
            + ",회차=" + quote(edTrainingOrder.Text);
  
    gfn_InsertSearchPrintHistory(
        "S"
        , this.ID
        , global.mnuLeft.cbxOpenForm.Text
        , ls_HistoryArg
    ); 

    tit_clearActionInfo();
	
	tit_addCsvSearchActionInfo("training:searchTrainingPassCancel", false);// 조회 트랜잭션
	
	// 서버 호출 
	tit_callService(
		""
		,""
		,""	// inDs
		,"ds_ioTrainingPassCancel=ds_oTrainingPassCancel"	// outDs
		,"TOMGGTMGNO=" + quote(cbxJibu.value)   //-- 지부
            +" TOYEAR=" + quote(edTrainingYear.text)  //-- 년도
            +" TOTCCOURSECD=" + quote(cbxCourse.value) // -- 직능            
            +" TOTRAININGORDER=" + quote(edTrainingOrder.text) // -- 수료회차                    
            	// args 
		,"lfn_AfterSearchTraining"
		,false
	);
	
	
	
	tit_addCsvSearchActionInfo("training:searchTrainingPassCancel_Group", false); // 수료인원 찾는 트랜잭션
	// 서버 호출 
	tit_callService(
		""
		,""
		,""	// inDs
		,"ds_ioTrainingPassCancel_Group=ds_oTrainingPassCancel_Group"	// outDs
		,"TOMGGTMGNO=" + quote(cbxJibu.value)   //-- 지부
            +" TOYEAR=" + quote(edTrainingYear.text)  //-- 년도
            +" TOTCCOURSECD=" + quote(cbxCourse.value) // -- 직능            
            +" TOTRAININGORDER=" + quote(edTrainingOrder.text) // -- 수료회차                    
            	// args 
		,"lfn_AfterSearchTraining"
		,false
	);
	
}

