/* 사용자 함수 */
/*
 * 기능 : 결강자 조회 후 처리해야 할 내용
 * parameter : nErrorCode - 에러코드
 *             strErrorMsg - 에러메시지
 * Return : 없음
 */
function lfn_AfterSearchInternetJusu(nErrorCode, strErrorMsg) {
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }
        
    var jubsuCnt = 0;
    var repayCnt = 0;
    var changeCnt = 0;
    var tatalAmt = 0;
    var jubsuAmt = 0;
    var repayAmt = 0;
    for (i=0;i<ds_oInternetJusu.GetCount();i++) {
        if (ds_oInternetJusu.GetColumn(i, "TOHLASTGUBUN") == "접수") {
            jubsuCnt++;
            tatalAmt += ds_oInternetJusu.GetColumn(i, "TSTRAININGAMOUNT");
            jubsuAmt += ds_oInternetJusu.GetColumn(i, "TSTRAININGAMOUNT");
        } else if (ds_oInternetJusu.GetColumn(i, "TOHLASTGUBUN") == "환불") {
            repayCnt++;
            tatalAmt += ds_oInternetJusu.GetColumn(i, "TSTRAININGAMOUNT");
            repayAmt += ds_oInternetJusu.GetColumn(i, "TSTRAININGAMOUNT");
        } else if (ds_oInternetJusu.GetColumn(i, "TOHLASTGUBUN") == "회차변경") {
            changeCnt++;
            tatalAmt += ds_oInternetJusu.GetColumn(i, "TSTRAININGAMOUNT");
            jubsuAmt += ds_oInternetJusu.GetColumn(i, "TSTRAININGAMOUNT");
        }
    }
    medTotalCnt.Value = ds_oInternetJusu.GetCount();
    medJubsuCnt.Value = jubsuCnt;
    medRepayCnt.Value = repayCnt;
    medOrderChangeCnt.Value = changeCnt;
    medTatalAmt.Value = tatalAmt;
    medJubsuAm.Value = jubsuAmt;
    medRepayAmt.Value = repayAmt;
    
    gfn_SearchMessage(gdInternetJusu); // 조회 메세지를 처리하고 그리드의 정렬 헤드 마크를 제거한다.
}

