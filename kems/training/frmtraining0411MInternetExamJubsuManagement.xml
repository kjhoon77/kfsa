function btnToExcel_OnClick(obj)  {
    
    if (ds_oExamJusu.GetRowCount() < 1) {
        gfn_Alert("출력할 데이터가 존재하지 않습니다.");
        return;
    }    
	// 개인정보 다운로드 사유 등록
	var parValue = "sCode=1203";	// COMMON.PERSONALDOWNLOADH
		parValue += " sRowCnt=" + ds_oExamJusu.GetRowCount();
		parValue += " sTitle=" + quote(gdInternetJusu.UserData);
	ls_IsRemark = Dialog("COM::frmCOM2100PDownloadRemark.xml", parValue, "", "", "", -1, -1);	
	if (ls_IsRemark == "" || ls_IsRemark == null || ls_IsRemark == "FAIL") {
		return false;
	}
	lfn_Excel(obj);
}


/****************************************************************************************
 * 기능 : 조회
 ****************************************************************************************/
function lfn_Search(obj) {
    
    var ls_SearchGubun = "";
    if (!gfn_FormValidate(this)) { // 입력 Validation Check
        return false;
    }
    
    if (radProcGubun.Value == "10") {
        ls_SearchGubun = cbxJubsuStatus1.Value;
    } 
    else if (radProcGubun.Value == "20") {
        ls_SearchGubun = cbxJubsuStatus2.Value;
    } 
    else if (radProcGubun.Value == "30") {
        ls_SearchGubun = cbxJubsuStatus3.Value;
    }
    
    if (length(ls_SearchGubun)<1) {
        ls_SearchGubun = radProcGubun.Value;
    }    
    ds_oExamJusu.ClearData(); // 데이타셋 초기화
    tit_clearActionInfo();
	tit_addCsvSearchActionInfo("training:searchInternetExamJubsu", false);
	// 서버 호출 
	tit_callService(
		""
		,""
		,""	// inDs
		,"ds_oExamJusu=ds_oInternetExamJubsu"	// outDs
		,"EJPERSONNM=" + quote(edPersonNm.Text)
            + " STARTJUBSUDATE=" + quote(calJubsuDateStart.Value)
            + " ENDJUBSUDATE=" + quote(calJubsuDateEnd.Value)
            + " EJHSTATUSCD=" + quote(ls_SearchGubun)
            + " EOHJUBSUGUBUN=" + quote(radWorkGubun.Value)
            + " EOEXAMORDER=" + quote(edExamOrder.Text)
            + " EOTCCOURSECD=" + quote(cbxCourse.Value)
            + " EOMGGTMGNO=" + quote(cbxJibu.Value) // args 
		,""
		,false
	);	
	medTotalCnt.Value = ds_oExamJusu.GetRowCount();	
}


