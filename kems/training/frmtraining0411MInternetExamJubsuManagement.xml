function radProcGubun_OnClick(obj){
    cbxJubsuStatus1.Value = "";
    cbxJubsuStatus2.Value = "";
    cbxJubsuStatus3.Value = "";
        
	if (radProcGubun.Value == "10") {
        cbxJubsuStatus1.Enable = true;
        cbxJubsuStatus2.Enable = false;
        cbxJubsuStatus3.Enable = false;
        btnPrint.Enable = false;
    } 
    else if (radProcGubun.Value == "20") {
        cbxJubsuStatus1.Enable = false;
        cbxJubsuStatus2.Enable = true;
        cbxJubsuStatus3.Enable = false;
        btnPrint.Enable = false;
    } 
    else if (radProcGubun.Value == "30") {
        cbxJubsuStatus1.Enable = false;
        cbxJubsuStatus2.Enable = false;
        cbxJubsuStatus3.Enable = true;
        btnPrint.Enable = true;
    } 
    else {
        cbxJubsuStatus1.Enable = false;
        cbxJubsuStatus2.Enable = false;
        cbxJubsuStatus3.Enable = false;
        btnPrint.Enable = false;
    }
}


function gdInternetJusu_OnCellDblClick(obj,nRow,nCell,nX,nY,nPivotIndex) {

    var sArg = "sJubsuNo=" + quote(ds_oExamJusu.GetColumn(nRow, "EJMGNO"))
        + " sJubsuStatus=" + quote(ds_oExamJusu.GetColumn(nRow, "EJHSTATUSCD"))
        + " sTjhSeq=" + quote(ds_oExamJusu.GetColumn(nRow, "EJHSEQ"))
        + " sSetlmSeq=" + quote(ds_oExamJusu.GetColumn(nRow, "EJHTSSEQ"))
        + " sTohSeq=" + quote(ds_oExamJusu.GetColumn(nRow, "EJHEOHSEQ"))        
        + " sEoMgno=" + quote(ds_oExamJusu.GetColumn(nRow, "EOMGNO"));    
    var li_FormnHeight = 0;
    
    if (ds_oExamJusu.GetColumn(nRow, "EJHEOHSEQ") > 1) {
        li_FormnHeight = 112;
    }    
    switch(ds_oExamJusu.GetColumn(nRow, "EJHSTATUSCD")){
        case 5:
        case 6:
        case 7:
        case 8:
            li_FormnHeight += 480;
            break;
        default :
            li_FormnHeight += 352;
    }    
    sArg += " sFormHeight=" + quote(li_FormnHeight);        
	var arrPos = Dialog("training::frmtraining0091PInternetJubsuStatistics.xml", sArg, 732, li_FormnHeight, "", -1, -1);	
	if (arrPos) {
        lfn_Search();
    }
}


function btnPrint_OnClick(obj) {

    ds_oInternetReportAll.ClearData();
    tit_clearActionInfo();
    tit_addCsvSearchActionInfo("training:searchInternetExamRepayReport", false);
    for (i=0; i<ds_oExamJusu.GetRowCount(); i++) {
        ds_oInternetReport.ClearData();
        tit_callService(
            ""
            ,""
            ,"" // inDs
            ,"ds_oInternetReport=ds_oExamJubsuPrint"	// outDs
            ,"EJMGNO=" + quote(ds_oExamJusu.GetColumn(i, "EJMGNO")) // args
            ,""
            ,false
            ,""
            ,true
        );
        
        ds_oInternetReportAll.InsertRow(0);
        ds_oInternetReportAll.CopyRow(0, ds_oInternetReport, 0);
    }
    
    gfn_CallReportPopup("frmtraining0091R01ExamRepayAppl.rex"
        , "ds_oInternetReportAll"
        , "rpDirpath=" + GF_FILE_SERVER_URL + "kems"
        , true
    );	
}

