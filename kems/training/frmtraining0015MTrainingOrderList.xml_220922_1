 */
function lfn_SetFormAuth(bStatus) {

}

/* 사용자 이벤트 함수 */
function btnMutilSort_OnClick(obj)
{
	gfn_GridMultiSort(gdTrainingOrder, 0); // 멀티 정렬을 실행한다.
}

function radSearchGubun_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{
	if (strCode == "ORDER") {
        edTrainingOrderStart.Enable = true;
        edTrainingOrderEnd.Enable = true;
        edTrainingOrderStart.Text = "001";
        edTrainingOrderEnd.Text = "999";
        calDateStart.Value = "";
        calDateEnd.Value = "";
        calDateStart.Enable = false;
        calDateEnd.Enable = false;
        edTrainingOrderStart.UserData = "I:시작회차";
        edTrainingOrderEnd.UserData = "I:종료회차";
        calDateStart.UserData = "";
        calDateEnd.UserData = "";
        edTrainingOrderStart.SetFocus();
	} else {
        edTrainingOrderStart.Value = "";
        edTrainingOrderEnd.Value = "";
        edTrainingOrderStart.Enable = false;
        edTrainingOrderEnd.Enable = false;
        calDateStart.Enable = true;
        calDateEnd.Enable = true;
        calDateStart.Value = left(GetDate(), 4) + "0101";
        calDateEnd.Value = left(GetDate(), 4) + "1231";
        calDateStart.UserData = "I:시작일자";
        calDateEnd.UserData = "I:종료일자";
        edTrainingOrderStart.UserData = "";
        edTrainingOrderEnd.UserData = "";
        calDateStart.SetFocus();
	}
}

/* 회차 포커스 아웃될때 자동 0 입력 */
function edTrainingOrder_OnKillFocus(obj)
{
	gfn_ComponentOnKillFocus(obj);

    if (obj.Text != "") {
        obj.Text = iif(gfn_IsNull(obj.Text), "", lpad(obj.Text, "0", 3));
    }
}

function edTrainingOrderEnd_EnterSearch(obj,nChar,bShift,bCtrl, bAlt,LLParam,HLParam)
{
	if (nChar == 13) { // Enter Key
        edTrainingOrderEnd.Text = iif(gfn_IsNull(edTrainingOrderEnd.Text), "", lpad(edTrainingOrderEnd.Text, "0", 3));
        lfn_Search();
    }
}

function btnToExcel_OnClick(obj) {
    if (ds_oTrainingOrder.GetRowCount() < 1) {
        gfn_Alert("출력할 데이터가 존재하지 않습니다.");
        return false;
    }
	lfn_Excel(obj);
}
