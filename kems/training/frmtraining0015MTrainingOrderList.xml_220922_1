 * 기능 : 조회
 */
function lfn_Search(obj) {
    if (edTrainingOrderEnd.Enable == true && edTrainingOrderEnd.Text == "") {
        gfn_Alert("종료회차를 입력해주십시오.");
        edTrainingOrderEnd.SetFocus();
        return false;
    }
    if (edTrainingOrderStart.Text > edTrainingOrderEnd.Text) {
        gfn_Alert("시작회차가 종료회차보다 늦습니다.");
        edTrainingOrderStart.SetFocus();
        return false;
    }
    if (calDateStart.Value > calDateEnd.Value) {
        gfn_Alert("시작일자가 종료일자보다 늦습니다.");
        edTrainingOrderStart.SetFocus();
        return false;
    }
    if (!gfn_FormValidate(this)) { // 입력 Validation Check
        return false;
    }
    
    medTotalCnt.Value = "";
    ds_oTrainingOrder.ClearData(); // 데이타셋 초기화
    
    // 조회이력을 저장한다.  
    ls_HistoryArg = "지부=" + cbxJibu.Text
                + ",년도=" + edTrainingYear.Text
                + ",직능=" + cbxCourse.Text
                + ",회차시작=" + edTrainingOrderStart.Text
                + ",회차종료=" + edTrainingOrderEnd.Text
                + ",교육시작일=" + left(calDateStart.Value, 8)
                + ",교육종료일=" + left(calDateEnd.Value, 8);
    gfn_InsertSearchPrintHistory(
        "S"
        , this.ID
        , global.mnuLeft.cbxOpenForm.Text
        , ls_HistoryArg
    );
    
	// 콜센터 직원용 이면
	if (gs_IsCallCenter == "Y") {
		// 콜센터 직원 조회이력 저장
		gfn_InsertCallCenterSearchPrintHistory(
				"S"
				, gs_CallCenterUID				, gds_OpenedForm.GetColumn(gds_OpenedForm.FindRow("MURL",this.ID),"MMENUID")
				, this.ID				, global.mnuLeft.cbxOpenForm.Text				, ls_HistoryArg
			);
	}

    tit_clearActionInfo();
	tit_addCsvSearchActionInfo("training:searchTrainingOrderStat", false);
	// 서버 호출 
	tit_callService(
		""
		,""
		,""	// inDs
		,"ds_oTrainingOrder=ds_oTrainingOrder"	// outDs
		,"TOMGJIBUCD=" + quote(cbxJibu.Value)
            + " TOYEAR=" + quote(edTrainingYear.Text)
            + " TCCOURSECD=" + quote(cbxCourse.Value)
            + " TOTRAININGORDERSTART=" + quote(edTrainingOrderStart.Text)
            + " TOTRAININGORDEREND=" + quote(edTrainingOrderEnd.Text)
            + " TOSTARTDATE=" + quote(left(calDateStart.Value, 8))
            + " TOENDDATE=" + quote(left(calDateEnd.Value, 8)) // args 
            + " MUNJEGUBUN=" + quote(radMunjeGubun.Value) // -- 직능상세구분
            + " COURSEGUBUN=" + quote(radCourseGubun.Value) // -- 과정구분
            + " ONLINEGUBUN=" + quote(cbxOnlineGubun.Value)	// --온라인구분
		,"lfn_AfterSearchTrainingOrder"
		,false
	);
}

/*
 * 기능 : 출력
 */
function lfn_Print(obj)
{
    // 조회이력을 저장한다.  
    ls_HistoryArg = "지부=" + cbxJibu.Text
                + ",년도=" + edTrainingYear.Text
                + ",직능=" + cbxCourse.Text
                + ",회차시작=" + edTrainingOrderStart.Text
                + ",회차종료=" + edTrainingOrderEnd.Text
                + ",교육시작일=" + left(calDateStart.Value, 8)
                + ",교육종료일=" + left(calDateEnd.Value, 8);
    gfn_InsertSearchPrintHistory(
        "P"
        , this.ID
        , global.mnuLeft.cbxOpenForm.Text
        , ls_HistoryArg
    );
    // to do Validation Check;
    
    //gfn_CallReportPopup("", "ds_oTrainingOrder", "");
}

/* 공통 필수 종료 */

/* 사용자 함수 */
/*
 * 기능 : 결강자 조회 후 처리해야 할 내용
 * parameter : nErrorCode - 에러코드
 *             strErrorMsg - 에러메시지
 * Return : 없음
 */
function lfn_AfterSearchTrainingOrder(nErrorCode, strErrorMsg) {
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }
    
    medTotalCnt.Value = ds_oTrainingOrder.rowcount;
    gfn_SearchMessage(gdTrainingOrder); // 조회 메세지를 처리하고 그리드의 정렬 헤드 마크를 제거한다.
}

/*
 * 기능 : Transaction 서비스 호출 후 처리해야 할 내용
 * parameter : strSvcID - 서비스ID
 *             nErrorCode - 에러코드
 *             strErrorMsg - 에러메시지
 * Return : 없음
 */
function lfn_AfterSaveTrainingOrder(nErrorCode, strErrorMsg) {
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }

    gfn_SaveMessage(); // 저장 메세지를 처리한다.
    lfn_Search(); // 결강자 조회
}

