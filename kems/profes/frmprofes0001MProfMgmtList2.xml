function lfn_Search(obj) {	               
	               
    ds_ioList.ClearData(); // 데이타셋 초기화
    tit_clearActionInfo();
	tit_addCsvSearchActionInfo("profes:searchProfes0001proMgmtList2", false);	
	// 서버 호출 
	tit_callService(
		 ""
		,""
		,""	// inDs
		,"ds_ioList=ds_ioList"	// outDs
		,"PFDATE=" + quote(calDate.Value)
		+" PFCHECK=" + quote(radCheck.Value)
		,"afterSearch"
		,false
	);	               
}

// 조회 후 callbackfunction afterSearch(nErrorCode, strErrorMsg)
{
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
		gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }
}


function btnToExcel_OnClick(obj)
{
