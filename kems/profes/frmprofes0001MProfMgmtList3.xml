}

function ds_ioList_CanRowPosChange(obj,nOldRow,nRow)
{ 
	// 클릭한 사함만 아래에 뿌려주기 위함
	var aName = ds_ioList.GetColumn(nRow, "APFGUBUNNM");
	var bName = ds_ioList.GetColumn(nRow, "BPFGUBUNNM");
	var cName = ds_ioList.GetColumn(nRow, "CPFGUBUNNM");
	var PFSABUN = ds_ioList.GetColumn(nRow, "PFSABUN");
	
	var PFNAME = "";

	
	if(length(aName) > 0)
	{
		PFNAME = aName;	
	}
	else if(length(bName) > 0)
	{
		PFNAME = bName;	
	}
	else if(length(cName) > 0)
	{
		PFNAME = cName;	
	}
	 
	lfn_NameSearch(PFSABUN);
}

function lfn_NameSearch(PFSABUN) {	               
	               
    ds_ioList3.ClearData(); // 데이타셋 초기화
