function lfn_Print(obj){

	// 신규 등록 일자 체크 
	if (!lfn_ChkDate()) {
		return;
	}
	
    lfn_Search();
    
    course = "";
    if (ds_oDATA.GetCount() < 1) {
        gfn_Alert("출력할 데이터가 없습니다.");
        return false;
    }

    var rpt = "";
    var params = "";
    var params2 = "";
    var params3 = "";
    var sTopMargin = "";
    var sLeftMargin = "";

        
	rpt = "frmcust6590NewDataMatch_" + cbxCourse.Value + ".rex";
	
	if (cbxCourse.Value == "8") { 
		sTopMargin = "100";
		sLeftMargin = "200";
	}
	
	if (cbxCourse.Value == "1") {
		params = "(소방안전관리자) 신규대상 연계현황"; 
	} else if (cbxCourse.Value == "3") {
		params = "(위험물안전관리자) 신규대상 연계현황"; 
	} else if (cbxCourse.Value == "5") {
		params = "(소방기술자) 신규대상 연계현황"; 
	} else if (cbxCourse.Value == "8") {
		params = "(위험물운송자) 신규대상 연계현황"; 
	}
	
	
	if (gfn_IsNull(cbxJibu.Value)) {
		params2 = "지부";
	} else {
		params2 = "지역";
	}
	
	var StartDate = substr(calStartDate.Value,0,4) + "." + substr(calStartDate.Value,4,2) + "." + substr(calStartDate.Value,6,2);
    var EndDate = substr(calEndDate.Value,0,4) + "." + substr(calEndDate.Value,4,2) + "." + substr(calEndDate.Value,6,2);
    params3 = "신규등록일자 : "+ StartDate + " ~ " + EndDate;
    
    params = "rd_TITLE=" + quote(params) + "^s_TITLE=" + quote(params2)+ "^DATE=" + quote(params3); 
    
    
	/*
	 * 기능 : 렉스퍼트 보고서를 팝업화면으로 호출한다.
	 * parameter : sReportName - 레포트명(예:레포트.rex)
	 *             sReportDataset - 데이터셋명(예:dsReport1||dsResport2||...)
	 *             sReportParams - 렉스퍼트용 파라메터(예: a에 1, b에 2, c에 3의 값을 넣는 경우, a=1^b=2^c=3...)
	 *             bLcsYn - 수첩여부
	 *             LeftMargin = 왼쪽
	 *             RightMargin = 오른쪽
	 *             TopMargin = 상단
	 *             BottomMargin = 하단
	 * return value : 정상출력 true, 출력 안했으면 false
	 */    
	 
    //gfn_CallReportPopup(rpt, "ds_oDATA", params);
    gfn_CallReportPopup(rpt, "ds_oDATA", params, '', '', sTopMargin, sLeftMargin);
}




function lfn_Search(obj) {

    SetWaitCursor(false);  
    ds_oDATA.ClearData(); // 데이타셋 초기화
    
    tit_clearActionInfo();    
   
    if (gfn_IsNull(cbxJibu.Value)) {
		tit_addCsvSearchActionInfo("cust:searchCust6590NewDataMatch", false);
    } else {
		tit_addCsvSearchActionInfo("cust:searchCust6590NewDataMatchJibu", false);
    }
    
    
	// 서버 호출 
	tit_callService(
         "",""
		,""	// inDs
		,"ds_oDATA=ds_oDATA"	// outDs
		,     " COURSE=" + quote(cbxCourse.Value)
	    	+ " JIBU=" + quote(cbxJibu.value)
            + " SDATE="  + quote(calStartDate.value + "000000") 
            + " EDATE="  + quote(calEndDate.value + "235959")
		, "lfn_AfterSearch"
		, false		, false  // 처리중 창 보여주기 (null or false : 보여줌) 
		, true); 
		   
}

function lfn_AfterSearch(nErrorCode, strErrorMsg) {
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);     
        course = "";
        return;
    }
}







