#include "LIB::cust.js"; // 공통업무 관련

/* 공통 필수 시작 */
function Form_OnLoadCompleted(obj) {
    gfn_Form_OnLoadCompleted(obj); // 공통 폼 로딩 이벤트 처리
    
    // 폼 환경설정
    ls_SystemGubun = gs_SystemGubun;
    ls_SystemMenuId = gs_SystemMenuId;
    var ls_FormId = split(sFormUrl, "::");
    ls_FormId = split(ls_FormId[1], ".");
    obj.Title = sFormTitle + "(" + ls_FormId[0] + ")";

    gfn_MakeRequiredInputMark(this); // 필수 입력란을 설정한다.

    ms_FormAuth = gfn_GetFormAuth(this); // 권한을 조회한다.
    if (ms_FormAuth == "W") { // 권한에 따른 버튼 설정
        lfn_SetFormAuth(true); // 저장 권한
    } else if (ms_FormAuth == "R") {
        lfn_SetFormAuth(false); // 읽기 권한
    } else {
        gfn_Alert(MSGI00080);
        close();
    }

    // 공통 코드 조회
    gfn_SearchCommonCode(ds_oBizGubun, "BIZCD", "Y", "");
    edCount.Text = 1000;
    
    edPersonNM.SetFocus();
}

/*-----------------------------------------------------------------
 * 기능 : 버튼 활성/비활성시킨다.
 * parameter : bStatus - 활성/비활성여부(true:활성, false:비활성)
 * return : 없음
 *----------------------------------------------------------------*/
function lfn_SetFormAuth(bStatus) {
    
}

function Form_OnSize(obj,nCx,nCy,nState)
{
    if (gs_IsResizingForm == "Y") {
        gdManagerList.Width = nCx - gdManagerList.Left - 8;
        shpGubunBox1.Width = nCx - shpGubunBox1.Left - 4;
        
        gdManagerPassList.Width = nCx - gdManagerPassList.Left - 8;
        shpGubunBox2.Width = nCx - shpGubunBox2.Left - 4;
    }
}

/*
 * 기능 : 종료
 */
function lfn_End(obj) {
    Close();
}

/*
 * 기능 : 조회
 */
function lfn_Search(obj) {
var ls_PersonKey = "";

    if (gfn_IsNull(edPersonNM.Text) && gfn_IsNull(medResidentNo.Value)) {
        gfn_Alert("이름이나 주민번호 중 하나는 입력해 주십시오.");
        edPersonNM.SetFocus();
        return;
    }
    
    if (!gfn_FormValidate(this)) { // 입력 Validation Check
        return false;
    }
    
	// personkey 조회
	if (!gfn_isNull(edPersonNM.Text) && (length(medResidentNo.Value) == 13)) {
		ls_PersonKey = gfn_SearchPERSONKEY(edPersonNM.Text, medResidentNo.Value, "");
	}
    
    // 조회이력을 저장한다.  
    ls_HistoryArg = "이름=" + quote(edPersonNM.Text)
                + ",주민번호=" + quote(substr(medResidentNo.Value,0,7));
    gfn_InsertSearchPrintHistory(
        "S"
        , this.ID
        , global.mnuLeft.cbxOpenForm.Text
        , ls_HistoryArg
    );
    
    ds_oManagerList.ClearData(); // 데이타셋 초기화

    tit_clearActionInfo();
	tit_addCsvSearchActionInfo("cust:searchCust3010ManagerList", false);
	// 서버 호출 
	tit_callService(
		""
		,""
		,""	// inDs
		,"ds_oManagerList=ds_oManagerList"	// outDs
		,"FMNM=" + quote(edPersonNM.Text)
		 + " FMBIRTHDAY=" + quote(left(medResidentNo.Value),7)
		 + " FMPERSONKEY=" + quote(ls_PersonKey)
		 + " ROWNUM=" + quote(edCount.Text)
		,"lfn_AfterSearchManagerList"
		,false
		,""
		,true
	);
}

/*
 * 기능 : 공통코드 조회 후 처리해야 할 내용
 * parameter : nErrorCode - 에러코드
 *             strErrorMsg - 에러메시지
 * Return : 없음
 */
function lfn_AfterSearchManagerList(nErrorCode, strErrorMsg) {
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }
    //trace(ds_oManagerList.SaveXML());

    gfn_SearchMessage(gdManagerList); // 조회 메세지를 처리하고 그리드의 정렬 헤드 마크를 제거한다.
}
