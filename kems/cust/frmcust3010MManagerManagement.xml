function ds_oManagerList_OnRowPosChanged(obj,nOldRow,nRow)
{
    if (nRow < 0) {
        return;
	}
	ds_oManagerPassList.ClearData(); // 데이타셋 초기화

    tit_clearActionInfo();
	tit_addCsvSearchActionInfo("cust:searchCust3010ManagerPassList", false);
	// 서버 호출 
	tit_callService(
		""
		,""
		,""	// inDs
		,"ds_oManagerPassList=ds_oManagerPassList"	// outDs
		,"FMNM=" + quote(ds_oManagerList.GetColumn(nRow, "FMNM"))
		 + " FMBIRTHDAY=" + quote(ds_oManagerList.GetColumn(nRow, "FMBIRTHDAY"))
		 + " FMPERSONKEY=" + quote(ds_oManagerList.GetColumn(nRow, "FMPERSONKEY"))
		,"lfn_AfterSearchManagerPassList"
		,false
	);
}

