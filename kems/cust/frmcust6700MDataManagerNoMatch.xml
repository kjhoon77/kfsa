/* 사용자 함수 */
/*
 * 기능 : 조회 후 처리해야 할 내용
 * parameter : nErrorCode - 에러코드
 *             strErrorMsg - 에러메시지
 * Return : 없음
 */
function lfn_AfterSearchDataManagerNoMatch(nErrorCode, strErrorMsg) {
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }
	if(ds_oDataManagerNoMatch.GetRowCount() > 0) {
		medTotalCnt.Value = ds_oDataManagerNoMatch.GetRowCount();
	} else {
		medTotalCnt.Value = 0;
	}
	gdDataManagerNoMatch.Redraw = true;
    gfn_SearchMessage(gdDataManagerNoMatch); // 조회 메세지를 처리하고 그리드의 정렬 헤드 마크를 제거한다.
}

