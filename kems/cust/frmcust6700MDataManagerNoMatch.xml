/*
 * 기능 : 조회
 */
function lfn_Search(obj) {


    ds_oDataManagerNoMatch.ClearData(); // 데이타셋 초기화

	gdDataManagerNoMatch.Redraw = false;
	
    tit_clearActionInfo();    

	tit_addCsvSearchActionInfo("cust:searchCust6700DataManagerNoMatch", false);
	
	// 서버 호출 
	tit_callService(
		""
		,""
		,""	// inDs
		,"ds_oDataManagerNoMatch=ds_oDataManagerNoMatch"	// outDs
		,"GTMGNO=" + quote(cbxJibu.Value)
		+ " REGCD=" + quote(cbxRegion.Value)
		+ " COURSECD=" + quote(cbxCourse.Value)	// arg
		,"lfn_AfterSearchDataManagerNoMatch"
		,false
	);
}

