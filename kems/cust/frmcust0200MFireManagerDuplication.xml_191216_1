/*
 * 기능 : 조회
 */
function lfn_Search(obj) {


    ds_oDuplicationList.ClearData(); // 데이타셋 초기화

	gdDuplicationList.Redraw = false;
	
    tit_clearActionInfo();    

	tit_addCsvSearchActionInfo("cust:searchCust0200FireManagerDuplication", false);
	
	// 서버 호출 
	tit_callService(
		""
		,""
		,""	// inDs
		,"ds_oDuplicationList=ds_oDuplicationList"	// outDs
		,"SJIBU=" + quote(cbxJibu.Value)
		+ " SCOURSE=" + quote(substr(cbxCourse.Value,0,1))
		+ " SCOMPARE=" + quote(cbxCompare.Value)
		+ " SCNT=" + quote(cbxDuplicationCnt.Value)	// arg
		,"lfn_AfterSearchFireManagerDuplication"
		,false
	);
}

