 */
function lfn_AfterSearchFireManagerDuplication(nErrorCode, strErrorMsg) {
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }
	if(ds_oDuplicationList.GetRowCount() > 0) {
		medTotalCnt.Value = ds_oDuplicationList.GetRowCount();
		/*switch(radOrder.Value)
		{
			case 0 :
				ds_oPostReturn.Sort("PRPDDATE:D,CNO:A");
			break;
			
			case 1 :
				ds_oPostReturn.Sort("CNO:A,PRPDDATE:D");
			break;
			
			case 2 :
				ds_oPostReturn.Sort("GTPRTSEQ:A,PRPDDATE:D,CNO:A");
			break;
		} */
	} else {
		medTotalCnt.Value = 0;
	}
	gdDuplicationList.Redraw = true;
    gfn_SearchMessage(gdDuplicationList); // 조회 메세지를 처리하고 그리드의 정렬 헤드 마크를 제거한다.
}

