/*
* 조회 쿼리 실행 후 실행(에러 처리)
*/
function lfn_searchAfter(nErrorCode, strErrorMsg) {
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }
    
    var smscount = 0;
    var mmscount = 0;
    
    if (radGubun.Value == "1") {		// 실무교육 접수자 일 경우
    
        medTotalCnt.Text = ds_ioSmsPracticeEduJubsu.count();            
        
        for (var i = 1; i <= ds_ioSmsPracticeEduJubsu.count; i++) {
            if (ds_ioSmsPracticeEduJubsu.GetColumn(i - 1, "SENDGUBUN") == "단문(SMS)") {
                smscount = smscount + 1;
			} else {
			
				mmscount = mmscount + 1;
			}
		}
		
		medSmsCnt.text = smscount;	// sms 건수
		
		medMmsCnt.text = mmscount;	// mms 건수
		
    } else if (radGubun.Value == "2") {		// 실무교육안내문 발행 일 경우    

        medTotalCnt.Text = ds_ioSmsEduData.count();   // 실무교육안내문 발행이력을 조회했을 경우 갯수
        
        for (var i = 1; i <= ds_ioSmsEduData.count; i++) {
            if (ds_ioSmsEduData.GetColumn(i - 1, "SENDGUBUN") == "단문(SMS)") {
                smscount = smscount + 1;
			} else {
			
				mmscount = mmscount + 1;
			}
		}
		
		medSmsCnt.text = smscount;	// sms 건수
		
		medMmsCnt.text = mmscount;	// mms 건수
     
    } else if (radGubun.Value == "3") {		// 실무교육이수일 경우    

        medTotalCnt.Text = ds_ioSmsEduIssue.count();   // 실무교육이수 이력을 조회했을 경우 갯수
        
        for (var i = 1; i <= ds_ioSmsEduIssue.count; i++) {
            if (ds_ioSmsEduIssue.GetColumn(i - 1, "SENDGUBUN") == "단문(SMS)") {
                smscount = smscount + 1;
			} else {
			
				mmscount = mmscount + 1;
			}
		}
		
		medSmsCnt.text = smscount;	// sms 건수
		
		medMmsCnt.text = mmscount;	// mms 건수
        
    }
    }
    
};

/*
* 업무구분에 따라 ds, format 변경함
*/
function radGubun_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{
	ds_ioSmsPracticeEduJubsu.ClearData();
	ds_ioSmsEduData.ClearData();
	ds_ioSmsEduIssue.ClearData();

	if (strCode == "1") { // 실무교육 접수자
		divWork1.Visible = true;	// 실무교육 접수자
		divWork2.Visible = false;	// 실무교육안내문 발행
		divWork3.Visible = false;	// 실무교육이수
	
        gdSmsStat.Format = "EduJubsu";
        gdSmsStat.BindDataset = "ds_ioSmsPracticeEduJubsu";
        SmsSendList_SQL ="cust:searchCust1180SmsPracticeEduJubsu";
        SmsSendList_DatasetSMMSG = "ds_ioSmsPracticeEduJubsu=ds_oSmsPracticeEduJubsu";   

        divWork1.rdSearchGubun.Value = "1";
		divWork1.calSendDateStart.value = left(GetDate(), 8);  // 접수 일자1
		divWork1.calSendDateEnd.Value = left(GetDate(), 4)+"1231";  // 접수일자2
	
		divWork1.calEduDateStart.value = left(GetDate(), 4)+"0101";  // 교육 일자1
		divWork1.calEduDateEnd.Value = left(GetDate(), 4)+"1231";    // 교육 일자2
		
    } else if (strCode == "2") { // 실무교육안내문 발행
		divWork2.Left = 4;
		divWork2.Top = 87;
    
		divWork1.Visible = false;	// 실무교육 접수자
		divWork2.Visible = true;	// 실무교육안내문 발행
		divWork3.Visible = false;	// 실무교육이수
    
        gdSmsStat.Format = "EduData";        
        gdSmsStat.BindDataset = "ds_ioSmsEduData";
        SmsSendList_SQL ="cust:searchCust1180SmsEduDataHistory";
        SmsSendList_DatasetSMMSG = "ds_ioSmsEduData=ds_oSmsEduData";  
        
        divWork2.rdSearchGubun.Value = "1";
		divWork2.calSendDateStart.value = left(GetDate(), 8);  // 접수 일자1
		divWork2.calSendDateEnd.Value = left(GetDate(), 4)+"1231";  // 접수일자2
		divWork2.calCreateDateStart.value = left(GetDate(), 4)+"0101";  // 안내문생성 일자1
		divWork2.calCreateDateEnd.Value = left(GetDate(), 4)+"1231";    // 안내문생성 일자2
		
    } else if (strCode == "3") { // 실무교육이수
		divWork3.Left = 4;
		divWork3.Top = 87;
    
		divWork1.Visible = false;	// 실무교육 접수자
		divWork2.Visible = false;	// 실무교육안내문 발행
		divWork3.Visible = true;	// 실무교육이수
		
        gdSmsStat.Format = "EduIssue";
        gdSmsStat.BindDataset = "ds_ioSmsEduIssue";
        SmsSendList_SQL ="cust:searchCust1180SmsEduIssue";
        SmsSendList_DatasetSMMSG = "ds_ioSmsEduIssue=ds_oSmsEduIssue";  
        
        divWork3.rdSearchGubun.Value = "1";
		divWork3.calSendDateStart.value = left(GetDate(), 8);  // 접수 일자1
		divWork3.calSendDateEnd.Value = left(GetDate(), 4)+"1231";  // 접수일자2
		divWork3.calIssueDateStart.value = left(GetDate(), 4)+"0101";  // 이수 일자1
		divWork3.calIssueDateEnd.Value = left(GetDate(), 4)+"1231";    // 이수 일자2
		
	} 
	medTotalCnt.Text = "0";
	medSmsCnt.Text = "0";
	medMmsCnt.Text = "0";
}

