/*
* 실무교육접수 조회구분 변경시 
*/
function divWork1_rdSearchGubun_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{
	if(strCode == "1") {   // 전체
        divWork1.calSendDateStart.Enable = true;        
        divWork1.calSendDateEnd.Enable = true;                
        divWork1.calEduDateStart.Enable = true;
        divWork1.calEduDateEnd.Enable = true;
    	
		divWork1.calSendDateStart.value = left(GetDate(), 8);  		// 발송일자1
		divWork1.calSendDateEnd.Value = left(GetDate(), 4)+"1231";  // 발송일자2
		divWork1.calEduDateStart.value = left(GetDate(), 4)+"0101";	// 교육일자1
		divWork1.calEduDateEnd.Value = left(GetDate(), 4)+"1231";   // 교육일자2
    	
    	
	} else if (strCode == "2") {  // 발송일자
        divWork1.calSendDateStart.Enable = true;        
        divWork1.calSendDateEnd.Enable = true;                
        divWork1.calEduDateStart.Enable = false;
        divWork1.calEduDateEnd.Enable = false;
        
		divWork1.calSendDateStart.value = left(GetDate(), 8);  		// 발송일자1
		divWork1.calSendDateEnd.Value = left(GetDate(), 4)+"1231";  // 발송일자2
        divWork1.calEduDateStart.Value = "";	
        divWork1.calEduDateEnd.Value = "";         
	
	} else if (strCode == "3") {  // 교육일자
        divWork1.calSendDateStart.Enable = false;        
        divWork1.calSendDateEnd.Enable = false;                
        divWork1.calEduDateStart.Enable = true;
        divWork1.calEduDateEnd.Enable = true;
        
        divWork1.calSendDateStart.Value = "";	
        divWork1.calSendDateEnd.Value = "";
		divWork1.calEduDateStart.value = left(GetDate(), 4)+"0101";	// 교육일자1
		divWork1.calEduDateEnd.Value = left(GetDate(), 4)+"1231";   // 교육일자2      
        
	}
}

