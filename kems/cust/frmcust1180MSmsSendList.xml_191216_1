/*
* 실무교육대상 발송 현황
*/
function lfn_serchEduPrinth_1()  
{
	ds_ioSmsPracticeEduJubsu.ClearData();
	ds_ioSmsEduData.ClearData();
	ds_ioSmsEduIssue.ClearData();
	
	if (radGubun.Value == "1") {	//    실무교육접수자
	
		tit_clearActionInfo();
		tit_AddCSVSearchActionInfo(SmsSendList_SQL, false); 
																			
		tit_callService(
			""
			,""
			,""// inDs
			,SmsSendList_DatasetSMMSG  // outDs
			,"JIBU=" + quote(divWork1.cbxJibu.value)     // 지부 
				+ " YEAR=" + quote(divWork1.edYear.text)      // 교육년도
				+ " COURSECD=" + quote(divWork1.cbxCourse.Value)      // 직능 (여기서는 코드로 조건주는 것이 아님)        
				+ " calSendDateStart=" + quote(left(divWork1.calSendDateStart.Value,8)) // 발송일자1
				+ " calSendDateEnd=" + quote(left(divWork1.calSendDateEnd.Value,8))	  // 발송일자2             
				+ " calEduDateStart=" + quote(left(divWork1.calEduDateStart.Value,8))   // 교육일자1        
				+ " calEduDateEnd=" + quote(left(divWork1.calEduDateEnd.Value,8))     // 교육일자2  args
				+ " SENDGUBUN=" + quote(divWork1.radSendGubun.value)//  -- 발송구분
			,"lfn_searchAfter"
			,false
			,""
			,true        
		);	
		
	} else if (radGubun.Value == "2") {	//    실무교육안내문 발행
		tit_clearActionInfo();
		tit_AddCSVSearchActionInfo(SmsSendList_SQL, false); 
																			
		tit_callService(
			""
			,""
			,""// inDs
			,SmsSendList_DatasetSMMSG  // outDs
			,"JIBU=" + quote(divWork2.cbxJibu.value)     // 지부 
