    if (IsExistVar("sCmgno")) {
        lfn_Search();
    } else {
        gfn_Alert("결제 정보가 없습니다.");
        lfn_Cancel();
    }
    
    cbxMonthGubun.SetFocus();
    cbxMonthGubun.Value = "YEAR2";

}

/**
 * 조회
 */
function lfn_Search(obj) {

    tit_clearActionInfo();
    tit_AddSearchActionInfo("cust:searchCust0019Sunap", false);
    tit_callService(
        ""
        , ""
        , "" // inDs
        , "ds_oSunap=ds_oSunap"
        , "CMGNO=" + quote(sCmgno)
            + " TYPE=" + quote(cbxMonthGubun.Value)
        , ""
        ,false
        ,false
        ,true
    );
    
}

