	cbxCourse.Index = 0;
	cbxCourse1.Index = 0;
	cbxCourse2.Index = 0;
	cbxCourse3.Index = 0;
	cbxCourse4.Index = 0;
	cbxCourse5.Index = 0;
	cbxCourse6.Index = 0;
	
	radBizGubun.Index = 0;
	edCustNo1.Text = "";
	edCustNo2.Text = "";
	calStartDate.Value = left(GetDate(),4)+"0101";
	calEndDate.Value = left(GetDate(),4)+"1231";;
