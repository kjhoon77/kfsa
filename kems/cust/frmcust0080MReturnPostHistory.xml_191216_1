/*
 * 기능 : 디폴트 데이타 셋팅
 */
function lfn_InitData(obj)
{
	cbxJibu.Value = gds_oUserInfo.GetColumn(0, "PDEPTCD");	//소속지부 셋팅
	radMemberGubun.Index = 0;
