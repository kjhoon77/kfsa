 * parameter : 없음
 * return : 없음
 */
function lfn_ViewImage(obj)
{
	var sArg = "sPRHDATE1=" + quote(left(ds_oPostReturn.GetColumn(ds_oPostReturn.currow, "PRPDDATE"),4));
		sArg += " sPRHDATE2=" + quote(ds_oPostReturn.GetColumn(ds_oPostReturn.currow, "PRPDDATE"));
		sArg += " sPRHIMAGE=" + quote(ds_oPostReturn.GetColumn(ds_oPostReturn.currow, "PRIMGAE"));
	Dialog("cust::frmcust0080PReturnPostViewerPopup.xml", sArg, 820, 388, "", -1, -1);	//이메일 상세보기 팝업을 호출한다.
}

// Excel Export(F11)
function btnToExcel_OnClick(obj) {
    if (ds_oTrainingJubsu.GetRowCount() < 1) {
        gfn_Alert("출력할 데이터가 존재하지 않습니다.");
        return false;
    }
	lfn_Excel(obj);
}

// 출력(F11)
function lfn_Print(obj)
{
	if(ds_oPostReturn.GetRowCount() > 0) {
		//  S : 출력구분에 따라 리포팅 구분 (2010-04-16 김민호)
		if(radPrint.Value == "1") {
			gfn_CallReportPopup("frmCust0080R01ReturnPostHistory.rex"  // 전체
						  , "ds_oPostReturn"
						  , "JIBU=" + cbxJibu.Text + "^BIZ_GUBUN=" + radBizGubun.Text + "^PD_DATE1=" + calStartDate.Value + "^PD_DATE2=" + calEndDate.Value
							, true);
		} else {						  
			gfn_CallReportPopup("frmCust0080R01ReturnPostHistoryG.rex"  // 관리자별 
						  , "ds_oPostReturn"
						  , "JIBU=" + cbxJibu.Text + "^BIZ_GUBUN=" + radBizGubun.Text + "^PD_DATE1=" + calStartDate.Value + "^PD_DATE2=" + calEndDate.Value
						  , true);        
		}
		//  E : 출력구분에 따라 리포팅 구분 (2010-04-16 김민호)
	} else {
		gfn_Alert("출력 할 데이터가 없습니다.");
	}
}
