function Form_OnSize(obj,nCx,nCy,nState)
{
    if (gs_IsResizingForm == "Y") {
        gdPostReturn.Height = nCy - gdPostReturn.Top - 4;
    }
}


/*
 * 기능 : 우편반송이력 조회
 */
function lfn_Search(obj) {

	var lsCourseList = ""; //직능리스트
	
	if(radCourse.Value == 0) {	//선택
		if(!gfn_IsNull(cbxCourse.Value)) {
			lsCourseList += toString(trim(left(cbxCourse.Value,1)));
		}
	} else if(radCourse.Value == 1) {	//다중선택
        for ( var i=1; i < 7; i++ ) {
            if (!gfn_IsNull(object("cbxCourse" + toString(i)).Value)) {
                lsCourseList += left(object("cbxCourse" + toString(i)).Value, 1) + ",";
            }
        }        
		lsCourseList = left(lsCourseList, length(lsCourseList)-1);
	}
	
    ds_oPostReturn.ClearData(); // 데이타셋 초기화

	gdPostReturn.Redraw = false;
	
	//  S : 출력구분에 따라  구분 (2010-04-16 김민호)
	 if(radPrint.Value == "0") {
		gdPostReturn.SetColProp(15,"Width",80);
	} else {
		gdPostReturn.SetColProp(15,"Width",0);
	}
	
	if(radMinap.Value == "1") {  // 미납금액 출력이면
		gdPostReturn.SetColProp(16,"Width",80);
	} else {
		gdPostReturn.SetColProp(16,"Width",0);
	}

    tit_clearActionInfo();    
    
    if(radPrint.Value == "0") {
		tit_addCsvSearchActionInfo("cust:searchCust0080PostReturnHistoryG", false); //  관리자별
	} else {
		tit_addCsvSearchActionInfo("cust:searchCust0080PostReturnHistory", false);  // 전체
	}
	//  E : 출력구분에 따라  구분 (2010-04-16 김민호)
	
	// 서버 호출 
	tit_callService(
		""
		,""
		,""	// inDs
		,"ds_oPostReturn=ds_oPostReturn"	// outDs
		,"SJIBU=" + quote(cbxJibu.Value)
		+ " SCGUBUN=" + quote(radMemberGubun.Value)
		+ " SCOURSE=" + quote(lsCourseList)
		+ " SBIZGUBUN=" + quote(radBizGubun.Value)
		+ " SCNO1=" + quote(edCustNo1.Text)
		+ " SCNO2=" + quote(edCustNo2.Text)
		+ " SPDDATE1=" + quote(calStartDate.Value)
		+ " SPDDATE2=" + quote(calEndDate.Value)
		+ " MINAPGUBUN=" + quote(radMinap.Value)
		,"lfn_AfterSearchPostReturnHistory"
		,false
	);
}

/* 공통 필수 종료 */

/* 사용자 함수 */
/*
 * 기능 : 우편반송이력 조회 후 처리해야 할 내용
 * parameter : nErrorCode - 에러코드
 *             strErrorMsg - 에러메시지
 * Return : 없음
 */
function lfn_AfterSearchPostReturnHistory(nErrorCode, strErrorMsg) {
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }
	if(ds_oPostReturn.GetRowCount() > 0) {
		medTotalCnt.Value = ds_oPostReturn.GetRowCount();
		switch(radOrder.Value)
		{
			case 0 :
				ds_oPostReturn.Sort("PRPDDATE:D,CNO:A");
			break;
			
			case 1 :
				ds_oPostReturn.Sort("CNO:A,PRPDDATE:D");
			break;
			
			case 2 :
				ds_oPostReturn.Sort("GTPRTSEQ:A,PRPDDATE:D,CNO:A");
			break;
		}
	} else {
		medTotalCnt.Value = 0;
	}
	gdPostReturn.Redraw = true;
    gfn_SearchMessage(gdPostReturn); // 조회 메세지를 처리하고 그리드의 정렬 헤드 마크를 제거한다.
}

/*
 * 기능 : 버튼 활성/비활성시킨다.
 * parameter : bStatus - 활성/비활성여부(true:활성, false:비활성)
 * return : 없음
 */
function lfn_SetFormAuth(bStatus) {
    // 콜센터 직원 사용 이면
	if (gs_IsCallCenter == "Y") {
		btnPrint.Enable = false; // 출력 버튼
		btnToExcel.Enable = false; // 엑셀출력 버튼
		btnPrintScreen.Enable = false; // 화면출력 버튼
	}
}

