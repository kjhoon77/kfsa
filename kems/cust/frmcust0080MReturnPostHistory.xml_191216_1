// 반송우편 이미지 보기
function btn_PostReturnImage_OnClick(obj)
{
	if(gfn_IsNull(ds_oPostReturn.GetColumn(ds_oPostReturn.currow, "PRPDDATE")) 
	|| gfn_IsNull(ds_oPostReturn.GetColumn(ds_oPostReturn.currow, "PRIMGAE")) ) {
