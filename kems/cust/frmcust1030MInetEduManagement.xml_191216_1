/**
 * 조회
 */
function lfn_Search(obj) {

    var vArgs = + "GTMGNO=" + quote(cbxJibu.Value)
        + " COURSECD=" + quote(cbxCourse.Value)
        + " EDUDATE=" + quote(calEduDate.Value)
        + " PROCSTARTDATE=" + quote(calProcDateStart.Value)
        + " PROCENDDATE=" + quote(calProcDateEnd.Value)
        + " STARTTIME=" + quote(medTimeStart.Value)
        + " ENDTIME=" + quote(medTimeEnd.Value)
        + " NAME=" + quote(edName.Text)
        + " ESEDUORDER=" + quote(edEduOrder.Text)   // 회차 
    ;
    
    switch (radProcGubun.Value) {
        case "10":
            vArgs += " STATUS=" + quote(cbxStatus1.Value);
            break;
            
        case "30":
            vArgs += " STATUS=" + quote(cbxStatus2.Value);
            break;
            
        case "50":
            vArgs += " STATUS=" + quote(cbxStatus3.Value);
            break;
    }

    tit_clearActionInfo();
    tit_addCsvSearchActionInfo("cust:searchCust1030InetJubsu", false);
    tit_callService(
        ""
        ,""
        ,""	// inDs
        ,"ds_oInetJubsu=ds_oInetJubsu"	// outDs
        ,vArgs	// args 
        ,""
        ,false
        ,false
        ,true
    );
    
    
    medTotalCnt.Value = ds_oInetJubsu.GetRowCount();
}

/**
 * 초기화
 */
function lfn_Cancel(obj) {
	radProcGubun.Value = "";
	cbxCourse.Value = "";
	calEduDate.Value = "";
	medTimeStart.Value = "";
	medTimeEnd.Value = "";
    calProcDateStart.Value = addDate(left(getDate(), 8), -14);
    calProcDateEnd.Value = left(getDate(), 8);
    edName.Text = "";
    edEduOrder.Text = "";
    radProcGubun_OnClick();
}

