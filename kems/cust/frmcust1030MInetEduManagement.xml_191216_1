function btnSearchSchedule_OnClick(obj) {

    var sArg = "sJibuCode=" + quote(cbxJibu.Value)
        + " sEduYear=" + quote(left(calEduDate.Value, 4))
        + " sCourseCode=" + quote(cbxCourse.Value)
    ;

    var arrRtn = Dialog("cust::frmcust1091PEduScheduleList.xml", sArg, 600, 400, "", -1, -1);

    if (length(arrRtn) > 0) {
        calEduDate.Value = arrRtn[0];
        medTimeStart.Value = arrRtn[1];
        medTimeEnd.Value = arrRtn[2];
        cbxCourse.Value = arrRtn[9];
    }

}

