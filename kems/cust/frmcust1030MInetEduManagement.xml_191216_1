/**
 * 업무처리구분 선택
 */
function radProcGubun_OnClick(obj){

    cbxStatus1.Value = "10";
    cbxStatus2.Value = "30";
    cbxStatus3.Value = "50";

    switch (radProcGubun.Value) {
    
        case "10":
            cbxStatus1.Enable = true;
            cbxStatus2.Enable = false;
            cbxStatus3.Enable = false;
            break;
            
        case "30":
            cbxStatus1.Enable = false;
            cbxStatus2.Enable = true;
            cbxStatus3.Enable = false;
            break;
            
        case "50":
            cbxStatus1.Enable = false;
            cbxStatus2.Enable = false;
            cbxStatus3.Enable = true;
            break;
            
        default :
            cbxStatus1.Enable = false;
            cbxStatus2.Enable = false;
            cbxStatus3.Enable = false;
    }

}

