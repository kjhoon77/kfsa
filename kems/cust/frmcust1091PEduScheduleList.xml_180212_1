/*
 * 기능 : 조회
 */
function lfn_Search(obj) {
    ds_oEduSchedule.ClearData(); // 데이타셋 초기화
	tit_clearActionInfo();
	
	tit_addCsvSearchActionInfo("cust:searchCust1091PopUpEduScheduleList", false);
	// 서버 호출 
	tit_callService(
		""
		,""
		,""	// inDs
		,"ds_oEduSchedule=ds_oCustData" // outDs
		,"JIBUCODE=" + quote(cbxJibu.Value) //-- 지부
			+" EDUYEAR=" + quote(edEduYear.Text) //-- 년도 
			+" COURSECODE=" + quote(cbxCourse.Value) //-- 직능 // args 
			+" EDUORDER=" + quote(edEduOrder.Text) //-- 교육 회차 
			+" EDUDATESTART=" + quote(Left(calEduDateStart.Value,8))
			+" EDUDATEEND=" + quote(Left(calEduDateEnd.Value,8))
		,"lfn_AfterSearchPopUpEduScheduleList"
		,false
		,false
		,true
	);
}

/*
 * 기능 : 조회 후 처리해야 할 내용
 * parameter : nErrorCode - 에러코드
 *             strErrorMsg - 에러메시지
 * Return : 없음
 */
function lfn_AfterSearchPopUpEduScheduleList(nErrorCode, strErrorMsg) {
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }
	
	// if (ds_oEduSchedule.GetRowCount() == 1) {
        // btnSelect_OnClick();
    // }
    
    gfn_SearchMessage(gdEduSchedule); // 조회 메세지를 처리하고 그리드의 정렬 헤드 마크를 제거한다.
}

/*
 * 기능 : 종료
 */
function lfn_End(obj) {
    Close(); // 종료
}

