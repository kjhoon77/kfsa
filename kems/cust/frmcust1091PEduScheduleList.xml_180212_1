#include "LIB::cust.js"; // 공통 관련

/* 공통 필수 시작 */
function Form_OnLoadCompleted(obj) {
    gfn_Form_OnLoadCompleted(obj); // 공통 폼 로딩 이벤트 처리
	
	// 공통 코드 조회
    gfn_SearchAllJibu(cbxJibu, "Y", "전체"); // 지부 코드 조회
    
    if (gds_oUserInfo.GetColumn(0, "ADMINGUBUN") == "SM" ||   // 시스템관리자
         gds_oUserInfo.GetColumn(0, "ADMINGUBUN") == "PM") {  // 교수 관리자(교수관리자는 교육관리 포함)
        cbxJibu.Enable = true;
    } else {     // 관리자가 아닐 경우
		cbxJibu.Enable = false;
    }
    
    gfn_SearchCustomerEduCourseFullName(ds_oCourse, "0", "전체"); // 대표직능코드 조회
    
    for (var i=(ds_oCourse.GetRowCount()-1); i>-1; i--) {
		if (ds_oCourse.GetColumn(i, "CD") == "60" || ds_oCourse.GetColumn(i, "CD") == "70") { // 개인회원, 단체회원 삭제
			ds_oCourse.DeleteRow(i);
		}
	}
    
    if (IsExistVar("sJibuCode")) {
	    cbxJibu.Value = sJibuCode;
    }
    
    if (IsExistVar("sEduYear")) {
	    if (length(sEduYear) == 0) {
			edEduYear.Text = left(GetDate(), 4);
	    } else {
			edEduYear.text = sEduYear;
	    }
	}
    
    if (IsExistVar("sCourseCode")) {
        cbxCourse.Value = sCourseCode;
    }
    
    // 교육일자
    if (IsExistVar("sEduDate")) {
		lbEduDate.Visible = true;
		lbEduRange.Visible = true;
		calEduDateStart.Visible = true;
		calEduDateEnd.Visible = true;
        calEduDateStart.Value = left(getDate(),8);
        calEduDateEnd.Value = left(getDate(),4)+"1231";
    }
    
    lfn_Search(); // 조회
}

/*
 * 기능 : Form이 종료되거나 화면 전환시
 */
function Form_OnUnloadCompleted(obj) {
    Close(); // 종료
}

