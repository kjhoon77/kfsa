function ds_oEduList_OnRowPosChanged(obj,nOldRow,nRow)
{
    Static7.Text = ds_oEduList.GetColumn(nRow, "EPPERSONNM")
                   + "[" + ds_oEduList.GetColumn(nRow, "BIRTHDAY") + "]";  
    
	ds_oEduPassList.ClearData(); // 데이타셋 초기화

    tit_clearActionInfo();
	tit_addCsvSearchActionInfo("cust:searchCust1001EduPassList", false);
	// 서버 호출 
	tit_callService(
		""
		,""
		,""	// inDs
		,"ds_oEduPassList=ds_oEduPassList"	// outDs
		,"FMNM=" + quote(ds_oEduList.GetColumn(ds_oEduList.currow, "EPPERSONNM"))
		 + " FMPERSONKEY=" + quote(ds_oEduList.GetColumn(ds_oEduList.currow, "EPPERSONKEY"))
		,"lfn_AfterSearchEduPassList"
		,false
	);
}

/*
 * 기능 : 공통코드 조회 후 처리해야 할 내용
 * parameter : nErrorCode - 에러코드
 *             strErrorMsg - 에러메시지
