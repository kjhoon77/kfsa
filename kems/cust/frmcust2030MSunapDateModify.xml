#include "LIB::cust.js"; // 공통업무 관련

/* 공통 필수 시작 */
function Form_OnLoadCompleted(obj) {
    gfn_Form_OnLoadCompleted(obj); // 공통 폼 로딩 이벤트 처리

    // 폼 환경설정
    ls_SystemGubun = gs_SystemGubun;
    ls_SystemMenuId = gs_SystemMenuId;
    var ls_FormId = split(sFormUrl, "::");
    ls_FormId = split(ls_FormId[1], ".");
    obj.Title = sFormTitle + "(" + ls_FormId[0] + ")";

    gfn_MakeRequiredInputMark(this); // 필수 입력란을 설정한다.

    ms_FormAuth = gfn_GetFormAuth(this); // 권한을 조회한다.
    if (ms_FormAuth == "W") { // 권한에 따른 버튼 설정
        lfn_SetFormAuth(true); // 저장 권한
    } else if (ms_FormAuth == "R") {
        lfn_SetFormAuth(false); // 읽기 권한
    } else {
        gfn_Alert(MSGI00080);
        close();
    }

    // 공통 코드 조회

    gfn_SearchCustomerCourseFullName(ds_oCourse, "0", "전체"); // 대표고객직능 조회
    cbxCourse.Value = "";
    gfn_SearchJibu(cbxJibu, "Y", "전체"); // 지부 코드 조회

    calStartDate.Value = addDate(left(getDate(), 8), -14);
    calEndDate.Value = left(getDate(), 8);
    calProcDate.Value = left(getDate(), 8);

}

/**
 * 폼 사이즈
 */
function Form_OnSize(obj,nCx,nCy,nState) {
}


/**
 * 버튼 실행 권한
 */
function lfn_SetFormAuth(bStatus) {

}


/////////////////////////////////////////////////////////////////////////////////
// 기본 버튼
/////////////////////////////////////////////////////////////////////////////////

/**
 * 초기화
 */
function lfn_Cancel(obj) {
    cbxJibu.Value = gds_oUserInfo.GetColumn(0, "PDEPTCD");
    cbxCourse.Value = "";
    calStartDate.Value = addDate(left(getDate(), 8), -14);
    calEndDate.Value = left(getDate(), 8);
    radSunapGubun.Value = "";
    radFeeGubun.Value = "";
	calProcDate.Value = "";

    gdInetSunap.SetCellProp("head", 0, "text", 0);
    medTotalCnt.Value = 0;
    medCheckCnt.Value = 0;
    medNoCheckCnt.Value = 0;
    ds_ioPosPrintM.ClearData();
}

/**
 * 조회
 */
function lfn_Search(obj) {

    tit_clearActionInfo();
    tit_addSearchActionInfo("cust:searchCust2030PosPrintM", false);
    tit_callService(
        ""
        ,""
        ,""
        ,"ds_ioPosPrintM=ds_oPosPrintM"	// outDs
        ,"GTMGNO=" + quote(cbxJibu.Value)
            + " PMCLAS=" + quote(cbxCourse.Value)
            + " STARTDATE=" + quote(calStartDate.Value)
            + " ENDDATE=" + quote(calEndDate.Value)
            + " PMPCGUBUN=" + quote(radSunapGubun.Value)
            + " PMPGMID=" + quote(radFeeGubun.Value)
        ,"lfn_AfterSearch"
        ,false
        ,false
        ,true
    );

}

/**
 * 저장
 */
function lfn_Save(obj) {
	if (medCheckCnt.Value == 0) {
        gfn_Alert("처리일자 변경대상이 없습니다.");
        return;
	}
	
	if (length(left(calProcDate.Value, 8)) < 1) {
        gfn_Alert("처리일자가 잘못되었습니다.");
        return;
	}
	
	if (!gfn_Confirm("처리일자를 변경 하시겠습니까?")) {
        return;
	}
	
    tit_clearActionInfo();
    tit_addMultiActionInfo("cust:updateCust2030PosPrintM", "", 0, "", "N");
    tit_callService(
        ""
        ,""
        ,"ds_iPosPrintM=ds_ioPosPrintM:U"	// inDs
        ,""	// outDs
        ,"SPROCDATE=" + quote(left(calProcDate.Value, 8))
        ,"lfn_AfterSave"
        ,false
        ,false
        ,true
    );
	
	lfn_Search();
	
}


/////////////////////////////////////////////////////////////////////////////////
// 이벤트
/////////////////////////////////////////////////////////////////////////////////

/**
 * 멀티정렬
 */
function btnMutilSort_OnClick(obj) {
	gfn_GridMultiSort(gdInetSunap, 0);
}

/**
 * 그리드 체크
 */
function ds_ioPosPrintM_OnColumnChanged(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex) {
	ds_ioPosPrintM_Backup.ClearData();
	ds_ioPosPrintM_Backup.Copy(ds_ioPosPrintM);
	ds_ioPosPrintM_Backup.Filter("SEL='1'");
	medCheckCnt.Value = ds_ioPosPrintM_Backup.GetRowCount();
	medNoCheckCnt.Value = toNumber(medTotalCnt.Value) - toNumber(medCheckCnt.Value);
}

/////////////////////////////////////////////////////////////////////////////////
// 공통
/////////////////////////////////////////////////////////////////////////////////

/**
 * 조회 후 처리
 */
function lfn_AfterSearch(nErrorCode, strErrorMsg) {

    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return "Transaction Error!";
    }

    gdInetSunap.SetCellProp("head", 0, "text", 0);
    medTotalCnt.Value = ds_ioPosPrintM.GetRowCount();
    medCheckCnt.Value = 0;
    medNoCheckCnt.Value = ds_ioPosPrintM.GetRowCount();

}


/**
 * 저장 후 처리
 */
function lfn_AfterSave(nErrorCode, strErrorMsg) {

    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return "Transaction Error!";
    }
    
    gfn_Alert("처리일자 변경이 완료되었습니다.");
}
