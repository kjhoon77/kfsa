			gdList1.SetCellProp("head",0,"text","(소방안전관리자) 데이터연계 중복/누락 현황"); 
		}
		else if (cbxCourse.Value == "3") {
			gdList1.SetCellProp("head",0,"text","(위험물안전관리자) 데이터연계 중복/누락 현황"); 
		}	
		else if (cbxCourse.Value == "5") {
			gdList1.SetCellProp("head",0,"text","(소방기술자) 데이터연계 중복/누락 현황"); 
		}
		else if (cbxCourse.Value == "8") {
			gdList1.SetCellProp("head",0,"text","(위험물운송자) 데이터연계 중복/누락 현황"); 
		}
	} else {
		gdList1.Format = "list_" + cbxCourse.Value;
		
		// 개인정보 다운로드 사유 등록
		var parValue = "sCode=1457";	// COMMON.PERSONALDOWNLOADH
			parValue += " sRowCnt=" + ds_oDATA.GetRowCount();
			parValue += " sTitle=" + quote(gdList1.UserData);
		var ls_IsRemark = Dialog("COM::frmCOM2100PDownloadRemark.xml", parValue, "", "", "", -1, -1);	
	
		if (ls_IsRemark == "" || ls_IsRemark == null || ls_IsRemark == "FAIL") {
			return false;
		}
	}

	gdList1.ExportExcelEx("sheet1");

}
