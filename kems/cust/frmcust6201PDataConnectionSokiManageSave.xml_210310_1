    /* 관계자정보 */
	ds_ioOwnerInfoSave.ClearData();
	var j = 0;
    for(var i=0; i<ds_ioOwnerInfo.GetRowCount(); i++) {
		if (ds_ioOwnerInfo.GetColumn(i, "SEL") == "1") {   // 
			ds_ioOwnerInfoSave.InsertRow(j);
