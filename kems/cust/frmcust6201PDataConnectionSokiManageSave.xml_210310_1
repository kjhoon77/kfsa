				// 자격증정보가 있으면 자격번호로 SES.VLICENSE, PERSONINFO에서 휴대폰 번호 찾기
				if (ds_ioFPISLicenseInfo.GetRowCount() > 0) {
					for(var i=0; i < ds_ioFPISLicenseInfo.GetRowCount(); i++) {
						if (!gfn_isNull(ds_ioFPISLicenseInfo.GetColumn(i, "CRQFC_NO"))) {	// 자격증번호가 있으면
							vHP = lfn_FindHP(edFMNM.Text, "", ds_ioFPISLicenseInfo.GetColumn(i, "CRQFC_NO"));
							if (!gfn_isNull(vHP)) {
								ds_ioManagerInfo.SetColumn(0, "FMHHPTEL", replace(vHP, "-", ""));
								break;
							}
						}
					}
				} else {		
					ds_ioManagerInfo.SetColumn(0, "FMHHPTEL", "");
				}
			}
		} else { // personkey 가 없으면
			// 자격증정보가 있으면 자격번호로 SES.VLICENSE, PERSONINFO에서 휴대폰 번호 찾기
			if (ds_ioFPISLicenseInfo.GetRowCount() > 0) {
				for(var i=0; i < ds_ioFPISLicenseInfo.GetRowCount(); i++) {
					if (!gfn_isNull(ds_ioFPISLicenseInfo.GetColumn(i, "CRQFC_NO"))) {	// 자격증번호가 있으면
						vHP = lfn_FindHP(edFMNM.Text, "", ds_ioFPISLicenseInfo.GetColumn(i, "CRQFC_NO"));
						if (!gfn_isNull(vHP)) {
							ds_ioManagerInfo.SetColumn(0, "FMHHPTEL", replace(vHP, "-", ""));
							break;
						}
					}
				}
			} else {		
				ds_ioManagerInfo.SetColumn(0, "FMHHPTEL", "");
			}
		}
	}
    
	ls_PERSONKEY = gfn_SearchPERSONKEY(edFMNM.Text, left(medFMResidentno.Value,6), replace(edPTel.text,"-",""));
	ds_ioManagerInfo.SetColumn(0, "FMPERSONKEY", ls_PERSONKEY);
	ls_DI = gds_ioSciCheck.GetColumn(0, "STRDUP");
	ls_CI = gds_ioSciCheck.GetColumn(0, "STRCI");
	ls_CIVER = gds_ioSciCheck.GetColumn(0, "STRVER");

    // 자격증 정보
    for(var i=0; i<ds_ioFPISLicenseInfo.GetRowCount(); i++) {
		ds_ioOtherLicenseInfo.InsertRow(i);
		if(!gfn_IsNull(ds_ioFPISLicenseInfo.GetColumn(i, "CRQFC_CODE"))) ds_ioOtherLicenseInfo.SetColumn(i, "OLHCD", ds_ioFPISLicenseInfo.GetColumn(i, "CRQFC_CODE"));
		if(!gfn_IsNull(ds_ioFPISLicenseInfo.GetColumn(i, "CRQFC_NO"))) ds_ioOtherLicenseInfo.SetColumn(i, "OLNO", ds_ioFPISLicenseInfo.GetColumn(i, "CRQFC_NO"));		
		if(!gfn_IsNull(ds_ioFPISLicenseInfo.GetColumn(i, "DELVRY_DE"))) ds_ioOtherLicenseInfo.SetColumn(i, "OLDATE", ds_ioFPISLicenseInfo.GetColumn(i, "DELVRY_DE"));
		ds_ioOtherLicenseInfo.SetColumn(i, "OLINGUBUN", "1");  // 입력구분
	}

    // 청:대표자 => 안전원:관계자 로 등록
    if(!gfn_IsNull(edFPISCEO.Text)) {
		var j = ds_ioOwnerInfo.GetRowCount(); // 안전원관계자 정보 개수    
		ds_ioOwnerInfo.InsertRow(j);    
		ds_ioOwnerInfo.SetColumn(j, "OGUBUN", "3"); // 인력구분 3:대표자
		ds_ioOwnerInfo.SetColumn(j, "ONM", edFPISCEO.Text);   //대표자
		ds_ioOwnerInfo.SetColumn(j, "OTEL", edFPISTel.Text);   //전화번호
		ds_ioOwnerInfo.SetColumn(j, "OSEQ", j+1);
		ds_ioOwnerInfo.SetColumn(j, "OINGUBUN", "1");    // 입력구분
		ds_ioOwnerInfo.SetColumn(j, "SEL", "1");
    }
   
    lfn_SetEnableStatus(true); // 입력가능 활성화
    lfn_SetCheckEnableStatus(true); // 변경내용 적용 후 체크박스 활성화 상태(활성화)
    lfn_SetCheckStatus(true);  // 변경내용 적용 후 체크박스 상태(체크)
    
    btnApply.Enable = False;
}




