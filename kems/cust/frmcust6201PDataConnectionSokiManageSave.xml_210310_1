				}
			} else { // personkey 가 없으면
				// 자격증정보가 있으면 자격번호로 SES.VLICENSE, PERSONINFO에서 휴대폰 번호 찾기
				if (ds_ioFPISLicenseInfo.GetRowCount() > 0) {
					for(var i=0; i < ds_ioFPISLicenseInfo.GetRowCount(); i++) {
						if (!gfn_isNull(ds_ioFPISLicenseInfo.GetColumn(i, "CRQFC_NO"))) {	// 자격증번호가 있으면
							vHP = lfn_FindHP(edFMNM.Text, "", ds_ioFPISLicenseInfo.GetColumn(i, "CRQFC_NO"));
							if (!gfn_isNull(vHP)) {
								ds_ioManagerInfo.SetColumn(0, "FMHHPTEL", replace(vHP, "-", ""));
								break;
							}
						}
					}
				} else {		
					ds_ioManagerInfo.SetColumn(0, "FMHHPTEL", "");
				}
			}
		}
	// 휴대폰번호가 없으면
	} else {
		// PERSONKEY로 휴대폰번호 찾기
		if (!gfn_isNull(medPersonKey.Value)) {
			vHP = lfn_FindHP("", sPersonkey, "");
			if (!gfn_isNull(vHP)) {	 //휴대폰번호 찾았으면
				ds_ioManagerInfo.SetColumn(0, "FMHHPTEL", replace(vHP, "-", ""));
