	else if (sStatus == "I") iArgsNm += " MPROCGUBUN=1";
	tit_addSingleActionInfo("cust:insertCust6201MatchInfo", "", 0, "");            // 매치처리
		
	// 서버 호출 
	tit_callService(
		""
		,""
		,iDsNm	// inDs
		,""	        // outDs
