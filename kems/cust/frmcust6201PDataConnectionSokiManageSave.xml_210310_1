	}
	
    /* 신규 및 수정이면 은행가상계좌 할당 2013.07.22 */
	if (lsCustomerStatus == "insert" || lsCustomerStatus == "update") {
		tit_addSingleActionInfo("cust:executeCust0010VirtualAccoutAllocation", "", 0, "");
