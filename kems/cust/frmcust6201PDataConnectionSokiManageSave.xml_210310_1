// 휴대폰번호 찾기
function lfn_FindHP(sNM, sPersonkey, sllcsno) {
var returnHP = "";
	// personkey로 조회
	if (!gfn_isNull(sPersonkey)) {
		ds_ioPersoninfoHP.ClearData();
		tit_clearActionInfo();
		tit_addSearchActionInfo("com:searchPersoninfoHP", false);
		// 서버 호출 
		tit_callService(
			""
			,""
			,""	// inDs
			,"ds_ioPersoninfoHP=ds_ioPersoninfoHP"	// outDs
			,"PERSONKEY=" + quote(sPersonkey) // args
			,""
			,false
			,false
			,true
		);
		
		// 휴대폰번호 조회됐으면 return
		if ((ds_ioPersoninfoHP.GetRowCount() > 0) &&
			(!gfn_isNull(ds_ioPersoninfoHP.GetColumn(0, "PHP")))) {
				returnHP = ds_ioPersoninfoHP.GetColumn(0, "PHP");
		
		// 휴대폰번호가 없으면 자격정보로 조회
		} else {
			if (!gfn_isNull(sllcsno)) {
				// 자격번호가 있으면 자격번호로 SES.VLICENSE, PERSONINFO에서 휴대폰 번호 찾기
				ds_ioPersoninfoHP.ClearData();
				tit_clearActionInfo();
				tit_addSearchActionInfo("com:searchPersonInfoHP", false);
				// 서버 호출 
				tit_callService(
					""
					,""
					,""	// inDs
					,"ds_ioPersoninfoHP=ds_ioPersoninfoHP"	// outDs
					,"NM=" + quote(sNM)
					  + " LLCSNO=" + quote(sllcsno) // args
					,""
					,false
					,false
					,true
				);
				// 휴대폰번호 조회됐으면 return
				if ((ds_ioPersoninfoHP.GetRowCount() > 0) &&
					(!gfn_isNull(ds_ioPersoninfoHP.GetColumn(0, "PHP")))) {
						returnHP = ds_ioPersoninfoHP.GetColumn(0, "PHP");
				} // if
			}	// if
		} // else
	
	//personkey가 없으면 자격번호로 조회
	} else {
		if (!gfn_isNull(sllcsno)) {
			// 자격번호가 있으면 자격번호로 SES.VLICENSE, PERSONINFO에서 휴대폰 번호 찾기
			ds_ioPersoninfoHP.ClearData();
			tit_clearActionInfo();
			tit_addSearchActionInfo("com:searchPersonInfoHP", false);
			// 서버 호출 
			tit_callService(
				""
				,""
				,""	// inDs
				,"ds_ioPersoninfoHP=ds_ioPersoninfoHP"	// outDs
				,"NM=" + quote(sNM)
				  + " LLCSNO=" + quote(sllcsno) // args
				,""
				,false
				,false
				,true
			);
			// 휴대폰번호 조회됐으면 return
			if ((ds_ioPersoninfoHP.GetRowCount() > 0) &&
				(!gfn_isNull(ds_ioPersoninfoHP.GetColumn(0, "PHP")))) {
					returnHP = ds_ioPersoninfoHP.GetColumn(0, "PHP");
			}
		}
	}
	return returnHP;
}



