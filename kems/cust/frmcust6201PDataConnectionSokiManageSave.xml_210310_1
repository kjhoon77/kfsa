// 청구지 주소 선택
function lfn_CbxPostGubun(strCode) {
	switch(strCode)
	{
		case 0 :	//대상물
		
			if(cbxAddrGubun.Value == 0) {	//구주소

				cbxHopeAddrGubun.Value = 0;	// 청구지 구/새주소 구분				
				cbxHopeAddrGubun_OnChanged(this, cbxHopeAddrGubun.Value);
				
				edHopeAddr1.Text = edAddr1.Text; //주소1
				edHopeAddr2.Text = edAddr2.Text;	//주소2
				medHopeZipCode.Value = medZipCode.Value;	//우편번호
				
				edHopeAddr1.Enable = false;
				edHopeAddr2.Enable = false;
				btnSearchZipCode2.Enable = false;
            
				if (btnApply.Enable == false) {
					if(gfn_IsNull(edAddr1.Text) || gfn_IsNull(edAddr2.Text) || gfn_IsNull(medZipCode.Value)) {	//원본 주소가 Null일때
						gfn_Alert("대상물 지번주소에 빈값이 존재합니다.\n먼저 대상물의 지번주소를 입력하세요.");
						edAddr1.SetFocus();	//대상물의 주소로 이동.
						return;
					}
				}
				
			} else if(cbxAddrGubun.Value == 1) {	//새주소
			
				cbxHopeAddrGubun.Value = 1;	// 청구지 구/새주소 구분				
				cbxHopeAddrGubun_OnChanged(this, cbxHopeAddrGubun.Value);
				
				edHopeRoadAddr1.Text = edRoadAddr1.Text; //주소1
				edHopeRoadAddr2.Text = edRoadAddr2.Text;	//주소2
				medHopeRoadZipCode.Value = medRoadZipCode.Value;	//우편번호
				
				edHopeRoadAddr1.Enable = false;
				edHopeRoadAddr2.Enable = false;
				btnSearchZipCode2.Enable = false;
				
				// 새주소 CUSTOMERROAD 테이블 적용
				
				// 회비
				ds_ioCustomerInfo.SetColumn(0, "CRFEEPNUCD", ds_ioBuildingInfo.GetColumn(0, "BRPNUCD"));
				
				ds_ioCustomerInfo.SetColumn(0, "CRFEEROADBUILDMGNO", ds_ioBuildingInfo.GetColumn(0, "BRROADBUILDMGNO"));
				
				ds_ioCustomerInfo.SetColumn(0, "CRFEEROADDETAIL", ds_ioBuildingInfo.GetColumn(0, "BRROADDETAIL"));
				
				ds_ioCustomerInfo.SetColumn(0, "CRFEEROADREMAIN", ds_ioBuildingInfo.GetColumn(0, "BRROADREMAIN"));
				
				ds_ioCustomerInfo.SetColumn(0, "CRFEESANGUBUN", ds_ioBuildingInfo.GetColumn(0, "BRSANGUBUN"));
				
				ds_ioCustomerInfo.SetColumn(0, "CRFEEMAINBUNJI", ds_ioBuildingInfo.GetColumn(0, "BRMAINBUNJI"));
				
				ds_ioCustomerInfo.SetColumn(0, "CRFEESUBBUNJI", ds_ioBuildingInfo.GetColumn(0, "BRSUBBUNJI"));
				
				ds_ioCustomerInfo.SetColumn(0, "CRFEEBUILDNM", ds_ioBuildingInfo.GetColumn(0, "BRBUILDNM"));
				
				ds_ioCustomerInfo.SetColumn(0, "CRFEEADDRETC", ds_ioBuildingInfo.GetColumn(0, "BRADDRETC"));
				
				ds_ioCustomerInfo.SetColumn(0, "CRCONVFEESTATUS", ds_ioBuildingInfo.GetColumn(0, "BRCONVSTATUS"));
				
				// 교육
				ds_ioCustomerInfo.SetColumn(0, "CREDUPNUCD", ds_ioBuildingInfo.GetColumn(0, "BRPNUCD"));
				
				ds_ioCustomerInfo.SetColumn(0, "CREDUROADBUILDMGNO", ds_ioBuildingInfo.GetColumn(0, "BRROADBUILDMGNO"));
				
				ds_ioCustomerInfo.SetColumn(0, "CREDUROADDETAIL", ds_ioBuildingInfo.GetColumn(0, "BRROADDETAIL"));
				
				ds_ioCustomerInfo.SetColumn(0, "CREDUROADREMAIN", ds_ioBuildingInfo.GetColumn(0, "BRROADREMAIN"));
				
				ds_ioCustomerInfo.SetColumn(0, "CREDUSANGUBUN", ds_ioBuildingInfo.GetColumn(0, "BRSANGUBUN"));
				
				ds_ioCustomerInfo.SetColumn(0, "CREDUMAINBUNJI", ds_ioBuildingInfo.GetColumn(0, "BRMAINBUNJI"));
				
				ds_ioCustomerInfo.SetColumn(0, "CREDUSUBBUNJI", ds_ioBuildingInfo.GetColumn(0, "BRSUBBUNJI"));
				
				ds_ioCustomerInfo.SetColumn(0, "CREDUBUILDNM", ds_ioBuildingInfo.GetColumn(0, "BRBUILDNM"));
				
				ds_ioCustomerInfo.SetColumn(0, "CREDUADDRETC", ds_ioBuildingInfo.GetColumn(0, "BRADDRETC"));
				
				ds_ioCustomerInfo.SetColumn(0, "CRCONVPGM", ds_ioBuildingInfo.GetColumn(0, "BRCONVPGM"));
				
				ds_ioCustomerInfo.SetColumn(0, "CRCONVEDUSTATUS", ds_ioBuildingInfo.GetColumn(0, "BRCONVSTATUS"));
            
				if (btnApply.Enable == false) {
					if(gfn_IsNull(edRoadAddr1.Text) || gfn_IsNull(medRoadZipCode.Value)) {	//원본 주소가 Null일때
						gfn_Alert("대상물 도로명주소에 빈값이 존재합니다.\n먼저 대상물의 도로명주소를 입력하세요.");
						edRoadAddr1.SetFocus();	//대상물의 주소로 이동.
						return;
					}
				}			
			
			}
		break;
		case 1 :	//선임자
			edHopeAddr1.Text = edOldPostAddr1.Text;	//주소1
            edHopeAddr2.Text = edOldPostAddr2.Text;	//주소2
            medHopeZipCode.Value = medOldPostZipCode.Value;	//우편번호
            
            edHopeAddr1.Enable = false;
            edHopeAddr2.Enable = false;
            btnSearchZipCode2.Enable = false;
            
            if(gfn_IsNull(edOldPostAddr1.Text) 
            || gfn_IsNull(edOldPostAddr2.Text) 
            || gfn_IsNull(medOldPostZipCode.Value)) {	//원본 주소가 Null일때
                gfn_Alert("선임자의 구주소에 빈값이 존재합니다.\n먼저 선임자의 구주소를 입력하세요.");
                return;
            }
		break;
		case 2 :	//직접
			if(cbxAddrGubun.Value == 0) {	//구주소
				cbxHopeAddrGubun.Value = 0;	// 청구지 구/새주소 구분				
				cbxHopeAddrGubun_OnChanged(this, cbxHopeAddrGubun.Value);

				edHopeAddr1.Text = "";	//주소1
				edHopeAddr2.Text = "";	//주소2
				medHopeZipCode.Value = "";	//우편번호
				
				edHopeAddr1.Enable = false;
				edHopeAddr2.Enable = false;
				btnSearchZipCode2.Enable = true;
				
				edHopeAddr1.SetFocus();
			} else if(cbxAddrGubun.Value == 1) {	//새주소
				cbxHopeAddrGubun.Value = 1;	// 청구지 구/새주소 구분				
				cbxHopeAddrGubun_OnChanged(this, cbxHopeAddrGubun.Value);
				
				edHopeRoadAddr1.Text = ""; //주소1
				edHopeRoadAddr2.Text = "";	//주소2
				medHopeRoadZipCode.Value = "";	//우편번호
				
				edHopeRoadAddr1.Enable = false;
				edHopeRoadAddr2.Enable = false;
				btnSearchZipCode2.Enable = true;
				
				edHopeRoadAddr1.SetFocus();
			}
		break;
	}
}

