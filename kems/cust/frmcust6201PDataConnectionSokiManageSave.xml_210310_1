            ds_ioManagerInfo.GetColumn(ds_ioManagerInfo.currow, "FMBIRTHDAY") == "주민번호없음") {  // 주민번호가 null 이거나 '주민번호없음' 이면 0으로 채움
            ds_ioManagerInfo.SetColumn(ds_ioManagerInfo.currow, "FMBIRTHDAY", "");
        }
		iArgsNm += " SFMBIRTHDAY=" + quote(ds_ioManagerInfo.GetColumn(ds_ioManagerInfo.currow, "FMBIRTHDAY"));		// 생년월일
        iArgsNm += " SFMPERSONKEY=" + quote(ds_ioManagerInfo.GetColumn(ds_ioManagerInfo.currow, "FMPERSONKEY"));	// 신규선임자 개인식별번호
        iArgsNm += " SFMDI=" + quote(ls_DI);																		// DI
        iArgsNm += " SFMCI=" + quote(ls_CI);																		// CI
        iArgsNm += " SFMCIVER=" + quote(ls_CIVER);																	// CIVER
        iArgsNm += " SFMAUTHYN=" + quote(iif(ls_AUTH=="Y","0","1"));								// 실명인증여부
		iArgsNm += " RETIREYN='N'";	// 교육이수후 퇴직 및 교육이관 여부
		
		iDsNm += " ds_ioManagerInfo=ds_ioManagerInfo:U";
		tit_addSingleActionInfo("cust:executeCust6001FireManagerChange", "", 0, "");  //선임자

    } else {
        iArgsNm += iArgsNmManagerUpdate;	// SFMMGNO 관리자관리번호만 가져옴
    }
    
  
    // 대상물 주소 변경시 포함된 고객 주소 일괄변경(주소 구분이 대상물인 경우만)
