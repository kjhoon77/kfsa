        sArg = "MMGNO=" + quote(sCMGNO);   // 고객관리번호를 arg로 넘김
    } else {
        sArg = "MNM=" + quote(edFMNM.Text)
			+ " BIRTHDAY=" + quote(left(medFMResidentno.Value,6))
			+ " HP=" + quote(replace(edPTel.text,"-",""))
            + " PERSONKEY=" + quote(medPersonKey.Value);
    }
    
    ds_ioManager.ClearData();
    tit_clearActionInfo(); //사람이 이미 있는지 검색한다.
    tit_addSearchActionInfo("cust:searchCust0018ManagerInfo", false);
