#include "LIB::cust.js"; // 공통업무 관련

/* 공통 필수 시작 */
function Form_OnLoadCompleted(obj) {

	gfn_Form_OnLoadCompleted(obj); // 공통 폼 로딩 이벤트 처리
    
    // 공통 코드 조회
    gfn_SearchCommonCode(ds_oBizGubun, "BIZCD", "Y", "전체");
    if (IsExistVar("sCmgno")) {  // 고객관리번호
        if (gfn_IsNull(sCmgno)) {
            gfn_Alert("고객정보가 없습니다.");
            lfn_Cancel();        
        } else {
            lfn_Search();
        } 
    } else {
        gfn_Alert("고객정보가 없습니다.");
        lfn_Cancel();
    }
    
    
    cbxMonthGubun.SetFocus();
    cbxMonthGubun.Value = "YEAR2";

}

/**
 * 조회
 */
function lfn_Search(obj) {
	// 데이타셋 초기화
	ds_oEduPassList.ClearData(); 
	ds_oSunap.ClearData();

    tit_clearActionInfo();
	tit_addSearchActionInfo("cust:searchCust0019Sunap", false);
	tit_addSearchActionInfo("cust:searchCust1001EduList", false);
	
    tit_callService(
        ""
        , ""
        , "" // inDs
        , "ds_oSunap=ds_oSunap ds_oEduList=ds_oEduList"
        , "CMGNO=" + quote(sCmgno)
        + " TYPE=" + quote(cbxMonthGubun.Value)
		,"lfn_AfterSearch"
		,false
    );
    
}

function lfn_AfterSearch(nErrorCode, strErrorMsg) {
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }    
}


/**
 * 년, 반기 구분 이벤트
 */
function cbxMonthGubun_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex) {
	lfn_Search();
}


/**
 * 멀티 정렬
 */
// 회비
function btnMutilSort_OnClick(obj) {
	gfn_GridMultiSort(gdSunap, 1);
}
// 교육
function btnMutilSort_OnClick2(obj)
{
	gfn_GridMultiSort(gdEduList, 0); // 멀티 정렬을 실행한다.
}

]]></Script>
