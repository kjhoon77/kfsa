    }

    var rpt = "";
    var params = "";
    var params2 = "";
    var sTopMargin = "";
    var sLeftMargin = "";

    
	rpt = "frmcust6570TotalDataMatch_" + cbxCourse.Value + ".rex";
	
	if (cbxCourse.Value == "8") { 
		sTopMargin = "100";
		sLeftMargin = "200";
	}
    
    params = "rd_TITLE=" + quote(params) + "^s_TITLE=" + quote(params2);  
    var StartDate = substr(calStartDate.Value,0,4) + "." + substr(calStartDate.Value,4,2) + "." + substr(calStartDate.Value,6,2);
    var EndDate = substr(calEndDate.Value,0,4) + "." + substr(calEndDate.Value,4,2) + "." + substr(calEndDate.Value,6,2);
    params += "^date=일자 : "+ StartDate + " ~ " + EndDate;
    
	/*
	 * 기능 : 렉스퍼트 보고서를 팝업화면으로 호출한다.
	 * parameter : sReportName - 레포트명(예:레포트.rex)
	 *             sReportDataset - 데이터셋명(예:dsReport1||dsResport2||...)
	 *             sReportParams - 렉스퍼트용 파라메터(예: a에 1, b에 2, c에 3의 값을 넣는 경우, a=1^b=2^c=3...)
	 *             bLcsYn - 수첩여부
	 *             LeftMargin = 왼쪽
	 *             RightMargin = 오른쪽
	 *             TopMargin = 상단
	 *             BottomMargin = 하단
	 * return value : 정상출력 true, 출력 안했으면 false
	 */    
	 
    //gfn_CallReportPopup(rpt, "ds_oDATA", params);
    gfn_CallReportPopup(rpt, "ds_oDATA", params, '', '', sTopMargin, sLeftMargin);
}




function lfn_Search(obj) {
    SetWaitCursor(false);  
	
	/* 체크 */    
    // 처리일자 체크 
    if (Length(calStartDate.value) != 8) {
		alert("처리일자를 입력해 주세요");
		calStartDate.SetFocus();
		return;
    }
    if (Length(calEndDate.value) != 8) {
		alert("처리일자를 입력해 주세요");
		calEndDate.SetFocus();
		return;
    }

    ds_oDATA.ClearData(); // 데이타셋 초기화
    
    tit_clearActionInfo();    
   
    tit_addCsvSearchActionInfo("cust:searchCust6570TotalDataMatch", false);
    
	// 서버 호출 
	tit_callService(
         "",""
		,""	// inDs
		,"ds_oDATA=ds_oDATA"	// outDs
		,     " COURSE=" + quote(cbxCourse.Value)
	    	+ " STATUS=" + quote(radStatus.value)
            + " SDATE="  + quote(calStartDate.value)
            + " EDATE="  + quote(calEndDate.value)
            + " MATCHGUBUN=" + quote(radMatchGubun.Value)    // 일치구분 
		, "lfn_AfterSearch"
		, false
		, false  // 처리중 창 보여주기 (null or false : 보여줌) 
		, true); 
		   
}

function lfn_AfterSearch(nErrorCode, strErrorMsg) {
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);     
        course = "";
        return;
    }
    
}







/***  엑셀   ***/
function lfn_Excel(obj) {	
    lfn_ExcelL(obj);
}

function lfn_ExcelL(obj)
{	
    lfn_Search();

	if(ds_oDATA.rowcount == 0){
		alert("데이터가 없습니다.");
		return;
	}
	
    gdList1.Format = "";

   
	gdList1.Format = "total_" + cbxCourse.Value;
   
    
	gdList1.ExportExcelEx("sheet1");

}
/***  엑셀 출력 끝  ***/

/*
	처리 구분변경시 
*/
function radMatchGubun_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{
	if(strCode  == "2") {
		radStatus.Enable = false;
	} else {
		radStatus.Enable = true;
	}
}
