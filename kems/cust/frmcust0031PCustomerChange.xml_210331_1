// 변경내용 적용
function btnChangeInfo_OnClick(obj)
{

    if((sFcgubun == "1" || sFcgubun == "3") && gfn_IsNull(sStatus)) {
        gfn_Alert("대상물을 선택 또는 신규추가 후 적용하십시오.");
        return;
    }
    
  
 	if((sFcgubun == "3") &&
       (ds_oJubsuInfo.GetColumn(0, "FCCOURSECD") <> cbxCourse.Value)) {
        gfn_Alert("신규신청정보의 직능과 대상물검색(대상물 선택 및 신규등록)으로 검색된 정보의 직능이 다릅니다.\n\n"
                 +"확인하신 후 직능을 맞게 수정하세요!");
        //return;
    }
   
    // 접수내용이 빈값이 아닐경우 기존정보에 적용
    // 대상물 정보

	if(!gfn_IsNull(edBuildingNmAfter.Text)) ds_ioBuildingInfo.SetColumn(0, "BNM", edBuildingNmAfter.Text);
