// 대상물 검색
function btnBuildingSearch_OnClick(obj)
{
	//팝업
	var sArg  = "sBuildingNm=" + quote(trim(ds_oJubsuInfo.GetColumn(0, "FCBULDNAME")));
		sArg += " sAddr1=" + quote(ds_oJubsuInfo.GetColumn(0, "FCADDR1"));
		sArg += " sAddr2=" + quote(ds_oJubsuInfo.GetColumn(0, "FCADDR2"));
		sArg += " sGtmgno=" + quote(ds_oJubsuInfo.GetColumn(0, "FCGTMGNO"));
	/*
     * arrNo[0] : OLD:검색된 경우, NEW:신규추가
     * arrNo[1] : 지부
     * arrNo[2] : 고객번호
     */
    var arrNo = "";
    if (left(ds_oJubsuInfo.GetColumn(0, "FCCOURSECD"),1) == "2") {	// 소방안전관리보조자
        arrNo = Dialog("cust::frmcust0034PSearchMainbuilding.xml", sArg, 600, 400, "", -1, -1);
    } else {
		arrNo = Dialog("cust::frmcust0032PSearchbuilding.xml", sArg, 600, 400, "", -1, -1);
	}


    if (length(arrNo) > 0) {
		if(arrNo[0] == "OLD") {	// 대상물이 검색된 경우
            sStatus = "U";      // 대상물 검색상태
            var sArg = "CGTMGNO=" + quote(arrNo[1]);	// 고객번호
            sArg += " CNO="	+ quote(arrNo[2]); 	// 지부코드
			
			if (!gfn_isNull(arrNo[1]) && !gfn_isNull(arrNo[2])) {
				lfn_SearchAllInfo(sArg);
			}
			if (left(ds_oJubsuInfo.GetColumn(0, "FCCOURSECD"),1) == "2") {	// 소방안전관리보조자
				sMainCmgno = arrNo[3];	// 메인 소방안전관리자 고객바코드번호 (소방안전관리보조자만)
			}
		} else if(arrNo[0] == "NEW") {	// 신규추가일경우
            sStatus = "I";      // 대상물 신규상태
            cbxJibu.Value = ds_oJubsuInfo.GetColumn(0, "FCGTMGNO");
            cbxJibu_OnChanged(object("cbxJibu"), cbxJibu.Value);
            cbxCourse.Value = ds_oJubsuInfo.GetColumn(0, "FCGROUPCD");
            cbxCourse_OnChanged(object("cbxCourse"), cbxCourse.Value);
            cbxDetailCourse.Value = ds_oJubsuInfo.GetColumn(0, "FCCOURSECD");
            if (!gfn_isNull(arrNo[3])) {
				cbxRegion.Value = arrNo[3];
			}
            
            lfn_SetEnableStatus(false);
            
            lfn_ClearData();					//초기화 (모든 데이터셋)	
            lfn_AddDataMain();					//Main 데이타셋 입력가능상태로 만들기
			lfn_AdditionStatus();
			if (left(ds_oJubsuInfo.GetColumn(0, "FCCOURSECD"),1) == "2") {	// 소방안전관리보조자
				sMainCmgno = arrNo[1];	// 메인 소방안전관리자 고객바코드번호 (소방안전관리보조자만)
				sArg = "BMGNO=" + arrNo[2];	// 메인 소방안전관리자 대상물관리번호 (소방안전관리보조자만)
				lfn_SearchBuildingInfo(sArg);

			}
		}
		
		lfn_SetEnableStatus(false);
    }
}



