				args += " MSENDDATE=''";			// 발송시간 예약일 경우만
				
				// 서버 호출 
				tit_callService(
					""
					,""
					,""	// inDs
					,""	// outDs
					,args // args 
					,""
					,false
				);
			}
		}		
    
