/*
 * 기능 : 변경내용 저장
 */
function lfn_Save(obj)
{
	if(gfn_IsNull(cbxDetailCourse.Value)) {		gfn_Alert(lbDetailCourse.Text + MSGW00010);	// 세부직능을 입력해 주십시오.		cbxDetailCourse.SetFocus();		return;	}


	if (left(ds_oJubsuInfo.GetColumn(0, "FCCOURSECD"),1) == "2") {	// 소방안전관리보조자
			if(gfn_IsNull(cbxElectQualify.Value)) {				gfn_Alert(lbElectQualify.Text + MSGW00010);	// 선임자격을 입력해 주십시오.				cbxElectQualify.SetFocus();				return;			}
	}

	if(sFcgubun == "1" || sFcgubun == "3") {      // 선임대상물 변경 또는 선임 신규
        if(sStatus == "U") {	// 대상물이 검색된 경우
            var sArg  = "sMmgno=" + quote(ds_ioManagerInfoTmp.GetColumn(ds_ioManagerInfoTmp.currow, "FMMGNO"));
                sArg += " sCmgno=" + quote(ds_ioCustomerInfo.GetColumn(ds_ioCustomerInfo.currow, "CMGNO"));
                sArg += " sFmnm=" + quote(edManagerNm.Text);
                sArg += " sFmresidentNo=" + quote(medResidentNo.Value);
                sArg += " sFmPersonkey=" + quote(medPersonKey.Value);
                sArg += " sFmAuthGubun=" + quote(ds_ioManagerInfo.GetColumn(0, "FMAUTHGUBUN")); // 0:인증 1:미인증
                sArg += " sFmHP=" + quote(edManagerPTel.Text);
                sArg += " sCourseCD=" + quote(cbxCourse.Value);
                sArg += " sFMHSTARTDATE=" + quote(left(calAppointDate.Value, 8));
            /*
             * arrManager[0] : 선임자관리번호, arrManager[1] : 최종선임이력순번
             */
            var arrManager = Dialog("cust::frmcust0033PManagerChange.xml", sArg, 520, 188, "", -1, -1);
            return false;
            if (arrManager == "OK") {
                Parent.lfn_Search();
                Close();
            }
		} else if(sStatus == "I") {	// 신규추가일경우
            if(!gfn_Confirm("신규 등록 하시겠습니까?")){
                return;
            }
            if(lfn_updateInfo()){
				lfn_SMSSend(newCMGNO);	// 문자발송
            
                gfn_Alert("저장을 완료하였습니다.");
                Parent.lfn_Search();
                Close();
            }; 
		}
    } else {                    // 기본정보 변경
        if (!gfn_Confirm("변경된 정보를 저장하시겠습니까?")) {
            return false;
        }
        
        if(lfn_updateInfo()){
			lfn_SMSSend(newCMGNO);		// 문자발송
			
            gfn_Alert("수정을 완료하였습니다.");
            Parent.lfn_Search();
            Close();
        };        
    }    
}

