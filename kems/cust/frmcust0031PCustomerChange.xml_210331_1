#include "LIB::cust.js"; // 공통 관련

var sStatus = "";       // 대상물 상태(U:조회, I:신규)
var sMainCmgno = "";    // 메인 소방안전관리자 고객바코드번호 (소방안전관리보조자만)
var newCMGNO = "";		// 저장시 신규로 발급되거나 기존 CMGNO
/* 공통 필수 시작 */
function Form_OnLoadCompleted(obj) {
    gfn_Form_OnLoadCompleted(obj); // 공통 폼 로딩 이벤트 처리
    gfn_MakeRequiredInputMark(this); // 필수 입력란을 설정한다.
    
    
    ms_FormAuth = gfn_GetFormAuth(this, sFormUrl); // 권한을 조회한다.    if (ms_FormAuth == "W") { // 권한에 따른 버튼 설정        lfn_SetFormAuth(true); // 저장 권한    } else if (ms_FormAuth == "R") {        lfn_SetFormAuth(false); // 읽기 권한    } else {        gfn_Alert(MSGI00080);        close();    }
    
    
    lfn_SearchJubsuInfo();      // 접수정보
    lfn_AdditionStatus();       // 신규추가상태
    
    lfn_SetEnableStatus(false); // 변경내용 적용전 상태(비활성화)
    
    // 공통 코드 조회
    gfn_SearchAllJibu(cbxJibu, "Y"); 											    // 지부 코드 조회
    gfn_SearchCodeRegion(gds_oUserInfo.GetColumn(0, "PDEPTCD"), ds_oRegion, "0");	// 지역 코드 조회
    gfn_SearchCustomerCourseFullName(ds_oCourse, "0");							    // 대표고객직능 조회
    gfn_SearchCustomerDetailCourseFullName("", ds_oDetailCourse, "0");			    // 세부고객직능 조회
    gfn_SearchCommonCode(ds_oEmailDomain, "EMAILDOMAIN", "Y", "직접입력"); 	        // 이메일도메인 조회
    gfn_SearchCommonCode(ds_oBizCd, "BIZCD", "Y", "지정요망"); 				        // 방관-용도코드
    gfn_SearchCodeRegion(cbxJibu.Value, ds_oRegion, "0");						    // 지역 코드 조회
    
    //상단 조회 초기값 셋팅
	cbxRegion.Value = ds_oRegion.GetColumn(0, "CD");
	cbxCourse.Value = ds_oCourse.GetColumn(0, "CD");
	cbxCourse_OnChanged(object("cbxCourse"), cbxCourse.Value);
	cbxDetailCourse.Value = ds_oDetailCourse.GetColumn(0, "CD");
	
	cbxEMailDomain.Value = "";

    if (sFcgubun == "1") {
        lbTitle.Text = "선임대상물 변경";
        lbComment.Text = "(다른대상물에 선임된 경우)";
        lbComment.Visible = true;
        lbChangeInfo.Text = "변경신청정보";
        btnChangeInfo.Text = "변경내용적용 ▷▶▶";
		btnElectEnd.Visible = false;
    } else if (sFcgubun == "3") {
        lbTitle.Text = "선임 신규";
        lbComment.Text = "(선임정보가 누락된 경우)";
        lbComment.Visible = true;
        lbChangeInfo.Text = "신규신청정보";        
        btnChangeInfo.Text = "신규내용적용 ▷▶▶";
		btnElectEnd.Visible = false;
    } else if (sFcgubun == "2") {
        lbTitle.Text = "기본정보 변경";
        lbComment.Text = "(현재대상물정보가 변경된 경우)";
        lbComment.Visible = true;
        btnBuildingSearch.Visible = false;
        lbChangeInfo.Text = "변경신청정보";
        btnChangeInfo.Text = "변경내용적용 ▷▶▶";
		btnElectEnd.Visible = false;
                
        var sArg = "CMGNO=" + quote(sCmgno);	// 기존고객번호
        if (!gfn_isNull(sCmgno)) {
			lfn_SearchAllInfo(sArg);                // 기존 고객정보 호출
		}
    } else if (sFcgubun == "4") {
		lbTitle.Text = "해임신청";
		btnEnd.Left = 282;
		window.Width = 376;
		
		btnBuildingSearch.Visible = false;
		btnChangeInfo.Visible = false;
		btnElectEnd.Visible = true;
    }
	//alert(ds_oJubsuInfo.GetColumn(0, "FCEDUYN"));

}

