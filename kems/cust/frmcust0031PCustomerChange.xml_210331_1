// 청구지 주소 선택
function lfn_CbxPostGubun(strCode) {
	switch(strCode)
	{
		case 0 :	//대상물
			edHopeAddr.Text = edNewAddr1.Text; 	//주소1
            edHopeAddr2.Text = edNewAddr2.Text;	//주소2
            medHopeZipCode.Value = medNewZipCode.Value;	//우편번호
            
            edHopeAddr.Enable = false;
            edHopeAddr2.Enable = false;
            btnSearchZipCode2.Enable = false;
            
            if(gfn_IsNull(edNewAddr1.Text) || gfn_IsNull(edNewAddr2.Text) || gfn_IsNull(medNewZipCode.Value)) {	//원본 주소가 Null일때
                gfn_Alert("대상물의 구주소에 빈값이 존재합니다.\n먼저 대상물의 구주소를 입력하세요.");
                edOldAddr1.SetFocus();	//대상물의 주소로 이동.
                return;
            }
		break;
		case 1 :	//선임자
			edHopeAddr.Text = edOldPostAddr1.Text;	//주소1
            edHopeAddr2.Text = edOldPostAddr2.Text;	//주소2
            medHopeZipCode.Value = medOldPostZipCode.Value;	//우편번호
            
            edHopeAddr.Enable = false;
            edHopeAddr2.Enable = false;
            btnSearchZipCode2.Enable = false;
            
            if(gfn_IsNull(edOldPostAddr1.Text) 
            || gfn_IsNull(edOldPostAddr2.Text) 
            || gfn_IsNull(medOldPostZipCode.Value)) {	//원본 주소가 Null일때
                gfn_Alert("선임자의 구주소에 빈값이 존재합니다.\n먼저 선임자의 구주소를 입력하세요.");
                return;
            }
		break;
		case 2 :	//직접
			edHopeAddr.Text = "";	//주소1
			edHopeAddr2.Text = "";	//주소2
			medHopeZipCode.Value = "";	//우편번호
			
			edHopeAddr.Enable = true;
			edHopeAddr2.Enable = true;
			btnSearchZipCode2.Enable = true;
			
			edHopeAddr.SetFocus();
		break;
	}
}


// 대표직능 변경
function cbxCourse_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{	
	if(left(strCode, 1) == "1" || left(strCode, 1) == "2") {	//소방안전관리(보조)자	
		lfn_BusinessHidden(true);			//용도 히든
		lfn_AddressHidden(true);			//대표주소 입력 히든처리
				
	} else if(left(strCode, 1) == "3") {	//위험물안전관리자	
		lfn_BusinessHidden(false);			//용도 히든
		lfn_AddressHidden(true);			//대표주소 입력 히든처리
				
	} else if(left(strCode, 1) == "5") {	//소방기술자
		lfn_BusinessHidden(false);			//용도 히든
		lfn_AddressHidden(true);			//대표주소 입력 히든처리
		
	} else if(left(strCode, 1) == "6") {	//개인회원
		lfn_BusinessHidden(false);			//용도 히든
		lfn_AddressHidden(true);			//대표주소 입력 히든처리
		
	} else if(left(strCode, 1) == "7") {	//단체회원
		lfn_BusinessHidden(false);			//용도 히든
		lfn_AddressHidden(true);			//대표주소 입력 히든처리
		
	} else if(left(strCode, 1) == "8") {	//위험물운송자
		lfn_BusinessHidden(false);			//용도 히든
		lfn_AddressHidden(true);			//대표주소 입력 히든처리
		
	} else if(left(strCode, 1) == "9") {	//교육희망자
		lfn_BusinessHidden(false);			//용도 히든
		lfn_AddressHidden(true);			//대표주소 입력 히든처리
	}
	lfn_FilterPersonGubun(strCode);			// 인력 구분 필터링
	lfn_FilterDetailCourse(strCode);		// 세부고객직능 필터링
	lfn_FilterElectQualify(strCode);		// 선임자격 히든처리
}

// 용도 히든처리
function lfn_BusinessHidden(bStatus) {
	lbBusiness.Visible  = bStatus;			//용도
	cbxBusiness.Visible = bStatus;			//용도
	
	//데이타값 초기화
	if( gfn_IsNull(ds_ioBuildingInfo.GetColumn(0, "BMGNO")) ) {
		if(bStatus == false) {
			cbxBusiness.Value = "";
			//cbxBusiness.Text = "";
		}
	}
}

// 대표주소 입력 히든처리 (위험물운송자)
function lfn_AddressHidden(bStatus) {
	// 히든처리
	// 대상물 주소
    lbDelegateAddr.Visible      = bStatus;
    edNewAddr1.Visible          = bStatus;
    cbxPostGubun1.Visible       = bStatus;
    medNewZipCode.Visible       = bStatus;
    btnSearchZipCode1.Visible   = bStatus;
    edNewAddr2.Visible          = bStatus;
    
	// 고객 주소
	lbHopeAddr.Visible          = bStatus;
    edHopeAddr.Visible          = bStatus;
    cbxPostGubun2.Visible       = bStatus;
    medHopeZipCode.Visible      = bStatus;
    btnSearchZipCode2.Visible   = bStatus;
    edHopeAddr2.Visible         = bStatus;
	
	// 데이터 초기화
	if( gfn_IsNull(ds_ioBuildingInfo.GetColumn(0, "BMGNO")) ) {
		if(bStatus == false) {
			// 대상물 주소
            edNewAddr1.Text       = "";
            cbxPostGubun1.Value   = "";
            medNewZipCode.Value   = "";
            edNewAddr2.Text       = "";
            // 고객 주소
            edHopeAddr.Text       = "";
            cbxPostGubun2.Value   = "";
            medHopeZipCode.Value  = "";
            edHopeAddr2.Text      = "";
		}
	}
}

