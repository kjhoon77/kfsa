/*
 * 기능 : 접수상태 업데이트
 */
function lfn_UpdateJubsuInfo(sStatus)
{
var args = "";	

	tit_clearActionInfo();

	tit_addSingleActionInfo("cust:updateCust0031PJubsuInfo", "", 0, "");
	args = "FCMGNO=" + quote(sFcmgno);
	args += " FCSEQ=" + quote(sFcseq);
	args += " FCSTATUS=" + quote(sStatus);

	if ((sStatus == "2") || (sStatus == "4") || (sStatus == "5")) {	// 2: 접수 4:보류  5: 신청취소

		// 서버 호출 
		tit_callService(
			""
			,""
			,""	// inDs
			,""	// outDs
			,args // args 
			,"lfn_AfterUpdateJubsuProcess"
			,false
		);

	} else if (sStatus == "3") {	// 완료처리일때

		// 서버 호출 
		tit_callService(
			""
			,""
			,""	// inDs
			,""	// outDs
			,args // args 
			,"lfn_AfterUpdateCompleteProcess"
			,false
		);
		
	}
	
	

    
}



