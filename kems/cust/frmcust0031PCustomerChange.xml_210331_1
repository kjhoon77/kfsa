/*
 * 선임자격 필터링
 */
function lfn_FilterElectQualify(sFilterGubun) {
	ds_oElectQualify.UnFilter();
	var fGubun;		//필터구분(직능)
	switch(sFilterGubun)
	{
		//소방안전관리자
		case 10 :
			fGubun = "10";
			
			lbElectQualify.Visible = true;		
			cbxElectQualify.Visible = true;
		break;

		//소방안전관리보조자
		case 20 :
			fGubun = "20";
			
			lbElectQualify.Visible = true;		
			cbxElectQualify.Visible = true;
		break;
		default :	//선임자격에 속하지 않는 직능

			lbElectQualify.Visible = false;
			cbxElectQualify.Visible = false;
			cbxElectQualify.Value = "";
			cbxElectQualify.Text = "";
		break;
	}
	
	var ls_FilterExpr = "";
    if (!gfn_IsNull(fGubun)) {
        ls_FilterExpr += "PCD=='" + trim(fGubun) + "'";
    }
    
    ds_oElectQualify.Filter(ls_FilterExpr);
    
	if(ds_oElectQualify.FindRow("DATA", cbxElectQualify.Text) == -1) {
		cbxElectQualify.Text = "";
    }
    
	//신규입력시 선임자격 디폴트 셋팅
    if( fGubun == "10" || fGubun == "20" ) {
		cbxElectQualify.Index = 0;
	} else {
		cbxElectQualify.Index = -1;
	}
}

