function lfn_FilterPersonGubun(sFilterGubun) {
	
	ds_oPersonGubun.UnFilter();
	var fGubun;		//필터구분(직능)
	lbSection.Visible = true;
	cbxPersonGubun.Visible = true;
	switch(sFilterGubun)
	{
		//공공기관
		case 13 :
			fGubun = "10";
		break;
		case 14 :
			fGubun = "10";
		break;
		//위험물
		case 30 :
			fGubun = "30";
		break;
		case 31 :
			fGubun = "30";
		break;
		case 32 :
			fGubun = "30";
		break;
		case 33 :
			fGubun = "30";
		break;
		case 34 :
			fGubun = "30";
		break;
		case 35 :
			fGubun = "30";
		break;
		case 36 :
			fGubun = "30";
		break;
		//소방기술자
		case 50 :
			fGubun = "50";
		break;
		case 51 :
			fGubun = "50";
		break;
		case 52 :
			fGubun = "50";
		break;
		case 53 :
			fGubun = "50";
		break;
		case 54 :
			fGubun = "50";
		break;
		case 55 :
			fGubun = "50";
		break;
		default :	//인력구분에 속하지 않는 직능
			lbSection.Visible = false;
			cbxPersonGubun.Visible = false;
			cbxPersonGubun.Value = "";
			cbxPersonGubun.Text = "";
		break;
	}
	
	var ls_FilterExpr = "";
    if (!gfn_IsNull(fGubun)) {
        ls_FilterExpr += "PCD=='" + trim(fGubun) + "'";
    }
    
    ds_oPersonGubun.Filter(ls_FilterExpr);
    
	if(ds_oPersonGubun.FindRow("DATA", cbxPersonGubun.Text) == -1) {
		cbxPersonGubun.Text = "";
    }
    
	//신규입력시 인력구분 디폴트 셋팅
    if( fGubun == "10" || fGubun == "30" || fGubun == "50" ) {
		cbxPersonGubun.Index = 0;
	} else {
		cbxPersonGubun.Index = -1;
	}
}

