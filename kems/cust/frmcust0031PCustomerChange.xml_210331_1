/*
 * 기능 : 접수자 정보 조회 후 처리해야 할 내용
 * parameter : nErrorCode - 에러코드
 *             strErrorMsg - 에러메시지
 * Return : 없음
 */
function lfn_AfterSearchInetChangeManagement(nErrorCode, strErrorMsg) {
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }
    if (ms_FormAuth == "W") { // 수정권한이 있으면
		if (ds_oJubsuInfo.GetColumn(0, "FCSTATUS") == "1") {        // 접수상태(신청)
			btnJubsu.Enable = true;
			btnDefer.Enable = true;
			btnCancle.Enable = true;
			btnComplete.Enable = false;
			btnElectEnd.Enable = false;
	
			btnBuildingSearch.Enable = false;
			btnChangeInfo.Enable = false;        
		} else if (ds_oJubsuInfo.GetColumn(0, "FCSTATUS") == "2") {        // 2접수상태(접수)
			btnJubsu.Enable = false;
			btnDefer.Enable = true;
			btnCancle.Enable = false;
			btnComplete.Enable = true;
			btnElectEnd.Enable = true;
			
			btnBuildingSearch.Enable = true;
			btnChangeInfo.Enable = true; 
		} else if (ds_oJubsuInfo.GetColumn(0, "FCSTATUS") == "3" || ds_oJubsuInfo.GetColumn(0, "FCSTATUS") == "5") { // 접수상태(수정완료, 신청취소)
			btnJubsu.Enable = false;
			btnDefer.Enable = false;
			btnCancle.Enable = false;
			btnComplete.Enable = false;
			btnElectEnd.Enable = false;
			
			btnBuildingSearch.Enable = false;
			btnChangeInfo.Enable = false; 
		} else {
			btnJubsu.Enable = true;
			btnDefer.Enable = false;
			btnCancle.Enable = true;
			btnComplete.Enable = false;
			btnElectEnd.Enable = false;
			
			btnBuildingSearch.Enable = false;
			btnChangeInfo.Enable = false; 
		}
		
		if (left(ds_oJubsuInfo.GetColumn(0, "FCCOURSECD"),1) == "8") {	// 위험물운송자
			lbHopeAddrAfter.Text = "상치장소";
			lbCar.Visible = true;
			lbCarNo.Visible = true;
			edCarNo.Visible = true;
			lbCarYear.Visible = true;
			edCarYear.Visible = true;
			lbCarPermissionNo.Visible = true;
			edCarPermissionNo.Visible = true;
			
			Shape7.Visible = true;
			lbHopeAddr.Text = "상치장소";
			lbNewCar.Visible = true;
			lbNewCarNo.Visible = true;
			edNewCarNo.Visible = true;
			lbNewCarYear.Visible = true;
			edNewCarYear.Visible = true;
			lbNewCarPermissionNo.Visible = true;
			edNewCarPermissionNo.Visible = true;
			
			cbxPostGubun2.Visible = false;
			medHopeZipCode.BindDataset = "ds_ioTankInfo";
			edHopeAddr.BindDataset = "ds_ioTankInfo";
			edHopeAddr2.BindDataset = "ds_ioTankInfo";
			
			medHopeZipCode.Column = "TPARKPLACEZIPCD";
			edHopeAddr.Column = "TPARKPLACEADDR1";
			edHopeAddr2.Column = "TPARKPLACEADDR2";
			
		} else {
			lbHopeAddrAfter.Text = "청구지주소";
			lbCar.Visible = false;
			lbCarNo.Visible = false;
			edCarNo.Visible = false;
			lbCarYear.Visible = false;
			edCarYear.Visible = false;
			lbCarPermissionNo.Visible = false;
			edCarPermissionNo.Visible = false;
			
			Shape7.Visible = false;
			lbHopeAddr.Text = "청구지주소";
			lbNewCar.Visible = false;
			lbNewCarNo.Visible = false;
			edNewCarNo.Visible = false;
			lbNewCarYear.Visible = false;
			edNewCarYear.Visible = false;
			lbNewCarPermissionNo.Visible = false;
			edNewCarPermissionNo.Visible = false;
			
			cbxPostGubun2.Visible = true;
			cbxPostGubun2.BindDataset = "ds_ioCustomerInfo";
			medHopeZipCode.BindDataset = "ds_ioCustomerInfo";
			edHopeAddr.BindDataset = "ds_ioCustomerInfo";
			edHopeAddr2.BindDataset = "ds_ioCustomerInfo";
			
			cbxPostGubun2.Column = "CFEEPOSTGUBUN";
			medHopeZipCode.Column = "CFEEPOSTZIPCD";
			edHopeAddr.Column = "CFEEPOSTADDR1";
			edHopeAddr2.Column = "CFEEPOSTADDR2";
			
			if((left(ds_oJubsuInfo.GetColumn(0, "FCCOURSECD"),1) == "1") || // 소방안전관리자
			   (left(ds_oJubsuInfo.GetColumn(0, "FCCOURSECD"),1) == "2")) {	// 소방안전관리보조자
				lbEnterQualify.Visible = true;
