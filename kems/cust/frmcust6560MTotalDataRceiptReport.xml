/***   기능 : 출력   ***/
function lfn_Print(obj){
    lfn_Search();
    
    course = "";
    if (ds_oDATA.GetCount() < 1) {
        gfn_Alert("출력할 데이터가 없습니다.");
        return false;
    }

    var rpt = "";
    var params = "";
    var params2 = "";
    var sTopMargin = "";
    var sLeftMargin = "";

	if (radPrtGubun.Value == "1") {  // 전국 
		rpt = "frmcust6560MTotalDataRceiptTotal_" + cbxCourse.Value + ".rex";
	} else {  // 본부 
		rpt = "frmcust6560MTotalDataRceiptBonbu_" + cbxCourse.Value + ".rex";
		if (cbxCourse.Value == "1") {  // 소방안전관리자
			params = cbxBonbu.Text + "(소방안전관리자) 전국 데이터수신 현황";
		} else if (cbxCourse.Value == "3") {  // 위험물 안전관리자
			params = cbxBonbu.Text + "(위험물안전관리자) 전국 데이터수신 현황";
		} else if (cbxCourse.Value == "5") {  // 소방기술자
			params = cbxBonbu.Text + "(소방기술자) 전국 데이터수신 현황";
		} else if (cbxCourse.Value == "8") {  // 위험물운송자
			params = cbxBonbu.Text + "(위험물운송자)전국 데이터수신 현황";
		}	
			
	}
	
	if (cbxCourse.Value == "8") { 
		sTopMargin = "100";
		sLeftMargin = "200";
	}
    
	
    
    params = "rd_TITLE=" + quote(params) + "^s_TITLE=" + quote(params2);
    var StartDate = substr(calStartDate.Value,0,4) + "." + substr(calStartDate.Value,4,2) + "." + substr(calStartDate.Value,6,2);
    var EndDate = substr(calEndDate.Value,0,4) + "." + substr(calEndDate.Value,4,2) + "." + substr(calEndDate.Value,6,2);
    params += "^date=일자 : "+ StartDate + " ~ " + EndDate;
    
	/*
	 * 기능 : 렉스퍼트 보고서를 팝업화면으로 호출한다.
	 * parameter : sReportName - 레포트명(예:레포트.rex)
	 *             sReportDataset - 데이터셋명(예:dsReport1||dsResport2||...)
	 *             sReportParams - 렉스퍼트용 파라메터(예: a에 1, b에 2, c에 3의 값을 넣는 경우, a=1^b=2^c=3...)
	 *             bLcsYn - 수첩여부
	 *             LeftMargin = 왼쪽
	 *             RightMargin = 오른쪽
	 *             TopMargin = 상단
	 *             BottomMargin = 하단
	 * return value : 정상출력 true, 출력 안했으면 false
	 */    
	 
    //gfn_CallReportPopup(rpt, "ds_oDATA", params);
    gfn_CallReportPopup(rpt, "ds_oDATA", params, '', '', sTopMargin, sLeftMargin);
}




