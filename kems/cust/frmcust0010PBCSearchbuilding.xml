	<Script><![CDATA[/*
 * 프로그램명 : frmcust0010PBCSearchbuilding.xml
 * 설명 : 대상물 검색 팝업(대상물변경)
 * 작성일 : 2010.01.14
 * 작성자 : 이규희
 */

#include "LIB::cust.js"; // 공통업무 관련

/* 공통 필수 시작 */
function Form_OnLoadCompleted(obj) {
    gfn_Form_OnLoadCompleted(obj); // 공통 폼 로딩 이벤트 처리
    
    // 공통 코드 조회
    gfn_SearchCommonCode(ds_oBizGubun, "BIZCD", "Y", "전체");
    
    if (!gfn_IsNull(sBuildingNm) || !gfn_IsNull(sBuildingAddr)) {
        edBuildindNm.Text = sBuildingNm;
        edAddrSearch.Text = sBuildingAddr;
        lfn_Search();
    }
    edBuildindNm.SetFocus();
}

function Form_OnUnloadCompleted(obj) {
    Close();
}

/*
 * 기능 : 종료
 */
function lfn_End(obj) {
    Close();
}

/*
 * 기능 : 조회
 */
function lfn_Search(obj) {
    var sKeyWord = edBuildindNm.Text;
    if (chkCust.Value == "0") {
        sKeyWord = lfn_ReplaceBnm(sKeyWord);  // 검색대상물명
    }
    
    medTotalCnt.Value = "";
    ds_oCustList.ClearData(); // 데이타셋 초기화

    tit_clearActionInfo();
	tit_addCsvSearchActionInfo("cust:searchCust0010PBCBuildingList", false);
	// 서버 호출 
	tit_callService(
		""
		,""
		,""	// inDs
		,"ds_oCustList=ds_oCustList"	// outDs
		,"BNM=" + quote(sKeyWord)
          + " BADDR=" + quote(edAddrSearch.Text)
          + " CGTMGNO=" + quote(sCgtmgno)
          + " CHECK=" + quote(chkCust.Value)
          + " CHECK2=" + quote(chkAddr.Value)
		,"lfn_AfterSearchbuilding"
		,false
	);
}

/* 공통 필수 종료 */

/*
 * 기능 : 공통코드 조회 후 처리해야 할 내용
 * parameter : nErrorCode - 에러코드
 *             strErrorMsg - 에러메시지
 * Return : 없음
 */
function lfn_AfterSearchbuilding(nErrorCode, strErrorMsg) {
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }
    //trace(ds_oCustList.SaveXML());
    medTotalCnt.Value = ds_oCustList.GetCount();
    gfn_SearchMessage(gdBuidingList); // 조회 메세지를 처리하고 그리드의 정렬 헤드 마크를 제거한다.
}

/* 이벤트 함수 종료 */


/* 사용자 함수 */
function gdBuidingList_OnCellDblClick(obj,nRow,nCell,nX,nY,nPivotIndex) {
    btnSelect_OnClick(obj);
}

function btnSelect_OnClick(obj) {
    var arrArg = Array(1);
    arrArg[0] = ds_oCustList.GetColumn(ds_oCustList.currow,  "BMGNO");		        // 대상물관리번호 
    
    Close(arrArg);
}

function gdBuidingList_OnKeyDown(obj,nChar,bShift,bCtrl,bAlt,nLLParam,nHLParam)
{
	if (nChar == 13) { // Enter Key
        btnSelect_OnClick(obj);
    }
}

/*
 * 기능 : 검색을 위한 대상물명 변환
 * parameter : 대상물명
 * return : 대상물명(BSEARCHNM)
 */
function lfn_ReplaceBnm(sBnm) {
    var sBSEARCHNM = sBnm;
    ds_oBnmCode.ClearData();
    
    tit_clearActionInfo();
    tit_addCsvSearchActionInfo("cust:searchCust0010BnmReplaceCode", false);
    // 서버 호출
    tit_callService(
        ""
        , ""
        , "" // inDs
        , "ds_oBnmCode=ds_oBnmCode" // outDs
        , ""
        , ""
        ,false
        ,false
        ,true
    );
    
    for (i=0;i<ds_oBnmCode.GetCount();i++) {
        sBSEARCHNM = Replace(sBSEARCHNM, ds_oBnmCode.GetColumn(i, "CCCDNM"), "");
    }
    
    return sBSEARCHNM;
}]]></Script>
