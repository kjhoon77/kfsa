// 변경내역 처리
function lfn_Save(obj)
{
	// 소방안전관리자
	if ((cbxCourse.Value == "10" && ds_ioBangChangeHList.GetCount() == 0) ||
		(cbxCourse.Value == "20" && ds_ioBangChangeHList.GetCount() == 0) ||
	    (cbxCourse.Value == "30" && ds_ioDangerChangeHList.GetCount() == 0) ||
	    (cbxCourse.Value == "80" && ds_ioTankChangeHList.GetCount() == 0) ||
	    (cbxCourse.Value == "50" && ds_ioSokiChangeHList.GetCount() == 0)) {
		gfn_Alert("변경할 내역이 없습니다.");
		return;
	}

	if(!gfn_Confirm("변경내역을 저장하시겠습니까?")){
		return;
	}

	if (cbxCourse.Value == "10") { // 소방안전관리자
		lfn_BuildingSave();

	} else if (cbxCourse.Value == "20") { // 소방안전관리보조자
		lfn_BuildingSave();

	} else if (cbxCourse.Value == "30") {  // 위험물안전관리자
		lfn_DangerSave();
		
	} else if (cbxCourse.Value == "50") {  // 소방기술자
		lfn_SokiSave();
		
	} else if (cbxCourse.Value == "80") {  // 위험물운송자
		lfn_TankSave();
	}
		
	if (errCnt == 0) {	// 에러가 없으면 완료
		gfn_Alert("변경 저장을 완료하였습니다.");
		lfn_Search(obj);
	}

}

// 소방안전관리자 변경내역 처리
function lfn_BuildingSave(obj)
{
/*				
	00	대상물정보 삭제
	01	대상물분류 변경
	02	대상물명칭 변경
	03	소방관서 변경
	04	주용도 변경
	05	전화번호 변경
	06	공공기관여부 변경
	07	사용여부 변경
	08	주소 변경
	09	선임자 삭제
	10	해임일자 변경
	11	해임처리
	12	선임처리
	13  선임일자 변경
*/	
	var iArgsNmBuildingUpdate = " BMGNO="  + quote(ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "BMGNO"));	// 대상물관리번호
	var iArgsNmCustomerUpdate = " CMGNO="  + quote(ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "CMGNO"));	// 고객관리번호
	var iArgsNmManagerUpdate  = " FMMGNO=" + quote(ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "FMMGNO"));		// 선임자관리번호	

	var procCnt = 0;	// 처리건수
	var iArgsNm = "";	// 입력 parameter
	var	iDsNm = "";		// 입력 dataset
	var oDsNm = "";		// 출력 dataset
	var UpdateGubun = "";  // 업데이트해야하면 Y 안해도 돼면 N	
	var BSEARCHNM = "";
	var sApplyGubun = "";
	errCnt = 0;
	var sDataChangeYN = "N";	// 실제 변경여부
	
	ls_DI = "";			// DI값
	ls_CI = "";			// CI값
	ls_CIVER = "";		// CI버전
	ls_PERSONKEY = "";	// 개인식별번호
	ls_AUTH = "";		// 실명인증여부
	
	// 변경이력에 사용할 dataset 세팅
	ds_ioCustomerInfoTmp.ClearData();
	ds_ioCustomerInfoTmp.InsertRow(0);						
	ds_ioCustomerInfoTmp.SetColumn(0, "COBMGNO", ds_ioKFSAInfo.GetColumn(0, "COBMGNO"));
	ds_ioCustomerInfoTmp.SetColumn(0, "CSTATUSGUBUN", ds_ioKFSAInfo.GetColumn(0, "CSTATUSGUBUN"));
	ds_ioCustomerInfoTmp.SetColumn(0, "CREGCD", ds_ioKFSAInfo.GetColumn(0, "CREGCD"));
	ds_ioCustomerInfoTmp.SetColumn(0, "CDELGUBUN", ds_ioKFSAInfo.GetColumn(0, "CDELGUBUN"));
	ds_ioCustomerInfoTmp.SetColumn(0, "CCOURSECD", ds_ioKFSAInfo.GetColumn(0, "CCOURSECD"));
	ds_ioCustomerInfoTmp.SetColumn(0, "COVERGUBUN", ds_ioKFSAInfo.GetColumn(0, "COVERGUBUN"));	

	dsCnt = ds_ioBangChangeHList.GetRowCount();
	// dataset  아래부터 위쪽으로 처리
    for(var i=ds_ioBangChangeHList.GetRowCount()-1; i>=0; i--) {
		if (errCnt > 0) {
			gfn_alert("에러가 발생하였습니다. 재조회 후 다시 처리하시기 바랍니다.");
			return;
		}
    
		if ((ds_ioBangChangeHList.GetColumn(i, "SEL") == "1") ||
		   (ds_ioBangChangeHList.GetColumn(i, "SEL2") == "1")) {   // 반영/미반영 선택된 row만 처리

			iArgsNm = "";
			
			if (ds_ioBangChangeHList.GetColumn(i, "SEL") == "1") {   // 반영 선택된 row만 처리
				sApplyGubun = "1";
				
				ds_ioBuildingInfo.ClearData();
				ds_ioCustomerInfo.ClearData();
				ds_ioManagerInfo.ClearData();
				ds_ioAdditionInfo.ClearData();
				ds_iStatus.ClearData();
			
				iDsNm = "";
				oDsNm = "";		
				UpdateGubun = "N";  // 업데이트해야하면 Y 안해도 돼면 N
				
				// 대상물 명칭 변경이면 searchNM을 가져옴
				if (ds_ioBangChangeHList.GetColumn(i, "MHPROCGUBUN") == "02") {	// 대상물 명칭 변경
					BSEARCHNM = lfn_ReplaceBnm(ds_ioBangChangeHList.GetColumn(i, "PROCVALUE"));  // 검색대상물명
					
				} else if (ds_ioBangChangeHList.GetColumn(i, "MHPROCGUBUN") == "08") { // 주소변경
					// 청주소코드로 주소 addr1, addr2 가져오기 // ds_ioChangeMList 에 세팅됨
					lfn_SearchMatchChangeAddr(ds_ioBangChangeHList.GetColumn(i, "MMMGNO")); 
				
					// 대상물 주소 변경시 포함된 고객 주소 일괄변경(주소 구분이 대상물인 경우만)
					if( ds_ioBuildingInfo.GetOrgColumn(0, "BADDR1") <> ds_ioBuildingInfo.GetColumn(0, "BADDR1") 
						|| ds_ioBuildingInfo.GetOrgColumn(0, "BADDR2") <> ds_ioBuildingInfo.GetColumn(0, "BADDR2") 
						|| ds_ioBuildingInfo.GetOrgColumn(0, "BZIPCD") <> ds_ioBuildingInfo.GetColumn(0, "BZIPCD")
						|| ds_ioBuildingInfo.GetOrgColumn(0, "BROADADDR1") <> ds_ioBuildingInfo.GetColumn(0, "BROADADDR1") 
						|| ds_ioBuildingInfo.GetOrgColumn(0, "BROADADDR2") <> ds_ioBuildingInfo.GetColumn(0, "BROADADDR2") ) {	//대상물 주소가 변경되었을때
						lfn_FindNotOverBizFireManagerList();		
					}
					
				} else if (ds_ioBangChangeHList.GetColumn(i, "MHPROCGUBUN") == "12") {	// 선임처리
					
					if (cbxCourse.Value == "10") { // 소방안전관리자
						// 청코드로 소방안전관리자정보 가져오기
						lfn_SearchFPISBangManager(ds_ioBangChangeHList.GetColumn(i, "MHVALUE1"),
												  ds_ioBangChangeHList.GetColumn(i, "MHVALUE2"),
												  ds_ioBangChangeHList.GetColumn(i, "MHVALUE3"));
					} else if (cbxCourse.Value == "20") { // 소방안전관리보조자
						// 청코드로 보조자정보 가져오기
						lfn_SearchFPISSubBangManager(ds_ioBangChangeHList.GetColumn(i, "MHVALUE1"),
												    ds_ioBangChangeHList.GetColumn(i, "MHVALUE2"),
												    ds_ioBangChangeHList.GetColumn(i, "MHVALUE3"));
					
					}
					//ALERT(ds_oFPISBangManager.SAVEXML());
					// 청에서 생년월일 6~7자리가 넘어옴
					if (length(replace(ds_oFPISBangManager.GetColumn(0, "IHIDNUM"),"-","")) == 8) {
						ds_oFPISBangManager.SetColumn(0, "IHIDNUM", substr(replace(ds_oFPISBangManager.GetColumn(0, "IHIDNUM"),"-",""),2));
					} else if (length(ds_oFPISBangManager.GetColumn(0, "IHIDNUM")) > 8) {
						ds_oFPISBangManager.SetColumn(0, "IHIDNUM", substr(replace(ds_oFPISBangManager.GetColumn(0, "IHIDNUM"),"-",""),0,7));
					}
					
					// 이름 또는 생년월일 없으면 처리안함
					if (gfn_IsNull(ds_oFPISBangManager.GetColumn(0, "NM")) || gfn_IsNull(ds_oFPISBangManager.GetColumn(0, "IHIDNUM"))) {
						//gfn_Alert("성명이나 생년월일이 없어서 처리할 수 없습니다.");
						//return false;
					} else {
											  
						// 주민번호 유효성 체크
						lfn_ResidentNoCheck(ds_oFPISBangManager.GetColumn(0, "IHIDNUM"));

						// 휴대폰번호 있으면 personkey 조회
						if(!gfn_IsNull(ds_oFPISBangManager.GetColumn(0, "MOBLPHON"))) {
							if (left(ds_oFPISBangManager.GetColumn(0, "MOBLPHON"),3) == "010" ||
								left(ds_oFPISBangManager.GetColumn(0, "MOBLPHON"),3) == "011" ||
								left(ds_oFPISBangManager.GetColumn(0, "MOBLPHON"),3) == "016" ||
								left(ds_oFPISBangManager.GetColumn(0, "MOBLPHON"),3) == "017" ||
								left(ds_oFPISBangManager.GetColumn(0, "MOBLPHON"),3) == "018" ||
								left(ds_oFPISBangManager.GetColumn(0, "MOBLPHON"),3) == "019") {
								ls_PERSONKEY = gfn_SearchPERSONKEY(ds_oFPISBangManager.GetColumn(0, "NM"),
																   ds_oFPISBangManager.GetColumn(0, "IHIDNUM"), // 6자리
																   ds_oFPISBangManager.GetColumn(0, "MOBLPHON"));
																   
								
							}
						}
						
						// 선임현황 조회 => 회원상태 조회
						lfn_SearchFireManager(ds_oFPISBangManager.GetColumn(0, "NM"),
											  ds_oFPISBangManager.GetColumn(0, "IHIDNUM"),	// 6자리 또는 7자리
											  ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "CMGNO"),
											  ls_PERSONKEY);
						// 신규선임자가 firemanagerh에 있는지 검색
						lfn_FireManagerCheck("INFO",
