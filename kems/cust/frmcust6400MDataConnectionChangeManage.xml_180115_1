// 그리드 헤드전체 체크
function grid_OnHeadClick(obj,nCell,nX,nY,nPivotIndex)
{
	var lobj_Dataset = object(obj.BindDataset);

	if (nCell == 9) { // 선택
        if (obj.GetCellProp("head", nCell, "text") == 1) { // 전체가 이미 선택된 경우(전체선택취소)
            obj.SetCellProp("head", nCell, "text", 0);
            for(i = 0; i < lobj_Dataset.RowCount(); i++) {
				if (lobj_Dataset.GetColumn(i, "SEL") != "X")            
                lobj_Dataset.SetColumn(i, "SEL", "0");
            }
        } else { // 전체가 이미 선택되지 않은 경우(전체선택)
            obj.SetCellProp("head", nCell, "text", 1);
            obj.SetCellProp("head", nCell+1, "text", 0);
            for(i = 0; i < lobj_Dataset.RowCount(); i++) {
				if (lobj_Dataset.GetColumn(i, "SEL") != "X") {
					lobj_Dataset.SetColumn(i, "SEL", "1");
					lobj_Dataset.SetColumn(i, "SEL2", "0");
				}
            }
        }
	} else if (nCell == 10) { // 선택
        if (obj.GetCellProp("head", nCell, "text") == 1) { // 전체가 이미 선택된 경우(전체선택취소)
            obj.SetCellProp("head", nCell, "text", 0);            
            for(i = 0; i < lobj_Dataset.RowCount(); i++) {
                if (lobj_Dataset.GetColumn(i, "SEL2") != "X") {
                    lobj_Dataset.SetColumn(i, "SEL2", "0");                    
                }                 
            }
        } else { // 전체가 이미 선택되지 않은 경우 전체선택
            obj.SetCellProp("head", nCell, "text", 1);
            obj.SetCellProp("head", nCell-1, "text", 0);
            for(i = 0; i < lobj_Dataset.RowCount(); i++) {
                if (lobj_Dataset.GetColumn(i, "SEL2") != "X") {
                    lobj_Dataset.SetColumn(i, "SEL2", "1");                    
                    lobj_Dataset.SetColumn(i, "SEL", "0");
                } 
            }
        }	
    }
}


