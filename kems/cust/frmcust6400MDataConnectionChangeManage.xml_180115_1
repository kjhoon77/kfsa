						if(ds_ioIsOverBizFireManager.rowcount > 0) {
							iDsNm  += " ds_ioIsOverBizFireManager=ds_ioIsOverBizFireManager";
							tit_addMultiActionInfo("cust:executeCust0010OverBizFinshProc", "", 0, "", "N");
						}			
					break;


	// 09 선임자 삭제 ////////////////////////////////////////////////////////////////
					case "09" :	//선임자 삭제
						iArgsNm += iArgsNmManagerUpdate;
						iArgsNm += iArgsNmCustomerUpdate;
	
						if (ds_ioKFSAInfo.GetColumn(0, "FMHENDGUBUN") == "해임") {  // 기존 상태가 해임이면 
							//
						} else {   // 해임처리
							ds_iStatus.InsertRow(0);
							ds_iStatus.SetColumn(0, "FMHSEQ", ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "FMHSEQ")); // 선임자관리순번
							ds_iStatus.SetColumn(0, "FMHENDGUBUN", "0");	// 해임처리
							//ds_iStatus.SetColumn(0, "FMHENDDATE", quote(ds_ioBangChangeHList.GetColumn(i, "MHVALUE1")));
							iArgsNm += " RETIREYN='Y'";	// 교육이수후 퇴직 및 교육이관 여부
							iDsNm += " ds_iStatus=ds_iStatus";						
							tit_addSingleActionInfo("cust:executeCust0018FireManagerEnd", "", 0, "");
							
							// 변경이력
							iDsNm += " ds_iCustomerInfoTmp=ds_ioCustomerInfoTmp";    
							tit_addSingleActionInfo("cust:executeCust0010UpdateCustomerChangeH", "", 0, "");							
						}
			
					break;

	// 10 해임일자 변경 ////////////////////////////////////////////////////////////////
					case "10" :	//해임일자 변경
						iArgsNm += iArgsNmManagerUpdate;
						iArgsNm += iArgsNmCustomerUpdate;					
						
						if (ds_ioKFSAInfo.GetColumn(0, "FMHENDGUBUN") == "해임") {
							ds_ioManagerInfo.InsertRow(0);
							ds_ioManagerInfo.SetColumn(0, "FMHSEQ", ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "FMHSEQ")); // 선임자관리순번						
							ds_ioManagerInfo.SetColumn(0, "FMHENDDATE", ds_ioSokiChangeHList.GetColumn(i, "PROCVALUE"));
	
							iDsNm += " ds_ioManagerInfo=ds_ioManagerInfo";
							tit_addSingleActionInfo("cust:updateCust6400ManagerInfo", "", 0, "");		//고객						
						} else { 
							ds_iStatus.InsertRow(0);
							ds_iStatus.SetColumn(0, "FMHSEQ", ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "FMHSEQ")); // 선임자관리순번
							ds_iStatus.SetColumn(0, "FMHENDGUBUN", "0");	// 해임처리
							ds_iStatus.SetColumn(0, "FMHENDDATE", ds_ioSokiChangeHList.GetColumn(i, "PROCVALUE"));
							iArgsNm += " RETIREYN='N'";	// 교육이수후 퇴직 및 교육이관 여부
							iDsNm += " ds_iStatus=ds_iStatus";						
							tit_addSingleActionInfo("cust:executeCust0018FireManagerEnd", "", 0, "");
							
							// 변경이력
							iDsNm += " ds_iCustomerInfoTmp=ds_ioCustomerInfoTmp";    
							tit_addSingleActionInfo("cust:executeCust0010UpdateCustomerChangeH", "", 0, "");							
						}
					break;

	// 11 해임처리 ////////////////////////////////////////////////////////////////
					case "11" :	//해임처리
						iArgsNm += iArgsNmManagerUpdate;
						iArgsNm += iArgsNmCustomerUpdate;
	
						if (ds_ioKFSAInfo.GetColumn(0, "CDELGUBUN") == "0") {  // 안전원데이터가 정상일 경우
							UpdateGubun = "Y";
							
							ds_ioCustomerInfo.InsertRow(0);
							ds_ioCustomerInfo.SetColumn(0, "CDELGUBUN", "4");   // 삭제-기타
							ds_ioCustomerInfo.SetColumn(0, "CDELDATE", left(GetDate(), 8));
						}
						
						if (UpdateGubun == "Y") { // 업데이트해야하면 Y 
							// 고객변경
							iDsNm += " ds_ioCustomerInfo=ds_ioCustomerInfo";
							tit_addSingleActionInfo("cust:updateCust6400CustomerInfo", "", 0, "");		//고객
							
							//정상에서 삭제로  // 해임처리
							ds_iStatus.InsertRow(0);
							ds_iStatus.SetColumn(0, "FMHSEQ", ds_ioKFSAInfo.GetColumn(0, "FMHSEQ")); // 선임자관리순번
							ds_iStatus.SetColumn(0, "FMHENDGUBUN", "0");	// 해임처리
							ds_iStatus.SetColumn(0, "FMHENDDATE", ds_ioBangChangeHList.GetColumn(i, "PROCVALUE"));
							iArgsNm += " RETIREYN='Y'";	// 교육이수후 퇴직 및 교육이관 여부
							iDsNm += " ds_iStatus=ds_iStatus";						
							tit_addSingleActionInfo("cust:executeCust0018FireManagerEnd", "", 0, "");
							
							// 변경이력
							iDsNm += " ds_iCustomerInfoTmp=ds_ioCustomerInfoTmp";    
							tit_addSingleActionInfo("cust:executeCust0010UpdateCustomerChangeH", "", 0, "");
							
						}
					
						iArgsNm += " sCMGNO=" + quote(ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "CMGNO"));	// 고객관리번호
						iArgsNm += " GTMGNO=" + quote(cbxJibu.Value);
						tit_addSingleActionInfo("cust:updateCust6400SokiMatchCancel", "", 0, "");

					break;

	// 12 선임처리 ////////////////////////////////////////////////////////////////
					case "12" :	//선임처리
						// 이름,주민번호 둘다 없으면 처리안함
						if (gfn_IsNull(ds_oFPISBangManager.GetColumn(0, "NM")) && gfn_IsNull(ds_oFPISBangManager.GetColumn(0, "IHIDNUM"))) {
							
						} else { // 이름 or 주민번호 둘 중 하나만 있으면 처리
							
							iArgsNm += iArgsNmManagerUpdate;
							iArgsNm += iArgsNmCustomerUpdate;
							
							iArgsNm += " I_NEWNAME=" + quote(ds_oFPISSokiManager.GetColumn(0, "NM"));				// 성명
							iArgsNm += " I_NEWBIRTHDAY=" + quote(replace(replace(ds_oFPISSokiManager.GetColumn(0, "IHIDNUM"),"*",""),"-",""));		// 생년월일
							iArgsNm += " I_NEWPERSONKEY=" + quote(ls_PERSONKEY);	// 개인식별번호
							iArgsNm += " I_NEWDI=" + quote(ls_DI);													// DI
							iArgsNm += " I_NEWCI=" + quote(ls_CI);													// CI
							iArgsNm += " I_NEWCIVER=" + quote(ls_CIVER);											// CIVER
							iArgsNm += " I_NEWAUTHYN=" + quote(ls_AUTH);		// 실명인증여부
							iArgsNm += " SCOURSE=" + quote(ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "CCOURSECD"));	// 직능
							iArgsNm += " SCMGNO=" + quote(ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "CMGNO"));	// 고객관리번호					
							iArgsNm += " SCSTATUS=" + quote(ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "CSTATUSGUBUN"));	// 고객상태
							iArgsNm += " SBNM=" + quote(ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "BNM"));	// 대상물명
							
							ds_ioManagerInfo.InsertRow(0);												
							ds_ioManagerInfo.SetColumn(0, "FMNM", ds_oFPISSokiManager.GetColumn(0, "NM"));
							ds_ioManagerInfo.SetColumn(0, "FMBIRTHDAY", replace(replace(ds_oFPISSokiManager.GetColumn(0, "IHIDNUM"),"*",""),"-",""));
							if (sRESIDENTERRChk == "0") {	//주민번호 정상
								ds_ioManagerInfo.SetColumn(0, "FMRESIDENTERR", "0");						
								tit_addSingleActionInfo("cust:executeCust0010NewMemberH", "", 0, "");					//MEMBERH	♣♣♣♣♣♣♣♣♣♣♣♣
							} else {
								ds_ioManagerInfo.SetColumn(0, "FMRESIDENTERR", "1");	//주민번호 오류						
							}						
	
							iArgsNm += " I_CMGNO=" + quote(ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "CMGNO"));	// 고객정보관리번호					
							iArgsNm += " I_STARTDATE=" + quote(ds_oFPISSokiManager.GetColumn(0, "APNT_DE_8"));		// 선임일자	
							ds_ioManagerInfo.SetColumn(0, "FMHSTARTDATE", ds_oFPISSokiManager.GetColumn(0, "APNT_DE_8"));// 선임일자	
							iDsNm += " ds_ioManagerInfo=ds_ioManagerInfo";        
							tit_addSingleActionInfo("cust:executeCust0010CustAppointFM", "", 0, "");				//신규선임

							// 변경이력
							iDsNm += " ds_iCustomerInfoTmp=ds_ioCustomerInfoTmp";    
							tit_addSingleActionInfo("cust:executeCust0010UpdateCustomerChangeH", "", 0, "");
						}
					break;

	// 13 선임일자 변경 ////////////////////////////////////////////////////////////////				
					case "13" :	//선임일자 변경
						iArgsNm += iArgsNmManagerUpdate;
						iArgsNm += iArgsNmCustomerUpdate;					
						
						if (ds_ioKFSAInfo.GetColumn(0, "FMHENDGUBUN") == "선임") {
							ds_ioManagerInfo.InsertRow(0);
							ds_ioManagerInfo.SetColumn(0, "FMHSEQ", ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "FMHSEQ")); // 선임자관리순번	
							ds_ioManagerInfo.SetColumn(0, "FMHSTARTDATE", ds_ioSokiChangeHList.GetColumn(i, "PROCVALUE"));
	
							iDsNm += " ds_ioManagerInfo=ds_ioManagerInfo";
							tit_addSingleActionInfo("cust:updateCust6400ManagerInfo", "", 0, "");		//고객						
						}
					break;				
					
				} // switch
			} else if (ds_ioSokiChangeHList.GetColumn(i, "SEL2") == "1") {  // if  반영 선택된 row만 처리
				sApplyGubun = "2";
				tit_clearActionInfo();					
			}
				
			/* 변경적용처리 */
			iArgsNm += " MMAPPLYGUBUN='1' MHAPPLYGUBUN=" + quote(sApplyGubun);
			iArgsNm += " MMMGNO=" +quote(ds_ioSokiChangeHList.GetColumn(i, "MMMGNO"));   // 매치변경관리번호
			iArgsNm += " MHSEQ=" + quote(ds_ioSokiChangeHList.GetColumn(i, "MHSEQ"));   // 매치변경관리순번
			
			tit_addSingleActionInfo("cust:updateCust6400MatchChangeApply", "", 0, "");            // 변경 적용처리			
			
			procCnt = procCnt + 1;  // 처리 수
			
			// 서버 호출 
			tit_callService(
				""
				,""
				,iDsNm		// inDs
				,oDsNm      // outDs
				,iArgsNm	// args
				,"lfn_AfterSokiSave"
				,""
				,""
				,true
			);			
			
		}// if  반영/미반영 선택된 row 처리
		//dsCnt = dsCnt - 1;  // 전체 row수에서 looping 돌때마다 1씩 뺀다 => lfn_AfterSave(저장 후 처리)에서 사용
		dsCnt = i;
		
	} // for

}

/*
 * 기능 : 저장 후 처리해야 할 내용
 * parameter : ErrorCode - 에러코드
 *             ErrorMsg - 에러메시지
 * Return : 없음
 */
function lfn_AfterSokiSave(nErrorCode, strErrorMsg) {    
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        errCnt = errCnt + 1;   // 에러 카운트
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }   
}


// 위험물운송자 변경내역 처리
function lfn_TankSave(obj)
{
/*				
	00	탱크 정보삭제
	14  차량번호 변경
*/	

	var iArgsNmTankUpdate1 = " TBMGNO="  + quote(ds_ioKFSATankInfo.GetColumn(ds_ioKFSATankInfo.currow, "TBMGNO"));	// 대상물관리번호
	var iArgsNmTankUpdate2 = " TSEQ=" + quote(ds_ioKFSATankInfo.GetColumn(ds_ioKFSATankInfo.currow, "TSEQ"));	// 탱크순번

	var procCnt = 0;	// 처리건수
	var iArgsNm = "";	// 입력 parameter
	var	iDsNm = "";		// 입력 dataset
	var oDsNm = "";		// 출력 dataset
	var UpdateGubun = "";  // 업데이트해야하면 Y 안해도 돼면 N	
	var sApplyGubun = "";
	errCnt = 0;
	
	ls_DI = "";			// DI값
	ls_CI = "";			// CI값
	ls_CIVER = "";		// CI버전
	ls_PERSONKEY = "";	// 개인식별번호
	ls_AUTH = "";		// 실명인증여부
	
	dsCnt = ds_ioTankChangeHList.GetRowCount();
	// dataset  아래부터 위쪽으로 처리
    for(var i=ds_ioTankChangeHList.GetRowCount()-1; i>=0; i--) {
		if (errCnt > 0) {
			gfn_alert("에러가 발생하였습니다. 재조회 후 다시 처리하시기 바랍니다.");
			return;
		}
    
		if ((ds_ioTankChangeHList.GetColumn(i, "SEL") == "1") ||
		   (ds_ioTankChangeHList.GetColumn(i, "SEL2") == "1")) {   // 반영/미반영 선택된 row만 처리

			iArgsNm = "";
			
			if (ds_ioTankChangeHList.GetColumn(i, "SEL") == "1") {   // 반영 선택된 row만 처리
				sApplyGubun = "1";
				
				ds_ioTankInfo.ClearData();
				ds_ioAdditionInfo.ClearData();
			
				iDsNm = "";
				oDsNm = "";		
				UpdateGubun = "N";  // 업데이트해야하면 Y 안해도 돼면 N
				
				tit_clearActionInfo();			
				
				switch(ds_ioTankChangeHList.GetColumn(i, "MHPROCGUBUN"))
				{
	// 00 탱크정보 삭제 ////////////////////////////////////////////////////////////////
					case "00" :	//탱크정보 삭제  =>   삭제 처리
						iArgsNm += iArgsNmTankUpdate1;
						iArgsNm += iArgsNmTankUpdate2;
						iArgsNm += " MDELREMARK=" + quote("탱크정보 삭제");
						iArgsNm += " MSABUN=" + quote(gds_oUserInfo.GetColumn(0,"PSABUN"));
						iArgsNm += " MMGNO=" + quote(ds_ioKFSATankInfo.GetColumn(ds_ioKFSATankInfo.currow, "MMGNO"));
						iArgsNm += " MFPIS1=" + quote(ds_ioTankChangeHList.GetColumn(i, "MMFPIS1"));
						iArgsNm += " MFPIS2=" + quote(ds_ioTankChangeHList.GetColumn(i, "MMFPIS2"));
						iArgsNm += " MKFSAPK=" + quote(ds_ioKFSATankInfo.GetColumn(ds_ioKFSATankInfo.currow, "TBMGNO"));	// 대상물관리번호
						iArgsNm += " MKFSAPK2=" + quote(ds_ioKFSATankInfo.GetColumn(ds_ioKFSATankInfo.currow, "TSEQ"));	// 탱크순번
						tit_addSingleActionInfo("cust:updateCust6300MatchCancel", "", 0, "");	//연계해제
	
						tit_addSingleActionInfo("cust:deleteCust0010TankInfo", "", 0, "");		//탱크
							
					break;
	
	// 14 차량번호 변경 ////////////////////////////////////////////////////////////////
					case "14" :	//차량번호 변경
						iArgsNm += iArgsNmTankUpdate1;
						iArgsNm += iArgsNmTankUpdate2;						
						iArgsNm += " TCARNO=" + quote(ds_ioTankChangeHList.GetColumn(i, "PROCVALUE"));
						tit_addSingleActionInfo("cust:updateCust0010TankInfo", "", 0, "");		//탱크
					
					break;
	
				} // switch
			} else if (ds_ioTankChangeHList.GetColumn(i, "SEL2") == "1") {  // if  반영 선택된 row만 처리
				sApplyGubun = "2";
				tit_clearActionInfo();					
			}
				
			/* 변경적용처리 */
			iArgsNm += " MMAPPLYGUBUN='1' MHAPPLYGUBUN=" + quote(sApplyGubun);
			iArgsNm += " MMMGNO=" +quote(ds_ioTankChangeHList.GetColumn(i, "MMMGNO"));   // 매치변경관리번호
			iArgsNm += " MHSEQ=" + quote(ds_ioTankChangeHList.GetColumn(i, "MHSEQ"));   // 매치변경관리순번
			
			tit_addSingleActionInfo("cust:updateCust6400MatchChangeApply", "", 0, "");            // 변경 적용처리			
			
			procCnt = procCnt + 1;  // 처리 수
			
			// 서버 호출 
			tit_callService(
				""
				,""
				,iDsNm		// inDs
				,oDsNm      // outDs
				,iArgsNm	// args
				,"lfn_AfterTankSave"
				,""
				,""
				,true
			);			
			
		}// if  반영/미반영 선택된 row 처리
		//dsCnt = dsCnt - 1;  // 전체 row수에서 looping 돌때마다 1씩 뺀다 => lfn_AfterSave(저장 후 처리)에서 사용
		dsCnt = i;
		
	} // for

}

