						ds_iCustomerModifyHistory.SetColumn(nRow, "CMHBEFOREDATA", ds_oDetailCourse.GetColumn( ds_oDetailCourse.FindRow("CD", ds_ioOldCustomerInfo.GetOrgColumn(0, "CCOURSECD")), "DATA" ) );
