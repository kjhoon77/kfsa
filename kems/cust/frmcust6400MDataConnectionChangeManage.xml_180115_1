function lfn_FireManagerCheck(gubun, sCMGNO, sMNM, sBIRTHDAY, sPERSONKEY){

    var sArg = "";
    
    if (gubun == "MGNO") {   // 선임자 조회
        sArg = "MMGNO=" + quote(sCMGNO);   // 고객관리번호를 arg로 넘김
    } else {
        sArg = "MNM=" + quote(sMNM)
             + " BIRTHDAY=" + quote(replace(replace(sBIRTHDAY,"*",""),"-",""))
             + " PERSONKEY=" + quote(sPERSONKEY);
    }
    
    ds_ioManager.ClearData();
    tit_clearActionInfo(); //사람이 이미 있는지 검색한다.
    tit_addSearchActionInfo("cust:searchCust0018ManagerInfo", false);
    // 서버 호출 
    tit_callService(
        ""
        ,""
        ,""	// inDs
        ,"ds_ioManager=ds_oManager"	// outDs
        ,sArg // args
        ,""
        ,false
        ,false
        ,true
    );
    
    if (ds_ioManager.GetRowCount() == 1) {
        sFmmgnoAfter = ds_ioManager.GetColumn(0, "MMGNO");
    } else {
        sFmmgnoAfter = "";
    }
}

