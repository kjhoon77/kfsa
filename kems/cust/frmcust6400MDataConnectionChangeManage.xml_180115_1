// 소방안전관리자 그리드 셀클릭
function gdBangChangeHList_OnCellClick(obj,nRow,nCell,nX,nY,nPivotIndex)
{
	if(nCell == 1) {	
		if (ds_ioBangChangeHList.GetColumn(nRow, "SEL") == "0") {
			ds_ioBangChangeHList.SetColumn(nRow, "SEL2", "0");	
		}
	}
	
	if(nCell == 2) {	
		if (ds_ioBangChangeHList.GetColumn(nRow, "SEL2") == "0") {
			ds_ioBangChangeHList.SetColumn(nRow, "SEL", "0");	
		}
	}		

	if(nCell == 9) {
		if (ds_ioBangChangeHList.GetColumn(nRow, "MHPROCGUBUN") == "03") {	//지역코드
			lfn_SearchRegionListPopup(nRow);
			
		} else if ((ds_ioBangChangeHList.GetColumn(nRow, "MHPROCGUBUN") == "01") ||
				  (ds_ioBangChangeHList.GetColumn(nRow, "MHPROCGUBUN") == "06")) {	// 세부직능코드
			lfn_SearchDetailCourseListPopup(nRow);			
			
		} else if (ds_ioBangChangeHList.GetColumn(nRow, "MHPROCGUBUN") == "04") {	// 용도코드
			lfn_SearchBizCDListPopup(nRow);			
		}
