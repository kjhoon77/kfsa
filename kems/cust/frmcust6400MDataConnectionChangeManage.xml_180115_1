							ds_ioManagerInfo.InsertRow(0);												
							ds_ioManagerInfo.SetColumn(0, "FMNM", ds_oFPISDangerManager.GetColumn(0, "NM"));
							ds_ioManagerInfo.SetColumn(0, "FMBIRTHDAY", replace(replace(ds_oFPISDangerManager.GetColumn(0, "IHIDNUM"),"*",""),"-",""));
							if (sRESIDENTERRChk == "0") {	//주민번호 정상
								ds_ioManagerInfo.SetColumn(0, "FMRESIDENTERR", "0");
								tit_addSingleActionInfo("cust:executeCust0010NewMemberH", "", 0, "");					//MEMBERH	♣♣♣♣♣♣♣♣♣♣♣♣
							} else {
								ds_ioManagerInfo.SetColumn(0, "FMRESIDENTERR", "1");	//주민번호 오류						
							}						
							
							// 휴대폰이면
							if (left(ds_oFireManagerInfo.GetColumn(ds_oFireManagerInfo.currow, "MOBLPHON"),3) == "010" ||
								left(ds_oFireManagerInfo.GetColumn(ds_oFireManagerInfo.currow, "MOBLPHON"),3) == "011" ||
								left(ds_oFireManagerInfo.GetColumn(ds_oFireManagerInfo.currow, "MOBLPHON"),3) == "016" ||
								left(ds_oFireManagerInfo.GetColumn(ds_oFireManagerInfo.currow, "MOBLPHON"),3) == "017" ||
								left(ds_oFireManagerInfo.GetColumn(ds_oFireManagerInfo.currow, "MOBLPHON"),3) == "018" ||
								left(ds_oFireManagerInfo.GetColumn(ds_oFireManagerInfo.currow, "MOBLPHON"),3) == "019") {
								ds_ioManagerInfo.SetColumn(0, "FMHHPTEL", ds_oFireManagerInfo.GetColumn(ds_oFireManagerInfo.currow, "MOBLPHON"));
							}

							iArgsNm += " I_CMGNO=" + quote(ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "CMGNO"));	// 고객정보관리번호					
							iArgsNm += " I_STARTDATE=" + quote(ds_oFPISDangerManager.GetColumn(0, "DMINDATE"));		// 선임일자	
							ds_ioManagerInfo.SetColumn(0, "FMHSTARTDATE", ds_oFPISDangerManager.GetColumn(0, "DMINDATE"));		// 선임일자	
							iDsNm += " ds_ioManagerInfo=ds_ioManagerInfo";        
							tit_addSingleActionInfo("cust:executeCust0010CustAppointFM", "", 0, "");		//신규선임
							
							// 변경이력
							iDsNm += " ds_iCustomerInfoTmp=ds_ioCustomerInfoTmp";    
							tit_addSingleActionInfo("cust:executeCust0010UpdateCustomerChangeH", "", 0, "");
						}
					break;
			
	// 13 선임일자 변경 ////////////////////////////////////////////////////////////////				
					case "13" :	//선임일자 변경
						iArgsNm += iArgsNmManagerUpdate;
						iArgsNm += iArgsNmCustomerUpdate;					
						
						if (ds_ioKFSAInfo.GetColumn(0, "FMHENDGUBUN") == "선임") {
							ds_ioManagerInfo.InsertRow(0);
							ds_ioManagerInfo.SetColumn(0, "FMHSEQ", ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "FMHSEQ")); // 선임자관리순번	
							ds_ioManagerInfo.SetColumn(0, "FMHSTARTDATE", ds_ioDangerChangeHList.GetColumn(i, "PROCVALUE"));
	
							iDsNm += " ds_ioManagerInfo=ds_ioManagerInfo";
							tit_addSingleActionInfo("cust:updateCust6400ManagerInfo", "", 0, "");		//고객						
						}
					break;				
					
				} // switch
			
			} else if (ds_ioDangerChangeHList.GetColumn(i, "SEL2") == "1") {  // if  반영 선택된 row만 처리
				sApplyGubun = "2";
				tit_clearActionInfo();					
			}
			
			
			/* 변경적용처리 */
			iArgsNm += " MMAPPLYGUBUN='1' MHAPPLYGUBUN=" + quote(sApplyGubun);
			iArgsNm += " MMMGNO=" +quote(ds_ioDangerChangeHList.GetColumn(i, "MMMGNO"));   // 매치변경관리번호
			iArgsNm += " MHSEQ=" + quote(ds_ioDangerChangeHList.GetColumn(i, "MHSEQ"));   // 매치변경관리순번
			tit_addSingleActionInfo("cust:updateCust6400MatchChangeApply", "", 0, "");            // 변경 적용처리			
			
			procCnt = procCnt + 1;  // 처리 수
			
			// 서버 호출 
			tit_callService(
				""
				,""
				,iDsNm		// inDs
				,oDsNm      // outDs
				,iArgsNm	// args
				,"lfn_AfterDangerSave"
				,""
				,""
				,true
			);			
		
		}// if  반영/미반영 선택된 row 처리
		//dsCnt = dsCnt - 1;  // 전체 row수에서 looping 돌때마다 1씩 뺀다 => lfn_AfterSave(저장 후 처리)에서 사용
		dsCnt = i;
		
	} // for

}

/*
 * 기능 : 저장 후 처리해야 할 내용
 * parameter : ErrorCode - 에러코드
 *             ErrorMsg - 에러메시지
 * Return : 없음
 */
function lfn_AfterDangerSave(nErrorCode, strErrorMsg) {    
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        errCnt = errCnt + 1;   // 에러 카운트
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }   
}


// 소방기술자 변경내역 처리
function lfn_SokiSave(obj)
{
/*				
	00	소방업체 정보삭제
	01	소방업체 분류변경
	02	소방업체명 변경
	03	소방관서 변경
	04	- 없음
	05	전화번호 변경
	06	- 없음
	07	- 없음
	08	주소 변경
	09	선임자 삭제
	10	해임일자 변경
	11	해임처리
	12	선임처리
	13  선임일자 변경
*/	

	var iArgsNmBuildingUpdate = " BMGNO="  + quote(ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "BMGNO"));	// 대상물관리번호
	var iArgsNmCustomerUpdate = " CMGNO="  + quote(ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "CMGNO"));	// 고객관리번호
	var iArgsNmManagerUpdate  = " FMMGNO=" + quote(ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "FMMGNO"));		// 선임자관리번호	

	var procCnt = 0;	// 처리건수
	var iArgsNm = "";	// 입력 parameter
	var	iDsNm = "";		// 입력 dataset
	var oDsNm = "";		// 출력 dataset
	var UpdateGubun = "";  // 업데이트해야하면 Y 안해도 돼면 N	
	var BSEARCHNM = "";
	var sApplyGubun = "";
	errCnt = 0;
	
	ls_DI = "";			// DI값
	ls_CI = "";			// CI값
	ls_CIVER = "";		// CI버전
	ls_PERSONKEY = "";	// 개인식별번호
	ls_AUTH = "";		// 실명인증여부
	
	// 변경이력에 사용할 dataset 세팅
	ds_ioCustomerInfoTmp.ClearData();
	ds_ioCustomerInfoTmp.InsertRow(0);						
	ds_ioCustomerInfoTmp.SetColumn(0, "COBMGNO", ds_ioKFSAInfo.GetColumn(0, "COBMGNO"));
	ds_ioCustomerInfoTmp.SetColumn(0, "CSTATUSGUBUN", ds_ioKFSAInfo.GetColumn(0, "CSTATUSGUBUN"));
	ds_ioCustomerInfoTmp.SetColumn(0, "CREGCD", ds_ioKFSAInfo.GetColumn(0, "CREGCD"));
	ds_ioCustomerInfoTmp.SetColumn(0, "CDELGUBUN", ds_ioKFSAInfo.GetColumn(0, "CDELGUBUN"));
	ds_ioCustomerInfoTmp.SetColumn(0, "CCOURSECD", ds_ioKFSAInfo.GetColumn(0, "CCOURSECD"));
	ds_ioCustomerInfoTmp.SetColumn(0, "COVERGUBUN", ds_ioKFSAInfo.GetColumn(0, "COVERGUBUN"));	

	dsCnt = ds_ioSokiChangeHList.GetRowCount();
	// dataset  아래부터 위쪽으로 처리
    for(var i=ds_ioSokiChangeHList.GetRowCount()-1; i>=0; i--) {
		if (errCnt > 0) {
			gfn_alert("에러가 발생하였습니다. 재조회 후 다시 처리하시기 바랍니다.");
			return;
		}
    
		if ((ds_ioSokiChangeHList.GetColumn(i, "SEL") == "1") ||
		   (ds_ioSokiChangeHList.GetColumn(i, "SEL2") == "1")) {   // 반영/미반영 선택된 row만 처리

			iArgsNm = "";
			
			if (ds_ioSokiChangeHList.GetColumn(i, "SEL") == "1") {   // 반영 선택된 row만 처리
				sApplyGubun = "1";
				
				ds_ioBuildingInfo.ClearData();
				ds_ioCustomerInfo.ClearData();
				ds_ioManagerInfo.ClearData();
				ds_ioAdditionInfo.ClearData();
				ds_iStatus.ClearData();
			
				iDsNm = "";
				oDsNm = "";		
				UpdateGubun = "N";  // 업데이트해야하면 Y 안해도 돼면 N
				
				// 대상물 명칭 변경이면 searchNM을 가져옴
				if (ds_ioSokiChangeHList.GetColumn(i, "MHPROCGUBUN") == "02") {	// 대상물 명칭 변경
					BSEARCHNM = lfn_ReplaceBnm(ds_ioSokiChangeHList.GetColumn(i, "PROCVALUE"));  // 검색대상물명
					
				} else if (ds_ioSokiChangeHList.GetColumn(i, "MHPROCGUBUN") == "08") { // 주소변경
					// 청주소코드로 주소 addr1, addr2 가져오기 // ds_ioChangeMList 에 세팅됨
					lfn_SearchMatchChangeAddr(ds_ioSokiChangeHList.GetColumn(i, "MMMGNO")); 
				
					// 대상물 주소 변경시 포함된 고객 주소 일괄변경(주소 구분이 대상물인 경우만)
					if( ds_ioBuildingInfo.GetOrgColumn(0, "BADDR1") <> ds_ioBuildingInfo.GetColumn(0, "BADDR1") 
						|| ds_ioBuildingInfo.GetOrgColumn(0, "BADDR2") <> ds_ioBuildingInfo.GetColumn(0, "BADDR2") 
						|| ds_ioBuildingInfo.GetOrgColumn(0, "BROADADDR1") <> ds_ioBuildingInfo.GetColumn(0, "BROADADDR1") 
						|| ds_ioBuildingInfo.GetOrgColumn(0, "BROADADDR2") <> ds_ioBuildingInfo.GetColumn(0, "BROADADDR2")
						|| ds_ioBuildingInfo.GetOrgColumn(0, "BZIPCD") <> ds_ioBuildingInfo.GetColumn(0, "BZIPCD") ) {	//대상물 주소가 변경되었을때
						lfn_FindNotOverBizFireManagerList();		
					}
					
				} else if (ds_ioSokiChangeHList.GetColumn(i, "MHPROCGUBUN") == "12") {	// 선임처리
	
					// 청코드로 선임자정보 가져오기
					lfn_SearchFPISSokiManager(ds_ioSokiChangeHList.GetColumn(i, "MHVALUE1"),
											  ds_ioSokiChangeHList.GetColumn(i, "MHVALUE2"),
											  ds_ioSokiChangeHList.GetColumn(i, "MHVALUE3"));
											  
					// 청에서 생년월일 6~7자리가 넘어옴
					if (length(replace(ds_oFPISSokiManager.GetColumn(0, "IHIDNUM"),"-","")) == 8) {
						ds_oFPISSokiManager.SetColumn(0, "IHIDNUM", substr(replace(ds_oFPISSokiManager.GetColumn(0, "IHIDNUM"),"-",""),2));
					} else if (length(ds_oFPISSokiManager.GetColumn(0, "IHIDNUM")) > 8) {
						ds_oFPISSokiManager.SetColumn(0, "IHIDNUM", substr(replace(ds_oFPISSokiManager.GetColumn(0, "IHIDNUM"),"-",""),0,6));
					}
	
					// 이름,주민번호 둘다 없으면 처리안함
					if (gfn_IsNull(ds_oFPISSokiManager.GetColumn(0, "NM")) && gfn_IsNull(ds_oFPISSokiManager.GetColumn(0, "IHIDNUM"))) {
						
					} else {
											  
						// 주민번호 유효성 체크
						lfn_ResidentNoCheck(ds_oFPISSokiManager.GetColumn(0, "IHIDNUM"));
						// 선임현황 조회 => 회원상태 조회
						lfn_SearchFireManager(ds_oFPISSokiManager.GetColumn(0, "NM"),
											  ds_oFPISSokiManager.GetColumn(0, "IHIDNUM"),
											  ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "CMGNO"),
											  ls_PERSONKEY);
						// 신규선임자가 firemanagerh에 있는지 검색
						lfn_FireManagerCheck("INFO",
											ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "CMGNO"),
											ds_oFPISSokiManager.GetColumn(0, "NM"),
											ds_oFPISSokiManager.GetColumn(0, "IHIDNUM"),
											ls_PERSONKEY);
					}
				}			
				
				tit_clearActionInfo();			
				
				switch(ds_ioSokiChangeHList.GetColumn(i, "MHPROCGUBUN"))
				{
	// 00 대상물 정보 삭제 ////////////////////////////////////////////////////////////////
					case "00" :	//대상물정보 삭제  =>   삭제 처리
						iArgsNm += iArgsNmCustomerUpdate; 				
	
						if (ds_ioSokiChangeHList.GetColumn(i, "MHVALUE1") == "N") { // 삭제로 변경됨
							if (ds_ioKFSAInfo.GetColumn(0, "CDELGUBUN") == "0") {  // 안전원데이터가 정상일 경우
								UpdateGubun = "Y";
								
								ds_ioCustomerInfo.InsertRow(0);
								ds_ioCustomerInfo.SetColumn(0, "CDELGUBUN", "4");   // 삭제-기타
								ds_ioCustomerInfo.SetColumn(0, "CDELDATE", left(GetDate(), 8));
							}
						}
						
						if (UpdateGubun == "Y") { // 업데이트해야하면 Y 
							// 고객변경
							iDsNm += " ds_ioCustomerInfo=ds_ioCustomerInfo";
							tit_addSingleActionInfo("cust:updateCust6400CustomerInfo", "", 0, "");		//고객
							
							iArgsNm += " SCMGNO="  + quote(ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "CMGNO"));	// 고객관리번호
							//정상에서 삭제로  // 해임처리
							tit_addSingleActionInfo("cust:executeCust0010CustomerDelete", "", 0, "");
	
							// 변경이력
							iDsNm += " ds_iCustomerInfoTmp=ds_ioCustomerInfoTmp";    
							tit_addSingleActionInfo("cust:executeCust0010UpdateCustomerChangeH", "", 0, "");
							
						}
					break;
	
	// 01 대상물분류 변경 ////////////////////////////////////////////////////////////////				
					case "01" :	//대상물분류 변경  => 직능변경
						iArgsNm += iArgsNmCustomerUpdate; 				
	
						ds_ioCustomerInfo.InsertRow(0);
						ds_ioCustomerInfo.SetColumn(0, "CCOURSECD", substr(ds_ioSokiChangeHList.GetColumn(i, "PROCVALUECD"),0,2));
						ds_ioCustomerInfo.SetColumn(0, "CCOCD", substr(ds_ioSokiChangeHList.GetColumn(i, "PROCVALUECD"),3,1));
	
						iDsNm += " ds_ioCustomerInfo=ds_ioCustomerInfo";
						tit_addSingleActionInfo("cust:updateCust6400CustomerInfo", "", 0, "");		//고객
	
						// 변경이력
						iDsNm += " ds_iCustomerInfoTmp=ds_ioCustomerInfoTmp";    
						tit_addSingleActionInfo("cust:executeCust0010UpdateCustomerChangeH", "", 0, "");					
						
					break;
					
	// 02 대상물명칭 변경 ////////////////////////////////////////////////////////////////
					case "02" :	//대상물명칭 변경
						iArgsNm += iArgsNmBuildingUpdate;
						
						ds_ioBuildingInfo.InsertRow(0);
						ds_ioBuildingInfo.SetColumn(0, "BNM", ds_ioSokiChangeHList.GetColumn(i, "PROCVALUE"));
						ds_ioBuildingInfo.SetColumn(0, "BSEARCHNM", BSEARCHNM); // 조회용 대상물명
						iDsNm += "ds_ioBuildingInfo=ds_ioBuildingInfo";
						tit_addSingleActionInfo("cust:updateCust6400BuildingInfo", "", 0, "");		//대상물        					
					
					break;
	
	// 03 소방관서 변경 ////////////////////////////////////////////////////////////////				
					case "03" :	//소방관서 변경
						iArgsNm += iArgsNmBuildingUpdate;
						iArgsNm += iArgsNmCustomerUpdate;
						
						// 대상물 소방관서 코드 변경
						ds_ioBuildingInfo.InsertRow(0);
						ds_ioBuildingInfo.SetColumn(0, "BFIRECD", ds_ioSokiChangeHList.GetColumn(i, "MHVALUE1"));
						iDsNm += "ds_ioBuildingInfo=ds_ioBuildingInfo";
						tit_addSingleActionInfo("cust:updateCust6400BuildingInfo", "", 0, "");		//대상물
											
						// 고객 지역변경
						ds_ioCustomerInfo.InsertRow(0);					
						ds_ioCustomerInfo.SetColumn(0, "CREGCD", ds_ioSokiChangeHList.GetColumn(i, "PROCVALUECD"));
						iDsNm += " ds_ioCustomerInfo=ds_ioCustomerInfo";
						tit_addSingleActionInfo("cust:updateCust6400CustomerInfo", "", 0, "");		//고객										
						
						// 변경이력
						iDsNm += " ds_iCustomerInfoTmp=ds_ioCustomerInfoTmp";    
						tit_addSingleActionInfo("cust:executeCust0010UpdateCustomerChangeH", "", 0, "");										
							
					break;
					
	// 05 전화번호 변경 ////////////////////////////////////////////////////////////////				
					case "05" :	//전화번호 변경
						iArgsNm += iArgsNmBuildingUpdate;
						
						// 대상물
						ds_ioBuildingInfo.InsertRow(0);
						ds_ioBuildingInfo.SetColumn(0, "BTEL", ds_ioSokiChangeHList.GetColumn(i, "MHVALUE1"));
						iDsNm += "ds_ioBuildingInfo=ds_ioBuildingInfo";
						tit_addSingleActionInfo("cust:updateCust6400BuildingInfo", "", 0, "");		//대상물					
					break;

	// 07 사용여부 변경 ////////////////////////////////////////////////////////////////				
					case "07" :	//사용여부 변경
						if (ds_ioSokiChangeHList.GetColumn(i, "MHVALUE1") == "Y") { // 사용으로 변경됨
							if (ds_ioKFSAInfo.GetColumn(0, "CDELGUBUN") != "0") {  // 안전원데이터가 삭제일 경우
								UpdateGubun = "Y"; // 업데이트해야하면 Y 
	
								ds_ioCustomerInfo.InsertRow(0);
								ds_ioCustomerInfo.SetColumn(0, "CDELGUBUN", "0");
								ds_ioCustomerInfo.SetColumn(0, "CDELDATE", "");
							}
						} else if (ds_ioSokiChangeHList.GetColumn(i, "MHVALUE1") == "N") { // 삭제로 변경됨
							if (ds_ioKFSAInfo.GetColumn(0, "CDELGUBUN") == "0") {  // 안전원데이터가 정상일 경우
								UpdateGubun = "Y"; // 업데이트해야하면 Y 
	
								ds_ioCustomerInfo.InsertRow(0);
								ds_ioCustomerInfo.SetColumn(0, "CDELGUBUN", "4");   // 삭제-기타
								ds_ioCustomerInfo.SetColumn(0, "CDELDATE", left(GetDate(), 8));
							}
						}
						
						if (UpdateGubun == "Y") { // 업데이트해야하면 Y 
						
							iArgsNm += iArgsNmCustomerUpdate; 					
							// 고객변경
							iDsNm += " ds_ioCustomerInfo=ds_ioCustomerInfo";
							tit_addSingleActionInfo("cust:updateCust6400CustomerInfo", "", 0, "");		//고객
							
							
							if (ds_ioSokiChangeHList.GetColumn(i, "MHVALUE1") == "Y") { // 사용으로 변경됨
								iArgsNm += " SCMGNO="  + quote(ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "CMGNO"));	// 고객관리번호
								iArgsNm += " SBADDR1=" + quote(ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "BADDR1"));
								iArgsNm += " SCOURSE=" + quote(ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "CCOURSECD"));
								iArgsNm += " SBNM=" + quote(ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "BNM"));
								oDsList += "ds_oOverBizProc=ds_oOverBizProc";
								tit_addSingleActionInfo("cust:executeCust0010CustStatusToRegularity", "", 0, "");		//비정상에서 정상으로												
							
							} else if (ds_ioSokiChangeHList.GetColumn(i, "MHVALUE1") == "N") { // 삭제로 변경됨
								iArgsNm += " SCMGNO="  + quote(ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "CMGNO"));	// 고객관리번호
								//정상에서 삭제로  // 해임처리
								tit_addSingleActionInfo("cust:executeCust0010CustomerDelete", "", 0, "");						
							}
	
							// 변경이력
							iDsNm += " ds_iCustomerInfoTmp=ds_ioCustomerInfoTmp";    
							tit_addSingleActionInfo("cust:executeCust0010UpdateCustomerChangeH", "", 0, "");
							
						}					
					break;

	// 08 주소 변경 ////////////////////////////////////////////////////////////////				
					case "08" :	//주소 변경
						iArgsNm += iArgsNmBuildingUpdate;
						iArgsNm += iArgsNmCustomerUpdate; 
						
						ds_ioBuildingInfo.InsertRow(0);
