							//BEFOREDATA
							if(!gfn_IsNull((ds_ioCustomerInfo.GetOrgColumn(0, ds_ioCustomerInfo.GetColID(i))))) {
								ds_iCustomerModifyHistory.SetColumn(nRow, "CMHBEFOREDATA", ds_ioOldCustomerInfo.GetOrgColumn(0, ds_ioCustomerInfo.GetColID(i)));
