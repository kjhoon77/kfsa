	<Script><![CDATA[/*
 * 프로그램명 : frmcust6400MDataConnectionChangeManager.xml
 * 설명 : 변경내역관리
 * 작성일 : 2010.06.01
 * 작성자 : 정현호
 */

#include "LIB::cust.js"; // 공통업무 관련

var	dsCnt = 0;    // MATCHCHANGEH dataset rowcount
var errCnt = 0;
var	sRESIDENTERRChk = "";

var ls_DI = "";			// DI값
var ls_CI = "";			// CI값
var ls_CIVER = "";		// CI버전
var ls_PERSONKEY = "";	// 개인식별번호
var ls_AUTH = "";		// 실명인증여부

/* 공통 필수 시작 */
function Form_OnLoadCompleted(obj) {
   gfn_Form_OnLoadCompleted(obj); // 공통 폼 로딩 이벤트 처리

    //폼 환경설정
    ls_SystemGubun = gs_SystemGubun;
    ls_SystemMenuId = gs_SystemMenuId;

    gfn_MakeRequiredInputMark(this); // 필수 입력란을 설정한다.

    ms_FormAuth = gfn_GetFormAuth(this); // 권한을 조회한다.
    if (ms_FormAuth == "W") { // 권한에 따른 버튼 설정
     //   lfn_SetFormAuth(true); // 저장 권한
    } else if (ms_FormAuth == "R") {
     //   lfn_SetFormAuth(false); // 읽기 권한
    } else {
        gfn_Alert(MSGI00080);
        close();
    }

    // 공통 코드 조회       
    gfn_SearchAllJibu(cbxJibu, "Y");           			// 지부 코드 조회    
	gfn_SearchCustomerCourseFullName(ds_ioCourse, "", "");		// 대표고객직능 조회
	
	// 소관,위관,소기,운송자만 세팅
	for(var i=ds_ioCourse.GetRowCount()-1; i>0; i--) {
		if ((ds_ioCourse.GetColumn(i, "CD") == "60") ||
		    (ds_ioCourse.GetColumn(i, "CD") == "70") ||
		    (ds_ioCourse.GetColumn(i, "CD") == "90"))
		ds_ioCourse.DeleteRow(i);
	}
	
	cbxCourse.Index = 0;

    // 콤보박스-지역
	cbxJibu_OnChanged("",cbxJibu.Value);//지역 코드 조회
    cbxRegion1.Index = 0;
    cbxRegion2.index = ds_oRegion.rowcount-1;  


	// 소속지부 셋팅
    if (gds_oUserInfo.GetColumn(0, "PDEPTCD") > 1000) {	
		if (ds_ioJibu.FindRow("CD", gds_oUserInfo.GetColumn(0, "PDEPTCD")) <> -1) {
			if(gds_oUserInfo.GetColumn(0, "PDEPTCD") <> cbxJibu.Value) {	
				cbxJibu.Value = gds_oUserInfo.GetColumn(0, "PDEPTCD");	
				lfn_JibuChange(cbxJibu.Value);	//지부변경	
			}
		}
		cbxJibu.Enable = false;
	}

	// 변경일자 초기화
	medDBProcDate1.Value = left(GetDate(), 6) + "01";
	medDBProcDate2.Value = left(GetDate(), 8);

}



// 지부변경
function cbxJibu_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{ 
	lfn_JibuChange(strCode);
}

// 1 지부변경 지역목록
function lfn_JibuChange(strCode)
{ 
	gfn_SearchCodeRegion(strCode, ds_oRegion, "0");	//지역 코드 조회
    cbxRegion1.Index = 0;
    cbxRegion2.index = ds_oRegion.rowcount-1;
    if (strCode == "1011") {	// 서울지부이면
		DivChangeM.lbOrgInfo.text = "서울소방본부 정보";
		DivTankChangeM.lbOrgInfo.text = "서울소방본부 정보";
		
	} else {					// 서울지부 외
		DivChangeM.lbOrgInfo.text = "소방청 정보";
		DivTankChangeM.lbOrgInfo.text = "소방청 정보";
	}
}

function lfn_Search(obj)
{
	var sMMBIZGUBUN  = "";

	if (cbxCourse.Value != "80") {		// 소방안전관리자, 소방안전관리보조자, 위험물안전관리자, 소방기술자
		// 정보 초기 화 
		divChangeM.edChangeMCount.Text = "";
	
		if (cbxCourse.Value == "10") {	sMMBIZGUBUN  = "0"; }		// 소방안전관리자
		else if (cbxCourse.Value == "20") {	sMMBIZGUBUN  = "4"; }	// 소방안전관리보조자
		else if (cbxCourse.Value == "30") { sMMBIZGUBUN  = "1"; }	// 위험물안전관리자
		else if (cbxCourse.Value == "50") { sMMBIZGUBUN  = "3"; }	// 소방기술자
		
		ds_ioChangeMList.ClearData();
			
		tit_clearActionInfo();
		tit_addCsvSearchActionInfo("cust:searchCust6400ChangeMList", false);
		
		// 서버 호출 
		tit_callService(
			""
			,""
			,""	// inDs
			,"ds_ioChangeMList=ds_ioChangeMList"	// outDs
			,"GTMGNO=" + quote(cbxJibu.Value)
		   + " MMBIZGUBUN=" + quote(sMMBIZGUBUN)			
		   + " CGROUPCD=" + quote(cbxCourse.Value)
		   + " REGCD1=" + quote(cbxRegion1.Value)
		   + " REGCD2=" + quote(cbxRegion2.Value)
		   + " DBPROCDATE1=" + quote(medDBProcDate1.Value)
		   + " DBPROCDATE2=" + quote(medDBProcDate2.Value)
		   + " APPLYGUBUN=" + quote(radChangeApplyGubun.Value)
		   + " SEARCHNUM=" + quote(spSearchNum.Text)
		   + " ADDSEARCHCD=" + quote(cbxSearch.Value)  
		   + " ADDSEARCH=" + quote(edAddSearch.Text)  
			,"lfn_AfterSearch"
			,false
		);
			
	} else if (cbxCourse.Value == "80") {
		sMMBIZGUBUN  = "2"; 					// 위험물운송자 		
		divTankChangeM.edChangeMCount.Text = "";		

		ds_ioTankChangeMList.ClearData();
			
		tit_clearActionInfo();
		tit_addCsvSearchActionInfo("cust:searchCust6400TankChangeMList", false);
		
		// 서버 호출 
		tit_callService(
			""
			,""
			,""	// inDs
			,"ds_ioTankChangeMList=ds_ioTankChangeMList"	// outDs
			,"GTMGNO=" + quote(cbxJibu.Value)
		   + " MMBIZGUBUN=" + quote(sMMBIZGUBUN)			
		   + " REGCD1=" + quote(cbxRegion1.Value)
		   + " REGCD2=" + quote(cbxRegion2.Value)
		   + " DBPROCDATE1=" + quote(medDBProcDate1.Value)
		   + " DBPROCDATE2=" + quote(medDBProcDate2.Value)
		   + " APPLYGUBUN=" + quote(radChangeApplyGubun.Value)
		   + " SEARCHNUM=" + quote(spSearchNum.Text) 
		   + " ADDSEARCHCD=" + quote(cbxSearch.Value)  
		   + " ADDSEARCH=" + quote(edAddSearch.Text)
			,"lfn_AfterSearch"
			,false
		);
	}
}

// 3 lfn_Search 조회후
function lfn_AfterSearch(nErrorCode, strErrorMsg) {
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }
	if (cbxCourse.Value != "80") {		// 소방안전관리자, 험물안전관리자, 소방기술자    
		if (ds_ioChangeMList.GetRowCount() == 0) {
			ds_ioChangeMList.ClearData();
			ds_oFireManagerInfo.ClearData();
			ds_ioKFSAInfo.ClearData();
			edChangeHCount.Text = "";
	
			// 소방안전관리자
			if (cbxCourse.Value == "10") {		
				ds_ioBangChangeHList.ClearData();
			// 소방안전관리보조자
			} else if (cbxCourse.Value == "20") {		
				ds_ioBangChangeHList.ClearData();
			// 위험물안전관리자
			} else if (cbxCourse.Value == "30") {
				ds_ioDangerChangeHList.ClearData();
			// 소방기술자
			} else if (cbxCourse.Value == "50") {
				ds_ioSokiChangeHList.ClearData();
			}
				
		} else {
			divChangeM.edChangeMCount.Text = ds_ioChangeMList.GetCount();
			lfn_KFSADetail_ChangehSearch(); 
		}
	} else {			// 위험물운송자 이면
		if (ds_ioTankChangeMList.GetRowCount() == 0) {
			ds_ioTankChangeMList.ClearData();
			ds_ioKFSATankInfo.ClearData();
			ds_ioTankChangeHList.ClearData();
			edChangeHCount.Text = "";
	
		} else {
			divTankChangeM.edChangeMCount.Text = ds_ioTankChangeMList.GetCount();
			lfn_KFSATankDetail_ChangehSearch(); 
		}
	}
}

function DivChangeM_gdChangeMList_OnCellClick(obj,nRow,nCell,nX,nY,nPivotIndex)
{
	if (nCell <> 8) {
		lfn_KFSADetail_ChangehSearch(nRow);  // 안전원 고객정보 상세조회 // 변경내역 상세 조회
	} else {	// 미반영 처리
		if (ds_ioChangeMList.GetColumn(nRow, "MMAPPLYGUBUN")  == "0")  {

			if(!gfn_Confirm("선택내역을 미반영처리 하시겠습니까?")){
				return false;
			}
			iArgsNm = "";
			errCnt = 0;
			// dataset  아래부터 위쪽으로 처리
			for(var i=ds_ioBangChangeHList.GetRowCount()-1; i>=0; i--) {
				if (errCnt > 0) {
					gfn_alert("에러가 발생하였습니다. 재조회 후 다시 처리하시기 바랍니다.");
					return;
				}

				tit_clearActionInfo();					
				/* 미반영 처리 */
				iArgsNm = "MMAPPLYGUBUN='1' MHAPPLYGUBUN='2'";
				iArgsNm += " MMMGNO=" +quote(ds_ioBangChangeHList.GetColumn(i, "MMMGNO"));   // 매치변경관리번호
				iArgsNm += " MHSEQ=" + quote(ds_ioBangChangeHList.GetColumn(i, "MHSEQ"));   // 매치변경관리순번
				tit_addSingleActionInfo("cust:updateCust6400MatchChangeApply", "", 0, "");            // 변경 적용처리			
				
				// 서버 호출 
				tit_callService(
					""
					,""
					,""		// inDs
					,""      // outDs
					,iArgsNm	// args
					,"lfn_AfterBuildingSave"
					,""
					,""
					,true
				);			
			
			} // for
			if (errCnt == 0) {	// 에러가 없으면 완료
				gfn_Alert("미반영처리를 완료하였습니다.");
				lfn_Search(obj);
			}
		} // if
	} // if
}

function lfn_KFSADetail_ChangehSearch(nRow)
{
	var sArg = "";
	var bizGubun = "";
	if (cbxCourse.Value == "10") bizGubun = "0";
	else if (cbxCourse.Value == "20") bizGubun = "4";
	else if (cbxCourse.Value == "30") bizGubun = "1";
	else if (cbxCourse.Value == "50") bizGubun = "3";
	else if (cbxCourse.Value == "80") bizGubun = "2";
	
	// 정보 초기 화 
	ds_oFireManagerInfo.ClearData();  // 선임자정보
	ds_ioKFSAInfo.ClearData();        // 안전원정보
	ds_ioBangChangeHList.ClearData();
	
	
    tit_clearActionInfo();
	oDsNM = "ds_oFireManagerInfo=ds_oFireManagerInfo";    
	if (bizGubun == "0") {
		sArg = "BILDSN=" + quote(ds_ioChangeMList.GetColumn(nRow, "BILD_SN")); 						// 소관
	} else if (bizGubun == "4") {
		sArg = "BILDSN=" + quote(ds_ioChangeMList.GetColumn(nRow, "BILD_SN"))  						// 소관보조자
		     + " APNT_DE_8=" + quote(ds_ioChangeMList.GetColumn(nRow, "APNT_DE_8")) 				// 소관보조자
		     + " QUALFUSER_SN=" + quote(ds_ioChangeMList.GetColumn(nRow, "QUALFUSER_SN"));			// 소관보조자
	} else if (bizGubun == "1") {
		sArg = "MNFCTRETC_SN=" + quote(ds_ioChangeMList.GetColumn(nRow, "MNFCTRETC_SN"));  			// 위관
	} else if (bizGubun == "3") {
		sArg = "BSNM_REGIST_NO=" + quote(ds_ioChangeMList.GetColumn(nRow, "BSNM_REGIST_NO"))		// 소기
			 + " ENTRPS_SE=" + quote(ds_ioChangeMList.GetColumn(nRow, "ENTRPS_SE"))					// 소기
			 + " ENTRPS_DETAIL_SE=" + quote(ds_ioChangeMList.GetColumn(nRow, "ENTRPS_DETAIL_SE"))	// 소기
