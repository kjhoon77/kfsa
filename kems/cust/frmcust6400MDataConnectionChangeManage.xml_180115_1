							ds_ioManagerInfo.SetColumn(0, "FMHSEQ", ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "FMHSEQ")); // 선임자관리순번						
							ds_ioManagerInfo.SetColumn(0, "FMHENDDATE", ds_ioBangChangeHList.GetColumn(i, "PROCVALUE"));
	
							iDsNm += " ds_ioManagerInfo=ds_ioManagerInfo";
							tit_addSingleActionInfo("cust:updateCust6400ManagerInfo", "", 0, "");		//고객						
							UpdateGubun = "N";
						} else { 
							ds_iStatus.InsertRow(0);
							ds_iStatus.SetColumn(0, "FMHSEQ", ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "FMHSEQ")); // 선임자관리순번
							ds_iStatus.SetColumn(0, "FMHENDGUBUN", "0");	// 해임처리
							ds_iStatus.SetColumn(0, "FMHENDDATE", ds_ioBangChangeHList.GetColumn(i, "PROCVALUE"));
							iArgsNm += " RETIREYN='N'";	// 교육이수후 퇴직 및 교육이관 여부
							iDsNm += " ds_iStatus=ds_iStatus";						
							tit_addSingleActionInfo("cust:executeCust0018FireManagerEnd", "", 0, "");

							UpdateGubun = "Y";
							
						}
					break;

	// 11 해임처리 ////////////////////////////////////////////////////////////////
					case "11" :	//해임처리
						iArgsNm += iArgsNmManagerUpdate;
						iArgsNm += iArgsNmCustomerUpdate;
						iArgsNm += " SCMGNO=" + quote(ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "CMGNO"));	// 고객관리번호						
						
						if (cbxCourse.Value == "10") { // 소방안전관리자
							if (ds_ioKFSAInfo.GetColumn(0, "FMHENDGUBUN") == "해임") {  // 기존 상태가 해임이면 해임일자만 변경
								ds_ioManagerInfo.InsertRow(0);
								ds_ioManagerInfo.SetColumn(0, "FMHSEQ", ds_ioKFSAInfo.GetColumn(0, "FMHSEQ")); // 선임자관리순번						
								ds_ioManagerInfo.SetColumn(0, "FMHENDDATE", ds_ioBangChangeHList.GetColumn(i, "PROCVALUE"));
		
								iDsNm += " ds_ioManagerInfo=ds_ioManagerInfo";
								tit_addSingleActionInfo("cust:updateCust6400ManagerInfo", "", 0, "");		//고객						
								
								UpdateGubun = "N";
							} else {   // 해임처리
								ds_iStatus.InsertRow(0);
								ds_iStatus.SetColumn(0, "FMHSEQ", ds_ioKFSAInfo.GetColumn(0, "FMHSEQ")); // 선임자관리순번
								ds_iStatus.SetColumn(0, "FMHENDGUBUN", "0");	// 해임처리
								ds_iStatus.SetColumn(0, "FMHENDDATE", ds_ioBangChangeHList.GetColumn(i, "PROCVALUE"));
								iArgsNm += " RETIREYN='Y'";	// 교육이수후 퇴직 및 교육이관 여부
								iDsNm += " ds_iStatus=ds_iStatus";						
								tit_addSingleActionInfo("cust:executeCust0018FireManagerEnd", "", 0, "");
								
								UpdateGubun = "Y";
								
								ds_ioManagerInfo.InsertRow(0);								
							}
						
						} else if (cbxCourse.Value == "20") { // 소방안전관리보조자
							if (ds_ioKFSAInfo.GetColumn(0, "CDELGUBUN") == "0") {  // 안전원데이터가 정상일 경우
								UpdateGubun = "Y";
								
								ds_ioCustomerInfo.InsertRow(0);
								ds_ioCustomerInfo.SetColumn(0, "CDELGUBUN", "4");   // 삭제-기타
								ds_ioCustomerInfo.SetColumn(0, "CDELDATE", left(GetDate(), 8));
							}
						
							if (UpdateGubun == "Y") { // 업데이트해야하면 Y 
								// 고객변경
								iDsNm += " ds_ioCustomerInfo=ds_ioCustomerInfo";
								tit_addSingleActionInfo("cust:updateCust6400CustomerInfo", "", 0, "");		//고객
								
								//정상에서 삭제로  // 해임처리
								ds_iStatus.InsertRow(0);
								ds_iStatus.SetColumn(0, "FMHSEQ", ds_ioKFSAInfo.GetColumn(0, "FMHSEQ")); // 선임자관리순번
								ds_iStatus.SetColumn(0, "FMHENDGUBUN", "0");	// 해임처리
								ds_iStatus.SetColumn(0, "FMHENDDATE", ds_ioBangChangeHList.GetColumn(i, "PROCVALUE"));
								iArgsNm += " RETIREYN='Y'";	// 교육이수후 퇴직 및 교육이관 여부
								iDsNm += " ds_iStatus=ds_iStatus";						
								tit_addSingleActionInfo("cust:executeCust0018FireManagerEnd", "", 0, "");
								
							}
							ds_ioManagerInfo.InsertRow(0);
						
							iArgsNm += " sCMGNO=" + quote(ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "CMGNO"));	// 고객관리번호
							iArgsNm += " GTMGNO=" + quote(cbxJibu.Value);
							tit_addSingleActionInfo("cust:updateCust6400SubManagerMatchCancel", "", 0, "");
						}
						// 2015-01-07 실무교육 유효기간 업데이트
						// 소방안전관리자 or 소방안전관리보조자일 경우
						//tit_addSingleActionInfo("cust:executeCust0010EduExpireDateUpdate", "", 0, "");

						
					break;

	// 12 선임처리 ////////////////////////////////////////////////////////////////
					case "12" :	//선임처리
						// 이름 또는 생년월일 없으면 처리안함
						if (gfn_IsNull(ds_oFPISBangManager.GetColumn(0, "NM")) || gfn_IsNull(ds_oFPISBangManager.GetColumn(0, "IHIDNUM"))) {
								UpdateGubun = "N";
						} else { // 이름 생년월일 있으면 처리
							
							iArgsNm += iArgsNmManagerUpdate;
							iArgsNm += iArgsNmCustomerUpdate;
							
							iArgsNm += " I_NEWNAME=" + quote(ds_oFPISBangManager.GetColumn(0, "NM"));				// 성명
							iArgsNm += " I_NEWBIRTHDAY=" + quote(replace(replace(ds_oFPISBangManager.GetColumn(0, "IHIDNUM"),"*",""),"-",""));		// 생년월일
							iArgsNm += " I_NEWPERSONKEY=" + quote(ls_PERSONKEY);	// 개인식별번호
							iArgsNm += " I_NEWDI=" + quote(ls_DI);													// DI
							iArgsNm += " I_NEWCI=" + quote(ls_CI);													// CI
							iArgsNm += " I_NEWCIVER=" + quote(ls_CIVER);											// CIVER
							iArgsNm += " I_NEWAUTHYN=" + quote(ls_AUTH);		// 실명인증여부
							iArgsNm += " SCOURSE=" + quote(ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "CCOURSECD"));	// 직능
							iArgsNm += " SCMGNO=" + quote(ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "CMGNO"));	// 고객관리번호					
							iArgsNm += " SCSTATUS=" + quote(ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "CSTATUSGUBUN"));	// 고객상태
							iArgsNm += " SBNM=" + quote(ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "BNM"));	// 대상물명
								
							ds_ioManagerInfo.InsertRow(0);												
							ds_ioManagerInfo.SetColumn(0, "FMNM", ds_oFPISBangManager.GetColumn(0, "NM"));
							ds_ioManagerInfo.SetColumn(0, "FMBIRTHDAY", replace(replace(ds_oFPISBangManager.GetColumn(0, "IHIDNUM"),"*",""),"-",""));
							if (sRESIDENTERRChk == "0") {	//주민번호 정상
								ds_ioManagerInfo.SetColumn(0, "FMRESIDENTERR", "0");
								tit_addSingleActionInfo("cust:executeCust0010NewMemberH", "", 0, "");					//MEMBERH	♣♣♣♣♣♣♣♣♣♣♣♣
							} else {
								ds_ioManagerInfo.SetColumn(0, "FMRESIDENTERR", "1");	//주민번호 오류						
							}						
							
							// 휴대폰이면
							if (left(ds_oFireManagerInfo.GetColumn(ds_oFireManagerInfo.currow, "MOBLPHON"),3) == "010" ||
								left(ds_oFireManagerInfo.GetColumn(ds_oFireManagerInfo.currow, "MOBLPHON"),3) == "011" ||
								left(ds_oFireManagerInfo.GetColumn(ds_oFireManagerInfo.currow, "MOBLPHON"),3) == "016" ||
								left(ds_oFireManagerInfo.GetColumn(ds_oFireManagerInfo.currow, "MOBLPHON"),3) == "017" ||
								left(ds_oFireManagerInfo.GetColumn(ds_oFireManagerInfo.currow, "MOBLPHON"),3) == "018" ||
								left(ds_oFireManagerInfo.GetColumn(ds_oFireManagerInfo.currow, "MOBLPHON"),3) == "019") {
								ds_ioManagerInfo.SetColumn(0, "FMHHPTEL", ds_oFireManagerInfo.GetColumn(ds_oFireManagerInfo.currow, "MOBLPHON"));
							}
							
							iArgsNm += " I_CMGNO=" + quote(ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "CMGNO"));	// 고객정보관리번호					
							iArgsNm += " I_STARTDATE=" + quote(ds_oFPISBangManager.GetColumn(0, "APNT_DE_8"));		// 선임일자	
							ds_ioManagerInfo.SetColumn(0, "FMHSTARTDATE", ds_oFPISBangManager.GetColumn(0, "APNT_DE_8"));// 선임일자	
							iDsNm += " ds_ioManagerInfo=ds_ioManagerInfo";        
							tit_addSingleActionInfo("cust:executeCust0010CustAppointFM", "", 0, "");				//신규선임

							if (cbxCourse.Value == "20") { // 소방안전관리보조자
								iArgsNm += " sCMGNO=" + quote(ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "CMGNO"));	// 고객관리번호					
								iArgsNm += " BILD_SN=" + quote(ds_ioBangChangeHList.GetColumn(i, "MHVALUE1"));
								iArgsNm += " APNT_DE_8=" + quote(ds_ioBangChangeHList.GetColumn(i, "MHVALUE2"));
								iArgsNm += " QUALFUSER_SN=" + quote(ds_ioBangChangeHList.GetColumn(i, "MHVALUE3"));
								iArgsNm += " GTMGNO=" + quote(cbxJibu.Value);
								tit_addSingleActionInfo("cust:updateCust6400SubManagerMatch", "", 0, "");
							}
							// 2018-01-06 PKG_CUSTOMERCHG.SP_MAIN 에서 처리함
							// 2015-01-07 실무교육 유효기간 업데이트
							// 소방안전관리자 or 소방안전관리보조자일 경우
							//tit_addSingleActionInfo("cust:executeCust0010EduExpireDateUpdate", "", 0, "");
							//iArgsNm += " FSTARTDATE=" + quote(ds_oFPISBangManager.GetColumn(0, "APNT_DE_8"));	// 선임일자
							//iArgsNm += " FNM=" + quote(ds_oFPISBangManager.GetColumn(0, "NM"));					// 성명
							//iArgsNm += " FPERSONKEY=" + quote(ls_PERSONKEY);									// 개인식별번호
							UpdateGubun = "N";

						}
					break;

	// 13 선임일자 변경 ////////////////////////////////////////////////////////////////				
					case "13" :	//선임일자 변경
						iArgsNm += iArgsNmManagerUpdate;
						iArgsNm += iArgsNmCustomerUpdate;					
						iArgsNm += " SCMGNO=" + quote(ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "CMGNO"));	// 고객관리번호											
						
						if (ds_ioKFSAInfo.GetColumn(0, "FMHENDGUBUN") == "선임") {
							ds_ioManagerInfo.InsertRow(0);
							ds_ioManagerInfo.SetColumn(0, "FMHSEQ", ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "FMHSEQ")); // 선임자관리순번	
							ds_ioManagerInfo.SetColumn(0, "FMHSTARTDATE", ds_ioBangChangeHList.GetColumn(i, "PROCVALUE"));
	
							iDsNm += " ds_ioManagerInfo=ds_ioManagerInfo";
							tit_addSingleActionInfo("cust:updateCust6400ManagerInfo", "", 0, "");		//고객
							
							// 2015-01-07 실무교육 유효기간 업데이트
							// 소방안전관리자 or 소방안전관리보조자일 경우
							tit_addSingleActionInfo("cust:executeCust0010EduExpireDateUpdate", "", 0, "");
							iArgsNm += " FSTARTDATE=" + quote(ds_ioBangChangeHList.GetColumn(i, "PROCVALUE"));	// 선임일자
							iArgsNm += " FNM=" + quote(ds_ioKFSAInfo.GetColumn(0, "FMNM"));					// 성명
							iArgsNm += " FPERSONKEY=" + quote(ls_PERSONKEY);
							UpdateGubun = "N";
						}
					break;				
					
				} // switch

				if (UpdateGubun == "Y") {
					//고객정보변경History     CustomerModifyHistory Insert
					lfn_CustomerModifyHistoryGenerator(ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "CMGNO"));	//고객정보변경History
		
					if (ds_iCustomerModifyHistory.GetRowCount() > 0) {
						iDsNm  += " ds_iCustomerModifyHistory=ds_iCustomerModifyHistory";
						tit_addMultiActionInfo("cust:insertCust0010CustomerModifyHistory", "", 0, "", "N");
					}
					// 변경이력
					iDsNm += " ds_iCustomerInfoTmp=ds_ioCustomerInfoTmp";    
					tit_addSingleActionInfo("cust:executeCust0010UpdateCustomerChangeH", "", 0, "");					
				}
				
			} else if (ds_ioBangChangeHList.GetColumn(i, "SEL2") == "1") {  // if  미반영 row이면
				sApplyGubun = "2";
				tit_clearActionInfo();					
			}
			
			if (ds_ioBangChangeHList.GetColumn(i, "SEL") == "1") {	// 반영인 경우만
				if (!(ds_ioBangChangeHList.GetColumn(i, "MHPROCGUBUN") == "09" ||	// 선임자 삭제
					ds_ioBangChangeHList.GetColumn(i, "MHPROCGUBUN") == "11" ||		// 해임처리
					ds_ioBangChangeHList.GetColumn(i, "MHPROCGUBUN") == "13")) {	// 선임일자변경 이 아니면
					
					// 휴대폰 번호가 있으면
					if (left(ds_oFPISBangManager.GetColumn(0, "MOBLPHON"),3) == "010" ||
						left(ds_oFPISBangManager.GetColumn(0, "MOBLPHON"),3) == "011" ||
						left(ds_oFPISBangManager.GetColumn(0, "MOBLPHON"),3) == "016" ||
						left(ds_oFPISBangManager.GetColumn(0, "MOBLPHON"),3) == "017" ||
						left(ds_oFPISBangManager.GetColumn(0, "MOBLPHON"),3) == "018" ||
						left(ds_oFPISBangManager.GetColumn(0, "MOBLPHON"),3) == "019") {
						if (!gfn_isNull(ds_oFireManagerInfo.GetColumn(ds_oFireManagerInfo.currow, "MOBLPHON"))) {
							if (replace(ds_oFireManagerInfo.GetColumn(ds_oFireManagerInfo.currow, "MOBLPHON"),"-","") <>
								replace(ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "FMHHPTEL"),"-","")) {
								ds_ioManagerInfo.SetColumn(0, "FMHHPTEL", ds_oFireManagerInfo.GetColumn(ds_oFireManagerInfo.currow, "MOBLPHON"));
								ds_ioManagerInfo.SetColumn(0, "FMPERSONKEY", ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "FMPERSONKEY"));
//								iArgsNm += " MOBLPHON=" + quote(ds_oFireManagerInfo.GetColumn(ds_oFireManagerInfo.currow, "MOBLPHON"));
//								tit_addSingleActionInfo("cust:updateCust6400HandPhoneInfo", "", 0, "");
								
							}
						}
					}
				}
			}
			
			/* 변경적용처리 */
			iArgsNm += " MMAPPLYGUBUN='1' MHAPPLYGUBUN=" + quote(sApplyGubun);
			iArgsNm += " MMMGNO=" +quote(ds_ioBangChangeHList.GetColumn(i, "MMMGNO"));   // 매치변경관리번호
			iArgsNm += " MHSEQ=" + quote(ds_ioBangChangeHList.GetColumn(i, "MHSEQ"));   // 매치변경관리순번
			tit_addSingleActionInfo("cust:updateCust6400MatchChangeApply", "", 0, "");            // 변경 적용처리
			procCnt = procCnt + 1;  // 처리 수
			
			// 서버 호출 
			tit_callService(
				""
				,""
				,iDsNm		// inDs
				,oDsNm      // outDs
				,iArgsNm	// args
				,"lfn_AfterBuildingSave"
				,""
				,""
				,true
			);			
			
		}// if  반영/미반영 선택된 row 처리
		//dsCnt = dsCnt - 1;  // 전체 row수에서 looping 돌때마다 1씩 뺀다 => lfn_AfterSave(저장 후 처리)에서 사용
		dsCnt = i;
		
	} // for

}

/*
 * 기능 : 저장 후 처리해야 할 내용
 * parameter : ErrorCode - 에러코드
 *             ErrorMsg - 에러메시지
 * Return : 없음
 */
function lfn_AfterBuildingSave(nErrorCode, strErrorMsg) {    
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        errCnt = errCnt + 1;   // 에러 카운트
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }   
}



// 위험물안전관리자 변경내역 처리
function lfn_DangerSave(obj)
{
/*				
	00	제조소등정보 삭제
	01	제조소등분류 변경
	02	대상물명칭 변경
	03	소방관서 변경
	04	허가번호 변경
	05	전화번호 변경
	06	- 없음
	07	사용여부 변경
	08	주소 변경
	09	선임자 삭제
	10	해임일자 변경
	11	해임처리
	12	선임처리
	13	선임일자 변경
*/	

	var iArgsNmBuildingUpdate = " BMGNO="  + quote(ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "BMGNO"));	// 대상물관리번호
	var iArgsNmCustomerUpdate = " CMGNO="  + quote(ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "CMGNO"));	// 고객관리번호
	var iArgsNmManagerUpdate  = " FMMGNO=" + quote(ds_ioKFSAInfo.GetColumn(ds_ioKFSAInfo.currow, "FMMGNO"));		// 선임자관리번호	

	var procCnt = 0;	// 처리건수
	var iArgsNm = "";	// 입력 parameter
	var	iDsNm = "";		// 입력 dataset
	var oDsNm = "";		// 출력 dataset
	var UpdateGubun = "";  // 업데이트해야하면 Y 안해도 돼면 N	
	var BSEARCHNM = "";
	var sApplyGubun = "";
	errCnt = 0;
	
	ls_DI = "";			// DI값
	ls_CI = "";			// CI값
	ls_CIVER = "";		// CI버전
	ls_PERSONKEY = "";	// 개인식별번호
	ls_AUTH = "";		// 실명인증여부
	
	// 변경이력에 사용할 dataset 세팅
	ds_ioCustomerInfoTmp.ClearData();
	ds_ioCustomerInfoTmp.InsertRow(0);						
	ds_ioCustomerInfoTmp.SetColumn(0, "COBMGNO", ds_ioKFSAInfo.GetColumn(0, "COBMGNO"));
	ds_ioCustomerInfoTmp.SetColumn(0, "CSTATUSGUBUN", ds_ioKFSAInfo.GetColumn(0, "CSTATUSGUBUN"));
	ds_ioCustomerInfoTmp.SetColumn(0, "CREGCD", ds_ioKFSAInfo.GetColumn(0, "CREGCD"));
	ds_ioCustomerInfoTmp.SetColumn(0, "CDELGUBUN", ds_ioKFSAInfo.GetColumn(0, "CDELGUBUN"));
	ds_ioCustomerInfoTmp.SetColumn(0, "CCOURSECD", ds_ioKFSAInfo.GetColumn(0, "CCOURSECD"));
	ds_ioCustomerInfoTmp.SetColumn(0, "COVERGUBUN", ds_ioKFSAInfo.GetColumn(0, "COVERGUBUN"));	

	dsCnt = ds_ioDangerChangeHList.GetRowCount();
	// dataset  아래부터 위쪽으로 처리
    for(var i=ds_ioDangerChangeHList.GetRowCount()-1; i>=0; i--) {
		if (errCnt > 0) {
			gfn_alert("에러가 발생하였습니다. 재조회 후 다시 처리하시기 바랍니다.");
			return;
		}
    
		if ((ds_ioDangerChangeHList.GetColumn(i, "SEL") == "1") ||
		   (ds_ioDangerChangeHList.GetColumn(i, "SEL2") == "1")) {   // 반영/미반영 선택된 row만 처리

			if (ds_ioDangerChangeHList.GetColumn(i, "SEL") == "1") {   // 반영 선택된 row만 처리
				sApplyGubun = "1";
		
				ds_ioBuildingInfo.ClearData();
				ds_ioCustomerInfo.ClearData();
				ds_ioManagerInfo.ClearData();
				ds_ioAdditionInfo.ClearData();
				ds_ioDangerInfo.ClearData();
				ds_iStatus.ClearData();
			
				iArgsNm = "";
				iDsNm = "";
