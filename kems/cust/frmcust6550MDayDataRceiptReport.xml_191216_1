			params += "^s_TITLE1=선임자";
			params += "^s_TITLE2=선임일자";
			params += "^s_TITLE3=전화번호";
		} else if (cbxCourse.Value == "3") { 
			params2 = "(위험물안전관리자) 일자별 데이터수신 리스트";
			params = "s_TITLE0=대상물명";
			params += "^s_TITLE1=선임자";
			params += "^s_TITLE2=선임일자";
			params += "^s_TITLE3=전화번호";
		} else if (cbxCourse.Value == "5") {
			params2 = "(소방기술자) 일자별 데이터수신 리스트"; 
			params = "s_TITLE0=업체명";
			params += "^s_TITLE1=기술자명";
			params += "^s_TITLE2=선임일자";
			params += "^s_TITLE3=전화번호";
		} else if (cbxCourse.Value == "8") { 
			params2 = "(위험물운송자)일자별 데이터수신 리스트"; 
			params = "s_TITLE0=대상명";
			params += "^s_TITLE1=차량번호";
			params += "^s_TITLE2=등록일자";
			params += "^s_TITLE3=차량년도";			
		}		
    }
    
    params += "^rd_TITLE=" + quote(params2); 
    params += "^bonbu=소방본부 : "+ cbxBonbu.Text;
    
	/*
	 * 기능 : 렉스퍼트 보고서를 팝업화면으로 호출한다.
	 * parameter : sReportName - 레포트명(예:레포트.rex)
	 *             sReportDataset - 데이터셋명(예:dsReport1||dsResport2||...)
	 *             sReportParams - 렉스퍼트용 파라메터(예: a에 1, b에 2, c에 3의 값을 넣는 경우, a=1^b=2^c=3...)
	 *             bLcsYn - 수첩여부
	 *             LeftMargin = 왼쪽
	 *             RightMargin = 오른쪽
	 *             TopMargin = 상단
	 *             BottomMargin = 하단
	 * return value : 정상출력 true, 출력 안했으면 false
	 */    
	 
    //gfn_CallReportPopup(rpt, "ds_oDATA", params);
    gfn_CallReportPopup(rpt, "ds_oDATA", params, '', '', sTopMargin, sLeftMargin);
}




function lfn_Search(obj) {
    SetWaitCursor(false);
    
    

    ds_oDATA.ClearData(); // 데이타셋 초기화
    
    tit_clearActionInfo();
    
    
    var qry = "";
    if (radPrtGubun.Value == "1") {     // 출력구분 : 집계    
		qry = "cust:searchCust6550MDayDataRceiptTotal";
    } else if (radPrtGubun.Value == "2") { // 출력구분 : 리스트 
        qry = "cust:searchCust6550MDayDataRceiptList";
    }
            
    tit_addCsvSearchActionInfo(qry, false);
    
	// 서버 호출 
	tit_callService(
         "",""
		,""	// inDs
		,"ds_oDATA=ds_oDATA"	// outDs
		,     "BONBU="    + quote(cbxBonbu.Value)
	    	+ " COURSE=" + quote(cbxCourse.Value)
            + " SDATE="  + quote(calStartDate.value)
            + " EDATE="  + quote(calEndDate.value)
		, "lfn_AfterSearch"
		, false
		, false  // 처리중 창 보여주기 (null or false : 보여줌) 
		, true); 
		   
}

function lfn_AfterSearch(nErrorCode, strErrorMsg) {
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);     
        course = "";
        return;
    }    
    
    // 조회결과 유무 체크 
    if (ds_oDATA.GetCount() > 0) {
		// 조회이력을 저장한다.  
		ls_HistoryArg = "출력물=" + quote(radPrtGubun.Value)
					+ ",소방본부=" + quote(cbxBonbu.Value)                
					+ ",수신대상=" + quote(cbxCourse.Value)
					+ ",수신일자1=" + quote(calStartDate.Value)
					+ ",수신일자2=" + quote(calEndDate.Value);  
	
		gfn_InsertFPISSearchPrintHistory(
			"P"
			, this.ID
			, global.mnuLeft.cbxOpenForm.Text
			, radPrtGubun.Text
			, ls_HistoryArg
		);
    }
}







