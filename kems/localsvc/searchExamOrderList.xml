	<member_var>
		<var id="Select_Sql" reserved="YES" type="Sql"><![CDATA[SELECT EO.EOMGNO AS EOMGNO  
           , EO.EOYEAR AS EOYEAR 
           , EO.EOEXAMORDER AS EOEXAMORDER
           , EO.EOEXAMDATE AS EOEXAMDATE  
           , TC.TCCOURSECD AS TCCOURSECD 
           , TC.TCCOURSENM AS EOTCCOURSENM 
           , GTC.GTDEPTNM AS JJIBUNICK
           , GTC.GTDEPTNM + "지부 " + TC.TCCOURSENM + " " + EO.EOYEAR + "년 " + EO.EOEXAMORDER + "회차" AS EXAMNM
   FROM EXAMORDER EO
           , TRAININGCOURSE TC
           , G_TEAMCODE GTC
 WHERE EO.EOTCCOURSECD = TC.TCCOURSECD    
      AND TC.TCMUNJEGUBUN = 'N' 
      AND TC.TCSEARCHGUBUN = 'E' 
      AND TC.TCUSEYN = 'Y'
      AND EO.EOMGGTMGNO = GTC.GTMGNO
]]></var>
	</member_var>
