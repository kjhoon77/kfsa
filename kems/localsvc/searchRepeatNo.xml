	<member_var>
		<var id="Select_Sql" reserved="YES" type="Sql"><![CDATA[SELECT APEOMGNO AS SSHEOMGNO
	, APREPEATNO AS SSHREPEATNO
	, 'N' AS SSHSENDYN
	, '평가 ' + APREPEATNO + '차' AS DATA
FROM ANSWERPAPER
WHERE APEOMGNO = :APEOMGNO
GROUP BY APEOMGNO, APREPEATNO
ORDER BY APREPEATNO]]></var>
	</member_var>
