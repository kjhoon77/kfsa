function lfn_Save(obj)
{
   //alert(ds_ioUser.savexml());

    tit_clearActionInfo();
    tit_addSaveActionInfo(
        "sys:insertIpccperson"	// insert
        , "sys:updateIpccperson"	// update
        , ""	// delete
        , ""	// flag column 명
        , ""	// key sql id
        , 0	// key 증가 값
        , "" 
        , 0 
        , "N"	// 실행타입
    );

	// 서버 호출 
	tit_callService(
		""
		,""
		,"ds_ioUser=ds_ioUser:U"	// inDs
		,""	// outDs
		,"" // args
		,"lfn_AfterSaverUser"
		,false
		
		,false
		
		,true
	);	
}

function lfn_AfterSaverUser(nErrorCode, strErrorMsg) {
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }
    gfn_SearchMessage(gdUser); // 조회 메세지를 처리하고 그리드의 정렬 헤드 마크를 제거한다.
    
    cbxJibu.Value = cbxJibu2.Value;
    
    edUser.Text = edNM.Text;
    
    lfn_Search();
}
