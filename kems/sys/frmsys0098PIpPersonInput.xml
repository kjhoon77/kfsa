function lfn_Search(obj) {
    ds_ioUser.ClearData(); // 데이타셋 초기화

    tit_clearActionInfo();
	tit_addSearchActionInfo("sys:searchIpccpersonList", false);
	// 서버 호출 
	tit_callService(
		""
		,""
		,""	// inDs
		,"ds_ioUser=ds_oUser"	// outDs
		,"DEPT=" + quote(cbxJibu.Value)
         	+ " USER=" + quote(edUser.Text)// args
		,"lfn_AfterSearchUser"
		,false
	);
}
