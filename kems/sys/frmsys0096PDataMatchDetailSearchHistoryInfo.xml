#include "LIB::sys.js"; // 공통업무 관련

// 파라미터 값
var FDSHBIZGUBUN = "";   // 대상구분
var FDSHSEARCHPK1 = "";  // 메인pk
var FDSHSEARCHPK2 = "";  // 위험물 운송자 
var FDSHSEARCHPK3 = "";  // 소기
var FDSHSEARCHPK4 = "";  // 소기
var FDSHSEARCHPK5 = "";  // 소기
var FDSHSEARCHPK6 = "";  // 소기

/* 공통 필수 시작 */
function Form_OnLoadCompleted(obj) {
    gfn_Form_OnLoadCompleted(obj); // 공통 폼 로딩 이벤트 처리

    // 폼 환경설정
    ls_SystemGubun = gs_SystemGubun;
    ls_SystemMenuId = gs_SystemMenuId;
    var ls_FormId = split(sFormUrl, "::");
    ls_FormId = split(ls_FormId[1], ".");
    //obj.Title = sFormTitle + "(" + ls_FormId[0] + ")";

    gfn_MakeRequiredInputMark(this); // 필수 입력란을 설정한다.

   
    ms_FormAuth = gfn_GetFormAuth(this); // 사용자정보을 조회한다.
    if (ms_FormAuth == "W") { // 사용자정보에 따른 버튼 설정
        lfn_SetFormAuth(true); // 저장 사용자정보
    } else {
        lfn_SetFormAuth(false); // 읽기 사용자정보
    }
    
    
    var adminGubun = gds_oUserInfo.GetColumn(0, "ADMINGUBUN");
    
    // 대상물 정보 검색 , 권한에 따라 보여주는 화면 변경 
    if (FDSHBIZGUBUN == "0") {
		// 탭설정 
		if (adminGubun == "SM") {
			TabMain.TabIndex = 0; 
			
		} else {
			TabMain.TabIndex = 4;
		}
		
		lbTitle = "소방대상물 정보";
		obj.Title = "소방대상물 정보";
		lfn_MFObjInfoSearch1();
		
    } else if (FDSHBIZGUBUN == "1") {
    
		// 탭설정 
		if (adminGubun == "SM") {
			TabMain.TabIndex = 1; 
		} else {
			TabMain.TabIndex = 5; 			
		}
		
		lbTitle = "제조소등 정보";
		obj.Title = "제조소등 정보";
		lfn_MFObjInfoSearch2();
		
		
		
    } else if (FDSHBIZGUBUN == "2") {
		lfn_MFObjInfoSearch3();
    } else if (FDSHBIZGUBUN == "3") {
		lfn_MFObjInfoSearch4();
    }
}

function Form_OnKeyDown(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam) {
