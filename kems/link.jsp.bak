<%@page contentType="text/html; charset=EUC-KR"%>
<% request.setCharacterEncoding("euc-kr"); %>
<%
	String initURL = (String) request.getParameter("url");
	String sabun = (String) request.getParameter("sabun");
	String jibu = (String) request.getParameter("jibu");
	String programid = (String) request.getParameter("programid");

%>
<html>
<head>
<title>Untitled Document</title>
<meta http-equiv="Content-Type" content="text/html; charset=euc-kr">
<SCRIPT LANGUAGE="JavaScript" src="./MiInstaller320.js"></SCRIPT>
<SCRIPT LANGUAGE="javascript">
	function fn_init()	{
		MiPlatformCtrl.Key      = "kems";
		MiPlatformCtrl.AutoSize = "false";
		MiPlatformCtrl.InitUrl  = "<%=initURL%>.xml";
		MiPlatformCtrl.startxml = "http://172.19.1.7:7001/kems/kems/src/kems/KEMS_POSETC_ci_main_Win32.xml";
		MiPlatformCtrl.DoRun();
	}
</SCRIPT> 
<SCRIPT LANGUAGE="javascript" for=MiPlatformCtrl event=NavigateComplete>
</SCRIPT>
<SCRIPT LANGUAGE=javascript FOR=MiPlatformCtrl EVENT=LoadCompleted(Disp,strReqID,strServiceID,strUrl)>

//	if ( strReqID == "TobeMain" ) //StartXml이 load된후
//	{
		MiPlatformCtrl.SetGlobalVariableValue( "gs_Sabun", "<%=sabun%>"); //global 변수에 값을 지정함.. 
		MiPlatformCtrl.SetGlobalVariableValue( "gs_JibuMgno", "<%=jibu%>"); //global 변수에 값을 지정함.. 
		MiPlatformCtrl.SetGlobalVariableValue( "gs_ProgramId", "<%=programid%>"); //global 변수에 값을 지정함.. 
//	}
</SCRIPT>
<SCRIPT LANGUAGE=javascript FOR=MiPlatformCtrl EVENT=UserNotify(nNotifyID,strMsg)>
</SCRIPT>
</head>

<body leftmargin="0" topmargin="0"  onload="fn_init()">
<table width="100%" height="100%" border="1" cellspacing="0" cellpadding="0">
	<tr>
		<td bgcolor="#FFFFFF" align="center">
			<SCRIPT LANGUAGE="javascript">callMiPlatform("MiPlatformCtrl");</SCRIPT>
		</td>
	</tr>
</table> 

</body>
</html>
