/*
 * 기능 : 조회
 */
function lfn_Search(obj) {

	
	if(cbChk.Value == 'N'){
		ds_oAgentStatList.ClearData(); // 데이타셋 초기화
	
		tit_clearActionInfo();
		tit_addCsvSearchActionInfo("cti:searchAgentStatList", false);
		// 서버 호출 
		tit_callService(
			""
			,""
			,""	// inDs
			,"ds_oAgentStatList=ds_oAgentStatList"	// outDs
			,"ASDATEFROM=" + quote(edCIDATEFrom.Value)
				+ " ASDATETO=" + quote(edCIDATETo.Value)
				+ " ASSABUN=" + quote(cbxUserList.Value)	// args 
			,""
			,false
		);
	}else{
	
		ds_oAgentStatSubList.ClearData(); // 데이타셋 초기화
	
		tit_clearActionInfo();
		tit_addCsvSearchActionInfo("cti:searchAgentStatSubList", false);
		// 서버 호출 
		tit_callService(
			""
			,""
			,""	// inDs
			,"ds_oAgentStatSubList=ds_oAgentStatSubList"	// outDs
			,"ASDATEFROM=" + quote(edCIDATEFrom.Value)
				+ " ASDATETO=" + quote(edCIDATETo.Value)
				+ " ASSABUN=" + quote(cbxUserList.Value)	// args 
			,""
			,false
		);
	}
	
}
/* 공통 필수 종료 */
 
  

function cbChk_OnClick(obj,strValue)
{
	if(strValue == "Y"){
		gdListSub.Visible = true;
		gdList.Visible = false;
	}else{
		gdList.Visible = true;
		gdListSub.Visible = false;
	}
	lfn_Search();
}
