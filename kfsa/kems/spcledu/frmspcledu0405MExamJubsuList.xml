heck        return false;    }    ds_oExamJubsu.ClearData(); // 데이타셋 초기화        // 조회이력을 저장한다.      ls_HistoryArg = "지부=" + quote(cbxJibu.Text)                + ",직능=" + quote(cbxCourse.Text)                + ",년도=" + quote(edExamYear.Text)                + ",회차시작=" + quote(edExamOrderStart.Text)                + ",회차종료=" + quote(edExamOrderEnd.Text)                + ",환불여부=" + quote(radRepayYn.Text)                + ",합격여부=" + quote(radPassYn.Text);    gfn_InsertSearchPrintHistory(        "S"        , this.ID        , global.mnuLeft.cbxOpenForm.Text        , ls_HistoryArg    );        medJubsuCnt.Value = "";    tit_clearActionInfo();	tit_addCsvSearchActionInfo("spcledu:searchExamJubsuStat", false);	// 서버 호출 	tit_callService(		""		,""		,""	// inDs		,"ds_oExamJubsu=ds_oExamJubsu"	// outDs		,"EOMGGTMGNO=" + quote(cbxJibu.Value)            + " EOTCCOURSECD=" + quote(cbxCourse.Value)            + " EOYEAR=" + quote(edExamYear.Text)            + " EOEXAMORDERSTART=" + quote(edExamOrderStart.Text)            + " EOEXAMORDEREND=" + quote(edExamOrderEnd.Text)            + " EJLASTSUNAPGUBUN=" + quote(radRepayYn.Value)            + " EJPASSYN=" + quote(radPassYn.Value)	// args 		,"lfn_AfterSearchExamJubsu"		,false	);}/* * 기능 : 접수자 목록출력 */function lfn_Print(obj) {    // to do Validation Check    if (ds_oExamJubsu.RowCount() < 1) {        gfn_Alert("출력할 데이터가 없습니다.");        return false;    }    // 출력이력을 저장한다.      ls_HistoryArg = "지부=" + quote(cbxJibu.Text)                + ",직능=" + quote(cbxCourse.Text)                + ",년도=" + quote(edExamYear.Text)                + ",회차시작=" + quote(edExamOrderStart.Text)                + ",회차종료=" + quote(edExamOrderEnd.Text)                + ",환불여부=" + quote(radRepayYn.Text)                + ",합격여부=" + quote(radPassYn.Text);    gfn_InsertSearchPrintHistory(        "P"        , this.ID        , "접수자목록출력"        , ls