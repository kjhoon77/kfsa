 */
function tabTab1_tab1_btnIncomepaymentPrint_OnClick(obj)
{
	if("" == tabTab1.tab1.edOimgno.Text) {
		alert("지급조서가 선택되지 않았습니다.");
		return;
	} else if("폐기" == tabTab1.tab1.edStatus.Text) {
		alert("폐기된 지급조서는 출력할 수 없습니다.");
		return;
	}
	
	ds_ioIncomePaymentMain1.ClearData(); // 데이타셋 초기화
	ds_ioIncomePaymentMain2.ClearData(); // 데이타셋 초기화
	
	ds_ioIncomePaymentSub1.ClearData(); // 데이타셋 초기화
	ds_ioIncomePaymentSub2.ClearData(); // 데이타셋 초기화
	
    tit_clearActionInfo();
	if("0" == tabTab1.tab1.rdOibizgubun.Value) {
		tit_addSearchActionInfo("otherincome:searchIncomepaymentMain1", false); // 기타소득
		
		// 서버 호출 		tit_callService(			""			,""			,""	// inDs			,"ds_ioIncomePaymentMain1=ds_ioIncomePaymentMain1" // outDs			,"OIMGNO1=" + quote(tabTab1.tab1.edOimgno.Text)    // args
			,"lfn_AfterSearchIncomepaymentMain"			,false		);
	} else {
		tit_addSearchActionInfo("otherincome:searchIncomepaymentMain2", false); // 사업소득
		
		// 서버 호출 		tit_callService(			""			,""			,""	// inDs			,"ds_ioIncomePaymentMain2=ds_ioIncomePaymentMain2" // outDs			,"OIMGNO1=" + quote(tabTab1.tab1.edOimgno.Text)    // args
			,"lfn_AfterSearchIncomepaymentMain"			,false		);
	}
}

function lfn_AfterSearchIncomepaymentMain(nErrorCode, strErrorMsg) {
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }
    
    if (ds_ioIncomePaymentMain1.GetCount() > 0) {
		ds_ioIncomePaymentSub1.ClearData(); // 데이타셋 초기화
		
