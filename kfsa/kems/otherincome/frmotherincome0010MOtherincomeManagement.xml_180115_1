function tabTab1_tab1_rdseljibu_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{
	var fjibu = substr(gds_oUserInfo.GetColumn(0, "PDEPTCD"),1,2);
	
	if("0" == tabTab1.tab1.rdpaymentstatus.Value) {
	
		if(0 == strCode) {
			ds_ioOtherIncomePayment.FilterExpr = "";
		} else if(1 == strCode) {
		
			var soijibu = "";
			jibuArray = Array(14);
		
			for(var i=0;i<=14;i++) {
				if(i < 10) {
					soijibu = "0" + i;
				} else {
					soijibu = i;
				}
				jibuArray[i] = soijibu;
			}
			
			if(-1 == IndexOf(jibuArray, fjibu)) {
				ds_ioOtherIncomePayment.FilterExpr = "substr(OIJIBU,3,2) == '00'";
			} else {
				ds_ioOtherIncomePayment.FilterExpr = "substr(OIJIBU,3,2) == '" + fjibu + "'";
