 */
function tabTab1_tab1_gdOtherIncome_OnCellClick(obj,nRow,nCell,nX,nY,nPivotIndex)
{
	ds_ioOtherIncomePayment_OnRowPosChanged(ds_ioOtherIncomePayment,nRow,nRow);
}

function ds_ioOtherIncomePayment_OnRowPosChanged(obj,nOldRow,nRow)
{
    // 그리드의 값을 입력란에 복사한다. 추가상태인 경우 복사 후 저장을 하기 위해 제외한다.
    if (nRow <> -1) {
		if(false == tabTab1.tab1.btnIncomepaymentNew.Enable) {
			tabTab1.tab1.btnIncomepaymentNew.Enable = true;
		}
		if(true == tabTab1.tab1.btnIncomepaymentSave.Enable) {
			tabTab1.tab1.btnIncomepaymentSave.Enable = false;
		}
		if(false == tabTab1.tab1.btnIncomepaymentPrint.Enable) {
			tabTab1.tab1.btnIncomepaymentPrint.Enable = true;
		}
		if(false == tabTab1.tab1.btnIncomepaymentFall.Enable) {
			tabTab1.tab1.btnIncomepaymentFall.Enable = true;
		}
		if(false == tabTab1.tab1.btnIncomepaymentDel.Enable) {
			tabTab1.tab1.btnIncomepaymentDel.Enable = true;
		}
    
        tabTab1.tab1.edOimgno.Text = obj.GetColumn(nRow, "OIMGNO"); // 관리번호
        tabTab1.tab1.edOiyear.Text = obj.GetColumn(nRow, "OIYEAR"); // 관리년도
        tabTab1.tab1.rdOibizgubun.Value = obj.GetColumn(nRow,"OIBIZGUBUN"); // 사업구분 0(기타소득), 1(사업소득)
        tabTab1.tab1.edJibu.Text = obj.GetColumn(nRow,"JIBU"); // 지부
        tabTab1.tab1.edOiprtno.Text = obj.GetColumn(nRow,"OIPRTNO"); // 발행순번
        tabTab1.tab1.edStatus.Text = obj.GetColumn(nRow,"STATUS"); // 지급조서상태
        //tabTab1.tab1.edOipaymentdate.Text = obj.GetColumn(nRow,"OIPAYMENTDATE"); // 지급일자
        tabTab1.tab1.edOijibu.Text = obj.GetColumn(nRow,"OIJIBU"); // 지부코드
        tabTab1.tab1.medPaymentdate.Text = obj.GetColumn(nRow,"PAYMENTDATE"); // 지급일자
        tabTab1.tab1.medRevertmonth.Text = obj.GetColumn(nRow,"REVERTMONTH"); // 귀속년월
        
        // 소득코드 변경
        if(0 == tabTab1.tab1.rdOibizgubun.Value) {
			tabTab1.tab1.cbxOiincomecd.InnerDataset = "ds_ioIncomecdG";
        } else {
			tabTab1.tab1.cbxOiincomecd.InnerDataset = "ds_ioIncomecdS";
        }
        tabTab1.tab1.cbxOiincomecd.Value = obj.GetColumn(nRow,"OIINCOMECD"); // 소득코드
        
        tabTab1.tab1.medOipayment.Text = obj.GetColumn(nRow,"OIPAYMENT"); // 지급총액
        tabTab1.tab1.medOiototal.Text = obj.GetColumn(nRow,"OIOTOTAL"); // 원천세총계
        tabTab1.tab1.edOiremark.Text = obj.GetColumn(nRow,"OIREMARK"); // 비고
        
        // 산출내역
