			ds_ioOtherIncomePayment.FilterExpr = "STATUS='정상'";
		} else if(1 == strCode) {
		
			var soijibu = "";
			jibuArray = Array(14);
		
			for(var i=0;i<=14;i++) {
				if(i < 10) {
					soijibu = "0" + i;
				} else {
					soijibu = i;
				}
				jibuArray[i] = soijibu;
			}
			
			if(-1 == IndexOf(jibuArray, fjibu)) {
				ds_ioOtherIncomePayment.FilterExpr = "STATUS='정상' && substr(OIJIBU,3,2) == '00'";
			} else {
				ds_ioOtherIncomePayment.FilterExpr = "STATUS='정상' && substr(OIJIBU,3,2) == '" + fjibu + "'";
			}
		}
		
	} else if("2" == tabTab1.tab1.rdpaymentstatus.Value) {

		if(0 == strCode) {
			ds_ioOtherIncomePayment.FilterExpr = "STATUS='폐기'";
		} else if(1 == strCode) {
		
			var soijibu = "";
			jibuArray = Array(14);
		
			for(var i=0;i<=14;i++) {
				if(i < 10) {
					soijibu = "0" + i;
				} else {
					soijibu = i;
				}
				jibuArray[i] = soijibu;
			}
			
			if(-1 == IndexOf(jibuArray, fjibu)) {
				ds_ioOtherIncomePayment.FilterExpr = "STATUS='폐기' && substr(OIJIBU,3,2) == '00'";
			} else {
				ds_ioOtherIncomePayment.FilterExpr = "STATUS='폐기' && substr(OIJIBU,3,2) == '" + fjibu + "'";
			}
		}
		
	}
}

