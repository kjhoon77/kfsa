	<Script><![CDATA[/*
 * 프로그램명 : frmcust0018PManagerList.xml
 * 설명 : 선임자 목록
 * 작성일 : 2009.12.16
 * 작성자 : 이규희
 */

#include "LIB::cust.js"; // 공통업무 관련

var ls_PERSONKEY = "";	// 개인식별번호
var ls_DI = ""; 

/* 공통 필수 시작 */
function Form_OnLoadCompleted(obj) {
    gfn_Form_OnLoadCompleted(obj); // 공통 폼 로딩 이벤트 처리

    
    if (IsExistVar("sManagerNm") || IsExistVar("sResidentNo")) {
        if (IsExistVar("sFlag")) {
            lbNotice.Visible = true;
            btnSearch.Enable = false;
            btnNew.Enable = false;
        }
        edManagerNM.Text = sManagerNm;
        medResidentNo.Value = sResidentNo;
        //radJibu.Value = gds_oUserInfo.GetColumn(0, "PDEPTCD");
        lfn_Search();
        edManagerNM.SetFocus();
    }
    
}

function frmcust0018PManagerList_OnKeyDown(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam)
{
	if (bAlt) {
        ms_BarHotKeyCount++;
        if (ms_BarHotKeyCount == 2) {
            ms_BarHotKeyCount = 0;
            medBarCode.SetFocus();
        }
    } else {
        ms_BarHotKeyCount = 0;

        switch (nChar) {
            case 113 : // 조회(F2)
                if (obj.btnSearch.Enable) {
                    obj.lfn_Search();
                }
                break;
            case 114 : // 신규추가(F3)
                if (obj.btnInput.Enable) {
                    obj.btnNew_OnClick();
                }
                break;
            case 27 : // 종료(Esc)
                if (obj.btnEnd.Enable) {
                    Close();
                }
                break;
        }
    }
}

function Form_OnUnloadCompleted(obj) {
    Close();
}

/*
 * 기능 : 종료
 */
function lfn_End(obj) {
    Close();
}

/*
 * 기능 : 조회
 */
function lfn_Search(obj) {
    if (gfn_IsNull(edManagerNM.Text) && gfn_IsNull(medResidentNo.Value)) {
        gfn_Alert("성명이나 주민번호(생년월일) 중 하나를 입력하십시오.");
        return;
    }
    

    if (!gfn_IsNull(edManagerNM.Text) && !gfn_IsNull(medResidentNo.Value)) {	// 성명&주민번호 입력한 경우
		if (length(medResidentNo.Value) == 13) {	// 주민번호가 13자리이면
			ls_PERSONKEY = gfn_SearchPERSONKEY(edManagerNM.Text, medResidentNo.Value, edHPTel.Text);
			ls_DI = gds_ioSciCheck.GetColumn(0, "STRDUP");
		} else {
			medResidentNo.SetFocus();
		}
	}
	
 	if ((length(medResidentNo.Value) == 13) && (gfn_IsNull(ls_DI))) {
		gfn_Alert("실명인증 실패로 주민번호대체키 값을 수신받지 못했습니다.\n\n" +
		          "다시 조회해 주세요!\n\n" +
		          " * 실패사유 : " + gds_ioSciCheck.GetColumn(0, "STRCI2"));
		ds_oManagerChange.ClearData();
		ds_oManagerList.ClearData();
		ds_oLicenseList.ClearData();
		medResidentNo.SetFocus();
		return false;
 	}
    
    
    ds_oManagerChange.ClearData(); // 데이타셋 초기화
	ds_oLicenseList.ClearData(); // 데이타셋 초기화
	
    tit_clearActionInfo();
	tit_addCsvSearchActionInfo("cust:searchCust0018ManagerChange", false);
	tit_addCsvSearchActionInfo("cust:searchCust0018LicenseList", false);
	// 서버 호출 
	tit_callService(
		""
		,""
		,""	// inDs
		,"ds_oManagerChange=ds_oManagerChange ds_oLicenseList=ds_oLicenseList"	// outDs
		,"FMNM=" + quote(edManagerNM.Text)            
		 + " BIRTHDAY=" + quote(left(medResidentNo.Value,6))
		 + " HP=" + quote(edHPTel.Text)
		 + " PERSONKEY=" + quote(ls_PERSONKEY)
         + " GUBUN=" + quote(radJibu.Value)
         + " JIBU=" + quote(gds_oUserInfo.GetColumn(0, "PDEPTCD"))
		 + " TPNM=" + quote(edManagerNM.Text)
		 + " TPBIRTHDAY=" + quote(left(medResidentNo.Value,6))
		 + " TPPERSONKEY=" + quote(ls_PERSONKEY)	// args
		,"lfn_AfterSearchManagerList"
		,false
		,""
		,true
	);
}

/* 공통 필수 종료 */

/*
 * 기능 : 공통코드 조회 후 처리해야 할 내용
 * parameter : nErrorCode - 에러코드
 *             strErrorMsg - 에러메시지
 * Return : 없음
 */
function lfn_AfterSearchManagerList(nErrorCode, strErrorMsg) {
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }
    //trace(ds_oManagerChange.SaveXML());

    if (ds_oManagerChange.GetCount() == 1) {
        var arrArg = Array(2);
        arrArg[0] = ds_oManagerChange.GetColumn(ds_oManagerChange.currow,  "FMMGNO");		        // 대상물관리번호     
        arrArg[1] = ds_oManagerChange.GetColumn(ds_oManagerChange.currow,  "FMLASTFMHSEQ");		        // 최종선임이력순번        
    }

    gfn_SearchMessage(gdManagerList); // 조회 메세지를 처리하고 그리드의 정렬 헤드 마크를 제거한다.
}

