{    
	if (nRow >= 0) {
        Static3.Text = ds_oManagerChange.GetColumn(nRow, "FMNM")
                       + "(" + ds_oManagerChange.GetColumn(nRow, "BIRTHDAY") + ")"
                       + " 기존 선임정보";
        lfn_ds_oManagerListSearch(nRow);
	}	
}


function lfn_ds_oManagerListSearch (nRow) {
    ds_oManagerList.ClearData(); // 데이타셋 초기화
    tit_clearActionInfo();
	tit_addCsvSearchActionInfo("cust:searchCust0011ManagerList", false);
	// 서버 호출 
	tit_callService(
		""
		,""
		,""	// inDs
		,"ds_oManagerList=ds_oManagerList"	// outDs
		,"FMNM=" + quote(ds_oManagerChange.GetColumn(nRow, "FMNM"))
		 + " BIRTHDAY=" + quote(ds_oManagerChange.GetColumn(nRow, "FMBIRTHDAY"))
		 + " PERSONKEY=" + quote(ds_oManagerChange.GetColumn(nRow, "FMPERSONKEY"))	// args
		 + " CHK=0"
		,"lfn_AfterSearchManager"
		,false
	);
}

// 선임현황 조회후 처리
function lfn_AfterSearchManager(nErrorCode, strErrorMsg) {
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
