function btnMutilSort_OnClick(obj)
{
	gfn_GridMultiSort(gdEdu, 1); // 멀티 정렬을 실행한다.
}

function gdEdu_OnHeadClick(obj,nCell,nX,nY,nPivotIndex)
{
	var lobj_Dataset = object(obj.BindDataset);
	if (nCell == 11) { // 선택
        if (obj.GetCellProp("head", nCell, "text") == 1) { // 전체가 이미 선택된 경우
            obj.SetCellProp("head", nCell, "text", 0);
            
            for(i = 0; i < lobj_Dataset.RowCount(); i++) {
                lobj_Dataset.SetColumn(i, "SEL1", "0");               
            }
        } else { // 전체가 이미 선택되지 않은 경우
            obj.SetCellProp("head", nCell, "text", 1);
            
            for(i = 0; i < lobj_Dataset.RowCount(); i++) {
                lobj_Dataset.SetColumn(i, "SEL1", "1");
                
            }
        }
	} else if (nCell >= 1) {  // 헤드 정렬
        lfn_SortGrid_OnHeadClick(obj,nCell,nX,nY,nPivotIndex);
	}
}

function ds_ioEduPassY_OnColumnChanged(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{
	var cnt = 0; // 결강자수
	var cnt2 = 0; // 결강자수
	for(i = 0; i < ds_ioEduPassY.RowCount(); i++) {
        if (ds_ioEduPassY.GetColumn(i, "SEL1") == 1) {
            cnt += 1; // 시험접수인원 카운트
        } else {
            cnt2+= 1; // 미접수인원
        }
    }
    medPassCntYet.Value = cnt;
    medNoPassCntYet.Value = cnt2;
}

