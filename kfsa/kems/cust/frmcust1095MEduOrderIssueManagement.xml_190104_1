    medNoPassCntYet.Value = "";
    
    if (!gfn_FormValidate(this)) { // 입력 Validation Check
        return false;
    }
    ds_ioEduPassY.ClearData(); // 데이타셋 초기화
    ds_ioEduPassYCnt.ClearData(); // 데이터셋 초기화
    ds_ioEduPassN.Clear(); // 결강자조회 데이터셋 초기화
    
    ls_HistoryArg = "지부=" + quote(cbxJibu.Text)
            + ",직능=" + quote(cbxCourse.Text)
            + ",년도=" + quote(edEduYear.Text)
            + ",회차=" + quote(edEduOrder.Text);
  
    gfn_InsertSearchPrintHistory(
        "S"
        , this.ID
        , global.mnuLeft.cbxOpenForm.Text
        , ls_HistoryArg
    ); 
    
    tit_clearActionInfo();
	tit_addCsvSearchActionInfo("cust:searchCust1095EduPassY", false);// 미이수 인원 조회
	
	// 서버 호출 
	tit_callService( 
		""
		,""
		,""	// inDs
		,"ds_ioEduPassY=ds_oEduPassY"	// outDs
	   
	    ,"ESGTMGNO=" + quote(cbxJibu.value)   //-- 지부
            +" ESYEAR=" + quote(edEduYear.text)  //-- 년도
            +" ESCOUSECD=" + quote(cbxCourse.value) // -- 직능            
            +" ESEDUORDER=" + quote(edEduOrder.text) // -- 이수회차 // args 
		,"lfn_AftersearchEduPassY"
		,false
	);
	
	tit_clearActionInfo();
	tit_addCsvSearchActionInfo("cust:searchCust1095EduPassYCnt", false); // 이수인원 찾는 트랜잭션 2)	
	// 서버 호출 
	tit_callService( 
		""
		,""
		,""	// inDs
		,"ds_ioEduPassYCnt=ds_oEduPassYCnt"	// outDs
	   
	    ,"ESGTMGNO=" + quote(cbxJibu.value)   //-- 지부
            +" ESYEAR=" + quote(edEduYear.text)  //-- 년도
            +" ESCOUSECD=" + quote(cbxCourse.value) // -- 직능            
            +" ESEDUORDER=" + quote(edEduOrder.text) // -- 이수회차	// args 
		,"lfn_AftersearchEduPassY"
		,false
	);
	
}


