    { 
    
		gfn_alert("이수 처리대상자를 선택해 주십시오.");      
        return false;
    }        
    if (!gfn_FormValidate(this)) { // 입력 Validation Check
		return false;
	}/*
	// 교보재관리와 연동되어 교재수불내역을 입력했는지 여부
	if (ds_ioEduPassY.GetColumn(0, "ITEMINPUTCNT") == '0') {
	
		gfn_Alert("교재수불내역이 입력되지 않았습니다. \n\n"
		         +"교보재관리 > 교재수불관리에서 ''실무교육일정 연계''를 하고\n\n"
		         +"교재 수불내역을 입력하신 후 이수처리를 진행하십시오!");
		
        return false;
	
	}
*/

    if(medEduDate.Value > left(GetDate(), 8)) {
    
		gfn_alert("교육일 이전에 이수처리를 할 수 없습니다.");  
    
		return false;
    } 


    if(!Confirm("선택한 인원에 대해 이수처리를 하시겠습니까?\n\n" +
                " * 유의사항 : 개인식별번호가 없는자료는 이수처리 되지 않습니다.")) {  //선택한것이 있는지 체크 메세지 보임
        return false;
    }

    lfn_Save_Pass();    // 이수처리
	lfn_SaveModifyHistory("U"); // 변경이력을 저장한다.
}	
function lfn_Save_Pass() {

var iDsList = "";	//input Dataset Listvar j = 0;


	ds_iEduPass.ClearData();
	// 선택된 대상만 이수처리
    for (var i=0; i<ds_ioEduPassY.GetRowCount(); i++) {

		if (ds_ioEduPassY.GetColumn(i, "SEL1") == 1) {
			if (gfn_isNull(ds_ioEduPassY.GetColumn(i, "EJPERSONKEY")) ||
				(ds_ioEduPassY.GetColumn(i, "EJPERSONKEY") == "NON-MATCH")) {
				gfn_Alert("개인식별번호가 없는자료는 이수처리 되지 않습니다.\n\n" +
				          "  * 접수번호 : " + ds_ioEduPassY.GetColumn(i, "EJHJUBSUNO") + "\n" +
				          "  * 성명 : " + ds_ioEduPassY.GetColumn(i, "EJNM"));
			} else {	// 개인식별번호 있는 자료만 이수처리 함
		
				ds_iEduPass.AddRow();
				ds_iEduPass.SetColumn(j, "EPCMGNO", ds_ioEduPassY.GetColumn(i, "EJCMGNO")); // 고객정보관리번호
				ds_iEduPass.SetColumn(j, "EPESMGNO", ds_ioEduPassY.GetColumn(i, "ESMGNO")); // 실무교육일정관리번호
				ds_iEduPass.SetColumn(j, "EPYEAR", ds_ioEduPassY.GetColumn(i, "ESYEAR")); // 교육대상년도
				ds_iEduPass.SetColumn(j, "EPCOURSECD", ds_ioEduPassY.GetColumn(i, "CCOURSECD")); // 세부직능
				ds_iEduPass.SetColumn(j, "EPSUCCESSIONGUBUN", "0"); // 승계구분[0:선임자교육 1:교육승계]
				ds_iEduPass.SetColumn(j, "EPPASSDATE", ds_ioEduPassY.GetColumn(i, "ESDATE")); // 교육일
				ds_iEduPass.SetColumn(j, "EPPERSONNM", ds_ioEduPassY.GetColumn(i, "EJNM")); // 이수자성명
				ds_iEduPass.SetColumn(j, "EPBIRTHDAY", ds_ioEduPassY.GetColumn(i, "EJBIRTHDAY")); // 이수자생년월일
				ds_iEduPass.SetColumn(j, "EPPERSONKEY", ds_ioEduPassY.GetColumn(i, "EJPERSONKEY")); // 이수자 개인식별번호
				ds_iEduPass.SetColumn(j, "EPPROCGTMGNO", gds_oUserInfo.GetColumn(0, "PDEPTCD")); // 처리지부
				ds_iEduPass.SetColumn(j, "EPCGATEWAYCD", ds_ioEduPassY.GetColumn(i, "CGATEWAYCD")); // 데이터연계직능
				ds_iEduPass.SetColumn(j, "FMMGNO", ds_ioEduPassY.GetColumn(i, "FMMGNO")); // 선임자관리번호
				ds_iEduPass.SetColumn(j, "EPPASSREMARK", ds_ioEduPassY.GetColumn(i, "EPPASSREMARK")); // 교육 중복이수 사유
				//ds_iEduPass.SetColumn(j, "CRECEIVEEXCEPTGUBUN", ds_ioEduPassY.GetColumn(i, "SEL2")); // 회지수령
				ds_iEduPass.SetColumn(j, "SCMGNO", ds_ioEduPassY.GetColumn(i, "EJCMGNO")); // 고객정보관리번호
				ds_iEduPass.SetColumn(j, "FSTARTDATE", ds_ioEduPassY.GetColumn(i, "FMHSTARTDATE")); // 선임일자
				ds_iEduPass.SetColumn(j, "FNM", ds_ioEduPassY.GetColumn(i, "EJNM")); // 성명
				ds_iEduPass.SetColumn(j, "FPERSONKEY", ds_ioEduPassY.GetColumn(i, "EJPERSONKEY")); // 개인식별번호
				ds_iEduPass.SetColumn(j, "CYBERISUGUBUN", ds_ioEduPassY.GetColumn(i, "CYBERISUGUBUN")); // 사이버교육이수여부
				
				if (ds_ioEduPassY.GetColumn(i, "EDUPERSONYN") == "Y" && gfn_isNull(ds_ioEduPassY.GetColumn(i, "EPPASSREMARK"))){
					
					gfn_Alert("타대상 이수내역이 있습니다. 중복이수처리 사유를 입력하십시오!");
					
					ds_ioEduPassY.row = i;
					
					return false;
				}
				
				j = j + 1;
				
			}
		}
		
	}

    tit_clearActionInfo();
	if (ds_iEduPass.GetCount() > 1) {
		tit_AddMultiActionInfo("cust:insertCust1090EduPassInfo"	, "", 0, "", "N");	
	} else {
		tit_AddSingleActionInfo("cust:insertCust1090EduPassInfo" , "", 0, "", "N");	
	}
	inDsList = "ds_iCustData=ds_iEduPass:U";

    if (cbxCourse.Value == "50") { // 소방기술자일 경우 멀티처리
		if (ds_iEduPass.GetCount() > 1) {
			tit_AddMultiActionInfo("cust:executeCust1095EduPassMultiInfo", "", 0, "", "N");	
		} else {		
    		tit_AddSingleActionInfo("cust:executeCust1095EduPassMultiInfo", "", 0, "", "N");
    	}
		inDsList += " ds_iEduPass=ds_iEduPass"; 
	}

		// 소방안전관리(보조)자 실무교육 유효기간 업데이트
	if (cbxCourse.Value == "10" || cbxCourse.Value == "20") {
		tit_AddMultiActionInfo("cust:executeCust0010EduExpireDateUpdate", "", 0, "");	
		
		inDsList += " ds_iEduPass=ds_iEduPass"; 
	}
    
    // 서버 호출 
   tit_callService(
        ""
        ,""
        ,inDsList	/// inDs
        ,""	// outDs
        ,"PSABUN=" + gds_oUserInfo.GetColumn(0, "PSABUN") // -- 사번 "   // args 
        ,"lfn_AfterSaveEdu"
        ,false
    );	

}


/* 공통 필수 종료 */

/* 사용자 함수 */
/*
 * 기능 : 시험자 조회 후 처리해야 할 내용
 * parameter : nErrorCode - 에러코드
 *             strErrorMsg - 에러메시지
 * Return : 없음
 */
function lfn_AftersearchEduPassY(nErrorCode, strErrorMsg) {
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }
   
    gfn_SearchMessage(gdEdu); // 조회 메세지를 처리하고 그리드의 정렬 헤드 마크를 제거한다.
    medEduDate.Text = ds_ioEduPassY.GetColumn(0, "ESDATE");  //해당년도 직능 회차의  교육일 표현
    
    medEduStartTime.Text = ds_ioEduPassY.GetColumn(0, "ESSTARTTIME");  //해당년도 직능 회차의 교육시작시간
    
    medEduEndTime.Text = ds_ioEduPassY.GetColumn(0, "ESENDTIME");  //해당년도 직능 회차의  교육종료시간
    
	edEduGubunNm.Text = ds_ioEduPassY.GetColumn(0, "ESGUBUNNM");  //해당년도 직능 회차의  교육구분
	calPassDate.Text = ds_ioEduPassY.GetColumn(0, "ESDATE");     //해당년도 직능 회차의  교육일 표현
	medPassCnt.Text = ds_ioEduPassYCnt.GetColumn(0, "PASSSUM");  // 이수인원을 표현
	
	ds_ioEduPassY_OnColumnChanged(ds_ioEduPassY);	// 이수,미이수예정인원
}

/*
 * 기능 : Transaction 서비스 호출 후 처리해야 할 내용
 * parameter : strSvcID - 서비스ID
 *             nErrorCode - 에러코드
 *             strErrorMsg - 에러메시지
 * Return : 없음
 */
function lfn_AfterSaveEdu(nErrorCode, strErrorMsg) {
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }
    gfn_SaveMessage(); // 저장 메세지를 처리한다.
    lfn_Search(); // 조회
   
}

