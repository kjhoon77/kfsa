#include "LIB::cust.js"; // 공통업무 관련

function btnCancelReason_OnClick(obj)
{
	if (edCancelReason.Text == "" || edCancelReason.Text == null) {
		alert("연계해지 사유를 입력해 주세요.");
		return;
	}
	Close(edCancelReason.Text);
}
function edKeyWord_OnKeyDown(obj,nChar,bShift,bCtrl, bAlt,LLParam,HLParam) {
    if (nChar == 13) { // Enter Key
        btnCancelReason_OnClick();
    }
}

