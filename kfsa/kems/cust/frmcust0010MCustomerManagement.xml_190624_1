			tit_addSingleActionInfo("cust:executeCust0010VirtualAccountOwnerNM", "", 0, "");
		}
		
	}
	
	if(lsCustomerStatus == "insert") {
		ds_ioCustomerInfo.SetColumn(0, "CMAINCMGNO", ls_MainCmgno);	// 보조자의 메인관리자 CMGNO
	}
	//trace("iDsList :: "+iDsList);	//개발완료 후 삭제 요망
	//trace("oDsList :: "+oDsList);	//개발완료 후 삭제 요망
	//trace("iArgsNm :: "+iArgsNm);	//개발완료 후 삭제 요망

//	return;
		
	// 서버 호출 	//trace(ds_ioBuildingInfo.savexml());
	tit_callService(
		""
		,""
		,iDsList	// inDs
		,oDsList	// outDs
		,iArgsNm	// args
		,"lfn_AfterSaveCaseCombination"
		,false
	);
}

/*
