/*
 * 조회 : MAIN (관리번호에 의한 조회)
 *        고객 정보(MAIN 1) - 관리번호
 */
function lfn_searchCust0010CustomerInfo5(sJibu, sCNo, sCNoBeforeYn, sCNoNextYn, sRegCD, SCourseCD) {
	
	if(gfn_IsNull(sJibu) || gfn_IsNull(sCNo)) {	//그룹조회
		gfn_Alert("잘못된 조회입니다.");
		return;
	}
	var sArg = "CNO="	+ quote(sCNo);		// 관리번호
    sArg += " CGTMGNO="	+ quote(sJibu);		        // 지부
    sArg += " CNOBEFOREYN="	+ quote(sCNoBeforeYn);    // 이전번호유무
    
    sArg += " CNONEXTYN="	+ quote(sCNoNextYn);    // 다음번호유무
    
    sArg += " CREGCD="	+ quote(sRegCD);    // 지역
    
    sArg += " CCOURSECD="	+ quote(SCourseCD);    // 직능
    

	lfn_ClearData();	//데이타셋 초기화(모두)
    // 조회이력을 저장한다.  
    ls_HistoryArg = "지부=" + cbxJibu.Text
                + ", 지역=" + cbxRegion.Text
                + ", 직능=" + cbxDetailCourse.Text
                + ", 관리번호=" + medCustNo.Text;
    if (sCNoBeforeYn == "Y") {
		ls_HistoryArg += " 의 이전번호 조회";
    } else if (sCNoNextYn == "Y") {
		ls_HistoryArg += " 의 다음번호 조회";
	}
                
    gfn_InsertSearchPrintHistory(
        "S"
        , this.ID
        , global.mnuLeft.cbxOpenForm.Text
        , ls_HistoryArg
    );
    
	// 콜센터 직원용 이면
	if (gs_IsCallCenter == "Y") {
		// 콜센터 직원 조회이력 저장
		gfn_InsertCallCenterSearchPrintHistory(
				"S"
				, gs_CallCenterUID				, gds_OpenedForm.GetColumn(gds_OpenedForm.FindRow("MURL",this.ID),"MMENUID")
				, this.ID				, global.mnuLeft.cbxOpenForm.Text				, ls_HistoryArg
			);
	}	if (!gfn_isNull(sJibu) && !gfn_isNull(sRegCD) && !gfn_isNull(SCourseCD) && !gfn_isNull(sCNo)) {
		lfn_SearchAllInfo(sArg);
	}
}


