				sArg = "sCMgno=" + quote(ds_oAllObject.GetColumn(0, "CMGNO"));
				sArg += " sIFCMGNO=" + quote(ds_oINetChangeInfo.GetColumn(0, "IFCMGNO"));
				sArg += " sIFCCHANGEDATE=" + quote(ds_oINetChangeInfo.GetColumn(0, "IFCCHANGEDATE"));
				sArg += " sFMNM=" + quote(ds_oAllObject.GetColumn(0, "FMNM"));
				sArg += " sBIRTHDAY=" + quote(ds_oAllObject.GetColumn(0, "FMBIRTHDAY"));
				sArg += " sPERSONKEY=" + quote(ds_oAllObject.GetColumn(0, "FMPERSONKEY"));
	
				vRtl = Dialog("cust::frmcust0010PFireManagerRestore.xml", sArg, 400, 220, "", -1, -1);	
			
				if (vRtl == "SUCC") {
				// 자료 조회
					lfn_Search();
				}
				
			} else if(ds_oINetChangeInfo.GetColumn(0, "IFCGUBUN") == "2") {	// 해임 처리이면
			
				//
			
			} else if(ds_oINetChangeInfo.GetColumn(0, "IFCGUBUN") == "3") {	// 비회원=>회원 변경이면
			
				if(!gfn_Confirm("비회원으로 복구하시겠습니까?")) {						return;				}
			
				ds_oRestoreStatus.ClearData();
				
				tit_clearActionInfo();
				tit_addSingleActionInfo("cust:executeCust0010FireManagerRestore", "", 0, "");
				
				// 서버 호출
				tit_callService(
					""
					,""
					,""    // inDs
					,"ds_oRestoreStatus=ds_oRestoreStatus"	// outDs
					,"sIFCMGNO=" + quote(ds_oINetChangeInfo.GetColumn(0, "IFCMGNO"))	// args
					,"lfn_AfterRestore"
					,false
					,false
					,true
				);
			}
		
		} else {
		
			gfn_Alert("삭제된 자료는 복구되지 않습니다!");			return;		
		
		}

	} else {

		gfn_Alert("고객정보를 조회하신 후 다시 클릭해주세요!");		return;
		
	}
}

