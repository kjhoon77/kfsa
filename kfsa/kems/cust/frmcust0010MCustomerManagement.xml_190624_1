{
	var bCheck = lfn_CompanyNoCheck(trim(div2.medCompanyNo.Value));
	
	if(bCheck == false) {	//실패
		if (!gfn_Confirm("사업자등록번호가 형식에 맞지않습니다.\n강제로 저장 하시겠습니까?")) {
            div2.medCompanyNo.SetFocus();
            return false;
        }
	}
}

/*
 * 기능 : 사업자등록번호 체크
 * parameter : 사업자등록번호
 * return : 성공/실패 (true/false)
 */
function lfn_CompanyNoCheck(sCompanyNo) {
	var sum = 0;
    var getlist = Array(10);
    var chkvalue = Array(10);
		chkvalue[0] = "1";
		chkvalue[1] = "3";
		chkvalue[2] = "7";
		chkvalue[3] = "1";
		chkvalue[4] = "3";
		chkvalue[5] = "7";
		chkvalue[6] = "1";
		chkvalue[7] = "3";
		chkvalue[8] = "5";
		
		chkvalue[9] = "1";
	for(var i=0; i<=9; i++)
	{		getlist[i] = substr(sCompanyNo, i, 1);
	}	
	
	for(var i=0; i<=7; i++)
	{	
		sum += toNumber(getlist[i]) * toNumber(chkvalue[i]);
	}

	var chkarr = "";
	
	chkarr = "0" + (toNumber(chkvalue[8]) * toNumber(getlist[8]));
	
	chkarr = substr(chkarr , length(chkarr)-2, length(chkarr));
	
	
	sum += floor(chkarr[0]) + floor(chkarr[1]);

	var remander = toNumber(10 - (toNumber(Sum) % 10)) % 10 ;
	
	if (getlist[9] == remander) {
	
		return true;
	
	} else {
	
		return false;
		
	}
	
}

