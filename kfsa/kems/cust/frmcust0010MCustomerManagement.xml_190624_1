	div0.cbxPostGubun.Value = 0;						//회비 청구지(0:대상물,1:선임자,2:직접)
	div4.cbxEduPostGubun.Value = 0;						//교육 청구지(0:대상물,1:선임자,2:직접)
	
    /* 교육/회비에서 링크 왔을시 */
    if (IsExistVar("sJibu") && IsExistVar("sCustNo")) {
        cbxJibu.Value = sJibu;
        medCustNo.Value = sCustNo;
        lfn_searchCust0010CustomerInfo(sJibu, sCustNo);

