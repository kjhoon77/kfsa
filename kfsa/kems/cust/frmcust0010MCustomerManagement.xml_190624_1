// 이수후퇴직 => 선임자교육으로 변경
function Div3_btnEduChange_OnClick(obj)
{
	if (left(getDate(),4)-1 <= ds_ioEducationPassInfo.GetColumn(ds_ioEducationPassInfo.currow, "EPYEAR")) {
		gfn_Alert(left(getDate(),4)-1+"년 이상의 자료가 아니면 변경이 불가합니다.");
		return false;
	}


	if (!gfn_Confirm("[이수후퇴직] 자료를 [선임자교육]으로 변경하시겠습니까?")) {		return false;	}	
	
	
	tit_clearActionInfo();	tit_addSingleActionInfo("cust:updateCust0010EduPass", "", 0, "");
	// 서버 호출 	tit_callService(		""		,""		,""	// inDs		,""	// outDs		,"EPMGNO=" + quote(ds_ioEducationPassInfo.GetColumn(ds_ioEducationPassInfo.currow, "EPMGNO"))		,"lfn_AfterUpdateEduPass"		,false		,false		,true	);	
}

