    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }
    
    gfn_Alert("[선임자교육]으로 변경되었습니다.");
    
    lfn_searchCust0010EducationPassInfoList();	//실무교육 정보	
    
}

// 청구지 반영
function btnPostGo_OnClick(obj)
{

	if( ds_ioCustomerInfo.GetRowType(0) == "insert" ) {	//고객 신규입력 상태이면
		if( gfn_IsNull(div4.edEduPost1.Text) || gfn_IsNull(div4.edEduPost2.Text) 
		|| gfn_IsNull(div0.edPost1.Text) || gfn_IsNull(div0.edPost2.Text)  //고객 주소가 미입력상태이면
		
		|| Div4.cbxAddrGubun.Value == 1 ) {	
			cbxAddPost.Value = 0;	//공통
			cbxAddPost_OnChanged(Object("cbxAddPost"), cbxAddPost.Value);	//강제이벤트(주소선택)
			Div4.cbxEduPostGubun.Value = 0;	//공통(교육)희망주소구분 대상물로 선택
			lfn_EventDiv4CbxEduPostGubun(Div4.cbxEduPostGubun.Value);
		}
	} else if (ds_ioCustomerInfo.GetRowType(0) == "update" || ds_ioCustomerInfo.GetRowType(0) == "normal" ) {	//고객 수정 상태이면 {
	

			cbxAddPost_OnChanged(Object("cbxAddPost"), cbxAddPost.Value);	//강제이벤트(주소선택)
			
			if (Div4.cbxEduPostGubun.Value == 0) { // 교육 대상물
			
				lfn_EventDiv4CbxEduPostGubun(Div4.cbxEduPostGubun.Value);
			
			}
			
			if (Div0.cbxPostGubun.Value == 0) {	// 회비 대상물
			
				lfn_EventDiv0CbxPostGubun(Div0.cbxPostGubun.Value);
				
			}
				
	}
}

function cbxStatusReason_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{
//ALERT(strCode);
	if (strCode > "01" && strCode < "10") {	// 회원가입사유이면 회원으로
		cbxStatus.Value = "0";
		cbxStatus_OnChanged("cbxStatus", cbxStatus.Value);
	} else if (strCode > "10") {	//회원탈퇴사유이면 비회원으로
		cbxStatus.Value = "1";
		cbxStatus_OnChanged("cbxStatus", cbxStatus.Value);
	}

}


function Form_OnActivate(obj)
{
	gs_SysID = "[stm_01,bsn_1000,dsc_0001]";	// 개인정보접속기록 시스템ID  
}




function medResidentNo_OnCharChanged(obj,strPreText,strPostText)
{
    if (gs_IsCallCenter == "Y") { // 콜센터 직원용 이면
		if (length(medResidentNo.Value) > 7) {
			medResidentNo.Value = substr(medResidentNo.Value,0,7);
		}
    }
}
