// 고객 한방(고객(+고객부가정보)+대상물+선임자 조회)조회
function lfn_SearchAllInfo(sArg) 
{    
    ds_oAllObject.ClearData(); // 데이타셋 초기화

    tit_clearActionInfo();
    tit_addSearchActionInfo("cust:searchCust0010AllSearch", false);
    // 서버 호출
    tit_callService(
        ""
        , ""
        , "" // inDs
        , "ds_oAllObject=ds_oAllObject" // outDs
        , sArg
        , "lfn_AfterSearchAllInfo"
        ,false
        ,false
        ,true
    );
}

/*
 * 기능 : 고객 한방조회 후 처리해야 할 내용
 * parameter : ErrorCode - 에러코드
 *             ErrorMsg - 에러메시지
 * Return : 없음
 */
function lfn_AfterSearchAllInfo(nErrorCode, strErrorMsg) {
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }

	if(ds_oAllObject.rowcount > 0) {
		lfn_SeparateDataset();
		lfn_AfterCustomerProc();
		lfn_AfterBuildingProc();
		lfn_AfterFireManagerProc();
		lfn_AfterCustAdditionProc();
		
		// 연계정보 여부 판단 
		if (edCivilAppeal.Text != "" && edCivilAppeal.Text != null) {			 
			btnSearchCivilAppeal.Enable = true;			
		} else {
			btnSearchCivilAppeal.Enable = false;
		}
		// 대상물 구/새주소 구분
		cbxAddrGubun_OnChanged(this,cbxAddrGubun.Value);
		lfn_ConvStatus_Change(lbBuildingConvStatus, ds_ioBuildingInfo.GetColumn(0, "BRCONVSTATUS"));
		
		/*청구지: 교육: 구/새주소 구분*/
		Div4_cbxAddrGubun_OnChanged(this, Div4.cbxAddrGubun.Value);
		
		/*청구지: 회비: 구/새주소 구분*/
		Div0_cbxAddrGubun_OnChanged(this, Div0.cbxAddrGubun.Value);

		// 선임자 구/새주소 구분
		tabTab3_tabpgTab3Page1_cbxPostAddrGubun_OnChanged(this,tabTab3.tabpgTab3Page1.cbxPostAddrGubun.Value);
		lfn_ConvStatus_Change(tabTab3.tabpgTab3Page1.lbFireManagerHConvStatus, ds_ioManagerInfo.GetColumn(0, "FMHRCONVSTATUS"));


		// 콜센터 직원용 이면
		if (gs_IsCallCenter == "Y") {
			if (ms_FormAuth == "W") { // 권한에 따른 버튼 설정
				// 수정권한 부여된 콜센터 직원 수정가능 항목만 Enable				lfn_SetCallCenterReadWrite();
			}		}

		lfn_SearchAllAddInfo();	//부가정보 한방조회
	} else {
		gfn_Alert(MSGI00010);	//검색된 레코드가 없습니다.
		lfn_Cancel();				//초기화
		if(length(ls_curComponent) > 0 ) {
			object(ls_curComponent).SetFocus();
		}
    }

}

