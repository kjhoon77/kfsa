// 교육통지서 출력
function lfn_EduPrint(obj) {

var issuechk = "";	// 교육이수 체크

	if( gfn_IsNull(ds_ioCustomerInfo.GetColumn(ds_ioCustomerInfo.currow, "CMGNO")) ) {
		gfn_Alert("교육통지서를 발행할 자료가 없습니다.");
		return;
	
	}

	for(var i=0; i<ds_ioEducationPassInfo.GetRowCount(); i++) {

		// 당해년도 교육이수사항이 있는지 체크
		if ((left(ds_ioEducationPassInfo.GetColumn(i, "EPPASSDATE"), 4) == left(GetDate(), 4)) &&
		   (ds_ioEducationPassInfo.GetColumn(i, "EPSUCCESSIONGUBUN") == "0" ||	// 선임자교육
		    ds_ioEducationPassInfo.GetColumn(i, "EPSUCCESSIONGUBUN") == "1" ||	// 교육승계
		    ds_ioEducationPassInfo.GetColumn(i, "EPSUCCESSIONGUBUN") == "4")) {	// 교육이관
		    issuechk = "Y";
		    break;
		} else {	// 당해년도 교육이수내역이 없거나 2:사이버교육,3:이수후퇴직인 경우
			issuechk = "N";
		}

	}

	if (issuechk == "Y") {	// 당해년도 교육이수사항이 있으면
	
		gfn_Alert(left(GetDate(), 4)+"년도 교육이수 이력이 있습니다. 교육통지자료가 생성되지 않습니다.!");
				return;	
	}
	
	var sArg = "sCmgno=" + quote(ds_ioCustomerInfo.GetColumn(ds_ioCustomerInfo.currow, "CMGNO"));
	Dialog(ls_Prefix + "::frmcust0010PEduNoticePrint.xml", sArg, 400, 272, sArg, -1, -1);
}

// 소방안전관리보조자 신규입력
function lfn_SupporterInput(obj) {
var ls_imsiCmgno = "";

	if (chkSameBuilding.Value == "Y") {	// 보조자 계속 입력일 경우
	
		if (cbxCourse.Value == "10") {	// 소방안전관리자에서 보조자 신규입력을 할 경우

			ls_imsiCmgno = ds_ioCustomerInfo.GetColumn(0, "CMGNO");
			
		} else if (cbxCourse.Value == "20") {	// 소방안전관리보조자에서 계속입력일 경우
		
			ls_imsiCmgno = ds_ioCustomerInfo.GetColumn(0, "CMAINCMGNO");
		
		}
	
	} else {
	
		ls_imsiCmgno = ds_ioCustomerInfo.GetColumn(0, "CMGNO");
	
	}		
var bMgno = ds_ioBuildingInfo.GetColumn(ds_ioBuildingInfo.currow, "BMGNO");

	if (gfn_isNull(ls_imsiCmgno)) {
	
		gfn_Alert("해당 대상물의 소방안전관리자를 먼저 조회한 후 보조자를 입력하세요!");
		
		medCustNo.SetFocus();
				return;
		
	}
		
	lfn_SetDeleteStatus(true);			//삭제된 정보 상태변경한다.
	lfn_InitButton(false);				//초기화시 버튼(비활성화)
	lfn_ClearData();					//초기화 (모든 데이터셋)
	lfn_AddDataMain();					//Main 데이타셋 입력가능상태로 만들기
	lfn_InputDataSetting();				//디폴트값 셋팅
	lfn_InitComboBoxIndex(9);			//콤보박스 index=0으로 초기화
	lfn_FireManagerNewOldGubun(true);	//선임자 신규상태
	meOldCustNO.Enable = false;			//구시스템번호(비활성화)
	cbxCourse.Value = "20";				//소방안전관리보조자
	
	cbxCourse_OnChanged(object("cbxCourse"), cbxCourse.Value);
	
	ls_MainCmgno = ls_imsiCmgno;					// Main 소방안전관리자 관리번호

	lfn_searchCust0010BuildingInfo2(bMgno);

	edBuildingNm.Enable = false;		// 대상물명 수정못하도록
	
	if( ds_ioCustomerInfo.GetRowType(0) == "insert" ) {	//고객 신규입력 상태이면
		if (cbxAddrGubun.Value == 0) {	// 지번주소이면
	
			if( gfn_IsNull(div4.edEduPost1.Text) || gfn_IsNull(div4.edEduPost2.Text) 
			|| gfn_IsNull(div0.edPost1.Text) || gfn_IsNull(div0.edPost2.Text)  //고객 주소가 미입력상태이면
			
			|| Div4.cbxAddrGubun.Value == 1 ) {	
				cbxAddPost.Value = 0;	//공통
				cbxAddPost_OnChanged(Object("cbxAddPost"), cbxAddPost.Value);	//강제이벤트(주소선택)
				Div4.cbxEduPostGubun.Value = 0;	//공통(교육)희망주소구분 대상물로 선택
				lfn_EventDiv4CbxEduPostGubun(Div4.cbxEduPostGubun.Value);
			}
		} else {	// 도로명주소이면
		
			if( gfn_IsNull(div4.edEduRoadPost1.Text) || gfn_IsNull(div4.edEduRoadPost2.Text) 
			|| gfn_IsNull(div0.edRoadPost1.Text) || gfn_IsNull(div0.edRoadPost2.Text) //고객 주소가 미입력상태이면
			 
			|| Div4.cbxAddrGubun.Value == 0 ) {	
				cbxAddPost.Value = 0;	//공통
				cbxAddPost_OnChanged(Object("cbxAddPost"), cbxAddPost.Value);	//강제이벤트(주소선택)
				Div4.cbxEduPostGubun.Value = 0;	//공통(교육)희망주소구분 대상물로 선택
				lfn_EventDiv4CbxEduPostGubun(Div4.cbxEduPostGubun.Value);
			}
		}
		
	}


}

