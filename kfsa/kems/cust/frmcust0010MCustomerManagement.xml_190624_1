		tit_addMultiActionInfo("cust:insertCust0010CustomerModifyHistory", "", 0, "", "N");
		

		
	}

	if(lsCustomerStatus == "insert" || lsCustomerStatus == "update"){
	
		if (lsCustomerStatus == "insert") {
		
			// 고객상태 변경시 은행가상계좌 할당
			
			oDsList += "ds_oVirtualAccoutAllocation=ds_oVirtualAccoutAllocation";	
			tit_addSingleActionInfo("cust:executeCust0010VirtualAccoutAllocation", "", 0, "");		
		
		} else if (lsCustomerStatus == "update") {
	
			var cFlag = lfn_CustomerStatusChanged();	//고객 변경 체크
	
			if (cFlag <> "4") { // 1:비회원=>회원, 2:회원=>비회원, 3:삭제=>정상
	
				// 고객상태 변경시 은행가상계좌 할당
			
				oDsList += "ds_oVirtualAccoutAllocation=ds_oVirtualAccoutAllocation";		
				tit_addSingleActionInfo("cust:executeCust0010VirtualAccoutAllocation", "", 0, "");
	
			} else if (cFlag == "4") {	// 4:정상=>삭제

				// 고객상태 삭제시 은행가상계좌 삭제
				
				oDsList += "ds_oVirtualAccoutDelete=ds_oVirtualAccoutDelete";		
				tit_addSingleActionInfo("cust:executeCust0010VirtualAccountDelete", "", 0, "");
							}
			
		}
			
	}
	
	if(lsBuildStatus == "update") {
		// 2013-08-12 대상물명 변경시 해당 고객에 대한 은행가상계좌 예금주 변경
		if (ds_ioBuildingInfo.GetOrgColumn(0, "BNM") <> ds_ioBuildingInfo.GetColumn(0, "BNM")) {
		
