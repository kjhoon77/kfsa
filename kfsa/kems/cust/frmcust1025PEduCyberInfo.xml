}



function Form_OnUnloadCompleted(obj) {

    Close();

}



function Form_OnKeyDown(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam) {

    switch (nChar) {

        case 27 : // 종료(Esc)

            lfn_End();

            break;

        case 113 : // 조회(F2)

            lfn_Search();

            break;

    }

}



/*

 * 기능 : 조회

 */

function lfn_Search(obj) {

    ds_oCyberLearning.ClearData(); // 데이타셋 초기화



    tit_clearActionInfo();

    tit_addSearchActionInfo("cust:searchCust1025CyberLearninginfo", false);

    // 서버 호출

    tit_callService(

        ""

        , ""

        , "" // inDs

        , "ds_oCyberLearning=ds_oCyberLearning" // outDs

        , "CCCOURSECD=" + quote(sCoursecd) // args

            + " CCEDUGUBUN='B'"
            
            + " CJYEAR=" + quote(sEsyear) // args

            + " CJGTMGNO=" + quote(sEsgtmgno)
            
            + " CJTRAINNINGORDER=" + quote(sEsorder) // args

            + " CJJUBSUNO=" + quote(sEjhjubsuno)
            
            + " CJMGNO=" + quote(sEjmgno)

        , "lfn_AfterSearchCustCyberLearninginfo"

        ,false

        ,false

        ,true

    );
}


/*

 * 기능 : 종료

 */

function lfn_End(obj) {

    Close();

}

/* 공통 필수 종료 */



/* 사용자 함수 */

/*

 * 기능 : 조회 후 처리해야 할 내용

 * parameter : nErrorCode - 에러코드

 *             strErrorMsg - 에러메시지

 * Return : 없음

 */

function lfn_AfterSearchCustCyberLearninginfo(nErrorCode, strErrorMsg) {

    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.

        gfn_ErrorMsg(nErrorCode, strErrorMsg);

        return;

    }

    if (ds_oCyberLearning.rowcount == 0) {
		
		gfn_Alert("조회된 정보가 없습니다.");
		
		lfn_End();
		
    }

}


