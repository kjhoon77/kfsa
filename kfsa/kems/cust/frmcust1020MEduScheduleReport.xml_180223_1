function lfn_Search(obj) {

    // 조회이력을 저장한다.  
    ls_HistoryArg = "지부=" + cbxJibu.Text
                + ",년도=" + edTrainingYear.Text
                + ",직능=" + cbxCourse.Text
                + ",교육시작일=" + left(calDateStart.Value, 8)
                + ",교육종료일=" + left(calDateEnd.Value, 8);
                
    gfn_InsertSearchPrintHistory(
        "S"
        , this.ID
        , global.mnuLeft.cbxOpenForm.Text
        , ls_HistoryArg
    );
    
	// 콜센터 직원용 이면
	if (gs_IsCallCenter == "Y") {
		// 콜센터 직원 조회이력 저장
		gfn_InsertCallCenterSearchPrintHistory(
				"S"
				, gs_CallCenterUID				, gds_OpenedForm.GetColumn(gds_OpenedForm.FindRow("MURL",this.ID),"MMENUID")
				, this.ID				, global.mnuLeft.cbxOpenForm.Text				, ls_HistoryArg
			);
	}
	
	tit_ClearActionInfo();
	
	tit_addCsvSearchActionInfo("cust:searchCust1010EduSchedule", false);   		  
	tit_callService(  
	                 ""
	               , ""  
	               , ""//inDs 
	               , "ds_ioTrainingOrder=ds_oTrainingOrder" //outDs
	               ,"ESGTMGNO=" + quote(cbxJibu.Value)
					+ " ESYEAR=" + quote(edTrainingYear.Text)
					+ " ESCOUSECD=" + quote(cbxCourse.Value)// args 
					+ " EDUYMDFR=" + quote(calDateStart.Value)// args 
					+ " EDUYMDTO=" + quote(calDateEnd.Value)// args 
	               , "afterSearch"  //callBack
	               , false
	               ,false
				   ,true
	               ); 
}

