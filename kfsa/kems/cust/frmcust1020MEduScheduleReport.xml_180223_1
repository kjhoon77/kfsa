 * 기능 : 종료
 */
function lfn_End(obj) {
    Close("");
}


/* 사용자 이벤트 함수 */
function btnMutilSort_OnClick(obj)
{
	gfn_GridMultiSort(gdList, 0); // 멀티 정렬을 실행한다.
}

function Form_OnSize(obj,nCx,nCy,nState)
{
    if (gs_IsResizingForm == "Y") {
        gdTrainingOrder.Height = nCy - gdTrainingOrder.Top - 4;
        gdTrainingOrder.Width = nCx - gdTrainingOrder.Left - 4;
    }
}
function lfn_Print(obj)
{
    // 조회이력을 저장한다.  
    ls_HistoryArg = "지부=" + cbxJibu.Text
                + ",년도=" + edTrainingYear.Text
                + ",직능=" + cbxCourse.Text
                + ",교육시작일=" + left(calDateStart.Value, 8)
                + ",교육종료일=" + left(calDateEnd.Value, 8);
                
    gfn_InsertSearchPrintHistory(
        "S"
        , this.ID
        , global.mnuLeft.cbxOpenForm.Text
        , ls_HistoryArg
    );
    
	// 콜센터 직원용 이면
	if (gs_IsCallCenter == "Y") {
		// 콜센터 직원 조회이력 저장
		gfn_InsertCallCenterSearchPrintHistory(
				"S"
				, gs_CallCenterUID				, gds_OpenedForm.GetColumn(gds_OpenedForm.FindRow("MURL",this.ID),"MMENUID")
				, this.ID				, global.mnuLeft.cbxOpenForm.Text				, ls_HistoryArg
			);
	}
	
	gfn_CallReportPopup("frmCust1020R01EduScheduleReport.rex"
				  , "ds_ioTrainingOrder"
				  , ""
				  , true);
}



function lfn_Cancel(obj)
{
	//cbxJibu.Value = gds_oUserInfo.GetColumn(0, "PDEPTCD");
	edTrainingYear.Text = left(GetDate(), 4);
	calDateStart.Value = substr(GetDate(),0,8);
    calDateEnd.Value = substr(GetDate(),0,8);
	ds_ioTrainingOrder.Clear();
}

// 결재용 참가원 출력(1장당 3회차)
function btnEduJoin_OnClick(obj)
{
var ls_RexFile = "";

    if (ds_ioTrainingOrder.RowCount() < 1) {
        gfn_Alert("출력할 데이터가 없습니다.");
        return false;
    }
    

    if (!gfn_Confirm("결재용 참가원을 출력하시겠습니까?")) {
        return false;
	}
    ls_RexFile = "frmCust1020R01EduJoin.rex";

    // to do Validation Check
    gfn_CallReportPopup(ls_RexFile
    
						, "ds_ioTrainingOrder"	
						, "참가원");
}

