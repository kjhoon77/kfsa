function btnMutilSort_OnClick(obj)
{
	gfn_GridMultiSort(gdManagerList, 0); // 멀티 정렬을 실행한다.
}

function btnToExcel_OnClick(obj) {
    if (ds_oManagerList.GetRowCount() < 1) {
        gfn_Alert("출력할 데이터가 존재하지 않습니다.");
        return false;
    }
	lfn_Excel(obj);
}

function btnSelect_OnClick(obj) {
    var arrArg = Array(1);
    arrArg[0] = ds_oManagerList.GetColumn(ds_oManagerList.currow,  "CMGNO");	// 고객관리번호 
    
    Close(arrArg);
}

function gdManagerList_OnCellDblClick(obj,nRow,nCell,nX,nY,nPivotIndex) {
    btnSelect_OnClick(obj);
}]]></Script>
