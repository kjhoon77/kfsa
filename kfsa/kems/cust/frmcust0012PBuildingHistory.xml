	<Script><![CDATA[/*
 * 프로그램명 : frmcust0012PBuildingHistory.xml
 * 설명 : 대상물선임현황
 * 작성일 : 2009.12.04
 * 작성자 : 이규희
 */

#include "LIB::training.js"; // 공통업무 관련

var bmgno = "";
/* 공통 필수 시작 */
function Form_OnLoadCompleted(obj) {
    gfn_Form_OnLoadCompleted(obj); // 공통 폼 로딩 이벤트 처리

    // 콜센터 직원 사용 이면
	if (gs_IsCallCenter == "Y") {
		btnToExcel.Enable = false; // 엑셀출력 버튼
		btnPrintScreen.Enable = false; // 화면출력 버튼
	}

    // 공통 코드 조회
    gfn_SearchCommonCode(ds_oGubun, "COCD", "Y", "");
    if (IsExistVar("sBmgno")) {   // sKeyword , sGubun 인자값
        bmgno = sBmgno;
        if (gfn_IsNull(bmgno)) {
            gfn_Alert("대상물 정보가 없습니다.");
            Close();
            return;
        }        
        lfn_searchCust0010BuildingInfo();
    }    
}

function Form_OnUnloadCompleted(obj) {
    Close();
}

/*
 * 기능 : 종료
 */
function lfn_End(obj) {
    Close();
}

function Form_OnSize(obj,nCx,nCy,nState)
{
    if (gs_IsResizingForm == "Y") {
        gdManagerList.Height = nCy - gdManagerList.Top - 4;
        gdManagerList.Width = nCx - gdManagerList.Left - 4;
    }
}

/*
 * 기능 : 조회
 */
function lfn_Search(obj) {
    
    if (!gfn_FormValidate(this)) { // 입력 Validation Check
        return false;
    }
    
    ds_oManagerList.ClearData(); // 데이타셋 초기화
    
    // 조회이력을 저장한다.  
    ls_HistoryArg = "대상물관리번호=" + quote(sBmgno);
    gfn_InsertSearchPrintHistory(
        "S"
        , this.ID
        , global.mnuLeft.cbxOpenForm.Text
        , ls_HistoryArg
    );

    tit_clearActionInfo();
	tit_addCsvSearchActionInfo("cust:searchCust0011ManagerList", false);
	// 서버 호출 
	tit_callService(
		""
		,""
		,""	// inDs
		,"ds_oManagerList=ds_oManagerList"	// outDs
		,"BMGNO=" + quote(sBmgno) // 대상물관리번호 
		 + " GUBUN=0"// args 
		,"lfn_AfterSearchManagerList"
		,false
	);
}

// 대상물 정보(MAIN 2)
function lfn_searchCust0010BuildingInfo() {

	ds_ioBuildingInfo.ClearData(); // 데이타셋 초기화
	
	// 조회이력을 저장한다.  
    ls_HistoryArg = "대상물관리번호=" + quote(sBmgno);
    gfn_InsertSearchPrintHistory(
        "S"
        , this.ID
        , global.mnuLeft.cbxOpenForm.Text
        , ls_HistoryArg
    );

    tit_clearActionInfo();
    tit_addSearchActionInfo("cust:searchCust0018BuildingInfo", false);
    // 서버 호출
    tit_callService(
        ""
        , ""
        , "" // inDs
        , "ds_ioBuildingInfo=ds_oBuildingInfo" // outDs
        , "BMGNO=" + quote(sBmgno) // 대상물관리번호
        , "lfn_AfterSearchCust0010BuildingInfo"
        ,false
        ,false
        ,true
    );
}

/*
 * 기능 : 선임자 조회 후 처리해야 할 내용
 * parameter : nErrorCode - 에러코드
 *             strErrorMsg - 에러메시지
 * Return : 없음
 */
function lfn_AfterSearchManagerList(nErrorCode, strErrorMsg) {
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }
    
    //trace(ds_oManagerList.SaveXML());
    gfn_SearchMessage(gdManagerList); // 조회 메세지를 처리하고 그리드의 정렬 헤드 마크를 제거한다.
}

/*
 * 기능 : 대상물 정보(MAIN 2) 조회 후 처리해야 할 내용
 * parameter : ErrorCode - 에러코드
 *             ErrorMsg - 에러메시지
 * Return : 없음
 */
function lfn_AfterSearchCust0010BuildingInfo(nErrorCode, strErrorMsg) {
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }
    edBuildingNm.Text = ds_ioBuildingInfo.GetColumn(0, "BNM");
    medNewZipCode.Value = ds_ioBuildingInfo.GetColumn(0, "BZIPCD");
    medOldZipCode.Value = ds_ioBuildingInfo.GetColumn(0, "BZIPCD");
    edNewAddr.Text = ds_ioBuildingInfo.GetColumn(0, "BADDRNEW");
    edOldAddr.Text = ds_ioBuildingInfo.GetColumn(0, "BADDROLD");
    edTelNo.Text = ds_ioBuildingInfo.GetColumn(0, "BTEL");
    lfn_Search();
}

