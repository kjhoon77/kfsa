 * 작성자 : 이규희
 */

#include "LIB::cust.js"; // 공통업무 관련

/* 공통 필수 시작 */
function Form_OnLoadCompleted(obj) {
    gfn_Form_OnLoadCompleted(obj); // 공통 폼 로딩 이벤트 처리
    
    // 공통 코드 조회
    gfn_SearchCommonCode(ds_oBizGubun, "BIZCD", "Y", "전체");
    
    if (!gfn_IsNull(sBuildingNm)) {
        edKeyWord.Text = sBuildingNm;
        lfn_Search();
    }
    edKeyWord.SetFocus();
    
	edAddrKeyWord1.Text = "";
	edAddrKeyWord2.Text = "";
	edAddrKeyWord1.Enable = false;
	edAddrKeyWord2.Enable = false;    
}

function Form_OnUnloadCompleted(obj) {
    Close();
}

/*
 * 기능 : 종료
 */
function lfn_End(obj) {
    Close();
}

/*
 * 기능 : 조회
 */
function lfn_Search(obj) {
    var sKeyWord = edKeyWord.Text;
    sKeyWord = lfn_ReplaceBnm(sKeyWord);  // 검색대상물명
    var sAddrKeyWord1 = edAddrKeyWord1.Text;
    var sAddrKeyWord2 = edAddrKeyWord2.Text;
    
    medTotalCnt.Value = "";
    ds_oCustList.ClearData(); // 데이타셋 초기화

    tit_clearActionInfo();
	tit_addCsvSearchActionInfo("cust:searchCust0032BuildingList", false);
	// 서버 호출 
	tit_callService(
		""
		,""
		,""	// inDs
		,"ds_oCustList=ds_oCustList"	// outDs
		,"KEYWORD=" + quote(sKeyWord)
			+ " ADDRKEYWORD1=" + quote(sAddrKeyWord1)
			+ " ADDRKEYWORD2=" + quote(sAddrKeyWord2)
            + " CGTMGNO=" + quote(sGtmgno)
		,"lfn_AfterSearchbuilding"
        ,false
        ,false
        ,true
	);
}

/* 공통 필수 종료 */

/*
 * 기능 : 공통코드 조회 후 처리해야 할 내용
 * parameter : nErrorCode - 에러코드
 *             strErrorMsg - 에러메시지
 * Return : 없음
 */
function lfn_AfterSearchbuilding(nErrorCode, strErrorMsg) {
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }
    //trace(ds_oCustList.SaveXML());
    medTotalCnt.Value = ds_oCustList.GetCount();
    ds_oCustList_OnRowPosChanged(ds_oCustList,-1,0);
    gfn_SearchMessage(gdBuidingList); // 조회 메세지를 처리하고 그리드의 정렬 헤드 마크를 제거한다.
}

/* 이벤트 함수 종료 */


/* 사용자 함수 */

function btnSelect_OnClick(obj) {
    if (ds_oManagerList.GetCount() < 1) {
        gfn_Alert("선택된 정보가 없습니다.");        
        return;
    }
    var arrArg = Array(3);
	arrArg[0] = "OLD";		        // 구분
    arrArg[1] = ds_oManagerList.GetColumn(ds_oManagerList.currow,  "CGTMGNO");		        // 지부
    arrArg[2] = ds_oManagerList.GetColumn(ds_oManagerList.currow,  "CNO");		        // 고객번호
    
    Close(arrArg);
}

function edBuildingNm_OnKeyDown(obj,nChar,bShift,bCtrl, bAlt,LLParam,HLParam) {
    if (nChar == 13) { // Enter Key
        lfn_Search();
    }
}

function ds_oCustList_OnRowPosChanged(obj,nOldRow,nRow)
{
    Static7.Text = "("+ds_oCustList.GetColumn(nRow, "BNM")+")";
	ds_oManagerList.ClearData(); // 데이타셋 초기화
    if (ds_oCustList.GetCount() == 0) {
        return;
    }
    tit_clearActionInfo();
	tit_addCsvSearchActionInfo("cust:searchCust0011ManagerList", false);
	// 서버 호출 
	tit_callService(
		""
		,""
		,""	// inDs
		,"ds_oManagerList=ds_oManagerList"	// outDs
		,"BMGNO=" + quote(ds_oCustList.GetColumn(nRow, "BMGNO"))
		 + " GUBUN=" + quote(chkCust.Value)
		,"lfn_AfterSearchManager"
        ,false
        ,false
        ,true
	);
}

/*
 * 기능 : 공통코드 조회 후 처리해야 할 내용
 * parameter : nErrorCode - 에러코드
 *             strErrorMsg - 에러메시지
 * Return : 없음
 */
function lfn_AfterSearchManager(nErrorCode, strErrorMsg) {
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }
    //trace(ds_oManagerList.SaveXML());

    gfn_SearchMessage(gdManagerList); // 조회 메세지를 처리하고 그리드의 정렬 헤드 마크를 제거한다.
}
