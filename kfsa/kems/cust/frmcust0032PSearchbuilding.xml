}

function btnNewAdd_OnClick(obj)
{
	var arrArg = Array(1);
	arrArg[0] = "NEW";		        // 구분
    
    Close(arrArg);
}

function gdManagerList_OnKeyDown(obj,nChar,bShift,bCtrl,bAlt,nLLParam,nHLParam)
