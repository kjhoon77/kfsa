function radSearchGubun_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{
	if (strCode == 0) {	// 대상물명
        edMainBnm.Enable = true;
        edAddrKeyWord1.Text = "";
        edAddrKeyWord2.Text = "";
        edAddrKeyWord1.Enable = false;
        edAddrKeyWord2.Enable = false;
        edMainBnm.Text = sBuildingNm;
        edSubBnm.Text = sBuildingNm;
    } else {			// 주소
		edMainBnm.Enable = false;
		edMainBnm.Text = "";
		edSubBnm.Text = "";
		edAddrKeyWord1.Enable = true;
        edAddrKeyWord2.Enable = true;
		edAddrKeyWord1.Text = sAddr1;
        edAddrKeyWord2.Text = sAddr2;
    }
        
}

