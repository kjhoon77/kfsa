    gfn_SearchCommonCode(ds_oBizGubun, "BIZCD", "Y", "전체");
    
    sMainCmgno = "";	// 메인 소방안전관리자 고객바코드번호
    sMainBmgno = "";	// 메인 소방안전관리자 대상물관리번호
    
    if (!gfn_IsNull(sBuildingNm)) {
        edMainBnm.Text = sBuildingNm;
        edSubBnm.Text = sBuildingNm;
        
        lfn_MainSearch();	// 메인 소방안전관리자 조회
        lfn_SubSearch();	// 소방안전관리보조자 조회
    }
    edMainBnm.SetFocus();
}

