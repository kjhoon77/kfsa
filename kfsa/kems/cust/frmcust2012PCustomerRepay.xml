	<Script><![CDATA[/*
 * 프로그램명 : frmcust2012PCustomerRepay.xml
 * 설명 : 환불 대상 선택
 * 작성일 : 2009.12.09
 * 작성자 : 김동현
 */

#include "LIB::cust.js"; // 공통업무 관련

/* 공통 필수 시작 */
function Form_OnLoadCompleted(obj) {

	gfn_Form_OnLoadCompleted(obj); // 공통 폼 로딩 이벤트 처리

    if (IsExistVar("sPmYear") && IsExistVar("sPmMgno")) {
        lfn_Search();
    } else {
        gfn_Alert("결제 정보가 없습니다.");
        lfn_Cancel();
    }
    
    edRemark.SetFocus();

}

/**
 * 윈도우 닫기 이벤트
 */
function Form_OnUnloadCompleted(obj) {
	close(false);
}

/**
 * 조회
 */
function lfn_Search(obj) {

    tit_clearActionInfo();
    tit_addSearchActionInfo("cust:searchCust2011CustomerFee", false);
    
    // 서버 호출 
    tit_callService(
        ""
        ,""
        ,""
        ,"ds_oCustomerFee=ds_oCustomerFee"
        ,"SPMYEAR=" + quote(sPmYear)
            + " SPMMGNO=" + quote(sPmMgno)// args 
        ,""
        ,false
        ,false
        ,true
    );

    ds_oCustomerFee.Filter("CFGUBUN='" + sGubun + "'");
    
    edJibu.Text = lfn_GetJibu(ds_oCustomerFee.GetColumn(0, "MGGTMGNO"));
    edFeeGubun.Text = iif(ds_oCustomerFee.GetColumn(0, "MEMBERGUBUN") == "0", "회비", "교육비");
    edBuyer.Text = ds_oCustomerFee.GetColumn(0, "PMBUYER");
    medTotalAmount.Value = ds_oCustomerFee.GetColumn(0, "PMAMT");
    edSetlmGubun.Text = lfn_GetSetlm(ds_oCustomerFee.GetColumn(0, "CFGUBUNCD"));
    medProcDate.Value = ds_oCustomerFee.GetColumn(0, "PROCDATE");
    medPonyDate.value = ds_oCustomerFee.GetColumn(0, "PONYDATE");
    
    ds_oCustomerFee_OnColumnChanged();
}

/**
 * 선택
 */
function btnSelect_OnClick(obj) {

    ds_oCustomerFee.Filter("SEL='1'");
    
    if (ds_oCustomerFee.GetRowCount() < 1) {
        gfn_Alert("환불 대상이 없습니다.");
        return;
    }

    var nRow = 0;
    var sCMgno = "0";
    
    for (var i=0; i<ds_oCustomerFee.GetRowCount(); i++) {
    
        nRow = ds_iReady.AddRow();
        ds_iReady.SetColumn(nRow, "SEL", "0");
        ds_iReady.SetColumn(nRow, "CFGUBUN", "1");
        ds_iReady.SetColumn(nRow, "CMGNO", ds_oCustomerFee.GetColumn(i, "CFCMGNO"));
        ds_iReady.SetColumn(nRow, "CFMGGTMGNO", ds_oCustomerFee.GetColumn(i, "MGGTMGNO"));
        ds_iReady.SetColumn(nRow, "CNO", ds_oCustomerFee.GetColumn(i, "CNO"));
        ds_iReady.SetColumn(nRow, "CREGCD", lfn_GetRegion(ds_oCustomerFee.GetColumn(i, "MGGTMGNO"), ds_oCustomerFee.GetColumn(i, "CREGCD")));
        ds_iReady.SetColumn(nRow, "CCOURSECD", ds_oCustomerFee.GetColumn(i, "CCOURSECD"));
        ds_iReady.SetColumn(nRow, "CFMEMBERGUBUN", ds_oCustomerFee.GetColumn(i, "MEMBERGUBUN"));
        ds_iReady.SetColumn(nRow, "BNM", ds_oCustomerFee.GetColumn(i, "BNM"));
        ds_iReady.SetColumn(nRow, "FMNM", ds_oCustomerFee.GetColumn(i, "FMNM"));
        ds_iReady.SetColumn(nRow, "CFPROCAMOUNT", ds_oCustomerFee.GetColumn(i, "SUNAPFEE"));
        ds_iReady.SetColumn(nRow, "CFPROCDATE", ds_oCustomerFee.GetColumn(i, "PROCDATE"));
        ds_iReady.SetColumn(nRow, "CFREMARK", edRemark.Text);
        ds_iReady.SetColumn(nRow, "CHECKDATA", ds_oCustomerFee.GetColumn(i, "PMAMT")
                                            + "|" + ds_oCustomerFee.GetColumn(i, "PMBUYER")
                                            + "|" + ds_oCustomerFee.GetColumn(i, "CFGUBUNCD")
                                            + "|" + ds_oCustomerFee.GetColumn(i, "PMPCORDERNO")
                           ); // 체크 row를 알기 위한 데이터
        ds_iReady.SetColumn(nRow, "CFMGGTNM", lfn_GetJibu(ds_oCustomerFee.GetColumn(i, "MGGTMGNO")));
        ds_iReady.SetColumn(nRow, "CCOURSENM", lfn_GetCourseNick(ds_oCustomerFee.GetColumn(i, "CCOURSECD")));
        ds_iReady.SetColumn(nRow, "CFMEMBERNM", ds_oCustomerFee.GetColumn(i, "FMNM"));
        sCMgno += ", " + ds_oCustomerFee.GetColumn(i, "CFCMGNO");

    }

    ds_iCustomerFee.ClearData();
    
    tit_clearActionInfo();
    tit_addSearchActionInfo("cust:searchCust2012CustomerFee", false);
    
    // 서버 호출 
    tit_callService(
        ""
        ,""
        ,""
        ,"ds_iCustomerFee=ds_oCustomerFee"
        ,"SPMYEAR=" + quote(ds_oCustomerFee.GetColumn(0, "PMYEAR"))
            + " SPMMGNO=" + quote(ds_oCustomerFee.GetColumn(0, "PMMGNO"))
            + " SCMGNO=" + quote(sCMgno)
           // + " LAST=" + quote("Y") // args 
        ,""
        ,false
        ,false
        ,true
    );

    parent.ds_iReady.Copy(ds_iReady);
    parent.ds_iCustomerFee.Copy(ds_iCustomerFee);

	close(true);
	
}

/**
 * 전체 선택
 */
function gdSunap_OnHeadClick(obj,nCell,nX,nY,nPivotIndex) {
	if (nCell == 0) {
        lfn_GridCheck(obj,nCell,nX,nY,nPivotIndex);
    } else {
	    lfn_SortGrid_OnHeadClick(obj,nCell,nX,nY,nPivotIndex);
	}
}

/**
 * 멀티 정렬
 */
function btnMutilSort_OnClick(obj) {
	gfn_GridMultiSort(gdSunap, 1);
}

/**
 * 체크
 */
function ds_oCustomerFee_OnColumnChanged(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex) {
	medAmount.Value = ds_oCustomerFee.Sum("iif(SEL=='1', SUNAPFEE, 0)");
	if(medAmount.Value == 0) {
        btnSelect.Enable = false;
	} else {
        btnSelect.Enable = true;
	}
}


