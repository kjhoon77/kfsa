/*
 *  키값으로 교육안내대상리스트 상세 조회 
*/
function lfn_Search_EducationList()
{
    ds_ioMEduDataDetail.ClearData(); // 데이터셋 초기화        	 
    tit_clearActionInfo();
    tit_AddSearchActionInfo("cust:searchCust1107FireDefDMDataDetail", false);        

    tit_callService(
        ""
        ,""
        ,""// inDs
		,"ds_ioMEduDataDetail=ds_oMEduDataDetail"  // outDs
        ,"fkeyA=" + quote(ds_ioMEduData.GetColumn(0, "fkeyA"))
          //args                                         
        ,"lfn_AfterSearchEducationList"
        ,false
        ,""
        ,true
        
    );
}

function lfn_AfterSearchEducationList(nErrorCode, strErrorMsg) {
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }
   
    edEduTotalCount.Text = ds_ioMEduDataDetail.count;
    
    alert("조회가 완료 되었습니다.");
    
}

function radReport_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{
	if(strCode == "1"){
        cbxReportData1.Enable = true;
        cbxReportData2.Enable = true;
        cbxReportData1.Value = "19700101";
        cbxReportData2.Value = left(GetDate(), 8);
	} else {
        cbxReportData1.Enable = false;
        cbxReportData2.Enable = false;	    
        cbxReportData1.Value = "";
        cbxReportData2.Value = "";
	}	
}

function radMgno_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{
	if(strCode == "1"){
        meNum1.Enable = true;
        meNum2.Enable = true;
	} else {
        meNum1.Enable = false;
        meNum2.Enable = false;	
        meNum1.Value = "";
        meNum2.Value = "";	        
	}
}

function btnToExcel_OnClick(obj)
{
    if (!gfn_FormValidate(this)) { // 입력 Validation Check
        return false;
    }    
    
    if (ds_ioMEduExcel.GetRowCount() == 0) {

        gfn_Alert("조회된 자료가 없습니다.");
        return false;
    
    }
    
    lfn_Excel();  // key값으로 대상리스트 상세 조회함 
    
    ds_ioMEduExcel.ClearData();

}



/*
*  이전년도 교육미필자 제외 여부
*/
function chkExceptDate_OnClick(obj,strValue)
{
	if(strValue == '1') {
        calEduExceptDate1.Enable = true;
        calEduExceptDate2.Enable = true;
        
        calEduExceptDate1.Value = toString(toNumber(left(GetDate(), 4))-1) + "0101";
        calEduExceptDate2.Value = toString(toNumber(left(GetDate(), 4))-1) + "1231";        
        
	} else {
	    calEduExceptDate1.Enable = false;
        calEduExceptDate2.Enable = false;
        calEduExceptDate1.value = "";
        calEduExceptDate2.value = "";
	}	
}

