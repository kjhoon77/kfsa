// 조회
function lfn_Search(obj)
{
      
    if (!gfn_FormValidate(this)) { // 입력 Validation Check
        return false;
    }    
    
    if (!gfn_Confirm("조회 시간이 많이 걸립니다. 진행하시겠습니까?\n"
                    +"조회중에 창을 닫으시면 안됩니다.")) { 
        return false;
    }
    
    if (radCourse.Value == "10") {	// 소방안전관리자
        gdList.Format = "grid10";
    } else if (radCourse.Value == "20") {	// 소방안전관리보조자
		gdList.Format = "grid20";
		gdList.SetCellProp("head", 2, "text", substr(calEduExpireDate11.Text,0,4)+"년");
    } else if (radCourse.Value == "30") {	// 위험물안전관리자
		gdList.Format = "grid30";
    } else if (radCourse.Value == "50") {	// 소방기술자
		gdList.Format = "grid50";
	} else if (radCourse.Value == "80") {	// 위험물운송자
		gdList.Format = "grid80";
    }
    
	var COURSEN = "";
	
	if(cbxCourse_1.Value != "")
		COURSEN += "'" + cbxCourse_1.Value + "'" + ",";
	if(cbxCourse_2.Value != "")
		COURSEN += "'" + cbxCourse_2.Value + "'" + ",";	
    if(cbxCourse_3.Value != "")
		COURSEN += "'" + cbxCourse_3.Value + "'" + ",";	
	if(cbxCourse_4.Value != "")
		COURSEN += "'" + cbxCourse_4.Value + "'" + ",";	
	if(cbxCourse_5.Value != "")
		COURSEN += "'" + cbxCourse_5.Value + "'" + ",";	
	if(cbxCourse_6.Value != "")
		COURSEN += "'" + cbxCourse_6.Value + "'" + ",";			
		
	COURSEN = left(COURSEN, length(COURSEN)-1);
    
    
	var BIZCHOICEN = "";
	
	if(cbxBusinessType_1.Value != "")
		BIZCHOICEN += "'" + cbxBusinessType_1.Value + "'" + ",";
	if(cbxBusinessType_2.Value != "")
		BIZCHOICEN += "'" + cbxBusinessType_2.Value + "'" + ",";	
    if(cbxBusinessType_3.Value != "")
		BIZCHOICEN += "'" + cbxBusinessType_3.Value + "'" + ",";	
	if(cbxBusinessType_4.Value != "")
		BIZCHOICEN += "'" + cbxBusinessType_4.Value + "'" + ",";	
	if(cbxBusinessType_5.Value != "")
		BIZCHOICEN += "'" + cbxBusinessType_5.Value + "'" + ",";	
	if(cbxBusinessType_6.Value != "")
		BIZCHOICEN += "'" + cbxBusinessType_6.Value + "'" + ",";			
		
	BIZCHOICEN = left(BIZCHOICEN, length(BIZCHOICEN)-1);
    
    ds_ioMEduExcel.Clear();        
    tit_clearActionInfo();
    
	tit_addSearchActionInfo("cust:searchCust1109EduTargetStatExcel", false);     // 조회
    
	// 서버 호출 
	tit_callService( 
		""
		,""
		,""	// inDs
		,"ds_ioMEduExcel=ds_oMEduExcel"  // outDs	  	  
	    , "radMemberGubun=" + quote(radMemberGubun.value)           //회원구분
        + " radCourse=" + quote(radCourse.value)		            //교육대상
        + " cbxJibu=" + quote(cbxJibu.value)            	 		//지부
        + " radGijun=" + quote(radGijun.value)             			//지부or소방본부 구분
        + " radRegion=" + quote(radRegion.value)          			//지역전체_선택 유무
        + " cbxRegion1=" + quote(cbxRegion1.value)         		    //지역1 (단일지역)
        + " cbxRegion2=" + quote(cbxRegion2.value)             		//지역2 (from)
        + " cbxRegion3=" + quote(cbxRegion3.value)       		    //지역3 (to)
        + " radCenter=" + quote(radCenter.value)            		//센터전체_선택 유무
        + " cbxMFCenter1=" + quote(cbxMFCenter1.value)              //센터1 (단일센터)
        + " cbxMFCenter2=" + quote(cbxMFCenter2.value)              //센터2 (from)
        + " cbxMFCenter3=" + quote(cbxMFCenter3.value)              //센터3 (to)
	    + " COURSEGUBUN=" + quote(radDetailCourse.Value)			//직능 구분
	    + " COURSE=" + quote(cbxCourse1.Value)					    //단일 직능
	    + " COURSEN=" + quote(COURSEN)								//다중직능
	    + " BIZGUBUN=" + quote(radBiz.Value)						//용도구분
        + " BUSINESSTYPE1=" + quote(cbxBusinessType1.value)   		//용도_선택1 
        + " BUSINESSTYPE2=" + quote(cbxBusinessType2.value)		   	//용도_선택2 
	    + " BIZCHOICEN=" + quote(BIZCHOICEN)						//다중직능
        + " radReport=" + quote(radReport.value)             		//선임신고일 전체_선택 유무
        + " cbxReportData1=" + quote(cbxReportData1.value)          //선임신고일1 (from)
        + " cbxReportData2=" + quote(cbxReportData2.value)          //선임신고일2 (to)
        + " calEduExpireDate11=" + quote(calEduExpireDate11.value)    //교육 유효기간start
        + " calEduExpireDate12=" + quote(calEduExpireDate12.value)    //교육 유효기간end
        + " calEduExpireDate21=" + quote(calEduExpireDate21.value)    //교육 유효기간start
        + " calEduExpireDate22=" + quote(calEduExpireDate22.value)    //교육 유효기간end
        + " calEduExpireDate31=" + quote(calEduExpireDate31.value)    //교육 유효기간start
        + " calEduExpireDate32=" + quote(calEduExpireDate32.value)    //교육 유효기간end
        + " calEduExpireDate41=" + quote(calEduExpireDate41.value)    //교육 유효기간start
        + " calEduExpireDate42=" + quote(calEduExpireDate42.value)    //교육 유효기간end
        + " chkExceptDate=" + quote(chkExceptDate.value)           	//이전년도 교육필자 제외 여부
        + " calEduExceptDate1=" + quote(calEduExceptDate1.value)	//이전교육일자1(from)
        + " calEduExceptDate2=" + quote(calEduExceptDate2.value)	//이전교육일자2 (to)
        + " radMgno=" + quote(radMgno.value)             			//관리번호 전체_선택 유무
        + " cno1=" + quote(meNum1.value)             				//관리번호1 (from)
        + " cno2=" + quote(meNum2.value)             				//관리번호2 (to)
        + " radNotMemberFirsSecondHalf=" + quote(radNotMemberFirsSecondHalf.value)             //비회원회비부과상하반기
		+ " radAgency=" + quote(radAgency.value)             		//업무대행 여부
        //+ " radMinap=" + quote(radMinap.value)						//미납여부
        + " chkDataYN1=" + quote(chkDataYN1.Value)					//공공기관 기관장
        + " chkDataYN2=" + quote(chkDataYN2.Value)					//공공기관 감독적직위
        + " chkDataYN3=" + quote(chkDataYN3.Value)					//공공기관 기타
        + " chkDataYN4=" + quote(chkDataYN4.Value)					//관리제외대상
        + " radEduJubsu=" + quote(radEduJubsu.Value)				//실무교육접수자
        //+ " chkDataYN5=" + quote(chkDataYN5.Value)					//이수후퇴직자
	      //-- (매개변수) // args 
		,"lfn_AftersearchEduExcel"
		,false
		,false
		,true
	);	
}

