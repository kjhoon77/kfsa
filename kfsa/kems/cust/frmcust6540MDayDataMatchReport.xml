function lfn_Print(obj){
    lfn_Search();
    
    course = "";
    if (ds_oDATA.GetCount() < 1) {
        gfn_Alert("출력할 데이터가 없습니다.");
        return false;
    }

    var rpt = "";
    var params = "";
    var params2 = "";
    var sTopMargin = "";
    var sLeftMargin = "";

    if (radPrtGubun.Value == "1") {    // 출력구분 : 지역별
        rpt = "frmcust6540Total_" + cbxCourse.Value + ".rex";
        
        if (cbxCourse.Value == "1")      { params = "(소방안전관리자) 일일데이터일치화 현황"; }
		else if (cbxCourse.Value == "3") { params = "(위험물관리자) 일일데이터일치화 리스트"; }
		else if (cbxCourse.Value == "5") { params = "(소방기술자) 일일데이터일치화 리스트"; }
		else if (cbxCourse.Value == "8") { 
			params = "(위험물운송자) 일일데이터일치화 리스트"; 
			params2 = "차량번호";
		}
		
		if (cbxCourse.Value == "8") { 
			sTopMargin = "100";
			sLeftMargin = "200";
		}
    }
    
	else if (radPrtGubun.Value == "2") {    // 출력구분 : 리스트 
		rpt = "frmcust6540List.rex";
		params2 = "선임자";

		if (cbxCourse.Value == "1")      { params = "(소방안전관리자) 일일데이터일치화 리스트"; }
		else if (cbxCourse.Value == "3") { params = "(위험물관리자) 일일데이터일치화 리스트"; }
		else if (cbxCourse.Value == "5") { params = "(소방기술자) 일일데이터일치화 리스트"; }
		else if (cbxCourse.Value == "8") { 
			params = "(위험물운송자) 일일데이터일치화 리스트"; 
			params2 = "차량번호";
		}		
    }
    
    params = "rd_TITLE=" + quote(params) + "^s_TITLE=" + quote(params2);  
    
	/*
	 * 기능 : 렉스퍼트 보고서를 팝업화면으로 호출한다.
	 * parameter : sReportName - 레포트명(예:레포트.rex)
	 *             sReportDataset - 데이터셋명(예:dsReport1||dsResport2||...)
	 *             sReportParams - 렉스퍼트용 파라메터(예: a에 1, b에 2, c에 3의 값을 넣는 경우, a=1^b=2^c=3...)
	 *             bLcsYn - 수첩여부
	 *             LeftMargin = 왼쪽
	 *             RightMargin = 오른쪽
	 *             TopMargin = 상단
	 *             BottomMargin = 하단
	 * return value : 정상출력 true, 출력 안했으면 false
	 */    
	 
    //gfn_CallReportPopup(rpt, "ds_oDATA", params);
    gfn_CallReportPopup(rpt, "ds_oDATA", params, '', '', sTopMargin, sLeftMargin);
}




function lfn_Search(obj) {
    SetWaitCursor(false);
	
    ds_oDATA.ClearData(); // 데이타셋 초기화
    
    /* 체크 */    
    // 처리일자 체크 
    if (Length(calStartDate.value) != 8) {
		alert("처리일자를 입력해 주세요");
		calStartDate.SetFocus();
		return;
    }
    if (Length(calEndDate.value) != 8) {
		alert("처리일자를 입력해 주세요");
		calEndDate.SetFocus();
		return;
    }
    
    /* SQL */
    var qry = "";
    if (radPrtGubun.Value == "1") {     // 출력구분 : 집계
		qry = "cust:searchCust6540Total";
    }    
	else if (radPrtGubun.Value == "2") { // 출력구분 : 리스트 
        qry = "cust:searchCust6540List_" + cbxCourse.Value;
    }
    
    tit_clearActionInfo();        
    tit_addCsvSearchActionInfo(qry, false);
	// 서버 호출 
	tit_callService(
         "",""
		,""	// inDs
		,"ds_oDATA=ds_oDATA"	// outDs
		,     "JIBU="    + quote(cbxJibu.Value)
	    	+ " COURSE=" + quote(cbxCourse.Value)
	    	+ " STATUS=" + quote(radStatus.value)
	    	+ " MATCHGUBUN=" + quote(radMatchGubun.Value)    // 일치구분 
            + " SDATE="  + quote(calStartDate.value)
            + " EDATE="  + quote(calEndDate.value)
            + " ORDER="  + quote(radOrder.Value)
		, "lfn_AfterSearch"
		, false
		, false  // 처리중 창 보여주기 (null or false : 보여줌) 
		, true); 
		   
}

function lfn_AfterSearch(nErrorCode, strErrorMsg) {
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);     
        course = "";
        return;
    }
    
    // 조회결과 유무 체크 
    if (ds_oDATA.GetCount() > 0) {
		// 조회이력을 저장한다.     
		ls_HistoryArg = "출력물=" + quote(radPrtGubun.Value)
					+ ",지부=" + quote(cbxJibu.Value)
					+ ",직능=" + quote(cbxCourse.Value)
					+ ",구분=" + quote(radStatus.Value)
					+ ",처리일자(시작)=" + quote(calStartDate.Value)
					+ ",처리일자(끝)=" + quote(calEndDate.Value);
	
		gfn_InsertFPISSearchPrintHistory(
			"P"
			, this.ID
			, global.mnuLeft.cbxOpenForm.Text
			, radPrtGubun.Text
			, ls_HistoryArg
		);
    }
}







