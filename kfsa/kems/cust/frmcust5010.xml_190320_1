//기능 : 연계데이타 조회 

function lfn_MappingSearch(cmgno) {

	ds_ioFireMapping.Clear();
	tit_clearActionInfo();
	tit_addSearchActionInfo("cust:searchCust5010FireMappingInfo", false);
	
	var MBIZGUBUN = "";	
	if (sCourse == 10 ) {
		MBIZGUBUN = "0";
	} else if (sCourse == 20 ) {
		MBIZGUBUN = "4";
	} else if (sCourse == 30 ) {
		MBIZGUBUN = "1";
	} else if (sCourse == 50 ) {
		MBIZGUBUN = "3";
	} else if (sCourse == 80 ) {
		MBIZGUBUN = "2";
	}
	// 서버 호출 
	tit_callService( 
		""
		,""
		,""	// inDs
		,"ds_ioFireMapping=ds_oFireMapping" 
	    ,"MKFSAPK=" + quote(cmgno)
	     + " MBIZGUBUN=" + quote(MBIZGUBUN)
		,"lfn_AftersearchMapping"
		,false
	);
}

// 연계데이타 조회 후 
function lfn_AftersearchMapping(nErrorCode, strErrorMsg) {
	if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }
    
    sMFPIS1 = ds_ioFireMapping.GetColumn(0,"MFPIS1");
	sMFPIS2 = ds_ioFireMapping.GetColumn(0,"MFPIS2");
	sMFPIS3 = ds_ioFireMapping.GetColumn(0,"MFPIS3");
	sMFPIS4 = ds_ioFireMapping.GetColumn(0,"MFPIS4");
	sMFPIS5 = ds_ioFireMapping.GetColumn(0,"MFPIS5");
	sMFPIS6 = ds_ioFireMapping.GetColumn(0,"MFPIS6");
	
	if (ds_ioFireMapping.GetCount() > 1) {
		edTObjNm.Visible = false;
		cbxTObjNm.Visible = true;
		edTObjCnt.Visible = true;
		cbxTObjNm.Value = ds_ioFireMapping.GetColumn(0,"MMGNO");
		edTObjCnt.text = ds_ioFireMapping.GetCount() + "건";
	} else {
		edTObjNm.Visible = true;
		cbxTObjNm.Visible = false;
		edTObjCnt.Visible = false;
	}
	if (gfn_isNull(sMFPIS1)) {
		sMFPIS1 = sMFPIS1_SN;
	}

    lfn_Search();
}


// 연계 해지 
function btnMatchCancel_OnClick(obj)
{	
	// 연계여부 확인
	if (ds_ioFireMapping.GetCount() < 1) {
		alert("연계된 정보가 없습니다.");
		return;
	}

	// 연계해지 사유 
	var par;
	if (sCourse == "50") { 
		par = "vNM="+quote(ds_ioFightFireCompanyPossessionHuman.GetColumn(ds_ioFightFireCompanyPossessionHuman.currow ,"NM"));
	} else {
		par = "";
	}
	var cancelReason = Dialog("cust::frmcust5011.xml", par, 520, 188, "", -1, -1);	
	if (cancelReason == "" || cancelReason == null) {
		alert("연계 사유를 입력해 주세요");
		return;
	} 

	tit_clearActionInfo();	
	
	
	// 파라미터 값
	var parValue = "MFPIS1=" + quote(sMFPIS1);  // 소방청 관리번호 
		parValue += " MKFSAPK=" + quote(sBarCode);  // 고객관리번호
		parValue += " MMGNO=" + quote(ds_ioFireMapping.GetColumn(0, "MMGNO"));	 // match 관리번호
		parValue += " MDELREMARK=" + quote(cancelReason);   // match 해지 사유
		parValue += " MSABUN=" + quote(gds_oUserInfo.GetColumn(0, "PSABUN"));  // 처리 사번
		 
		// 부가정보
		parValue += " SCMGNO=" + quote(sBarCode);
		parValue += " ADCD=8";
		parValue += " ADCONTENT=" + quote("[연계해지사유]" + cancelReason);
		parValue += " CTCD=" + quote(sCTCD);

	if (sCourse == "10") {   
        tit_addSingleActionInfo("cust:updateCust6000MatchCancel", "", 0, "");
        tit_addSingleActionInfo("cust:insertCust0010AdditionInfo", "", 0, "");   // 부가정보
        
		// 선임자 정보 체크 
		if (ds_ioFireSurveyAdministratorInfo.GetCount() > 0) {
			parValue += " GUBUN='1'";  // 선임자 정보가 있음	
		} 
	} else 	if (sCourse == "20") {   
		tit_addSingleActionInfo("cust:updateCust5010FireMappingCancel", "", 0, "");     // 연계취소 
		tit_addSingleActionInfo("cust:updateCust6010KFManagerMatch", "", 0, "");   // 소방청 선임자
		tit_addSingleActionInfo("cust:updateCust6000KFMatch", "", 0, "");   // 한국소방안전원 
        tit_addSingleActionInfo("cust:insertCust0010AdditionInfo", "", 0, "");   // 부가정보

		parValue += " GUBUN='2'";  // 선임자 정보가 있으면 연계해지
		parValue += " APNTDE="+ quote(sMFPIS3);
		parValue += " QUALFUSERSN="+ quote(sMFPIS4);

    } else if (sCourse == "30") {
		tit_addSingleActionInfo("cust:updateCust6100MatchCancel", "", 0, "");
		tit_addSingleActionInfo("cust:insertCust0010AdditionInfo", "", 0, "");   // 부가정보
		
		if (ds_ioFireMapping.GetCount() > 1) {
			parValue = "MFPIS1=" + quote(ds_ioFireMapping.GetColumn(cbxTObjNm.Index,"MFPIS1"));  // 소방청 관리번호 
			parValue += " MMGNO=" + quote(ds_ioFireMapping.GetColumn(cbxTObjNm.Index,"MMGNO"));	 // match 관리번호			
		} else {
			parValue = "MFPIS1=" + quote(sMFPIS1);  // 소방청 관리번호
			parValue += " MMGNO=" + quote(ds_ioFireMapping.GetColumn(0,"MMGNO"));	 // match 관리번호		
		}
		parValue += " MKFSAPK=" + quote(sBarCode);  // 고객관리번호
		parValue += " MDELREMARK=" + quote(cancelReason);   // match 해지 사유
		parValue += " MSABUN=" + quote(gds_oUserInfo.GetColumn(0,"PSABUN"));  // 처리 사번
		parValue += " SCMGNO=" + quote(sBarCode); 		// 부가정보
		parValue += " ADCD=8";		// 부가정보
		parValue += " ADCONTENT=" + quote("[연계해지사유]" + cancelReason);				// 부가정보
		parValue += " MGUBUN='2'";    // 허가번호 처리 구분
        parValue += " DPERMISSIONNO=" + quote(ds_ioDanMnfctretcInfo.GetColumn(0,"PRMISN_NO")); // 허가 번호 
        parValue += " CTCD=" + quote(sCTCD);
        
		// 선임자 정보 체크 
		if (ds_ioDanManagerInfo.GetCount() > 0) {
			parValue += " GUBUN='1'";  // 선임자 정보가 있음			
			parValue += " DMMGNO=" + ds_ioDanManagerInfo.GetColumn(ds_ioDanManagerInfo.currow,"DMMGNO");
		} else {
			parValue += " MANAGER=0";  // 선임자 정보가 없음
		}
    } else if (sCourse == "50") {
/*		parValue += " MFPIS3=" + quote(sMFPIS3);  // 소방청 관리번호
		parValue += " MFPIS4=" + quote(sMFPIS4);  // 소방청 관리번호
		parValue += " MFPIS5=" + quote(sMFPIS5);  // 소방청 관리번호
*/		
		parValue = "MFPIS3=" + quote(ds_ioFightFireCompanyPossessionHuman.GetColumn(ds_ioFightFireCompanyPossessionHuman.currow,"ENTRPS_SN"));
		parValue += " MFPIS4=" + quote(ds_ioFightFireCompanyPossessionHuman.GetColumn(ds_ioFightFireCompanyPossessionHuman.currow,"HOLD_QUALFUSER_SN"));
		parValue += " MFPIS5=" + quote(ds_ioFightFireCompanyPossessionHuman.GetColumn(ds_ioFightFireCompanyPossessionHuman.currow,"QUALFUSER_SN"));
		
		parValue += " MKFSAPK=" + quote(ds_ioFightFireCompanyPossessionHuman.GetColumn(ds_ioFightFireCompanyPossessionHuman.currow,"MKFSAPK"));  // 고객관리번호
		parValue += " MMGNO=" + quote(ds_ioFightFireCompanyPossessionHuman.GetColumn(ds_ioFightFireCompanyPossessionHuman.currow,"MMGNO"));  // match 관리번호
		parValue += " MDELREMARK=" + quote(cancelReason);   // match 해지 사유
		parValue += " MSABUN=" + quote(gds_oUserInfo.GetColumn(0, "PSABUN"));  // 처리 사번
		 
		// 부가정보
		parValue += " SCMGNO=" + quote(ds_ioFightFireCompanyPossessionHuman.GetColumn(ds_ioFightFireCompanyPossessionHuman.currow,"MKFSAPK"));  // 고객관리번호
		parValue += " ADCD=8";
		parValue += " ADCONTENT=" + quote("[연계해지사유]" + cancelReason);
		parValue += " CTCD=" + quote(sCTCD);
		
		tit_addSingleActionInfo("cust:updateCust6200MatchCancel", "", 0, "");
		tit_addSingleActionInfo("cust:insertCust0010AdditionInfo", "", 0, "");   // 부가정보

    } else if (sCourse == "80") {
		parValue += " MFPIS2=" + quote(sMFPIS2);  // 소방청 관리번호
		parValue += " MKFSAPK2=" + quote(sMKFSAPK2);  // 소방청 관리번호
		
		tit_addSingleActionInfo("cust:updateCust6300MatchCancel", "", 0, "");
		tit_addSingleActionInfo("cust:insertCust0010AdditionInfo", "", 0, "");   // 부가정보
		
    } else {
		return;
    } 

	// 서버 호출 
	tit_callService( 
		""
		,""
		,""	// inDs
		,"" // outDs	  	  
	    , parValue
		,"lfn_AfterMapping"
		,false
	);
}

// 연계 후
function lfn_AfterMapping(errCode, errMsg) {
	if(errCode != 0 ){ alert("오류가 발생했습니다."); }
	else{
		alert("정상적으로 처리 되었습니다.");		
		close();
	}
}




