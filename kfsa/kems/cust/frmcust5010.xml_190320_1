		tit_addSearchActionInfo("cust:searchCust5010DanStatusSuccession", false);        //11) 소방청 위험물         >> 허가정보등 기본정보 조회 >> 지위승계
		tit_addSearchActionInfo("cust:searchCust5010DanManagerInfo", false);             //12) 소방청 위험물        >> 허가정보등 기본정보 조회 >> 선임자        
    } else if (sCourse == "50") {
		// 소방 관리자
		tit_addSearchActionInfo("cust:searchCust5010FightFireCompany", false);                  //11) 소방청 소방업체(소방기술자)  >> 기본정보
