		tit_addSearchActionInfo("cust:searchCust5010FireSurveySubAdministratorInfo", false);//2) 소방청 소방안전관리대상물 >> 개황 >> 소방안전관리보조자 정보
		tit_addSearchActionInfo("cust:searchCust5010FireSurveyLicenseInfo", false);      //3) 소방청 소방안전관리대상물 >> 개황 >> 소방안전관리자정보 >> 보유자격증
		tit_addSearchActionInfo("cust:searchCust5010FireSurveyLicenseInfo2", false);      //3) 소방청 소방안전관리대상물 >> 개황 >> 소방안전관리보조자정보 >> 보유자격증
		tit_addSearchActionInfo("cust:searchCust5010FireSurveyRelationInfo", false);     //4) 소방청 소방안전관리대상물 >> 개황 >> 관계자정보	
		tit_addSearchActionInfo("cust:searchCust5010FireSurveyDong", false);             //5) 소방청 소방안전관리대상물 >> 동 현황 조회	
		tit_addSearchActionInfo("cust:searchCust5010FireSurveyFloorStatus", false);      //6) 소방청 소방안전관리대상물 >> 층별 현황 조회	
		tit_addSearchActionInfo("cust:searchCust5010FireSurveySFloorStatus", false);     //7) 소방청 소방안전관리대상물 >> 특수용도별 현황 조회	
		tit_addSearchActionInfo("cust:searchCust5010FireSurveyFloorE", false);           //8) 소방청 소방안전관리대상물 >> 층설비별 내역 조회	
    } else if (sCourse == "30" || sCourse == "80") {
		// 위험물
		tit_addSearchActionInfo("cust:searchCust5010DanMnfctretcInfo", false);           //9) 소방청 소방안전관리대상물 >> 허가정보등 기본정보 조회 >> 기본정보
