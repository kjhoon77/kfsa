	<Script><![CDATA[/*
 * 프로그램명 : frmcust5010.xml
 * 설명 : 소방민원정보시스템 연계정보 조회
 * 작성일 : 2009.12.12
 * 작성자 : 이창희
 */
 /*
 sMFPIS1_SN
 sMFPIS2_SN
 sMFPIS3_SN
 sMFPIS4_SN
 sMFPIS5_SN
 sMFPIS6_SN
 sCTCD
*/
#include "LIB::cust.js"; // 공통업무 관련

var sMFPIS1 = "";
var sMFPIS2 = "";
var sMFPIS3 = "";
var sMFPIS4 = "";
var sMFPIS5 = "";
var sMFPIS6 = "";
var menuId = "";
function Form_OnLoadCompleted(obj)
{
	gfn_Form_OnLoadCompleted(obj); // 공통 폼 로딩 이벤트 처리
    // 폼 환경설정
    ls_SystemGubun = gs_SystemGubun;
    ls_SystemMenuId = gs_SystemMenuId;
    var ls_FormId = split(sFormUrl, "::");
    ls_FormId = split(ls_FormId[1], ".");
    obj.Title = sFormTitle + "(" + ls_FormId[0] + ")";

    gfn_MakeRequiredInputMark(this); // 필수 입력란을 설정한다.

    ms_FormAuth = gfn_GetFormAuth(this); // 권한을 조회한다.
    if (ms_FormAuth == "W") { // 권한에 따른 버튼 설정
        lfn_SetFormAuth(true); // 저장 권한
    } else if (ms_FormAuth == "R") {
        lfn_SetFormAuth(false); // 읽기 권한
    } else {
        gfn_Alert(MSGI00080);
        close();
    }   
    
    menuId = gds_oLeftMenu.GetColumn(gds_oLeftMenu.FindRow("MURL","cust::"+this.id+".xml"),"MMENUID");
	
	// 직능에 따라 기본적으로 보여 주는 것이 다름
    if (sCourse == "10") {     
        tabTab1.tabpgTab1Page1.Enable = true;
        tabTab1.TabIndex = 0; 
        if (sCivilAppeal == -1) {
			tabTab1.tabpgTab1Page1.tabTab1.Visible=false;
			tabTab1.tabpgTab1Page1.tabTab2.Visible=true;
			
        } else {
			tabTab1.tabpgTab1Page1.tabTab1.Visible=true;
			tabTab1.tabpgTab1Page1.tabTab2.Visible=false;
        }
        lbTitle.Value = "소방대상물 관리(소방안전관리)";
        
    } else if (sCourse == "20") {     
        tabTab1.tabpgTab1Page1.Enable = true;
        tabTab1.TabIndex = 0; 
        if (sCivilAppeal == -1) {
			tabTab1.tabpgTab1Page1.tabTab1.Visible=false;
			tabTab1.tabpgTab1Page1.tabTab2.Visible=true;
			tabTab1.tabpgTab1Page1.tabTab2.tabpgTab1Page1.tabTab1.TabIndex = 1;
        } else {
			tabTab1.tabpgTab1Page1.tabTab1.Visible=true;
			tabTab1.tabpgTab1Page1.tabTab2.Visible=false;
			tabTab1.tabpgTab1Page1.tabTab1.tabpgTab1Page1.tabTab1.TabIndex = 1;
		}
        lbTitle.Value = "소방안전관리보조자";
        
    } else if (sCourse == "30" || sCourse == "80") {
        tabTab1.tabpgTab1Page2.Enable = true;     
        tabTab1.TabIndex = 1; 
        lbTitle.Value = "제조소 등 관리(위험물)";
        
        if (sCourse == "30") {
			tabTab1.tabpgTab1Page2.tabTab2.Visible = "false";
        } else {			
			tabTab1.tabpgTab1Page2.tabTab1.Visible = "false";
        }
        
    } else if (sCourse == "50") {
        tabTab1.tabpgTab1Page3.Enable = true;
        tabTab1.TabIndex = 2;  
        lbTitle.Value = "소방관련업체 관리(소방기술자)";
    }

    // 연계가 되어져 있다면 조회
    if(sCivilAppeal > 0) {
		lfn_MappingSearch(sBarCode);

    } else if (sCivilAppeal == -1){
		if (sCourse == "10" || sCourse == "20" || sCourse == "30" || sCourse == "80") {
			sMFPIS1 = sMFPIS1_SN;
			if (sCourse == "80") {			
				sMFPIS2 = sMFPIS2_SN;
			}
		} else if (sCourse == "50") {
			sMFPIS3 = sMFPIS3_SN;
			sMFPIS4 = sMFPIS4_SN;
			sMFPIS5 = sMFPIS5_SN;
		}
		btnMatchCancel.Enable = false;
		lfn_Search();
	} else  {
		alert("연계된 정보가 없습니다.");
		close();		
	}

}

