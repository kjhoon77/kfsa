/**************************************************************
로그 남기기 
**************************************************************/
function logWrite(gubun) {
	var ls_HistoryArg = "";
	var Action = "";
	var menuNM = "";
	if (gubun == "1") {
		// 조회이력을 저장한다.  
		ls_HistoryArg = " 고객관리번호=" + sBarCode;
		ls_HistoryArg += ", 직능구분=" + sCourse;
		Action = "처자료 상세조회";
		menuNM = "고객정보관리 연계정보";
	} else {
		if (sCourse == "10") {
			// 조회이력을 저장한다.  
			ls_HistoryArg = " 건축물일련번호=" + sMFPIS1;
			ls_HistoryArg += ", 직능구분=" + sCourse;
			Action = "처자료 상세조회";
			menuNM = "데이타연계관리 소방안전관리대상물";
		} else if (sCourse == "20") {
			// 조회이력을 저장한다.  
			ls_HistoryArg = " 건축물일련번호=" + sMFPIS1;
			ls_HistoryArg = ", 선임일자=" + sMFPIS3;
			ls_HistoryArg = ", 자격자관리번호=" + sMFPIS4;
			ls_HistoryArg += ", 직능구분=" + sCourse;
			Action = "처자료 상세조회";
			menuNM = "데이타연계관리 소방안전관리보조자";
		} else if (sCourse == "30") {
			// 조회이력을 저장한다.  
			ls_HistoryArg = " 제조소등일련번호=" + sMFPIS1;
			ls_HistoryArg += ", 직능구분=" + sCourse;
			Action = "처자료 상세조회";
			menuNM = "데이타연계관리 위험물안전관리자";
		} else if (sCourse == "80") {
			// 조회이력을 저장한다.  
			ls_HistoryArg = " 제조소등일련번호=" + sMFPIS1;
			ls_HistoryArg = " 탱크순번" + sMFPIS2;
			ls_HistoryArg += ", 직능구분=" + sCourse;
			Action = "처자료 상세조회";
			menuNM = "데이타연계관리 위험물운송자";
		} else if (sCourse == "50") {
			// 조회이력을 저장한다.  
			ls_HistoryArg = " 업체관리번호=" + sMFPIS3;
			ls_HistoryArg = " 보유자격자일련번호=" + sMFPIS4;
			ls_HistoryArg = " 자격자일련번호=" + sMFPIS5;
			ls_HistoryArg += ", 직능구분=" + sCourse;
			Action = "처자료 상세조회";
			menuNM = "데이타연계관리 소방기술자";
		} 
		
	}
	
    gfn_InsertFPISSearchPrintHistory(
        "S"
        , this.ID
        , menuNM
        , Action
        , ls_HistoryArg
    );
}

