}

/*
 * 기능 : 조회
 */
function lfn_Search(obj) {
     if (!gfn_FormValidate(this)) { // 입력 Validation Check
        return false;
    }

    ///////////////////// 조회이력 남기기
    ls_HistoryArg = "지부=" + quote(cbxJibu.value)   //-- 지부
              +" 년도=" + quote(edExamYear.text)  //-- 년도 
              +" 직능=" + quote(cbxCourse.value)   //-- 직능              
              +" 회차=" + quote(edExamOrder.text) // -- 회차             
              +" 접수번호1=" + quote(edJubsuSeqStart.text)
              +" 접수번호2=" + quote(edJubsuSeqEnd.text); // -- 접수번호// args 
    
      
        gfn_InsertSearchPrintHistory(
            "S"
            , this.ID
            , global.mnuLeft.cbxOpenForm.Text
            , ls_HistoryArg
        ); 
    /////////////////////  
    
    ds_ioExamSeatPrint.ClearData(); // 데이타셋 초기화

    tit_clearActionInfo();
	tit_addCsvSearchActionInfo("training:searchExamSeatPrint", false);
	// 서버 호출 
	tit_callService(
		""
		,""
		,""	// inDs
		,"ds_ioExamSeatPrint=ds_oExamSeatPrint"	// outDs
		,"EOMGGTMGNO=" + quote(cbxJibu.value)   //-- 지부
              +" EOYEAR=" + quote(edExamYear.text)  //-- 년도                                      //-- 년도              
              +" EOTCCOURSECD=" + quote(cbxCourse.value)   //-- 직능              
              +" EOEXAMORDER=" + quote(edExamOrder.text) // -- 회차
              +" EOHJUBSUNO=" + quote(edJubsuSeqStart.text)
              +" EOHJUBSUNO2=" + quote(edJubsuSeqEnd.text) // -- 접수번호// args 
		,"lfn_AfterSearchExamJubsu"
		,false
	);
}

/*
 * 기능 : 출력
 */
function lfn_Print(obj)
{
    // to do Validation Check
   
 
    if(ds_ioExamSeatPrint.count() == 0) {
        alert("출력하실 시험좌석번호표 자료가 없습니다. 다시 조회를 바랍니다.");
        return false;
    }
    
    if(!Confirm("시험좌석번호표 보고서를 출력하시겠습니까?보고서 미리보기를 하실수 있습니다. 출력량이 많을 경우 선택적으로 출력 바랍니다.")){  //
        return false;
    }
    
    ds_ioReport.copy(ds_ioExamSeatPrint);  // 1) 원본 복사           
    ds_ioReport.Filter("SEL = '1'"); // 2) 복사 후 필터함         
    ds_ioReport2.copyf(ds_ioReport);  // 3) 필터한 값을 실제 사용 위해 다른 곳에 복사     
    ds_ioReport.ClearData(); // 4) 데이타셋 초기화  레포트 출력위해 필터된 2)번의 데이터셋을 초기화함

    if(ds_ioReport2.count() == 0) {
        alert("출력하실 시험좌석번호표 자료가 없습니다. 왼쪽에 선택을 해 주시기 바랍니다..");
        return false;
    }
        ///////////////////// 조회이력 남기기
    ls_HistoryArg = "지부=" + quote(cbxJibu.value)   //-- 지부
              +" 년도=" + quote(edExamYear.text)  //-- 년도 
              +" 직능=" + quote(cbxCourse.value)   //-- 직능              
              +" 회차=" + quote(edExamOrder.text) // -- 회차             
              +" 접수번호1=" + quote(edJubsuSeqStart.text)
              +" 접수번호2=" + quote(edJubsuSeqEnd.text); // -- 접수번호// args 
    
      
        gfn_InsertSearchPrintHistory(
            "P"
            , this.ID
            , global.mnuLeft.cbxOpenForm.Text
            , ls_HistoryArg
        ); 
    /////////////////////  
    
    if (radPoto.Value == "N") {
		gfn_CallReportPopup("frmtraining0415R01ExamSeatNo.rex", "ds_ioReport2", "");
		
	} else if (radPoto.Value == "Y") {
	
		gfn_CallReportPopup("frmtraining0415R01ExamSeatNoPhoto.rex", "ds_ioReport2", "rpDirpath=" + GF_FILE_SERVER_URL + "kems");
	
	}
}
