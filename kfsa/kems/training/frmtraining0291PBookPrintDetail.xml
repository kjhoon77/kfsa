#include "LIB::COMMON.js"; // 공통 관련

/* 공통 필수 시작 */
function Form_OnLoadCompleted(obj) {
    gfn_Form_OnLoadCompleted(obj); // 공통 폼 로딩 이벤트 처리
    
	edSabun.Text = gds_oUserInfo.GetColumn(0, "PSABUN"); 
	edPersonnm.Text = gds_oUserInfo.GetColumn(0, "PPERSONNM"); 
		
    calJubsuStartDate.Value = GetDate();
    calJubsuEndDate.Value = GetDate();
}

function Form_OnUnloadCompleted(obj) {
    Close();
}

function Form_OnKeyDown(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam) {
    switch (nChar) {
        case 27 : // 종료(Esc)
            lfn_End();
            break;
        case 113 : // 조회(F2)
            lfn_Search();
            break;
    }
}

/*
 * 기능 : 조회
 */
function lfn_Search(obj) {
    if (gfn_IsNull(edSabun.Text) || gfn_IsNull(calJubsuStartDate.Value)|| gfn_IsNull(calJubsuEndDate.Value)) {
        gfn_Alert("등록자, 등록기간을 입력해 주십시오."); 
        return false;
    }
    ds_olist.ClearData(); // 데이타셋 초기화
    tit_clearActionInfo();
	tit_addSearchActionInfo("training:searchTraining0291PList", false);
	// 서버 호출 
	tit_callService(
		""
		,""
		,""	// inDs
		,"ds_olist=ds_olist"	// outDs
		,"REGSABUN=" + quote(edSabun.Text)
            + " FRDATE=" + quote(left(calJubsuStartDate.Value,8))
            + " TODATE=" + quote(left(calJubsuEndDate.Value,8))
		,"lfn_AfterSearchList"
		,false
	);
}

function lfn_AfterSearchList(nErrorCode, strErrorMsg) {
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }
}
/*
 * 기능 : 종료
 */
function lfn_End(obj) { 
    Close();
}
/* 공통 필수 종료 */ 

function Button0_OnClick(obj)
{
	alert(left(calJubsuStartDate.Value,8)) ;
}

function gdList_OnCellClick(obj,nRow,nCell,nX,nY,nPivotIndex)
{
	
	ds_olist_OnRowPosChanged(ds_olist,nRow,nRow);
}

function ds_olist_OnRowPosChanged(obj,nOldRow,nRow)
{
	
	// 그리드이 값 복사
    if (nRow < 0 ) return false ;
    
    ds_olistDetail.ClearData();
    var vTBPMGNO = obj.GetColumn(nRow, "TBPMGNO");
    if(vTBPMGNO <> ""){			
		tit_clearActionInfo();
		tit_addCsvSearchActionInfo("training:searchTraining0291PListDetail", false);
		tit_callService(
			""
			,""
			,""	// inDs
			,"ds_olistDetail=ds_olistDetail"	// outDs
			,"V_TBPMGNO=" + quote(vTBPMGNO) // args 
			,""
			,false
			,false
			,true
		);	
		
		
    }
    
}
