#include "LIB::COMMON.js"; // 공통 관련

/* 공통 필수 시작 */
function Form_OnLoadCompleted(obj) {
    gfn_Form_OnLoadCompleted(obj); // 공통 폼 로딩 이벤트 처리

    if (IsExistVar("sBsmgno")) {
    
		if (sBsbccd == "105") {	// 강습-접수(일괄방송)
		
			gdSmsStat.Format = "Training";
		
		} else if (sBsbccd == "205") {	// 시험-접수(일괄방송)
		
			gdSmsStat.Format = "Exam";
		
		} else if (sBsbccd == "305") {	// 실무-접수(일괄방송)
		
			gdSmsStat.Format = "EduJubsu";
		
		} else if (sBsbccd == "306") {	// 실무-교육안내문(일괄방송)
		
			gdSmsStat.Format = "EduData";
		
		} else if (sBsbccd == "307") {	// 실무-교육이수(일괄방송)
		
			gdSmsStat.Format = "EduIssue";

		} else if (sBsbccd == "401") {	// 초빙강사
		
			gdSmsStat.Format = "Speaker";
		
		}
		
            lfn_Search();
    }

}

function Form_OnUnloadCompleted(obj) {
    Close();
}

function Form_OnKeyDown(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam) {
    switch (nChar) {
        case 27 : // 종료(Esc)
            lfn_End();
            break;
        case 113 : // 조회(F2)
            lfn_Search();
            break;
    }
}

/*
 * 기능 : 조회
 */
function lfn_Search(obj) {
    ds_oReserveSend.ClearData(); // 데이타셋 초기화

    tit_clearActionInfo();
	tit_addSearchActionInfo("training:searchTraining0694ReserveSendList", false);
	// 서버 호출 
	tit_callService(
		""
		,""
		,""	// inDs
		,"ds_oReserveSend=ds_oReserveSend"	// outDs
		,"BSMGNO=" + quote(sBsmgno) 
		
		 + " BSBCCD=" + quote(sBsbccd)// args
		,"lfn_AfterSearchReserveSendList"
		,false
	);
}

/*
 * 기능 : 종료
 */
function lfn_End(obj) {
    Close();
}
/* 공통 필수 종료 */

/* 사용자 함수 */
/*
 * 기능 : 조회 후 처리해야 할 내용
 * parameter : nErrorCode - 에러코드
 *             strErrorMsg - 에러메시지
 * Return : 없음
 */
function lfn_AfterSearchReserveSendList(nErrorCode, strErrorMsg) {
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }

    gfn_SearchMessage(gdSmsStat); // 조회 메세지를 처리하고 그리드의 정렬 헤드 마크를 제거한다.
}

/* 사용자 이벤트 함수 */
function lfn_SortGrid_OnHeadClick(obj,nCell,nX,nY,nPivotIndex) {
    var lobj_Dataset = object(obj.BindDataset);
    // 데이터가 있는 경우 해당 컬럼을 기준으로 정렬한다.
    if (lobj_Dataset.GetRowCount() > 1) {
        gfn_GridCellSort(obj, nCell); // 해당 셀정렬을 호출한다.
    }
}

function btnMutilSort_OnClick(obj)
{
	gfn_GridMultiSort(gdSmsStat, 0); // 멀티 정렬을 실행한다.
}
