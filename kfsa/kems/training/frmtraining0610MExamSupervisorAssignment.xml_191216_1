	<Script><![CDATA[/*
 * 프로그램명 : frmtraining0610MExamSupervisorAssignment.xml
 * 설명 : 시험감독관배정
 * 작성일 : 2009.08.29
 * 작성자 : 이규희
 */

#include "LIB::training.js"; // 공통업무 관련

var ls_ExamRoomMgno = "";
var ls_ExamSupervisorMgno = "";
/* 공통 필수 시작 */
function Form_OnLoadCompleted(obj) {
    gfn_Form_OnLoadCompleted(obj); // 공통 폼 로딩 이벤트 처리

    // 폼 환경설정
    ls_SystemGubun = gs_SystemGubun;
    ls_SystemMenuId = gs_SystemMenuId;
    var ls_FormId = split(sFormUrl, "::");
    ls_FormId = split(ls_FormId[1], ".");
    obj.Title = sFormTitle + "(" + ls_FormId[0] + ")";

    gfn_MakeRequiredInputMark(this); // 필수 입력란을 설정한다.

    ms_FormAuth = gfn_GetFormAuth(this); // 권한을 조회한다.
    if (ms_FormAuth == "W") { // 권한에 따른 버튼 설정
        lfn_SetFormAuth(true); // 저장 권한
    } else if (ms_FormAuth == "R") {
        lfn_SetFormAuth(false); // 읽기 권한
    } else {
        gfn_Alert(MSGI00080);
        close();
    }

    // 공통 코드 조회
    gfn_SearchTrainingCourse("E", ds_oCourse, "Y", "전체"); // 직능 코드 조회
    cbxCourse.Value = "22";
    gfn_SearchJibu(cbxJibu, "Y", "전체"); // 지부 코드 조회
    gfn_SearchCommonCode(ds_oSupervisorGubun, "SUPERVISORGUBUN", "Y"); // 감독관구분코드 조회
    radGubun.Value = "01";

    edExamYear.Text = left(GetDate(), 4);
}

function Form_OnKeyDown(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam) {
    // to do

    gfn_Form_OnKeyDown(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam); // 공통 키 이벤트 처리
}

function Form_OnSize(obj,nCx,nCy,nState)
{
    if (gs_IsResizingForm == "Y") {
        gdExamSupervisor.Height = nCy - gdExamSupervisor.Top - 4;
        gdExamSupervisor.Width = nCx - gdExamSupervisor.Left - 4;
    }
}

/*
 * 기능 : 조회
 */
function lfn_Search(obj) {
    ds_ioExamSupervisor.ClearData(); // 데이타셋 초기화
    
    // 조회이력을 저장한다.  
    ls_HistoryArg = "지부=" + quote(cbxJibu.Text)
                + ",년도=" + quote(edExamYear.Text)
                + ",직능=" + quote(cbxCourse.Text)
                + ",회차=" + quote(edExamOrder.Text)
                + ",시험장=" + quote(edExamRoom.Text)
                + ",감독관관리번호=" + quote(ls_ExamSupervisorMgno);
    gfn_InsertSearchPrintHistory(
        "S"
        , this.ID
        , global.mnuLeft.cbxOpenForm.Text
        , ls_HistoryArg
    );
    medTotalCnt.Value = "";
    tit_clearActionInfo();
	tit_addCsvSearchActionInfo("training:searchExamSupervisorAssign", false);
	// 서버 호출 
	tit_callService(
		""
		,""
		,""	// inDs
		,"ds_ioExamSupervisor=ds_oExamSupervisor"	// outDs
		,"EOMGJIBUCD=" + quote(cbxJibu.Value)
            + " EOYEAR=" + quote(edExamYear.Text)
            + " TCCOURSECD=" + quote(cbxCourse.Value)
            + " EOEXAMORDER=" + quote(edExamOrder.Text)
            + " EREXAMROOM=" + quote(edExamRoom.Text)
            + " SIMGNO=" + quote(ls_ExamSupervisorMgno) // args 
		,"lfn_AfterSearchExamSupervisor"
		,false
	);
}

/*
 * 기능 : 추가
 */
function lfn_Input(obj) {
	if (!gfn_Confirm(MSGI00070)) { // 신규 추가 하시겠습니까?\n입력완료 후 저장 버튼을 누르세요.
        return false;
	}

	mb_IsNewData = true; // 저장버튼이 클릭 설정
    lfn_Reset(); // 값 초기화

    // 기본값 설정
    radGubun.Value = "01";
}

/*
 * 기능 : 삭제
 */
function lfn_Delete(obj) {
    if (ds_ioExamSupervisor.currow < 0) {
        gfn_Alert(MSGI00030); // 삭제할 레코드가 없습니다.
        return false;
    }

	if (!gfn_Confirm(MSGW00030)) { // 정말로 삭제 하시겠습니까?
        return false;
	}
	
	lfn_SaveModifyHistory("D"); // 변경이력을 저장한다.

    ds_ioExamSupervisor.DeleteRow(ds_ioExamSupervisor.currow); // 선택된 ROW를 삭제한다.

    gfn_FormInit(this); // 입력란 초기화

	tit_clearActionInfo();
	tit_addSaveActionInfo(
		""	// insert
		, ""	// update
		, "training:deleteExamSupervisorAssign"	// delete
		, ""	// flag column 명
		, ""	// key sql id
		, 0	// key 증가 값
		, "" 
		, 0 
		, "N"	// 실행타입
	);
	// 서버 호출 
	tit_callService(
		""
		,""
		,"ds_iExamSupervisor=ds_ioExamSupervisor:U"	// inDs
		,""	// outDs
		,""	// args 
		,"lfn_AfterSaveExamSupervisor"
		,false
	);
}

/*
 * 기능 : 저장
 */
function lfn_Save(obj) {
    if (!gfn_FormValidate(this)) { // 입력 Validation Check
        return false;
    }   

    if (mb_IsNewData || ds_ioExamSupervisor.GetCount() == 0) { // 신규 저장
        // 키 중복 체크
        if (gfn_IsNull(ms_ExamMgno)) {
            gfn_Alert(MSGI00200); // "시험을 먼저 선택해 주십시오.";
            edExamOrder.SetFocus();
            return false;
        }
        
        for (i=0;i<ds_oExamSupervisorChk.GetRowCount();i++) {
            if (ds_oExamSupervisorChk.GetColumn(i, "SAGUBUN") == radGubun.Value
                || ds_oExamSupervisorChk.GetColumn(i, "SASIMGNO") == ls_ExamSupervisorMgno) {
                gfn_Alert("이미 등록된 감독관입니다.");
                return false;
            }
        }        
    
        if (!gfn_Confirm(MSGW00020)) { // 정말로 추가 저장 하시겠습니까?
            return false;
        }

        // 입력된 값을 그리드로 복사한다.
        ds_ioExamSupervisor.InsertRow(0);
        ds_ioExamSupervisor.SetColumn(0, "EOMGNO", ms_ExamMgno);
        ds_ioExamSupervisor.SetColumn(0, "EREXAMROOM", edExamRoom.Text);
        ds_ioExamSupervisor.SetColumn(0, "SIMGNO", ls_ExamSupervisorMgno);
        ds_ioExamSupervisor.SetColumn(0, "SAGUBUN", radGubun.Value);
        
        lfn_SaveModifyHistory("I"); // 변경이력을 저장한다.
    } else { // 수정 저장
        if (ds_ioExamSupervisor.GetColumn(ds_ioExamSupervisor.currow, "EOMGNO") != ms_ExamMgno) {
            gfn_Alert("년도 직능 회차는 수정이 불가능합니다.");
            cbxCourse.SetFocus();
            return false;
        }
        if (!gfn_Confirm(MSGW00040)) { // 정말로 수정 저장 하시겠습니까?
            return false;
        }

        // 입력된 값을 그리드로 복사한다.
        ds_ioExamSupervisor.SetColumn(ds_ioExamSupervisor.currow, "SAGUBUN", radGubun.Value);
        
        lfn_SaveModifyHistory("U"); // 변경이력을 저장한다.
    }

    gfn_FormInit(this); // 입력란 초기화

    tit_clearActionInfo();
    tit_addSaveActionInfo(
        "training:insertExamSupervisorAssign"	// insert
        , "training:updateExamSupervisorAssign"	// update
        , ""	// delete
        , ""	// flag column 명
        , ""	// key sql id
        , 0	// key 증가 값
        , "" 
        , 0 
        , "N"	// 실행타입
    );
    // 서버 호출 
    tit_callService(
        ""
        ,""
        ,"ds_iExamSupervisor=ds_ioExamSupervisor:U"	// inDs
        ,""	// outDs
        ,""	// args 
        ,"lfn_AfterSaveExamSupervisor"
        ,false
    );
}
