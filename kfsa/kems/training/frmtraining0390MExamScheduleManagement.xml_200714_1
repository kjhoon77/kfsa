	<Script><![CDATA[/*
 * 프로그램명 : frmtraining0390MExamScheduleManagement.xml
 * 설명 : 시험일정관리
 * 작성일 : 2009.08.22
 * 작성자 : 이규희
 */

#include "LIB::training.js"; // 공통업무 관련

var ls_ExamPlaceSeq = ""; // 시험장 일련번호

/* 공통 필수 시작 */
function Form_OnLoadCompleted(obj) {
    gfn_Form_OnLoadCompleted(obj); // 공통 폼 로딩 이벤트 처리

    // 폼 환경설정
    ls_SystemGubun = gs_SystemGubun;
    ls_SystemMenuId = gs_SystemMenuId;
    var ls_FormId = split(sFormUrl, "::");
    ls_FormId = split(ls_FormId[1], ".");
    obj.Title = sFormTitle + "(" + ls_FormId[0] + ")";

    gfn_MakeRequiredInputMark(this); // 필수 입력란을 설정한다.

    ms_FormAuth = gfn_GetFormAuth(this); // 권한을 조회한다.
    if (ms_FormAuth == "W") { // 권한에 따른 버튼 설정
        lfn_SetFormAuth(true); // 저장 권한
    } else if (ms_FormAuth == "R") {
        lfn_SetFormAuth(false); // 읽기 권한
    } else {
        gfn_Alert(MSGI00080);
        close();
    }
/*		// 2019.05.02 전지부 권한부여
	 // [특급소방안전관리자] 평가관리과와 관리자만 시험 공개일시 입력/수정 권한 부여  
	 
	 
	if (gds_oUserInfo.GetColumn(0, "PDEPTCD") == "0253" || 
	
	    gds_oUserInfo.GetColumn(0, "PSABUN") == "20041104") {
		
		calExam1OpenDate.Enable = true;
		
		medExam1OpenTime.Enable = true;

		calExam2OpenDate.Enable = true;
		
		medExam2OpenTime.Enable = true;
		
	} else {
	
		calExam1OpenDate.Enable = false;
		
		medExam1OpenTime.Enable = false;

		calExam2OpenDate.Enable = false;
		
		medExam2OpenTime.Enable = false;
		
	}
*/
	// 시스템관리자만 tjmgno, tpmgno가 보이도록 설정
	if (gds_oUserInfo.GetColumn(0, "ADMINGUBUN") == "SM") {
		lbImsiEOMGNO.Visible = true;
		edImsiEOMGNO.Visible = true;
		edImsiEOMGNO.Text = "";
	} else {
		lbImsiEOMGNO.Visible = false;
		edImsiEOMGNO.Visible = false;
		edImsiEOMGNO.Text = "";
	}

    // 공통 코드 조회
    gfn_SearchTrainingCourse("E", ds_oCourse, "Y", ""); // 직능 코드 조회
    cbxCourse.Value = "22";
    gfn_SearchJibu(cbxJibu, "Y", "전체"); // 지부 코드 조회
    gfn_SearchCommonCode(ds_oInternetJubsuStatus, "IJUBSUGUBUN", "Y", "선택"); // 인터넷접수상태 조회
    cbxInternetJubsuStatus = "03"; // 접수예정
    edStdYear.Text = left(GetDate(), 4);
    
	radSMSSendYN.Value = "0";
    radSMSSendYN_OnChanged(radSMSSendYN, "0");
    edExamOrder.SetFocus();
    
    // 시험공고일시, 접수일시
    lbGonggoDate.Visible = true;
	calGonggoDate.Visible = true;
	medGonggoTime.Visible = true;
	lbJubsuDate.Visible = true;
	lbJubsuDate.Text = "접수시작일시";
	calJubsuStartDate.Visible = true;
	medJubsuStartTime.Visible = true;
	lbJubsuTerm.Visible = false;
	calJubsuEndDate.Visible = false;
	medJubsuEndTime.Visible = false;
	    lfn_Search();
}

function Form_OnKeyDown(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam) {
    // to do

    gfn_Form_OnKeyDown(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam); // 공통 키 이벤트 처리
}

function Form_OnSize(obj,nCx,nCy,nState)
{
    if (gs_IsResizingForm == "Y") {
        gdExamOrder.Height = nCy - gdExamOrder.Top - 4;
        gdExamOrder.Width = nCx - gdExamOrder.Left - 4;
    }
}

/*
 * 기능 : 조회
 */
function lfn_Search(obj) {
    if (edStdYear.Text == "") { // 입력 Validation Check
        gfn_Alert("년도를 입력하십시오.");
        edStdYear.SetFocus();
        return false;
    }
    ds_ioExamOrder.ClearData(); // 데이타셋 초기화
    
    // 조회이력을 저장한다.  
    ls_HistoryArg = "지부=" + quote(cbxJibu.Text)
                + ",년도=" + quote(edStdYear.Text)
                + ",직능=" + quote(cbxCourse.Text);
    gfn_InsertSearchPrintHistory(
        "S"
        , this.ID
        , global.mnuLeft.cbxOpenForm.Text
        , ls_HistoryArg
    );
    
    medTotalCnt.Value = "";

    tit_clearActionInfo();
	tit_addCsvSearchActionInfo("training:searchExamOrder", false);
	// 서버 호출 
	tit_callService(
		""
		,""
		,""	// inDs
		,"ds_ioExamOrder=ds_oExamOrder"	// outDs
		,"EOMGJIBUCD=" + quote(cbxJibu.Value)
            + " EOYEAR=" + quote(edStdYear.Text)
            + " TCCOURSECD=" + quote(cbxCourse.Value)	// args 
		,"lfn_AfterSearchExamOrder"
		,false
	);
}

/*
 * 기능 : 추가
 */
function lfn_Input(obj) {
	if (!gfn_Confirm(MSGI00070)) { // 신규 추가 하시겠습니까?\n입력완료 후 저장 버튼을 누르세요.
        return false;
	}

	mb_IsNewData = true; // 저장버튼이 클릭 설정
    lfn_Cancel(); // 값 초기화

    // 기본값 설정
    edExamOrder.Text = ToInteger(ds_ioExamOrder.GetColumn(0, "EOEXAMORDER")) + 1; 
    edExamOrder_OnKillFocus(edExamOrder);
    calExamDate.Value = left(GetDate(), 8);
    if (cbxCourse.Value == "10") {
    
		radChasuGubun.Value = "1";	// 특급차수
		
		tmExamTimeStart.Value = "0930";	
		tmExamTimeEnd.Value = "1130";
		
		tmExamTimeStart2.Value = "0930";	
		tmExamTimeEnd2.Value = "1130";
		
		tmExamTimeStart2.Enable = true;
		
		tmExamTimeEnd2.Enable = true;
		
		radChasuGubun_OnChanged("radChasuGubun", radChasuGubun.Value); 	// 특급 차수구분
    
    } else {
    
		tmExamTimeStart.Value = "1600";	
		tmExamTimeEnd.Value = "1700";
		
		tmExamTimeStart2.Value = "";	
		tmExamTimeEnd2.Value = "";
		
		tmExamTimeStart2.Enable = false;
		
		tmExamTimeEnd2.Enable = false;
		
	}
	
    radInetJubsuYn.Value = "N";
    
    if (cbxCourse.Value == "10") {
    
		radExamGubun4.Index = 0;
		
    } else if (cbxCourse.Value == "21" || cbxCourse.Value == "22" || cbxCourse.Value == "23") {
    
		radExamGubun1.Index = 0;
    
    } else {
    
		radExamGubun3.Index = 0;
    
    }
    
	radSMSSendYN.Value = "0";	// 안내문자 미발송
    radSMSSendYN_OnChanged(obj, "0");
    edExamOrder.SetFocus();
}

/*
 * 기능 : 삭제
 */
function lfn_Delete(obj) {
    if (ds_ioExamOrder.currow < 0) {
        gfn_Alert(MSGI00030); // 삭제할 레코드가 없습니다.
        return false;
    }
/*    
    if (ds_ioExamOrder.GetColumn(ds_ioExamOrder.currow, "EOJUBSUMAXCNT") != 0) {
        gfn_Alert("삭제가 불가능합니다."); // 삭제할 레코드가 없습니다.
        return false;
	}
*/
    ds_ioJubsu.ClearData(); // 데이타셋 초기화
    tit_clearActionInfo();
	tit_addCsvSearchActionInfo("training:searchExamJubsuListCnt", false);
	// 서버 호출 
	tit_callService(
		""
		,""
		,""	// inDs
