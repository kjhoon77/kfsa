/*
 * 기능 : 저장
 */
function lfn_Save(obj) {
	// 특급이면
	if (cbxCourse.Value == "10") {
	
		if (gfn_isNull(radExamGubun4.Value)) {
		
			gfn_Alert("특급소방안전관리자 회차구분이 선택되지 않았습니다.");
			
			return false;
		}
		
		if (gfn_isNull(radChasuGubun.Value)) {
		
			gfn_Alert("특급소방안전관리자 차수가 선택되지 않았습니다.");
			
			return false;
			
		}
	
	}
	
    if (!gfn_FormValidate(this)) { // 입력 Validation Check
        return false;
    }
    // 안내문자 자동발송
    if (radSMSSendYN.Value == "1") {
    
		if (gfn_isNull(calSendDate.Value)) {
	
			gfn_Alert("안내문자 자동발송일이 입력되지 않았습니다.");
	
			calSendDate.SetFocus();	
			return false;
			
			
		}
    
    
		if (!gfn_isNull(calExamDate.Value) && (calExamDate.Value <= calSendDate.Value)) {	
			gfn_Alert("안내문자 자동발송일이 시험일보다 늦습니다.");	
			calSendDate.Value = substr(AddDate(calExamDate.Value,-2) ,0,8);
	
			calSendDate.SetFocus();	
			return false;	
		}	}
	
	if (!gfn_isNull(calGonggoDate.Value) && gfn_isNull(medGonggoTime.Value)) {
	
		gfn_Alert("시험공고일시의 시간이 입력되지 않았습니다.");
		medGonggoTime.SetFocus();
		return false;
		
	}
	
	if (!gfn_isNull(calJubsuStartDate.Value) && gfn_isNull(medJubsuStartTime.Value)) {
	
		gfn_Alert("접수시작일시의 시간이 입력되지 않았습니다.");
		medJubsuStartTime.SetFocus();
		return false;
		
	}
	

    if (mb_IsNewData) { // 신규 저장
        // 키 중복 체크
    
        if (!gfn_Confirm(MSGW00020)) { // 정말로 추가 저장 하시겠습니까?
            return false;
        }
        
        ds_oExamOrderMgno.ClearData(); // 데이타셋 초기화

        tit_clearActionInfo();
        tit_addCsvSearchActionInfo("training:searchExamOrderMgno", false);
        // 서버 호출 
        tit_callService(
            ""
            ,""
            ,""	// inDs
            ,"ds_oExamOrderMgno=ds_oExamOrderMgno"	// outDs
            ,"EOMGTMGNO=" + quote(cbxJibu.Value)
                + " EOYEAR=" + quote(edStdYear.Text)
                + " EOTCCOURSECD=" + quote(cbxCourse.Value)
                + " EOEXAMORDER=" + quote(edExamOrder.Text)// args 
            ,""
            ,false
            ,""
            ,true
        );
        
        if (ds_oExamOrderMgno.GetCount() >= 1) {
            gfn_Alert("중복된 회차입니다.");
            edExamOrder.SetFocus();
            return;
        }
        
        // 입력된 값을 그리드로 복사한다.
        ds_ioExamOrder.InsertRow(0);
        ds_ioExamOrder.SetColumn(0, "EOMGJIBUCD", cbxJibu.Value);
        ds_ioExamOrder.SetColumn(0, "EOYEAR", edStdYear.Text);              
        ds_ioExamOrder.SetColumn(0, "TCCOURSECD", cbxCourse.Value);             
