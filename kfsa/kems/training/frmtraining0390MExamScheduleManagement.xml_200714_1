        ds_ioExamOrder.SetColumn(0, "EOREGION", edRegion.Text);
        lfn_SaveModifyHistory("I"); // 변경이력을 저장한다.
    } else { // 수정 저장
	
		// 중복된 회차가 있는지 검색
		for (var i = 0; i < ds_ioExamOrder.GetRowCount(); i++) {
		
