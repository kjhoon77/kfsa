            ds_ioExamOrder.SetColumn(0, "EOCHASUGUBUN", radChasuGubun.Value);	// 특급차수
            
            ds_ioExamOrder.SetColumn(0, "EOEXAMORDERGUBUN", radExamGubun4.Value);
        } else if (cbxCourse.Value == '21' || cbxCourse.Value == '22' || cbxCourse.Value == '23') {
