	<Script><![CDATA[/*
 * 프로그램명 : frmtraining0257PLicenseIssueCancel.xml
 * 설명 : 발급이력이력취소팝업
 * 작성일 : 2023.02.01
 * 작성자 : jjh 
 */

#include "LIB::COMMON.js"; // 공통 관련
var vCancelYn = "";

/* 공통 필수 시작 */
function Form_OnLoadCompleted(obj) {
    gfn_Form_OnLoadCompleted(obj); // 공통 폼 로딩 이벤트 처리
    if (IsExistVar("sLHLLCSNO")) {
        lfn_Search(obj);
    }
    edCanselRemark.Text = "발급취소";
    edCanselRemark.SetFocus();
}

function Form_OnUnloadCompleted(obj) {
    Close(vCancelYn);
}

function Form_OnKeyDown(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam) {
    switch (nChar) {
        case 27 : // 종료(Esc)
            lfn_End();
            break;
        case 115 : // 저장(F4)
            lfn_Save();
            break;
    }
}

/*
 * 기능 : 조회
 */
function lfn_Search(obj) {
    medLcsNo.Value = sLHLLCSNO;
    edGubun.Text = iif(sLHSEQ==1, "신규발급", "재발급");    
    edSEQ.Text = sLHSEQ;
}

/*
 * 기능 : 저장
 */
function lfn_Save(obj) {
    if (!gfn_Confirm("발급이력을 취소 하시겠습니까?")) {
        return false;
    }
	// 출력내역 취소
	vCancelYn = "Y"; // 출력취소여부 확인  
	vCancelYn = edCanselRemark.Text;
    Close(vCancelYn);
}

/*
 * 기능 : 종료
 */
function lfn_End(obj) {
    Close(vCancelYn);
} 
