function radProcGubun_OnClick(obj){
    cbxJubsuStatus1.Value = "";
    cbxJubsuStatus2.Value = "";
    cbxJubsuStatus3.Value = "";
        
	if (radProcGubun.Value == "10") {
        cbxJubsuStatus1.Enable = true;
        cbxJubsuStatus2.Enable = false;
        cbxJubsuStatus3.Enable = false;
        btnPrint.Enable = false;
    } else if (radProcGubun.Value == "20") {
        cbxJubsuStatus1.Enable = false;
        cbxJubsuStatus2.Enable = true;
        cbxJubsuStatus3.Enable = false;
        btnPrint.Enable = false;
    } else if (radProcGubun.Value == "30") {
        cbxJubsuStatus1.Enable = false;
        cbxJubsuStatus2.Enable = false;
        cbxJubsuStatus3.Enable = true;
        btnPrint.Enable = true;
    } else {
        cbxJubsuStatus1.Enable = false;
        cbxJubsuStatus2.Enable = false;
        cbxJubsuStatus3.Enable = false;
        btnPrint.Enable = false;
    }
}

function gdInternetJusu_OnCellDblClick(obj,nRow,nCell,nX,nY,nPivotIndex) {
    //if (nCell == 11) {
        var sArg = "sJubsuNo=" + quote(ds_oTrainingJusu.GetColumn(nRow, "TJMGNO"))
            + " sJubsuStatus=" + quote(ds_oTrainingJusu.GetColumn(nRow, "TJHSTATUSCD"))
