 * 기능 : 조회
 */
function lfn_Search(obj) {

    var ls_SearchGubun = "";

    if (!gfn_FormValidate(this)) { // 입력 Validation Check
        return false;
    }

    if (radProcGubun.Value == "10") {
        ls_SearchGubun = cbxJubsuStatus1.Value;
    } else if (radProcGubun.Value == "20") {
        ls_SearchGubun = cbxJubsuStatus2.Value;
    } else if (radProcGubun.Value == "30") {
        ls_SearchGubun = cbxJubsuStatus3.Value;
    }

    if (length(ls_SearchGubun)<1) {
        ls_SearchGubun = radProcGubun.Value;
    }
    
    ds_oTrainingJusu.ClearData(); // 데이타셋 초기화

    tit_clearActionInfo();
	tit_addCsvSearchActionInfo("training:searchInternetTrainingJubsu", false);
	// 서버 호출 
	tit_callService(
		""
		,""
		,""	// inDs
		,"ds_oTrainingJusu=ds_oInternetTrainingJubsu"	// outDs
		,"TJPERSONNM=" + quote(edPersonNm.Text)
            + " STARTJUBSUDATE=" + quote(calJubsuDateStart.Value)
            + " ENDJUBSUDATE=" + quote(calJubsuDateEnd.Value)
            + " TJHSTATUSCD=" + quote(ls_SearchGubun)
            + " TOHJUBSUGUBUN=" + quote(radWorkGubun.Value)
            + " TOTRAININGORDER=" + quote(edTrainingOrder.Text)
            + " TOTCCOURSECD=" + quote(cbxCourse.Value)
            + " TOMGGTMGNO=" + quote(cbxJibu.Value) // args 
		,""
		,false
	);
	
	medTotalCnt.Value = ds_oTrainingJusu.GetRowCount();

}
