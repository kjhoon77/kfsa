    gfn_Form_OnKeyDown(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam); // 공통 키 이벤트 처리
}


function lfn_Search(obj)
{
    ds_ioApproveList.ClearData(); // 데이타셋 초기화

    medJubsuCnt.Value = "";

    tit_clearActionInfo();

	tit_addCsvSearchActionInfo("training:searchTraining0364MInternetLicenseApprovalList", false);

	// 서버 호출 

	tit_callService(

		""

		,""

		,""	// inDs

		,"ds_ioApproveList=ds_ioApproveList"	// outDs

		,"AJGTMGNO=" + quote(cbxJibu.Value)

            + " AJCCOURSECD=" + quote(cbxCourse.Value)

            + " AJFROMDT=" + quote(cldFromdt.Value)

            + " AJTODT=" + quote(cldTodt.Value)

            + " AJSTATUS=" + quote(cbxStatus.Value)
            	
            + " AJGUBUN=" + quote(cbAjgubun.Value)	// args 
            
            + " AJISSUEGUBUN=" + quote(cbx_Issuegubun.Value)
      
		,"lfn_AfterSearchTraining0364MInternetLicenseApprovalList"

		,false
	);	
}

function lfn_AfterSearchTraining0364MInternetLicenseApprovalList(nErrorCode, strErrorMsg) {

    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.

        gfn_ErrorMsg(nErrorCode, strErrorMsg);

        return;

    }

    medJubsuCnt.Value = ds_ioApproveList.RowCount();

    gfn_SearchMessage(gdList); // 조회 메세지를 처리하고 그리드의 정렬 헤드 마크를 제거한다.

}

function btnToExcel_OnClick(obj)
{
   if (ds_ioApproveList.GetRowCount() < 1) {

        gfn_Alert("출력할 데이터가 존재하지 않습니다.");

        return false;

    }

	lfn_Excel(obj);		
}


function Form_OnSize(obj,nCx,nCy,nState) {
    if (gs_IsResizingForm == "Y") {
        gdList.Height = nCy - gdList.Top - 1;
        gdList.Width = nCx - gdList.Left - 1;
    }
}



