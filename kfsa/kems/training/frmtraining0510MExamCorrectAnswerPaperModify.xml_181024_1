	<Script><![CDATA[/*
 * 프로그램명 : frmtraining0510MExamCorrectAnswerPaperModify.xml
 * 설명 : 정답지편집
 * 작성일 : 2009.10.21
 * 작성자 : 이규희
 */

#include "LIB::training.js"; // 공통업무 관련

var passWord = "";  // 정답지 수정 비밀번호
/* 공통 필수 시작 */
function Form_OnLoadCompleted(obj) {
    gfn_Form_OnLoadCompleted(obj); // 공통 폼 로딩 이벤트 처리

    // 폼 환경설정
    ls_SystemGubun = gs_SystemGubun;
    ls_SystemMenuId = gs_SystemMenuId;
    var ls_FormId = split(sFormUrl, "::");
    ls_FormId = split(ls_FormId[1], ".");
    obj.Title = sFormTitle + "(" + ls_FormId[0] + ")";

    gfn_MakeRequiredInputMark(this); // 필수 입력란을 설정한다.

    if (gds_oUserInfo.GetColumn(0, "PDEPTCD") == "0243") { // 권한에 따른 버튼 설정
        lfn_SetFormAuth(true); // 저장 권한
        //gfn_MakeQitemes(50, true); // 정답 입력란을 50개, 활성으로 생성한다.        
        
        gfn_MakeQitemes100(100, false); // 정답 입력란을 100개, 비활성으로 생성한다.
    } else {
        lfn_SetFormAuth(false); // 읽기 권한
        //gfn_MakeQitemes(50, false); // 정답 입력란을 50개, 비활성으로 생성한다.
        
        gfn_MakeQitemes100(100, false); // 정답 입력란을 100개, 비활성으로 생성한다.
        lbPassword.Visible = true;
        edPassword.Visible = true;
    }
    // 공통 코드 조회
    gfn_SearchTrainingCourse("E", ds_oCourse, "Y", "전체"); // 직능 코드 조회
    cbxCourse.Value = "22";    
    
    // 정답 항목들 길이 제한.
    for (var iQitemNumber = 0; iQitemNumber < 50; iQitemNumber++) {   
        object("ed" + ms_MAKE_QITEMES_EDIT_NAME + "Ans" + toString(iQitemNumber+1)).MaxLength = 4;
        object("ed" + ms_MAKE_QITEMES_EDIT_NAME + "Cnt" + toString(iQitemNumber+1)).MaxLength = 1;
        object("ed" + ms_MAKE_QITEMES_EDIT_NAME + "Pnt" + toString(iQitemNumber+1)).MaxLength = 2;
    }
}

function Form_OnKeyDown(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam) {
    // to do

    gfn_Form_OnKeyDown(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam); // 공통 키 이벤트 처리
}

function Form_OnSize(obj,nCx,nCy,nState)
{
    if (gs_IsResizingForm == "Y") {
        gdLoadedAnswer.Height = nCy - gdLoadedAnswer.Top - 4;
    }
}

/*
 * 기능 : 조회
 */
function lfn_Search(obj) {
    if (gds_oUserInfo.GetColumn(0, "PDEPTCD") != "0243") {
        if (gfn_IsNull(edPassword.Text)) {
            gfn_Alert("정답 편집 권한용 암호를 입력하여주십시오.");
            edPassword.SetFocus();
            return;
        }
        if (edPassword.Text != gfn_Encode(left(GetDate(), 8))) {
            gfn_Alert("정답 편집 권한용 암호가 틀립니다. 다시 입력해 주십시오.");
            edPassword.SetFocus();
            return;
        }
    }
    
    lfn_SetFormAuth(true); // 저장 권한    
	if (cbxCourse.Value == "10") {	// 특급이면 100개 활성화
	
		gfn_EnableQitemes(true, 100); // 정답 입력란을 100개, 활성화한다.
		
	} else {
	
		gfn_EnableQitemes(true, 50); // 정답 입력란을 50개, 활성화한다.
		
	}
/*    
    for (var iQitemNumber = 0; iQitemNumber < 50; iQitemNumber++) {   
        object("ed" + ms_MAKE_QITEMES_EDIT_NAME + "Cnt" + toString(iQitemNumber+1)).Enable = false;
    }    
*/    
    lfn_InitDsioExcelDelete();
    
    ds_ioLoadedAnswer.ClearData(); // 데이타셋 초기화    
    
    transaction("select",
                "SvcLdb::searchCorrectAnswer.xml",
                "",
                "ds_ioLoadedAnswer=output",
                "EPCOURSECD=" + quote(cbxCourse.Value),  // args 
                "lfn_AfterSearchAnswer"); 
}

/*
 * 기능 : 추가
 */
function lfn_Input(obj) {
	if (!gfn_Confirm(MSGI00070)) { // 신규 추가 하시겠습니까?\n입력완료 후 저장 버튼을 누르세요.
        return false;
	}

	mb_IsNewData = true; // 저장버튼이 클릭 설정
    lfn_Cancel(); // 값 초기화

    // 기본값 설정
}



/*
 * 기능 : 저장
 */
function lfn_Save(obj) {/*
    if (!gfn_FormValidate(this)) { // 입력 Validation Check
        return false;
    }
*/    
    if (!gfn_Confirm(MSGW00040)) { // 정말로 수정 저장 하시겠습니까?
        return false;
    }
    
    if (ToNumber(edRemainPoint.Text) < 100) {
        gfn_Alert("총점이 100점 미만입니다.\n문항당 배점을 다시한번 확인해 주십시오.");
        return false;
    }
    
    if (ToNumber(edRemainPoint.Text) > 100) {
        gfn_Alert("총점이 100점을 초과했습니다.\n문항당 배점을 다시한번 확인해 주십시오.");
        return false;
    }

    if (ds_ioExcelDetail.GetCount() < 1) { // 로딩할 데이타가 없는 경우
        gfn_Alert("저장할 정답이 없습니다.");
        return false;
    } else { // 저장 실행
        
        var ln_RowNo = "";
        ds_ioExcelCopy.ClearData();
        ds_ioExcelCopy.ApplyChange();

		// 특급소방안전관리자 2과목이면
		if (ds_ioLoadedAnswer.GetColumn(ds_ioLoadedAnswer.currow, "EPCOURSECD") == "10" 
		    && ds_ioLoadedAnswer.GetColumn(ds_ioLoadedAnswer.currow, "EPSUBJECTGUBUN") == "2") {
        			for (var iQitemNumber = 0; iQitemNumber < 50; iQitemNumber++) {   	
				ln_RowNo = ds_ioExcelCopy.AddRow();                        	
				ds_ioExcelCopy.SetColumn(ln_RowNo, "CACORRECTANSWER", object("ed" + ms_MAKE_QITEMES_EDIT_NAME + "Ans" + toString(iQitemNumber+51)).Value);	
				ds_ioExcelCopy.SetColumn(ln_RowNo, "CACORRECTCNT", object("ed" + ms_MAKE_QITEMES_EDIT_NAME + "Cnt" + toString(iQitemNumber+51)).Value);	
				ds_ioExcelCopy.SetColumn(ln_RowNo, "CAITEMSCORE", object("ed" + ms_MAKE_QITEMES_EDIT_NAME + "Pnt" + toString(iQitemNumber+51)).Value);	
				ds_ioExcelCopy.SetColumn(ln_RowNo, "CAITEMNO", iQitemNumber+51);	
			}
			
		// 1,2,3급 소방안전관리자 2과목이면
		} else if ((ds_ioLoadedAnswer.GetColumn(ds_ioLoadedAnswer.currow, "EPCOURSECD") == "21" ||
		            ds_ioLoadedAnswer.GetColumn(ds_ioLoadedAnswer.currow, "EPCOURSECD") == "22" ||
		            ds_ioLoadedAnswer.GetColumn(ds_ioLoadedAnswer.currow, "EPCOURSECD") == "23")
		         && ds_ioLoadedAnswer.GetColumn(ds_ioLoadedAnswer.currow, "EPSUBJECTGUBUN") == "2") {
        			for (var iQitemNumber = 0; iQitemNumber < 50; iQitemNumber++) {   	
				ln_RowNo = ds_ioExcelCopy.AddRow();                        	
				ds_ioExcelCopy.SetColumn(ln_RowNo, "CACORRECTANSWER", object("ed" + ms_MAKE_QITEMES_EDIT_NAME + "Ans" + toString(iQitemNumber+26)).Value);	
				ds_ioExcelCopy.SetColumn(ln_RowNo, "CACORRECTCNT", object("ed" + ms_MAKE_QITEMES_EDIT_NAME + "Cnt" + toString(iQitemNumber+26)).Value);	
				ds_ioExcelCopy.SetColumn(ln_RowNo, "CAITEMSCORE", object("ed" + ms_MAKE_QITEMES_EDIT_NAME + "Pnt" + toString(iQitemNumber+26)).Value);	
				ds_ioExcelCopy.SetColumn(ln_RowNo, "CAITEMNO", iQitemNumber+26);	
			}
			
		// 위관,통합자 2과목이면
		} else if ((ds_ioLoadedAnswer.GetColumn(ds_ioLoadedAnswer.currow, "EPCOURSECD") == "24" ||
		            ds_ioLoadedAnswer.GetColumn(ds_ioLoadedAnswer.currow, "EPCOURSECD") == "29")
		         && ds_ioLoadedAnswer.GetColumn(ds_ioLoadedAnswer.currow, "EPSUBJECTGUBUN") == "2") {
        			for (var iQitemNumber = 0; iQitemNumber < 80; iQitemNumber++) {   	
				ln_RowNo = ds_ioExcelCopy.AddRow();                        	
				ds_ioExcelCopy.SetColumn(ln_RowNo, "CACORRECTANSWER", object("ed" + ms_MAKE_QITEMES_EDIT_NAME + "Ans" + toString(iQitemNumber+26)).Value);	
				ds_ioExcelCopy.SetColumn(ln_RowNo, "CACORRECTCNT", object("ed" + ms_MAKE_QITEMES_EDIT_NAME + "Cnt" + toString(iQitemNumber+26)).Value);	
				ds_ioExcelCopy.SetColumn(ln_RowNo, "CAITEMSCORE", object("ed" + ms_MAKE_QITEMES_EDIT_NAME + "Pnt" + toString(iQitemNumber+26)).Value);	
				ds_ioExcelCopy.SetColumn(ln_RowNo, "CAITEMNO", iQitemNumber+41);	
			}
		
		} else {
		
			for (var iQitemNumber = 0; iQitemNumber < 50; iQitemNumber++) {   	
				ln_RowNo = ds_ioExcelCopy.AddRow();                        	
				ds_ioExcelCopy.SetColumn(ln_RowNo, "CACORRECTANSWER", object("ed" + ms_MAKE_QITEMES_EDIT_NAME + "Ans" + toString(iQitemNumber+1)).Value);	
				ds_ioExcelCopy.SetColumn(ln_RowNo, "CACORRECTCNT", object("ed" + ms_MAKE_QITEMES_EDIT_NAME + "Cnt" + toString(iQitemNumber+1)).Value);	
				ds_ioExcelCopy.SetColumn(ln_RowNo, "CAITEMSCORE", object("ed" + ms_MAKE_QITEMES_EDIT_NAME + "Pnt" + toString(iQitemNumber+1)).Value);	
				ds_ioExcelCopy.SetColumn(ln_RowNo, "CAITEMNO", iQitemNumber+1);	
			}		
		
		
		}
        
        EncodeDS(ds_ioExcelCopy, "CAITEMNO", "CACORRECTANSWER", ds_ioExcelCopyTmp);	//정답 암호화
                
        transaction("Save",
                    "SvcLdb::updateCorrectAnswer.xml",
