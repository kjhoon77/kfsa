				object("ed" + ms_MAKE_QITEMES_EDIT_NAME + "Ans" + toString(iQitemNumber+41)).Value = ds_ioExcelDetail.getColumn(iQitemNumber, "CACORRECTANSWER");		
				object("ed" + ms_MAKE_QITEMES_EDIT_NAME + "Cnt" + toString(iQitemNumber+41)).Value = ds_ioExcelDetail.getColumn(iQitemNumber, "CACORRECTCNT");		
				object("ed" + ms_MAKE_QITEMES_EDIT_NAME + "Pnt" + toString(iQitemNumber+41)).Value = ds_ioExcelDetail.getColumn(iQitemNumber, "CAITEMSCORE");
	
		}
		
		
