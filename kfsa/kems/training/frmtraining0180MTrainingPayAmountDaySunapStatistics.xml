}

/*
 * 기능 : 조회
 */
function lfn_Search(obj) {
    if (!gfn_FormValidate(this)) { // 조회 Validation Check
        return false;
    }
    if (calDateStart.Value > calDateEnd.Value) {
        gfn_Alert("시작일자가 종료일자보다 늦습니다.");
        calDateStart.SetFocus();
        return false;
    }
    
    medTotalCnt.Value = "";
    
    ds_oTrainingPayAmount.ClearData(); // 데이타셋 초기화
    
    // 조회이력을 저장한다.  
    ls_HistoryArg = "지부=" + quote(cbxJibu.Text)
                + ",직능=" + quote(cbxCourse.Text)
                + ",처리일자시작=" + quote(left(calDateStart.Value, 8))
                + ",처리일자종료=" + quote(left(calDateEnd.Value, 8))
                + ",정렬방법=" + quote(radSort.Text);
    gfn_InsertSearchPrintHistory(
        "S"
        , this.ID
        , global.mnuLeft.cbxOpenForm.Text
        , ls_HistoryArg
    );

    tit_clearActionInfo();
	tit_addCsvSearchActionInfo("training:searchTrainingPayAmountDaySunapStat", false);
	// 서버 호출 
	tit_callService(
		""
		,""
		,""	// inDs
		,"ds_oTrainingPayAmount=ds_oTrainingPayAmount"	// outDs
		,"TOMGGTMGNO=" + quote(cbxJibu.Value)
            + " TOTCCOURSECD=" + quote(cbxCourse.Value)
            + " TSPROCDATESTART=" + quote(left(calDateStart.Value, 8))
            + " TSPROCDATEEND=" + quote(left(calDateEnd.Value, 8))
            + " TJPERSONNM=" + quote(edPersonNm.Text)
            + " SORT=" + quote(radSort.Value)
            + " GUBUN=" + quote(radGubun.Value)
            + " COURSEGUBUN=T"// args 
		,"lfn_AfterSearchTrainingJubsu"
		,false
	);
}

