    // to do Validation Check
    if (ds_oTrainingPayAmount.RowCount() < 1) {
        gfn_Alert("출력할 데이터가 존재하지 않습니다.");
        return false;
    }
    // 출력이력을 저장한다.  
     ls_HistoryArg = "지부=" + quote(cbxJibu.Text)
                + ",직능=" + quote(cbxCourse.Text)
                + ",처리일자시작=" + quote(left(calDateStart.Value, 8))
                + ",처리일자종료=" + quote(left(calDateEnd.Value, 8))
                + ",정렬방법=" + quote(radSort.Text);
    gfn_InsertSearchPrintHistory(
        "P"
        , this.ID
        , global.mnuLeft.cbxOpenForm.Text
        , ls_HistoryArg
    );
    
    gfn_CallReportPopup("frmtraining0180R01TrainingPayAmountDaySunapStatistics.rex"
                      , "ds_oTrainingPayAmount"
                      , "TCCOURSENM=" + cbxCourse.Text);
}
