function btnToExcel_OnClick(obj) {
    if (ds_oHealthQuestion.GetRowCount() < 1) {
        gfn_Alert("출력할 데이터가 존재하지 않습니다.");
        return false;
    }
	// 개인정보 다운로드 사유 등록
	var parValue = "sCode=1118";	// COMMON.PERSONALDOWNLOADH
		parValue += " sRowCnt=" + ds_oHealthQuestion.GetRowCount();
		parValue += " sTitle=" + quote(gdHealth.UserData);
	var ls_IsRemark = Dialog("COM::frmCOM2100PDownloadRemark.xml", parValue, "", "", "", -1, -1);	

	if (ls_IsRemark == "" || ls_IsRemark == null || ls_IsRemark == "FAIL") {
		return false;
	}
	lfn_Excel(obj);

}

//	업무구분 변경
function radBizGubun_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{
	ds_oHealthQuestion.ClearData();
	if (strCode == "0") {	// 강습교육
		gfn_SearchTrainingCourse("T", ds_oCourse, "Y", ""); // 직능 코드 조회		cbxCourse.Value = "22";
		lbEduDate.Text = "교육시작일자";
		gdHealth.SetCellProp("head", 7, "text", "교육기간");
	
	} else if (strCode == "1") { // 시험
		gfn_SearchTrainingCourse("E", ds_oCourse, "Y", ""); // 직능 코드 조회		cbxCourse.Value = "22";
		lbEduDate.Text = "시험일자";
		gdHealth.SetCellProp("head", 7, "text", "시험일자");
	
	} else if (strCode == "2") { // 실무교육

		gfn_SearchCustomerEduCourseFullName(ds_oCourse, "0", "");	// 대표고객직능 조회
		cbxCourse.Value = "10";
		lbEduDate.Text = "교육일자";
		gdHealth.SetCellProp("head", 7, "text", "교육일자");
	
	}
	
}


