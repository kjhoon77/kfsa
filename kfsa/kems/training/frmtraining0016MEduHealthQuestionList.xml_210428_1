function radSearchGubun_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{
	if (strCode == "ORDER") {
        edOrderStart.Enable = true;
        edOrderEnd.Enable = true;
        edOrderStart.Text = "001";
        edOrderEnd.Text = "100";
        calDateStart.Value = "";
        calDateStart.Enable = false;
        edOrderStart.UserData = "S:시작회차";
        edOrderEnd.UserData = "S:종료회차";
        calDateStart.UserData = "";

	} else {
        edOrderStart.Value = "";
        edOrderEnd.Value = "";
        edOrderStart.Enable = false;
        edOrderEnd.Enable = false;
        calDateStart.Enable = true;
        calDateStart.Value = left(GetDate(), 8);
        calDateStart.UserData = "S:시작일자";
        edOrderStart.UserData = "";
        edOrderEnd.UserData = "";
	}
}

