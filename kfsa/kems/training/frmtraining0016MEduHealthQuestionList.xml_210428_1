 * 기능 : 출력
 */
function lfn_Print(obj)
{
    // 출력이력을 저장한다.  
    ls_HistoryArg = "지부=" + quote(cbxJibu.Text)
                + ",년도=" + quote(edYear.Text)
                + ",직능=" + quote(cbxCourse.Text)
                + ",회차시작=" + quote(edOrderStart.Text)
                + ",회차종료=" + quote(edOrderEnd.Text)
                + ",교육(시험)일자=" + quote(calDateStart.Value);
    gfn_InsertSearchPrintHistory(
        "P"
        , this.ID
        , global.mnuLeft.cbxOpenForm.Text
        , ls_HistoryArg
    );
    
    
    //gfn_CallReportPopup("frmtraining0030R01ExamJubsuList.rex", "ds_oExamJubsu", "");
}
