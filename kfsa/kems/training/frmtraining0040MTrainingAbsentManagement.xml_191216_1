function lfn_AfterSearchTrainingAbsent(nErrorCode, strErrorMsg) {
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }
    /*
    if (ds_ioTrainingAbsent.GetCount() < 1) {
        gfn_Alert("출결자료가 존재하지 않습니다.\n확인하시고 출결자료를 생성하여 주십시오.");
        return;
    }*/

	if (ds_ioTrainingAbsent.GetColumn(0, "TOMUNJEGUBUN") = "C") {		// 혼합과정일 경우 1일차 안내

		ds_oAbsentDateGubun.SetColumn(0, "DATA", "사이버");
		
	} else {
	
		ds_oAbsentDateGubun.SetColumn(0, "DATA", "1일차");
	
	}
//	alert(ds_ioTrainingAbsent.GetColumn(0, "TOMUNJEGUBUN"));
//	alert(ds_oAbsentDateGubun.savexml());

    // 직능별 교육일차
	if (ds_ioTrainingAbsent.GetColumn(0, "TOTCCOURSECD") == "61") {
        radAbsentDateGubun.Width = 110;
        radAbsentDateGubun.InnerDataset = "ds_oAbsentDateGubun1";
	} else if (ds_ioTrainingAbsent.GetColumn(0, "TOTCCOURSECD") == "28") {
        radAbsentDateGubun.Width = 221;
        radAbsentDateGubun.InnerDataset = "ds_oAbsentDateGubun2";
	} else if (ds_ioTrainingAbsent.GetColumn(0, "TOTCCOURSECD") == "22" && ds_ioTrainingAbsent.GetColumn(0, "TOMUNJEGUBUN") == "Y"
               || ds_ioTrainingAbsent.GetColumn(0, "TOTCCOURSECD") == "23" 
               
               || ds_ioTrainingAbsent.GetColumn(0, "TOTCCOURSECD") == "24" 
               
               || ds_ioTrainingAbsent.GetColumn(0, "TOTCCOURSECD") == "50") {
        radAbsentDateGubun.Width = 313;
        radAbsentDateGubun.InnerDataset = "ds_oAbsentDateGubun3";
	} else if (ds_ioTrainingAbsent.GetColumn(0, "TOTCCOURSECD") == "22" && ds_ioTrainingAbsent.GetColumn(0, "TOMUNJEGUBUN") == "N"
               || ds_ioTrainingAbsent.GetColumn(0, "TOTCCOURSECD") == "29") {
        radAbsentDateGubun.Width = 407;       
        radAbsentDateGubun.InnerDataset = "ds_oAbsentDateGubun4";
	} else if (ds_ioTrainingAbsent.GetColumn(0, "TOTCCOURSECD") == "20"
               || ds_ioTrainingAbsent.GetColumn(0, "TOTCCOURSECD") == "21"
               || ds_ioTrainingAbsent.GetColumn(0, "TOTCCOURSECD") == "60") {
        radAbsentDateGubun.Width = 500;
        radAbsentDateGubun.InnerDataset = "ds_oAbsentDateGubun";
	} else if (ds_ioTrainingAbsent.GetColumn(0, "TOTCCOURSECD") == "10") {
        radAbsentDateGubun.Width = 650;
        radAbsentDateGubun.InnerDataset = "ds_oAbsentDateGubun5";
       
    }
	
	medTotalCnt.Value = ds_ioTrainingAbsent.GetCount();
    medEduDateStart.Value = ds_ioTrainingAbsent.GetColumn(0, "TOSTARTDATE");
    medEduDateEnd.Value = ds_ioTrainingAbsent.GetColumn(0, "TOENDDATE");
    
    if (ds_ioTrainingAbsent.GetColumn(0, "TOMUNJEGUBUN") = "C") {		// 혼합과정일 경우 1일차 안내
    
		lbCyberNotice.Visible = true;
		
    } else {
    
		lbCyberNotice.Visible = false;
    }
    
    gfn_SearchMessage(gdTrainingAbsent); // 조회 메세지를 처리하고 그리드의 정렬 헤드 마크를 제거한다.
}

