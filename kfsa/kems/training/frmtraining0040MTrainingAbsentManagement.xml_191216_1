/*
 * 기능 : 최초 출석부 생성저장
 */
var flag = 0;
function lfn_SaveAttendance(obj) {
    if (!gfn_FormValidate(this)) { // 조회 Validation Check
        return false;
    }    
    
    ds_oTrainingJubsuCnt.ClearData(); // 데이타셋 초기화    
    // 조회이력을 저장한다.  
    ls_HistoryArg = "지부=" + quote(cbxJibu.Text)
                + ",직능=" + quote(cbxCourse.Text)
                + ",년도=" + quote(edTrainingYear.Text)
                + ",회차=" + quote(edTrainingOrder.Text);
    gfn_InsertSearchPrintHistory(
        "S"
        , this.ID
        , global.mnuLeft.cbxOpenForm.Text
        , ls_HistoryArg
    );
    tit_clearActionInfo();
	tit_addSearchActionInfo("training:searchTrainingJubsuPersonCnt", false);
	// 서버 호출 
	tit_callService(
		""
		,""
		,""	// inDs
		,"ds_oTrainingJubsuCnt=ds_oTrainingJubsuCnt"	// outDs
		,"TOMGJIBUCD=" + quote(cbxJibu.Value)
            + " TCCOURSECD=" + quote(cbxCourse.Value)
            + " TOYEAR=" + quote(edTrainingYear.Text)
            + " TOTRAININGORDER=" + quote(edTrainingOrder.Text)// args 
		,""
		,false
		,""
		,true
	);	

	if (ds_oTrainingJubsuCnt.GetCount() < 1) {
        gfn_Alert("해당회차에 접수자가 존재하지 않습니다.");
        return;
	}
	
	if (ds_ioTrainingAbsent.GetCount() < 1) {
        if (!gfn_Confirm("출석부를 생성 하시겠습니까?")) {
            return false;
        }
	}
	
	if (ds_ioTrainingAbsent.GetCount() >= 1) {
        if (!gfn_Confirm("출석부를 추가생성 하시겠습니까?")) {
            return false;
        }
	}
/*
	if (ds_ioTrainingAbsent.GetCount() >= 1) {
        gfn_Alert("이미 생성되어 있습니다.");
        return;
	}	
*/
	// 직능별 교육일차
	if (ds_oTrainingJubsuCnt.GetColumn(0, "TOTCCOURSECD") == "61"	// 소방기술세미나
	
		|| ds_oTrainingJubsuCnt.GetColumn(0, "TOTCCOURSECD") == "51") {	// 심폐소생술
        flag = 1;
	} else if (ds_oTrainingJubsuCnt.GetColumn(0, "TOTCCOURSECD") == "28") {
        flag = 2;
	} else if (ds_oTrainingJubsuCnt.GetColumn(0, "TOTCCOURSECD") == "22" && ds_oTrainingJubsuCnt.GetColumn(0, "TOMUNJEGUBUN") == "Y"
               || ds_oTrainingJubsuCnt.GetColumn(0, "TOTCCOURSECD") == "23"
               
               || ds_oTrainingJubsuCnt.GetColumn(0, "TOTCCOURSECD") == "24"
                
               || ds_oTrainingJubsuCnt.GetColumn(0, "TOTCCOURSECD") == "50") {
        flag = 3;
	} else if (ds_oTrainingJubsuCnt.GetColumn(0, "TOTCCOURSECD") == "22" && ds_oTrainingJubsuCnt.GetColumn(0, "TOMUNJEGUBUN") == "N"
               || ds_oTrainingJubsuCnt.GetColumn(0, "TOTCCOURSECD") == "29") {
        flag = 4;
	} else if (ds_oTrainingJubsuCnt.GetColumn(0, "TOTCCOURSECD") == "20"
               || ds_oTrainingJubsuCnt.GetColumn(0, "TOTCCOURSECD") == "21"
               || ds_oTrainingJubsuCnt.GetColumn(0, "TOTCCOURSECD") == "60") {
        flag = 5;
	} else if (ds_oTrainingJubsuCnt.GetColumn(0, "TOTCCOURSECD") == "10") {
        flag = 10;
        
    }
        
    tit_clearActionInfo();
	tit_addSingleActionInfo("training:insertTrainingAbsent"
        , ""
        , 0
        , ""
    );
	// 서버 호출 
	tit_callService(
		""
		,""
		,""	// inDs
		,""	// outDs
		,"TOMGJIBUCD=" + quote(cbxJibu.Value)
            + " TCCOURSECD=" + quote(cbxCourse.Value)
            + " TOYEAR=" + quote(edTrainingYear.Text)
            + " TOTRAININGORDER=" + quote(edTrainingOrder.Text)
            + " FLAG=" + quote(flag)// args 
		,"lfn_AfterSaveTrainingAbsent"
		,false
	);
}
