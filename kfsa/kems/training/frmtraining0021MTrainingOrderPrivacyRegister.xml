/* 사용자 이벤트 함수 */
function btnMutilSort_OnClick(obj) {
	gfn_GridMultiSort(gdTrainingJubsu, 0); // 멀티 정렬을 실행한다.
}

/* 회차 포커스 아웃될때 자동 0 입력 */
function edTrainingOrder_OnKillFocus(obj) {
	gfn_ComponentOnKillFocus(obj);

    if (obj.Text != "") {
        obj.Text = iif(gfn_IsNull(obj.Text), "", lpad(obj.Text, "0", 3));
    }
}

function edTrainingOrderEnd_EnterSearch(obj,nChar,bShift,bCtrl, bAlt,LLParam,HLParam)
{
	if (nChar == 13) { // Enter Key
        edTrainingOrderEnd.Text = iif(gfn_IsNull(edTrainingOrderEnd.Text), "", lpad(edTrainingOrderEnd.Text, "0", 3));
        lfn_Search();
    }
}

function btnToExcel_OnClick(obj) {
    if (ds_oTrainingJubsu.GetRowCount() < 1) {
        gfn_Alert("출력할 데이터가 존재하지 않습니다.");
        return false;
    }
	lfn_Excel(obj);
}

// 더블클릭하여 개인정보동의서 등록하기
function gdTrainingJubsu_OnCellDblClick(obj,nRow,nCell,nX,nY,nPivotIndex)
{

	li_rowpos = nRow;	// 선택한 row위치 memory

	var sArg = "sWorkGubun='01'"	// 강습관리
	
	    + " sBizGubun='011'" 		// 강습접수

		+ " sCourse=" + quote(ds_oTrainingJubsu.GetColumn(nRow, "TOTCCOURSECD"))
		
		+ " sPersonNM=" + quote(ds_oTrainingJubsu.GetColumn(nRow, "TJPERSONNM"))
		
		+ " sBirthday=" + quote(ds_oTrainingJubsu.GetColumn(nRow, "TJBIRTHDAY"))
	
		+ " sBizMgno1=" + quote(ds_oTrainingJubsu.GetColumn(nRow, "TJMGNO"));

	var arrPrivacy = Dialog("COM::frmCOM6100PPrivacyManage.xml", sArg, 624, 456, "", -1, -1);
	
	if (arrPrivacy) {
		lfn_Search();
	}
}

// 등록하기 버튼클릭하여 개인정보동의서 등록하기
function gdTrainingJubsu_OnCellClick(obj,nRow,nCell,nX,nY,nPivotIndex)
{

	if (nCell == 10) {
		ds_oTrainingJubsu.row = nRow;	
		gdTrainingJubsu_OnCellDblClick(obj,nRow,nCell,nX,nY,nPivotIndex);
    }
}
