    // 조회이력을 저장한다.  
    ls_HistoryArg = "지부=" + quote(cbxJibu.Text)
                + ",직능=" + quote(cbxCourse.Text)
                + ",년도=" + quote(edTrainingYear.Text)
                + ",회차시작=" + quote(edTrainingOrderStart.Text)
                + ",회차종료=" + quote(edTrainingOrderEnd.Text)
                + ",수납여부=" + quote(radRepayYn.Text)
                + ",수료여부=" + quote(radPassYn.Text);
                
    gfn_InsertSearchPrintHistory(
        "S"
        , this.ID
        , global.mnuLeft.cbxOpenForm.Text
        , ls_HistoryArg
    );
    
    course = cbxCourse.Text;  // 레포트에 직능이름 보내는 파라메터값

    tit_clearActionInfo();
	tit_addCsvSearchActionInfo("training:searchTrainingJubsuPrivacy", false);
	// 서버 호출 
	tit_callService(
		""
		,""
		,""	// inDs
		,"ds_oTrainingJubsu=ds_oTrainingJubsu"	// outDs
		,"PCMAINCD='11'"
		
		    + " TOMGGTMGNO=" + quote(cbxJibu.Value)
            + " TOTCCOURSECD=" + quote(cbxCourse.Value)
            + " TOYEAR=" + quote(edTrainingYear.Text)
            + " TOTRAININGORDERSTART=" + quote(edTrainingOrderStart.Text)
            + " TOTRAININGORDEREND=" + quote(edTrainingOrderEnd.Text)
            + " TJLASTSUNAPGUBUN=" + quote(radRepayYn.Value)
            + " TJPASSYN=" + quote(radPassYn.Value)
             
            + " REGISTERYN=" + quote(radRegisterYn.Value) // args 
		,"lfn_AfterSearchTrainingJubsuPrivacy"
		,false
	);
}


/* 공통 필수 종료 */

/* 사용자 함수 */
/*
 * 기능 : 결강자 조회 후 처리해야 할 내용
 * parameter : nErrorCode - 에러코드
 *             strErrorMsg - 에러메시지
 * Return : 없음
 */
function lfn_AfterSearchTrainingJubsuPrivacy(nErrorCode, strErrorMsg) {
    if (nErrorCode != 0) { // 트랜잭션 에러를 처리한다.
        gfn_ErrorMsg(nErrorCode, strErrorMsg);
        return;
    }

    gfn_SearchMessage(gdTrainingJubsu); // 조회 메세지를 처리하고 그리드의 정렬 헤드 마크를 제거한다.
    
    medJubsuCnt.Value = ds_oTrainingJubsu.RowCount();
    
    ds_oTrainingJubsu.rowposition = li_rowpos;	// 선택했던 커서로 이동
    
    li_rowpos = 0; // 커서 위치 memory 초기화
    
}


