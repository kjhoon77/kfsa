//조회
function lfn_Search(TPMGNO)
{
    
    
    ds_ProfesList.ClearData(); // 데이타셋 초기화

    tit_clearActionInfo();
	tit_addCsvSearchActionInfo("profes:searchPFM0007History", false);
	
	// 서버 호출
 	var sParm = "";
	sParm  = " THMGNO=" + quote(TPMGNO);   
	
	//alert(sParm);
	// 서버 호출 
	tit_callService(
		""
		,""
		,""	// inDs
		,"ds_ProfesList=ds_oProfesList"	// outDs
		, sParm  // args 
		,"afterSearch"
		,false
	);
}

